      <TableDataSource Name="Table23" Alias="GUARANTOR_list" DataType="System.Int32" Enabled="true" SelectCommand="select G.BID_ID, listagg(fio, ', ') WITHIN GROUP(ORDER BY BID_ID) as fio,&#13;&#10;         listagg(fio_r, ', ') WITHIN GROUP(ORDER BY BID_ID) as fio_r,               &#13;&#10;       count(*) count_g&#13;&#10;from ( select bg.bid_id,  &#13;&#10;              wcs_utl.get_answ_text(bg.bid_id, 'CL_1', bg.ws_id, bg.garantee_num) || ' ' ||&#13;&#10;              wcs_utl.get_answ_text(bg.bid_id, 'CL_2', bg.ws_id, bg.garantee_num) || ' ' ||&#13;&#10;              wcs_utl.get_answ_text(bg.bid_id, 'CL_3', bg.ws_id, bg.garantee_num) as fio,&#13;&#10;              wcs_utl.get_answ_text(bg.bid_id, 'CL_123_R', bg.ws_id, bg.garantee_num) as fio_r&#13;&#10;       from   v_wcs_bid_garantees bg&#13;&#10;       where  bg.bid_id = :p_bid_id&#13;&#10;         and  bg.garantee_id in('GUARANTOR')&#13;&#10;     )g&#13;&#10;group by BID_ID">
