      <TableDataSource Name="Table8" Alias="program" DataType="System.Int32" Enabled="true" SelectCommand="select  wcs_utl.get_answ_refer(b.id, 'OB22') as ob22,&#13;&#10;        case when wcs_utl.get_answ_refer(b.id, 'OB22') in('220257', '220258', '220347', '220348') &#13;&#10;             then 1&#13;&#10;             else 0 end as is_state_budget,&#13;&#10;        wcs_utl.get_answ_bool(b.id, 'COMPENSATION_PRG_AVALIABLE') as COMPENSATION_PRG_AVALIABLE,&#13;&#10;        wcs_utl.get_answ_list(b.id, 'COMPENSATION_PRG_TYPE') as COMPENSATION_PRG_TYPE,&#13;&#10;--        decode(wcs_utl.get_answ_list(b.id,'COMPENSATION_PRG_TYPE'),1,wcs_utl.get_answ_list(b.id, 'PRG_REGION_NAME_1'),'Кабінет Міністрів') as PRG_REGION_TYPE,&#13;&#10;        (select listagg(EVENT_NAME, ', ') WITHIN GROUP(ORDER BY EVENT_NAME) as list_EVENT_NAME&#13;&#10;         from   (select distinct ER.EVENT_NAME from BARS.WCS_ANSWERS a,VW_ESCR_REF er &#13;&#10;                 where  ER.GOOD_ID=a.val_refer and&#13;&#10;                        a.BID_ID=:p_bid_id and&#13;&#10;                        a.QUESTION_ID like 'PRG_ADR___GOOD_TYPE%'&#13;&#10;                )x&#13;&#10;        ) as  list_EVENT_NAME       &#13;&#10;  from wcs_bids b&#13;&#10; where b.id = :p_bid_id">
