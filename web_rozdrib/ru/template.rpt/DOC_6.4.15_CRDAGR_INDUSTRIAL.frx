      <TableDataSource Name="Table12" Alias="key_mortgage" DataType="System.Int32" Enabled="true" SelectCommand="  select (case &#13;&#10;           when (select decode(count(*),0,0,1) from v_wcs_bid_garantees where bid_id = :p_bid_id and garantee_id = 'MORTGAGE' and type_id = 0 and subj_obu_id in (25101,25103)) = 1&#13;&#10;            and (select decode(count(*),0,0,1) from v_wcs_bid_garantees where bid_id = :p_bid_id and garantee_id = 'MORTGAGE_LAND' and type_id = 0 and subj_obu_id in (37101,37102,37105)) = 0&#13;&#10;             then 1&#13;&#10;           when (select decode(count(*),0,0,1) from v_wcs_bid_garantees where bid_id = :p_bid_id and garantee_id = 'MORTGAGE' and type_id = 0 and subj_obu_id in (25101,25103,25102)) = 0&#13;&#10;            and (select decode(count(*),0,0,1) from v_wcs_bid_garantees where bid_id = :p_bid_id and garantee_id = 'MORTGAGE_LAND' and type_id = 0 and subj_obu_id in (37101,37102,37105)) = 1&#13;&#10;             then 2 &#13;&#10;           when (select decode(count(*),0,0,1) from v_wcs_bid_garantees where bid_id = :p_bid_id and garantee_id = 'MORTGAGE' and type_id = 0 and subj_obu_id in (25102)) = 1&#13;&#10;            and (select decode(count(*),0,0,1) from v_wcs_bid_garantees where bid_id = :p_bid_id and garantee_id = 'MORTGAGE_LAND' and type_id = 0 and subj_obu_id in (37102,37105)) = 1&#13;&#10;             then 3&#13;&#10;          end) as key,&#13;&#10;          (select count(1) from v_wcs_bid_garantees where bid_id = :p_bid_id) as key_garantees&#13;&#10;           from dual">
