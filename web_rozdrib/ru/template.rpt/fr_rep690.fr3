    <TfrxODACQuery Name="q" UserName="q" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="           select  o.nlsb, o.nam_b ,        sum(CASE &#13;&#10;                                                   WHEN p.tt='$P9'and p.dk = 0 THEN p.s/100&#13;&#10;                                                   ELSE 0&#13;&#10;                                                END) as kol,&#13;&#10;                                            sum(CASE &#13;&#10;                                                   WHEN p.tt='$PK'and p.dk = 0 THEN p.s/100&#13;&#10;                                                   ELSE 0&#13;&#10;                                                END)   as sum, K.CP_ID, K.CENA_KUP  &#13;&#10;            from oper o, opldok p, operw w, cp_kod k&#13;&#10;            where o.vdat = :fdat and&#13;&#10;                  p.fdat = :fdat and&#13;&#10;                  p.sos  = 5&#13;&#10;              and o.tt in ('$PK')&#13;&#10;              and o.ref = p.ref &#13;&#10;              and o.ref = w.ref and w.tag = 'KODCP'&#13;&#10;              and to_number(value) = K.ID &#13;&#10;              and (exists (select 1 from accounts where nbs = '2801' and ob22 = '03' and acc = p.acc and p.dk = 0)&#13;&#10;                or exists (select 1 from accounts where nbs = '9812' and ob22 = '66' and acc = p.acc and p.dk = 0))&#13;&#10;              group by  o.nlsb, o.nam_b , K.CP_ID, K.CENA_KUP&#13;&#10;              order by  o.nlsb                                       " PropData=""/>
