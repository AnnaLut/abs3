    <OracleDataConnection Name="OBMMFO6_DEV" ConnectionString="rijcmlqHytUMhhPg9gG4fal5Q6rRB0BvRbh0nDbCWvtDA1bLoXA5F1HMEY1qSyYCzmMlYGK5S75auXYOG6vOu2k3E2pQCShlcDV/bjmNdvrfE7Q/d4=">
      <TableDataSource Name="Table" Alias="ReportData" DataType="System.Int32" Enabled="true" SelectCommand="select sx.*, &#13;&#10;dpt_bonus.get_MMFO_ZPcard_count(sx.rnk, sx.dep_dat_begin) cnt_zp,&#13;&#10;nvl(sx.bonus_wb,0) + greatest(nvl(sx.bonus_zp,0), nvl(sx.bonus_excl,0)) BONUS &#13;&#10;from (&#13;&#10;SELECT&#13;&#10;dd.deposit_id            &#13;&#10;, dd.dat_begin         &#13;&#10;, dd.kv                   &#13;&#10;, dd.acc                  &#13;&#10;, dv.vidd                 &#13;&#10;, dv.type_name            &#13;&#10;, dd.rnk                  &#13;&#10;, trunc(ir.bdat) bdat      &#13;&#10;, nvl(ir.ir, 0) ir        &#13;&#10;, nvl(ir.br, 0) br        &#13;&#10;, dd.kf                   &#13;&#10;, dd.wb                   &#13;&#10;, brwb.bonus_value_fact BONUS_WB &#13;&#10;, case when dv.type_cod = 'MPRG' then&#13;&#10;   (select nvl(max(ddc.dat_begin), dd.dat_begin)&#13;&#10;   from bars.dpt_deposit_clos ddc&#13;&#10;   where ddc.deposit_id = dd.deposit_id&#13;&#10;   and nvl(ddc.cnt_dubl,0) = trunc(nvl(dd.cnt_dubl,0)/12) * 12)&#13;&#10;   else dd.dat_begin end dep_dat_begin&#13;&#10;, brzp.bonus_value_fact BONUS_ZP &#13;&#10;, (select coalesce(to_char(min(t1.daos),'dd.mm.yyyy'),' - ') FROM bars.accounts t1 WHERE t1.rnk = dd.rnk&#13;&#10;   AND T1.NBS = '2625' AND T1.OB22 IN ('24', '27', '31') and (t1.dazs is null or t1.dazs &gt;:sFdat1) ) DAT_MIN_OPEN_ZP &#13;&#10;, (select coalesce(to_char(max(t2.dazs),'dd.mm.yyyy'),' - ') FROM bars.accounts t2 WHERE t2.rnk = dd.rnk&#13;&#10;   AND T2.NBS = '2625' AND T2.OB22 IN ('24', '27', '31')) DAT_MAX_CLOS_ZP &#13;&#10;, bars.kost(dd.acc, :sFdat1) OST &#13;&#10;, brsm.bonus_value_fact BONUS_EXCL &#13;&#10;, (lead(ir.bdat, 1, to_date('31.12.4999', 'DD.MM.YYYY')) over (partition by ir.acc order by ir.bdat))-1 pdat&#13;&#10;, nvl(lag(ir.ir, 1) over (partition by ir.acc order by ir.bdat),0) ex_ir&#13;&#10;, nvl(lag(ir.br, 1) over (partition by ir.acc order by ir.bdat),0) ex_br&#13;&#10;, nvl(lag(ir.bdat, 1) over (partition by ir.acc order by ir.bdat), ir.bdat) ex_bdat   &#13;&#10;FROM&#13;&#10;bars.dpt_deposit dd &#13;&#10;inner join bars.dpt_vidd dv on dv.vidd = dd.vidd&#13;&#10;left join (select * from  bars.dpt_bonus_requests br1&#13;&#10;          where br1.bonus_id = dpt_bonus.get_bonus_id('DPWB') and &#13;&#10;          trunc(br1.request_date) = :sFdat1 and br1.request_state = 'ALLOW' and br1.request_deleted = 'N') brwb on brwb.dpt_id = dd.deposit_id&#13;&#10;left join (select * from  bars.dpt_bonus_requests br2&#13;&#10;          where br2.bonus_id = dpt_bonus.get_bonus_id('DPZP') and&#13;&#10;          trunc(br2.request_date) = :sFdat1 and br2.request_state = 'ALLOW' and br2.request_deleted = 'N') brzp on brzp.dpt_id = dd.deposit_id&#13;&#10;left join (select * from  bars.dpt_bonus_requests br3 &#13;&#10;          where br3.bonus_id = dpt_bonus.get_bonus_id('EXCL') and&#13;&#10;          trunc(br3.request_date) = :sFdat1 and br3.request_state = 'ALLOW' and br3.request_deleted = 'N') brsm on brsm.dpt_id = dd.deposit_id  &#13;&#10;left join bars.int_ratn ir on ir.acc = dd.acc and id = 1         &#13;&#10;WHERE&#13;&#10;dd.deposit_id in ( select dpt_id from bars.dpt_bonus_requests dbr&#13;&#10;  where request_state = 'ALLOW'&#13;&#10;  and request_deleted = 'N'&#13;&#10;  and request_user = 1&#13;&#10;  and trunc(request_date) = :sFdat1&#13;&#10;  and dbr.kf = dd.kf ) &#13;&#10;) sx&#13;&#10;where sx.bdat between :sFdat1 and :sFdat1 + 1">
