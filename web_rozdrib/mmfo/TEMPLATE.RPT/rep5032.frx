      <TableDataSource Name="Table" Alias="sql" DataType="System.Int32" Enabled="true" SelectCommand="select nvl(T.CORPORATION_NAME, 'Інші') as CORPORATION_NAME,&#13;&#10;       t.col2 + t.col3 as col1, &#13;&#10;       t.col2,&#13;&#10;       t.col3,&#13;&#10;       t.col5 + t.col6 as col4,&#13;&#10;       t.col5,&#13;&#10;       t.col6,&#13;&#10;       t.col8 + t.col9 as col7,&#13;&#10;       t.col8,&#13;&#10;       t.col9&#13;&#10;from &#13;&#10;(select org.CORPORATION_NAME,&#13;&#10;       sum(case when d.ROWTYPE = 0        /*остаток на начало дня*/&#13;&#10;                and  substr(d.NLS, 1, 4) = '2603'&#13;&#10;                and  d.KOD_ANALYT = '1' &#13;&#10;                then d.OSTQ - d.OBKRQ + d.OBDBQ&#13;&#10;                else 0 end) / (:dat2 - :dat1 +1) as col2,&#13;&#10;       sum(case when d.ROWTYPE = 0&#13;&#10;                and  substr(d.NLS, 1, 4) = '2603'&#13;&#10;                and  d.KOD_ANALYT = '4' &#13;&#10;                then d.OSTQ - d.OBKRQ + d.OBDBQ&#13;&#10;                else 0 end) / (:dat2 - :dat1 +1) as col3,&#13;&#10;&#13;&#10;&#13;&#10;       sum(case when d.ROWTYPE = 0        /*дебет по счетам*/&#13;&#10;                and  substr(d.NLS, 1, 4) = '2603'&#13;&#10;                and  d.KOD_ANALYT = '1' &#13;&#10;                then d.OBDBQ else 0 end) / ((:dat2 - :dat1 +1) - (select count(*) from holiday where kv = 980 and holiday between :dat1 and :dat2)) as col5,&#13;&#10;       sum(case when d.ROWTYPE = 0&#13;&#10;                and  substr(d.NLS, 1, 4) = '2603'&#13;&#10;                and  d.KOD_ANALYT = '4' &#13;&#10;                then d.OBDBQ else 0 end) / ((:dat2 - :dat1 +1) - (select count(*) from holiday where kv = 980 and holiday between :dat1 and :dat2)) as col6,&#13;&#10;                  &#13;&#10;       &#13;&#10;       sum(case when d.ROWTYPE = 0        /*кредит по счетам*/&#13;&#10;                and  substr(d.NLS, 1, 4) = '2603'&#13;&#10;                and  d.KOD_ANALYT = '1' &#13;&#10;                then d.OBKRQ else 0 end) / ((:dat2 - :dat1 +1) - (select count(*) from holiday where kv = 980 and holiday between :dat1 and :dat2)) as col8,&#13;&#10;       sum(case when d.ROWTYPE = 0&#13;&#10;                and  substr(d.NLS, 1, 4) = '2603'&#13;&#10;                and  d.KOD_ANALYT = '4' &#13;&#10;                then d.OBKRQ else 0 end) / ((:dat2 - :dat1 +1) - (select count(*) from holiday where kv = 980 and holiday between :dat1 and :dat2)) as col9                 &#13;&#10;from OB_CORPORATION_DATA d&#13;&#10;left join V_ORG_CORPORATIONS org on TO_CHAR(d.KOD_CORP) = org.base_extid and TO_CHAR(d.KOD_USTAN) = org.EXTERNAL_ID&#13;&#10;where d.KOD_CORP = 8&#13;&#10;and   d.ROWTYPE != 2&#13;&#10;and kfile_pack.get_correct_session_id(d.corporation_id,d.kf,d.session_id)=1&#13;&#10;and kfile_pack.get_correct_nbs(d.corporation_id,substr(d.nls,1,4))=1&#13;&#10;and   trunc(file_date) between :dat1 and :dat2&#13;&#10;group by d.KOD_USTAN, org.CORPORATION_NAME) T">
