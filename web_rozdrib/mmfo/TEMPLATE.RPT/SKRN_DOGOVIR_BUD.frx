        <TableRow Name="Row96" Height="37.8" AutoSize="true">
          <TableCell Name="Cell236" Text="&lt;b&gt;Публічне акціонерне товариство&#13;&#10;“Державний ощадний банк України”&lt;/b&gt;" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 10pt" HtmlTags="true"/>
          <TableCell Name="Cell237" Border.Lines="Left" Border.LeftLine.Style="DashDotDot" Text="&lt;b&gt;[GetVal(&quot;SKRN_FIO&quot;)]&lt;/b&gt;" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 10pt" HtmlTags="true"/>
