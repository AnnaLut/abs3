        <TableRow Name="Row74">
          <TableCell Name="Cell192" Text="користування індивідуальним сейфом " HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell193" Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row75">
