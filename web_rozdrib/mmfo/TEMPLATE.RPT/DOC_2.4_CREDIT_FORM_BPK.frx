    private void _StartReport(object sender, EventArgs e)
    {
      // прячем информацию о текущих кредитах
      Decimal CRD1 = (Decimal)Report.GetColumnValue(&quot;WCS_BID_DATA.CRD1&quot;);
      Decimal CRD2 = (Decimal)Report.GetColumnValue(&quot;WCS_BID_DATA.CRD2&quot;);
      Decimal CRD3 = (Decimal)Report.GetColumnValue(&quot;WCS_BID_DATA.CRD3&quot;);
      
      if (CRD1 != 2)
      {
        Cell187.Text = &quot;&quot;;
      }
      if (CRD2 != 2)
      {
        Cell198.Text = &quot;&quot;;
      }
      if (CRD3 != 2)
      {
        Cell195.Text = &quot;&quot;;
      }
      // прячем информацию о прошлых кредитах
      if (CRD1 != 0 &amp;&amp; CRD1 != 1 &amp;&amp; CRD1 != 2)
      {
        Cell188.Text = &quot;&quot;;
      }
      if (CRD1 != 0)
      {
        Cell207.Text = &quot;&quot;;
      }
      if (CRD2 != 0)
      {
        Cell201.Text = &quot;&quot;;
      }
      if (CRD3 != 0)
      {
        Cell204.Text = &quot;&quot;;
      }

      // отображаем диапазон по доходах
      Decimal TOTAL_INCOME = (Decimal)Report.GetColumnValue(&quot;WCS_BID_DATA.TOTAL_INCOME&quot;);
      if (1000 &lt;= TOTAL_INCOME &amp;&amp; TOTAL_INCOME &lt; 2000)
      {
        Cell186.Text = &quot;1 тис.грн.-2 тис.грн.;&quot;;
      } else if (2000 &lt;= TOTAL_INCOME &amp;&amp; TOTAL_INCOME &lt; 3000)                                    
      {
        Cell186.Text = &quot;2 тис.грн.-3 тис.грн.;&quot;;
      } else if (3000 &lt;= TOTAL_INCOME &amp;&amp; TOTAL_INCOME &lt; 5000)                                    
      {
        Cell186.Text = &quot;3 тис.грн.-5 тис.грн.;&quot;;
      } else if (5000 &lt;= TOTAL_INCOME &amp;&amp; TOTAL_INCOME &lt; 10000)                                    
      {
        Cell186.Text = &quot;5 тис.грн.-10 тис.грн.;&quot;;
      } else if (10000 &lt;= TOTAL_INCOME &amp;&amp; TOTAL_INCOME &lt; 25000)                                    
      {
        Cell186.Text = &quot;10 тис.грн.-25 тис. грн.;&quot;;
      } else if (25000 &lt;= TOTAL_INCOME &amp;&amp; TOTAL_INCOME &lt; 50000)                                    
      {
        Cell186.Text = &quot;25 тис.грн.-50 тис.грн.;&quot;;
      } else if (50000 &lt;= TOTAL_INCOME &amp;&amp; TOTAL_INCOME &lt; 100000)                                    
      {
        Cell186.Text = &quot;50 тис.грн.-100 тис. грн.;&quot;;
      } else if (100000 &lt;= TOTAL_INCOME)                                    
      {
        Cell186.Text = &quot;більше 100 тис. грн.&quot;;
      }
    }
  }
}
</ScriptText>
