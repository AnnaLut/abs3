using Oracle.DataAccess.Client;

namespace FastReport
{
  public class ReportScript
  {
    private void _StartReport(object sender, EventArgs e)
    {
      OracleDataConnection frxconn = (OracleDataConnection)Report.Dictionary.Connections[0];
      OracleConnection connection = (OracleConnection)frxconn.GetConnection();
      OracleCommand command = connection.CreateCommand();
      command.CommandText = &quot;begin bars.cc_gpk_svk(:p_nd); end;&quot;;
      
      Parameter par = Report.GetParameter(&quot;ND&quot;);
      command.Parameters.Add(&quot;p_nd&quot;, par.Value);
      
      connection.Open();
      try
      {
        command.ExecuteNonQuery();
      }
      finally
      {
        connection.Close();
      }
    }
  }
}
