      <TableDataSource Name="Table3" Alias="Zatr" DataType="System.Int32" Enabled="true" SelectCommand="select  decode(wcs_utl.get_answ(:ND, 'CL_0_113'),0,'',null,'', 'Послуги суб’єкта  оціночної діяльності '|| wcs_utl.get_answ(:ND, 'CL_0_113')||' грн.(з урахуванням тарифів суб’єкта оціночної діяльності ) , одноразово.&#10;')   as  Otenka, &#10;          (case when  wcs_utl.get_answ(:ND, 'CL_0_114') is not null or   &#10;                             wcs_utl.get_answ(:ND, 'CL_0_115') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_116') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_117') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_118') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_119') is not null &#10;          then 'Послуги нотаріуса:&#10;' else '&#10;'          end)       Notarius  ,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_114'),0,'',null,'',' * нотаріальне посвідчення договору купівлі-продажу, в тому числі Державне мито та внески до Пенсійного фонду '|| wcs_utl.get_answ(:ND, 'CL_0_114')||' грн.;&#10;')   as  PN1,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_115'),0,'',null,'',' *нотаріальне посвідчення договору застави/ іпотечного договору, в тому числі Державне мито '|| wcs_utl.get_answ(:ND, 'CL_0_115')||' грн.;&#10;')   as  PN2,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_116'),0,'',null,'',' *надання витягів з Державного реєстру обтяжень рухомого майна або Єдиного реєстру заборон відчуження об’єктів нерухомого майна '|| wcs_utl.get_answ(:ND, 'CL_0_116')||' грн.;&#10;')   as  PN3,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_117'),0,'',null,'',' *надання витягів з Державного земельного кадастру '|| wcs_utl.get_answ(:ND, 'CL_0_117')||' грн.;&#10;')   as  PN4,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_118'),0,'',null,'',' *внесення запису про обтяження нерухомості до Державного реєстру іпотек та єдиного реєстру заборон відчуження об’єктів нерухомого майна '|| wcs_utl.get_answ(:ND, 'CL_0_118')||' грн.;&#10;')   as  PN5,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_119'),0,'',null,'',' *інші послуги нотаріуса '|| wcs_utl.get_answ(:ND, 'CL_0_119')||' грн. &#10;' )   as  PN6,&#10;         (case when  wcs_utl.get_answ(:ND, 'CL_0_114') is not null or   &#10;                             wcs_utl.get_answ(:ND, 'CL_0_115') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_116') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_117') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_118') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_119') is not null &#10;          then ' (з урахуванням тарифів нотаріуса одноразово)&#10;'          &#10;else '&#10;'&#10;end)       Notarius_end  ,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_120'),0,'',null,'',' Послуги ДП „Інформаційний центр” : внесення запису про обтяження рухомого майна до Державного реєстру обтяжень рухомого майна '|| wcs_utl.get_answ(:ND, 'CL_0_120')||' грн. (з урахуванням тарифів ДП “Інформаційний центр” ) одноразово.&#10;')   as  P_DP_IC,&#10;          (case when  wcs_utl.get_answ(:ND, 'CL_0_121') is not null or   &#10;                             wcs_utl.get_answ(:ND, 'CL_0_122') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_123') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_124') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_125') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_126') is not null &#10;          then 'Сплата страхових платежів ( за 1 рік):&#10;'  else '&#10;'          end) strah,  &#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_121'),0,'',null,'',' * страхування нерухомого майна '|| wcs_utl.get_answ(:ND, 'CL_0_121')||' грн.;&#10;')   as  SP1,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_122'),0,'',null,'',' * страхування рухомого майна (в т.ч. повне КАСКО) '|| wcs_utl.get_answ(:ND, 'CL_0_122')||' грн.;&#10;')   as  SP2,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_123'),0,'',null,'',' * страхування життя Позичальника '|| wcs_utl.get_answ(:ND, 'CL_0_123')||' грн.;&#10;')   as  SP3,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_124'),0,'',null,'',' * страхування відповідальності Позичальника '|| wcs_utl.get_answ(:ND, 'CL_0_124')||' грн.;&#10;')   as  SP4,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_125'),0,'',null,'',' * страхування Позичальника від нещасного випадку '|| wcs_utl.get_answ(:ND, 'CL_0_125')||' грн.;&#10;')   as  SP5,&#10;          decode(wcs_utl.get_answ(:ND, 'CL_0_126'),0,'',null,'',' * страхування фінансового ризику втрати права власності на майно (страхування титулу) '|| wcs_utl.get_answ(:ND, 'CL_0_126')||' грн.&#10;')   as  SP6,&#10;(case when  wcs_utl.get_answ(:ND, 'CL_0_121') is not null or   &#10;                             wcs_utl.get_answ(:ND, 'CL_0_122') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_123') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_124') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_125') is not null or&#10;                             wcs_utl.get_answ(:ND, 'CL_0_126') is not null &#10;          then  '(з урахуванням тарифів страхової компанії) '||wcs_utl.get_answ_formated(:ND, 'CL_0_127')||' &#10;'    &#10;else '&#10;'     end) strah_end,            &#10;          -- nvl(wcs_utl.get_answ_formated(:ND, 'CL_0_127'),'')   as  SP00,&#10;           decode(wcs_utl.get_answ(:ND, 'CL_0_128'),0,'',null,'','Витяг з реєстру прав власності на нерухоме майно (довідка-характеристика БТІ) : ' ||wcs_utl.get_answ(:ND, 'CL_0_128')||' грн., з урахуванням відповідних тарифів, одноразово.&#10;')   as  BTI,&#10;           decode(wcs_utl.get_answ(:ND, 'CL_0_129'),0,'',null,'','Інші  послуги : '|| wcs_utl.get_answ(:ND, 'CL_0_130')||' '|| wcs_utl.get_answ(:ND, 'CL_0_129')||' грн., з урахуванням відповідних тарифів ')   as  IP,&#10;           decode(wcs_utl.get_answ(:ND, 'CL_0_129'),0,'',null,'',' - '||wcs_utl.get_answ_formated(:ND, 'CL_0_130_1'))   as  SP_IP&#10;from dual&#10;">
