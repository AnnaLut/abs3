  public class ReportScript
  {

    private void _StartReport(object sender, EventArgs e)
    {
      // прячем разделы в зависимоти от проекта
      Decimal PROJECT = (Decimal)Report.GetColumnValue(&quot;CARD_DATA.PROJECT&quot;);
      
      // 0 Зарплатний
      if (PROJECT != 0)
      {
        Cell50.Text = &quot;&quot;;
        Cell52.Text = &quot;&quot;;
      }
      // 1 Пенсійний
      if (PROJECT != 1)
      {
        Cell56.Text = &quot;&quot;;
        Cell58.Text = &quot;&quot;;
      }
      // 2 Економний
      if (PROJECT != 2)
      {
        Cell60.Text = &quot;&quot;;
        Cell62.Text = &quot;&quot;;
      }
      // 3 Стандартний
      if (PROJECT != 3)
      {
        Cell64.Text = &quot;&quot;;
        Cell66.Text = &quot;&quot;;
      }
      // 4 Преміум
      if (PROJECT != 4)
      {
        Cell68.Text = &quot;&quot;;
        Cell70.Text = &quot;&quot;;
      }
    }
  }
}
</ScriptText>
