    string to_teaxt (int num)
    {
      string text_num =&quot;&quot; ;
      string[] num19 = new string[9] {&quot;першого&quot;, &quot;другого&quot;, &quot;третього&quot;, &quot;четвертого&quot;, &quot;п'ятого&quot;, &quot;шостого&quot; ,&quot;сьомого&quot;, &quot;восьмого&quot;,&quot;дев'ятого&quot;};
      string[] num1119 = new string[9] {&quot;одинадцятого&quot;, &quot;дванадцятого&quot;, &quot;тринадцятого&quot;, &quot;чотирнадцятого&quot;, &quot;п'ятнадцятого&quot;, &quot;шістнадцятого&quot; ,&quot;сімнадцятого&quot;, &quot;вісімнадцятого&quot;,&quot;дев'ятнадцятого&quot;};
      string[] num1090 = new string[9] {&quot;десять&quot;, &quot;двадцять&quot;, &quot;тридцять&quot;, &quot;сорок&quot;, &quot;п'ятдесят&quot;, &quot;шістдесят&quot; ,&quot;сімдесят&quot;, &quot;вісімдесят&quot;,&quot;дев'яносто&quot;};
      string[] num1090r = new string[9] {&quot;десятого&quot;, &quot;двадцятого&quot;, &quot;тридцятого&quot;, &quot;сорокового&quot;, &quot;п'ятдесятого&quot;, &quot;шістдесятого&quot; ,&quot;сімдесятого&quot;, &quot;вісімдесятого&quot;,&quot;дев'яностого&quot;};
      if ((num&gt;0)&amp;&amp;(num&lt;100))
        if (num&lt;10)
          text_num += num19[num-1];
        else  if (num%10 == 0) 
          text_num += num1090r[num/10-1];
        else  if (num&lt;20) 
          text_num += num1119[num%10-1];
        else  text_num += num1090[num/10-1]+ &quot; &quot;+num19[num%10-1];  
      else  text_num +=&quot; не коректне чмсло&quot;;
      return text_num;
    }
    string date_to_teaxt (DateTime DateType)
    {
      string[] nameMonth = new string[12] {&quot;cічня&quot;, &quot;лютого&quot;, &quot;березня&quot;, &quot;квітня&quot;, &quot;травня&quot;, &quot;червня&quot; ,&quot;липня&quot;, &quot;серпня&quot;,&quot;вересня&quot;,&quot;жовтня&quot;,&quot;листопада&quot;,&quot;грудня&quot;};
      string DateText;
      int DayDate = Day(DateType); 
      int YearDate1 = Year(DateType)/100;
      int YearDate2 = Year(DateType)%100;
      DateText = to_teaxt(DayDate);
      DateText += &quot; &quot;+nameMonth[Month(DateType)-1];
      if (YearDate1==19) DateText+=&quot; одна тисяча дев'ятсот &quot;+to_teaxt(YearDate2);
      else if (YearDate1==20) if (YearDate2==0) DateText+=&quot; дві тисячного&quot;; 
                              else DateText += &quot; дві тисячі &quot;+to_teaxt(YearDate2);
           else DateText +=&quot; не коректний рік&quot;;   
      return DateText;
    }  
  }
}</ScriptText>
