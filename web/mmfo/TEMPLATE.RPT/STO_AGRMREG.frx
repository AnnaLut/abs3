      <TableDataSource Name="Table2" Alias="Cust" DataType="System.Int32" Enabled="true" SelectCommand="SELECT &#13;&#10;distinct max(SD.IDD) as lvl,&#13;&#10;c.nmk as NMK&#13;&#10;FROM BARS.STO_LST SA, BARS.STO_DET SD, bars.FREQ F, bars.customer c&#13;&#10;Where SD.IDS = SA.IDS &#13;&#10;and SA.RNK = :rnk &#13;&#10;and F.FREQ = SD.FREQ&#13;&#10;and  SA.RNK = c.rnk&#13;&#10;GROUP BY c.nmk&#13;&#10;order by 1,2">
