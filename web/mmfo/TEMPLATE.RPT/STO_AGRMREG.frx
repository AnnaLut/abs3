      <TableDataSource Name="Table" Alias="RegularSTO" DataType="System.Int32" Enabled="true" SelectCommand="  SELECT SD.IDD AS lvl,&#13;&#10;         SD.NAZN A1,&#13;&#10;         SD.NLSB A2,&#13;&#10;         SD.MFOB A3,&#13;&#10;         SD.POLU A4,&#13;&#10;         SD.OKPO A5,&#13;&#10;         SD.NLSA A6,&#13;&#10;         CASE&#13;&#10;            WHEN f.freq = 2 AND SA.IDG = 12&#13;&#10;            THEN&#13;&#10;               'Згідно з графіком погашення КД №'&#13;&#10;               || c.cc_id&#13;&#10;               || ' від '&#13;&#10;               || TO_CHAR (C.SDATE, 'dd/mm/yyyy')&#13;&#10;            ELSE&#13;&#10;               LTRIM (RTRIM (F.NAME))&#13;&#10;         END&#13;&#10;            F1,&#13;&#10;         LTRIM (&#13;&#10;            RTRIM (&#13;&#10;               DECODE (&#13;&#10;                  NVL (TRIM (TRANSLATE (SD.FSUM, ' +-.0123456789', ' ')), ' '),&#13;&#10;                  ' ', TO_CHAR (TO_NUMBER (SD.FSUM) / 100,&#13;&#10;                                '9999999999999990D00'),&#13;&#10;                  'Згідно з графіком погашення КД №'&#13;&#10;                  || c.cc_id&#13;&#10;                  || ' від '&#13;&#10;                  || TO_CHAR (C.SDATE, 'dd/mm/yyyy'))))&#13;&#10;            AS A7,&#13;&#10;         TO_CHAR (SD.IDD) A8,&#13;&#10;         TO_CHAR (SD.Dat1, 'dd.mm.yyyy') D1,&#13;&#10;         TO_CHAR (SD.Dat2, 'dd.mm.yyyy') D2,&#13;&#10;         COALESCE (w.nd, ba.nd) ND,&#13;&#10;         TO_CHAR (a.daos, 'dd.mm.yyyy') AS ND_DATE,&#13;&#10;         TO_CHAR (sysdate, 'dd.mm.yyyy') as BD&#13;&#10;    FROM BARS.STO_LST SA,&#13;&#10;         BARS.STO_DET SD,&#13;&#10;         FREQ F,&#13;&#10;         bars.cc_deal C,&#13;&#10;         bars.accounts a,&#13;&#10;         bars.w4_acc w,&#13;&#10;         bars.bpk_acc ba&#13;&#10;   WHERE     SD.IDS = SA.IDS&#13;&#10;         AND SA.RNK = :rnk&#13;&#10;         AND A.nls = SD.NLSA&#13;&#10;         AND a.acc = W.ACC_PK(+)&#13;&#10;         AND a.acc = ba.ACC_PK(+)&#13;&#10;         AND F.FREQ = SD.FREQ&#13;&#10;         AND SD.IDD = TO_NUMBER (:IDD)&#13;&#10;        AND ROWNUM = 1&#13;&#10;ORDER BY 1, 2">
