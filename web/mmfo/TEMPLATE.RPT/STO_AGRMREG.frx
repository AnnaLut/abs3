        <CommandParameter Name="rnk" DataType="113" Expression="[rnk]" DefaultValue="1137956"/>
      </TableDataSource>
      <TableDataSource Name="Table3" Alias="BranchParam" DataType="System.Int32" Enabled="true" SelectCommand="select bp1.val as Name_branch, bp2.val as ADR_BRANCH, bp3.val as BPK_BOSS_FIO&#13;&#10;from branch b, branch_parameters bp1,  branch_parameters bp2,  branch_parameters bp3 &#13;&#10;where bp1.tag(+) = 'NAME_BRANCH'&#13;&#10;and bp2.tag(+) = 'ADR_BRANCH'&#13;&#10;and bp3.tag (+) = 'BPK_BOSS_FIO'&#13;&#10;and b.branch = (select a.branch from BARS.STO_DET SD,bars.accounts a where  SD.IDD = TO_NUMBER (:IDD) AND A.nls = SD.NLSA)&#13;&#10;and b.branch=bp1.branch(+)&#13;&#10;and b.branch=bp2.branch(+)&#13;&#10;and b.branch=bp3.branch(+)">
