  <Dictionary>
    <OracleDataConnection Name="Connection" ConnectionString="rijcmlqHytUMhhPg9gG4fal5Q6rRB0BvRbh0nDbCWvtDA1bLoXA5F1HMEY1qSyYCzmMlYGK5S75auXYOG6vOu2k3E2pQLoqtCWIgrrfBw+Gs9nunzU=">
      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="select&#13;&#10;case when okpob is null and typ is not null then typ &#13;&#10;     when okpob is not null and typ = 'VI' then  okpob end as okpo_otr,&#13;&#10;case when okpob is null and typ = 'IV' then 'Теплові генерації всього:'&#13;&#10;     when okpob is null and typ = 'VI' then 'Всьго інші:'&#13;&#10;     when okpob is null and typ is null then 'Всього перераховано:'&#13;&#10;     else max(namb) end as namb,  &#13;&#10;sum(clmn_3a+clmn_5a+clmn_8a+clmn_9a) clmn1, &#13;&#10;sum(clmn_3a+clmn_5a) clmn2, &#13;&#10;sum(clmn_3a) clmn3,&#13;&#10;case when sum(clmn_3a)+sum(clmn_5a)=0 then 0 else  round(sum(clmn_3a)/(sum(clmn_3a)+sum(clmn_5a)),4)*100 end clmn4, &#13;&#10;sum(clmn_5a) clmn5, &#13;&#10;case when sum(clmn_3a)+sum(clmn_5a)=0 then 0 else  round(sum(clmn_5a)/(sum(clmn_3a)+sum(clmn_5a)),4)*100 end clmn6,&#13;&#10;sum(clmn_8a+clmn_9a) clmn7, &#13;&#10;sum(clmn_8a) clmn8, &#13;&#10;sum(clmn_9a) clmn9&#13;&#10;from &#13;&#10;(select d.okpob,&#13;&#10;case when okpob = '21515381' then 'I'&#13;&#10;     when okpob = '37995466' then 'II'&#13;&#10;     when okpob = '24584661' then 'III'&#13;&#10;     when okpob in ('22927045','00130872','23343582','23269555','31831942','00131305') then 'IV'&#13;&#10;     when okpob = '20588716' then 'V'&#13;&#10;     else 'VI' end as typ,&#13;&#10;case when okpob = '21515381' then 'ДП &quot;Енергоринок&quot;'&#13;&#10;     when okpob = '37995466' then 'ШЕВЧЕНКІВСЬКЕ РВДК У М.КИЄВІ (інвестиційна складова)'&#13;&#10;     when okpob = '24584661' then 'НАЕК &quot;Енергоатом&quot;'&#13;&#10;     when okpob = '20588716' then 'ВАТ &quot;Центренерго&quot;'&#13;&#10;     else max(d.namb) end as namb,&#13;&#10;sum(case when a.nls='26032301861'      and d.mfob in     (select kf from kf_ru) and a.kod_analyt  =  '01' then d.sq else 0 end)/100 clmn_3a,&#13;&#10;sum(case when a.nls='26032301861'      and d.mfob not in (select kf from kf_ru) and a.kod_analyt  =  '01' then d.sq else 0 end)/100 clmn_5a,&#13;&#10;sum(case when substr(a.nls,1,4)='2603' and d.mfob in     (select kf from kf_ru) and a.kod_analyt  &lt;&gt; '01' then d.sq else 0 end)/100 clmn_8a,&#13;&#10;sum(case when substr(a.nls,1,4)='2603' and d.mfob not in (select kf from kf_ru) and a.kod_analyt  &lt;&gt; '01' then d.sq else 0 end)/100 clmn_9a&#13;&#10;from ob_corp_data_acc a&#13;&#10;  join v_ob_corp_rep_nbs q on q.rep_id = 5039 and q.nbs = substr(a.nls,1,4) and Q.CORP_ID = a.corp_id&#13;&#10;  join ob_corp_data_doc d on A.ACC = d.acc and A.SESS_ID = d.sess_id and a.kf = d.kf&#13;&#10;where a.corp_id=5&#13;&#10;  and a.is_last = 1 &#13;&#10;  and a.kf='300465' &#13;&#10;  and d.okpoa='21515381' &#13;&#10;  and a.fdat between :sFdat1 and :sFdat2&#13;&#10;group by okpob&#13;&#10;)&#13;&#10;group by rollup(typ,okpob)&#13;&#10;HAVING GROUPING_ID(typ,okpob) IN (1, 3) or (GROUPING_ID(typ,okpob) = 0 and  typ in ('IV', 'VI'))&#13;&#10;order by typ  NULLS FIRST, okpob  NULLS FIRST">
