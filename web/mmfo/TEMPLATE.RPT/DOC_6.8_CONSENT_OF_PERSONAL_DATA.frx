  <Dictionary>
    <OracleDataConnection Name="sberdev" ConnectionString="rijcmlqcFv7/QVsbsa1pDpr21P0mHXdl2BpgMbYS4w+AnBeGDddnQ5MuNy1h3sM21aiXmM/Cuhpa50j33ttcMpXStpiaA==">
      <TableDataSource Name="Table7" Alias="list_dog" DataType="System.Int32" Enabled="true" SelectCommand="select INN,MAX(fio) FIO ,MAX(fio_o) fio_o,MAX(fio_r) fio_r,MAX(pasp) PASP,&#13;&#10;       max(kr_zay) kr_zay,max(kr_zay_dat) kr_zay_dat,&#13;&#10;       max(dog_rod) dog_rod,max(dog_oru) dog_oru,max(dog) dog,&#13;&#10;       max(pz_pr) pz_pr,max(pz_pr_rod) pz_pr_rod,max(pz_pr_dat) pz_pr_dat,max(pz_pr_oru) pz_pr_oru&#13;&#10;from(&#13;&#10;select B.INN , 1 n,&#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_1') || ' ' || &#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_2') || ' ' || &#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_3') AS fio,&#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_123_O') fio_o,&#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_123_R') fio_r,                                &#13;&#10;       'паспорт серії: '|| wcs_utl.get_answ (b.bid_id,'CL_7')|| &#13;&#10;       ' № '|| wcs_utl.get_answ (b.bid_id,'CL_8')|| &#13;&#10;       ' виданий '|| trim(wcs_utl.get_answ (b.bid_id,'CL_9'))|| &#13;&#10;       ' '|| wcs_utl.get_answ (b.bid_id,'CL_11')||' р.' AS pasp,&#13;&#10;       'Кредитної заявки' kr_zay,  &#13;&#10;       'Кредитній заявці' kr_zay_dat,      &#13;&#10;       decode((select s.PRODUCT_ID from WCS_SUBPRODUCTS s where s.ID=b.SUBPRODUCT_ID),&#13;&#10;       'PRD_MORTGAGE','Договору про іпотечний кредит',&#13;&#10;       'PRD_HULL_INSURANCE','Договору відновлюваної кредитної лінії',&#13;&#10;       case when b.SUBPRODUCT_ID in('SBP_CONSUMER_0_5_2_EP','SBP_CONSUMER_5_10_2_EP') &#13;&#10;            then 'Договору відновлюваної кредитної лінії'&#13;&#10;            else 'Кредитного договору' &#13;&#10;       end) dog_rod,&#13;&#10;       decode((select s.PRODUCT_ID from WCS_SUBPRODUCTS s where s.ID=b.SUBPRODUCT_ID),&#13;&#10;       'PRD_MORTGAGE','Договором про іпотечний кредит',&#13;&#10;       'PRD_HULL_INSURANCE','Договором відновлюваної кредитної лінії',&#13;&#10;       case when b.SUBPRODUCT_ID in('SBP_CONSUMER_0_5_2_EP','SBP_CONSUMER_5_10_2_EP') &#13;&#10;            then 'Договором відновлюваної кредитної лінії'&#13;&#10;            else 'Кредитним договором' &#13;&#10;       end) dog_oru,&#13;&#10;       decode((select s.PRODUCT_ID from WCS_SUBPRODUCTS s where s.ID=b.SUBPRODUCT_ID),&#13;&#10;       'PRD_MORTGAGE','отриманням іпотечного кредиту',&#13;&#10;       'PRD_HULL_INSURANCE','отриманням відновлюваної кредитної лінії',&#13;&#10;       case when b.SUBPRODUCT_ID in('SBP_CONSUMER_0_5_2_EP','SBP_CONSUMER_5_10_2_EP') &#13;&#10;            then 'отриманням відновлюваної кредитної лінії'&#13;&#10;            else 'отриманням Кредиту' &#13;&#10;       end) dog,&#13;&#10;       'Позичальник' pz_pr,&#13;&#10;       'Позичальника' pz_pr_rod,&#13;&#10;       'Позичальнику' pz_pr_dat,&#13;&#10;       'Позичальником' pz_pr_oru&#13;&#10; from  v_wcs_bids b&#13;&#10;where  b.bid_id = :p_bid_id&#13;&#10;union all  &#13;&#10;select distinct &#13;&#10;       decode(nvl(wcs_utl.get_answ(b.bid_id, 'GRT_0', b.ws_id , B.GARANTEE_NUM),0),&#13;&#10;       0,&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CODE_002', b.ws_id , B.GARANTEE_NUM),&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CODE_002')) inn, 2 n,&#13;&#10;       wcs_utl.get_answ(b.bid_id,'CL_1',b.ws_id , B.GARANTEE_NUM)|| ' ' || &#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_2',b.ws_id , B.GARANTEE_NUM)|| ' ' || &#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_3',b.ws_id , B.GARANTEE_NUM) AS fio,&#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_123_O',b.ws_id , B.GARANTEE_NUM) fio_o,&#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_123_R',b.ws_id , B.GARANTEE_NUM) fio_r,                                &#13;&#10;       'паспорт серії: ' || wcs_utl.get_answ (:p_bid_id,'CL_7',b.ws_id , B.GARANTEE_NUM)|| &#13;&#10;       ' № ' || wcs_utl.get_answ (b.bid_id,'CL_8',b.ws_id , B.GARANTEE_NUM)|| &#13;&#10;       ' виданий ' || trim(wcs_utl.get_answ (b.bid_id,'CL_9',b.ws_id , B.GARANTEE_NUM))||&#13;&#10;       ' '|| wcs_utl.get_answ (b.bid_id,'CL_11',b.ws_id , B.GARANTEE_NUM)||' р.' AS pasp, &#13;&#10;       decode(B.GARANTEE_ID,'GUARANTOR','Заявки поручителя','Заявки майнового поручителя') kr_zay,&#13;&#10;       decode(B.GARANTEE_ID,'GUARANTOR','Заявці поручителя','Заявці майнового поручителя') kr_zay_dat,&#13;&#10;       decode(B.GARANTEE_ID,'GUARANTOR','Договору поруки','VEHICLE','Договору забезпечення','Іпотечного договору') dog_rod,&#13;&#10;       decode(B.GARANTEE_ID,'GUARANTOR','Договором поруки','VEHICLE','Договором забезпечення','Іпотечним договором') dog_oru,&#13;&#10;       decode(B.GARANTEE_ID,'GUARANTOR','наданням Поруки','VEHICLE','наданням Застави','наданням Іпотеки') dog,       &#13;&#10;       decode(B.GARANTEE_ID,'GUARANTOR','Поручитель','Майновий поручитель') pz_pr,&#13;&#10;       decode(B.GARANTEE_ID,'GUARANTOR','Поручителя','Майнового поручителя') pz_pr_rod,&#13;&#10;       decode(B.GARANTEE_ID,'GUARANTOR','Поручителю','Майновому поручителю') pz_pr_dat,&#13;&#10;       decode(B.GARANTEE_ID,'GUARANTOR','Поручителем','Майновим поручителем') pz_pr_oru&#13;&#10;from   V_WCS_BID_GARANTEES b&#13;&#10;where  b.bid_id = :p_bid_id  &#13;&#10;) dog&#13;&#10;--where dog.inn= wcs_utl.get_answ(:p_bid_id, 'CODE_002', :p_ws_id, :p_ws_number)   &#13;&#10;group by inn">
