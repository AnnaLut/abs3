    <DataBand Name="Data1" Width="718.2" Height="1181.25" CanGrow="true" CanShrink="true" DataSource="Table7">
      <TableObject Name="Table1" Left="9.45" Width="699.31" Height="1058.4">
        <TableColumn Name="Column1" Width="233.09"/>
