    <OracleDataConnection Name="Connection" ConnectionString="rijcmlqHytUMhhPg9gG4fal5Q6rRB0BvRbh0nDbCWvtDA1bLoXA5F1HMEY1qSyYCzmMlYGK5S75auXYOG6vOu2k3E2pQJ6vOkvoT+iyIsIh7ofGL7o=">
      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="select corporation_name, &#13;&#10;       case when corporation_name is null then null else okpo end as okpo,&#13;&#10;       nbs,&#13;&#10;       kod_analyt,&#13;&#10;       cnt,&#13;&#10;       in_ost,&#13;&#10;       obdbq,&#13;&#10;       obkrq,&#13;&#10;       out_ost,&#13;&#10;       cnt_doc,&#13;&#10;       cnt_doc_db,&#13;&#10;       cnt_doc_cr, &#13;&#10;       sFdat1,&#13;&#10;       sFdat2,&#13;&#10;       case when corporation_name is null then null else namk end as namk,&#13;&#10;       case when corporation_name is null then null else kod_ustan end as kod_ustan &#13;&#10;     from (select r.corporation_name,&#13;&#10;                  a.okpo,&#13;&#10;                  substr(a.nls,1,4) as nbs,&#13;&#10;                  a.kod_analyt,&#13;&#10;                  count(a.acc) as cnt,&#13;&#10;                  sum(case when a.fdat=:sFdat1 then nvl(a.ostq,0)- nvl(a.obkrq,0)+nvl(a.obdbq,0) else 0 end)/100 as in_ost,&#13;&#10;                  sum(a.obdbq)/100 obdbq,&#13;&#10;                  sum(a.obkrq)/100 obkrq,&#13;&#10;                  sum(case when a.fdat=:sFdat2 then a.ostq else 0 end)/100 as out_ost,&#13;&#10;                  sum((select count(d.ref) from ob_corp_data_doc d where a.acc = d.acc and a.sess_id = d.sess_id))  as cnt_doc,&#13;&#10;                  sum((select count(d.ref) from ob_corp_data_doc d where a.acc = d.acc and a.sess_id = d.sess_id and d.dk = 0)) as cnt_doc_db,&#13;&#10;                  sum((select count(d.ref) from ob_corp_data_doc d where a.acc = d.acc and a.sess_id = d.sess_id and d.dk = 1)) as cnt_doc_cr, &#13;&#10;                  to_char(:sFdat1, 'dd.mm.yyyy') as sFdat1,&#13;&#10;                  to_char(:sFdat2, 'dd.mm.yyyy') as sFdat2,&#13;&#10;                  ': '||a.namk as namk,&#13;&#10;                  'Код установи - '||to_char(a.kod_ustan)||'. ЄДРПОУ - ' as kod_ustan&#13;&#10;               from ob_corp_data_acc a&#13;&#10;               join v_ob_corp_rep_nbs q on q.rep_id = 5048 and q.nbs = substr(a.nls,1,4) and q.corp_id = a.corp_id&#13;&#10;               left join v_root_corporation r on to_char(a.corp_id)=r.external_id          &#13;&#10;               where a.corp_id=:corporation_id&#13;&#10;               and a.is_last = 1&#13;&#10;               and a.fdat between :sFdat1 and :sFdat2&#13;&#10;               and instr(decode(:okpo,'%',a.okpo,:okpo),a.okpo)&gt;0             &#13;&#10;               and instr(decode(:name,'%',a.kf,:name),a.kf)&gt;0&#13;&#10;               and instr(decode(:nbs,'%',substr(a.nls,1,4),:nbs),substr(a.nls,1,4))&gt;0&#13;&#10;               and instr(decode(:kod_analyt,'%',nvl(a.kod_analyt,' '),:kod_analyt),nvl(a.kod_analyt,' '))&gt;0&#13;&#10;               and instr(decode(:kod_ustan,'%',nvl(a.kod_ustan, -1),:kod_ustan),nvl(a.kod_ustan, -1))&gt;0&#13;&#10;               group by grouping sets((r.corporation_name, a.okpo, a.namk, a.kod_ustan, substr(a.nls,1,4), a.kod_analyt),(substr(a.nls,1,4),a.kod_analyt)))&#13;&#10;               order by corporation_name nulls last, okpo, namk, nbs, kod_analyt nulls last">
