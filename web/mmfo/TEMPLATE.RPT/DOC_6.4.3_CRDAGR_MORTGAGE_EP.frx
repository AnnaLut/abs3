    private void _StartReport(object sender, EventArgs e)
    {
      //Cell116.Text = (String)Report.GetColumnValue(&quot;klient_info.fio&quot;);
      OracleDataConnection frxconn = (OracleDataConnection)Report.Dictionary.Connections[0];
      OracleConnection connection = (OracleConnection)frxconn.GetConnection();
      OracleCommand command = connection.CreateCommand();
      command.CommandText = &quot;select * from v_wcs_bid_garantees where bid_id=:p_bid_id&quot;;
      
      Parameter par = Report.GetParameter(&quot;p_bid_id&quot;);
      command.Parameters.Add(&quot;p_bid_id&quot;, par.Value);
      
      connection.Open();
      
      DataTable table = new DataTable();
      table.Load(command.ExecuteReader());
      int count = table.Rows.Count;
      int m = 0;
      
      for (int i = 0; i &lt; table.Rows.Count; i++)
      {
        /*Пункт 1.1.21 и 1.1.22*/
        if (Convert.ToString(table.Rows[i][1]) == &quot;GUARANTOR&quot;)
        {
          Row42.Visible = true;
          Row43.Visible = true;
        }
        if (Convert.ToString(table.Rows[i][1]) == &quot;MORTGAGE&quot;)        
        {
          m++; 
        }
        if (Convert.ToString(table.Rows[i][1]) == &quot;MORTGAGE_LAND&quot;)
        {
          m++;
        }
      }
      
      connection.Close();
      
      /*Пункт 1.1.12 и 3.1.1.1(3.1.1.2)*/
      /*if (m == 1)
      {
        Row29.Visible = true;
        Row54.Visible = true;
      }
      else if (m == 2)
      {
        Row25.Visible = true;
        Row56.Visible = true;
      }
      else
      {
        Row26.Visible = true;
        Row55.Visible = true;
      } */
      /*Пункт 1.1.14*/
      if (count == 1)
      {
        Row31.Visible = true;
      }
      else
      {
        Row33.Visible = true;
      }
      
      
      if ((String)Report.GetColumnValue(&quot;Guarantor.GARANTEE_ID&quot;) == &quot;MORTGAGE&quot;)
      {
        Row42.Visible = true;
        Row43.Visible = true;
      } 
      
      /*Пункт 2.4.2*/
      if (Convert.ToInt16((String)Report.GetColumnValue(&quot;DataSubProdukt.REPAYMENT_METHOD&quot;)) == 2)
      {
        Row34.Visible = true;
        Row68.Visible = true;
      }
      else
      {
        Row14.Visible = true;
        Row69.Visible = true;
      }
      /*Пункт 6.3.8*/
      /*if (Convert.ToInt16((String )Report.GetColumnValue(&quot;Marital_status.STAT_MERIED&quot;)) != 0)
      {
      Row164.Visible = true;
      }
      else if (Convert.ToInt16((String )Report.GetColumnValue(&quot;Marital_status.STAT_MERIED&quot;)) != 4)
      {
      Row165.Visible = true;
      }
      else
      {
      Row166.Visible = true;        
      } */
      }
  }
}
