      <TableDataSource Name="Table4" Alias="Table" DataType="System.Int32" Enabled="true" SelectCommand="with dates as&#13;&#10; (select holiday as date_f, dat_next_u (holiday, 0) as date_d&#13;&#10;    from holiday&#13;&#10;   where kv = 980&#13;&#10;  union all&#13;&#10;  select fdat, fdat from fdat),&#13;&#10;corp as &#13;&#10;(select corp.base_corp  as corp_l1,&#13;&#10;       case&#13;&#10;         when level = 3 then&#13;&#10;          (select c.external_id&#13;&#10;             from ob_corporation c&#13;&#10;            where c.id = corp.parent_id)&#13;&#10;         else corp.external_id&#13;&#10;       end              as corp_l2,&#13;&#10;       corp.external_id as corp_l3&#13;&#10;  from ob_corporation corp&#13;&#10; START WITH corp.parent_id is null&#13;&#10; connect by prior corp.id = corp.parent_id)  &#13;&#10;select c.CORPORATION_NAME as name, ost.kod_ustan, ost.corp_id,&#13;&#10;       nvl(round(ost.s2600_2604)/100, 0) as AVG_2600_4, &#13;&#10;       nvl(round(ost.s2600_2604_v)/100, 0) as AVG_2600_4_V,&#13;&#10;       nvl(round(ost.s2603)/100, 0) as AVG_2603,&#13;&#10;       nvl(round(ost.s2610)/100, 0) as AVG_2610,&#13;&#10;       nvl(round(ost.s2605)/100, 0) as AVG_2605 &#13;&#10;from&#13;&#10;OB_CORPORATION c &#13;&#10;join &#13;&#10;(SELECT SUM(case when cd.nbs in ('2600', '2604') then cd.ostq-cd.obkrq+cd.obdbq else 0 end) / count (distinct (d.date_f)) as s2600_2604,&#13;&#10;        SUM(case when cd.nbs in ('2600', '2604') and cd.kv&lt;&gt;980 then cd.ostq-cd.obkrq+cd.obdbq else 0 end) / count (distinct (d.date_f)) as s2600_2604_v, &#13;&#10;        SUM(case when cd.nbs = '2603' then cd.ostq-cd.obkrq+cd.obdbq else 0 end) / count (distinct (d.date_f)) as s2603,&#13;&#10;        SUM(case when cd.nbs = '2610' then cd.ostq-cd.obkrq+cd.obdbq else 0 end) / count (distinct (d.date_f)) as s2610,  &#13;&#10;        SUM(case when cd.nbs = '2605' then cd.ostq-cd.obkrq+cd.obdbq else 0 end) / count (distinct (d.date_f)) as s2605,               &#13;&#10;        corp.corp_l2 as kod_ustan, cd.corp_id&#13;&#10; FROM ob_corp_data_acc cd&#13;&#10; join corp on to_char(cd.kod_ustan) = corp.corp_l3 and to_char(CD.CORP_ID) = corp.corp_l1&#13;&#10; join dates d on d.date_d=cd.fdat &#13;&#10; join v_ob_corp_rep_nbs q on q.nbs = cd.nbs and Q.CORP_ID = cd.corp_id&#13;&#10; WHERE cd.is_last = 1 &#13;&#10;       AND q.rep_id = 5021 &#13;&#10;       AND CD.CORP_ID = 17&#13;&#10;       AND cd.nbs in ('2600', '2604', '2603', '2610', '2605') &#13;&#10;       AND d.date_f between :sFdat1 and :sFdat2&#13;&#10;       AND cd.fdat between :sFdat1 and :sFdat2+10&#13;&#10; group by corp.corp_l2, cd.corp_id) ost on c.EXTERNAL_ID = to_char(ost.kod_ustan) and c.base_corp = ost.corp_id &#13;&#10; order by ost.kod_ustan">
