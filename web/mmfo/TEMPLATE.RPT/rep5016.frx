    <OracleDataConnection Name="Connection" ConnectionString="rijcmlqHytUMhhPg9gG4fal5Q6rRB0BvRbh0nDbCWvtDA1bLoX3V4zY8FFHzkIAqmYIxZTfhjnw0iMVk0YjYK1Ztlx8Ig==">
      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="SELECT t1.name, t1.ord, t2.kf ourmfo, nvl(t2.sum_post,0)sum_post, nvl(t3.sum_gu_pf,0)sum_gu_pf,t1.kod_s&#13;&#10;    FROM clim_mfo t1&#13;&#10;         LEFT JOIN&#13;&#10;         (  SELECT cd.kf, SUM (nvl(cd.sq,0)) / 100 sum_post&#13;&#10;              FROM ob_corporation_data cd&#13;&#10;             WHERE     cd.corporation_id = 1&#13;&#10;                   and kfile_pack.get_correct_session_id(cd.corporation_id,cd.kf,cd.session_id)=1&#13;&#10;                   and kfile_pack.get_correct_nbs(cd.corporation_id,substr(cd.nls,1,4))=1&#13;&#10;                   AND cd.rowtype = 1&#13;&#10;                   AND cd.nlsa LIKE '2560%'&#13;&#10;                   AND cd.nlsb LIKE '2604%'&#13;&#10;                   AND trunc(cd.file_date) &gt;= TRUNC(:rpt_date, 'DDD') AND trunc(cd.file_date) &lt; TRUNC(:rpt_date + 1, 'DDD')&#13;&#10;          GROUP BY cd.kf) t2&#13;&#10;            ON t1.mfo = t2.kf&#13;&#10;         LEFT JOIN&#13;&#10;         (  SELECT cd.kf, SUM (nvl(cd.sq,0)) / 100 sum_gu_pf&#13;&#10;              FROM  ob_corporation_data cd&#13;&#10;             WHERE     cd.corporation_id = 1&#13;&#10;                   and kfile_pack.get_correct_session_id(cd.corporation_id,cd.kf,cd.session_id)=1&#13;&#10;                   and kfile_pack.get_correct_nbs(cd.corporation_id,substr(cd.nls,1,4))=1&#13;&#10;                   AND cd.rowtype = 1&#13;&#10;                   and okpob in (select corporation_code from ob_corporation where parent_id=1 )&#13;&#10;                   and nlsb in (select nls from ob_corporation_data a where rowtype = 0 and a.kf=cd.kf and trunc(a.file_date) &gt;= TRUNC(:rpt_date, 'DDD') AND trunc(a.file_date) &lt; TRUNC(:rpt_date + 1, 'DDD')  and kod_analyt=40 )&#13;&#10;                   --AND cd.kod_analyt = '40'&#13;&#10;                   AND trunc(cd.file_date) &gt;= TRUNC(:rpt_date, 'DDD') AND trunc(cd.file_date) &lt; TRUNC(:rpt_date + 1, 'DDD')&#13;&#10;          GROUP BY cd.kf) t3&#13;&#10;            ON t1.mfo = t3.kf&#13;&#10;ORDER BY t1.ord">
