        <TableRow Name="Row6" Height="13.23">
          <TableCell Name="Cell23" Text="2. Умови Депозиту" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 7pt, style=Bold" ColSpan="5"/>
          <TableCell Name="Cell376"/>
          <TableCell Name="Cell434"/>
          <TableCell Name="Cell492"/>
