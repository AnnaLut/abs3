    private void _FinishReport(object sender, EventArgs e)
    {
      OracleDataConnection frxconn = (OracleDataConnection)Report.Dictionary.Connections[0];
      
      using ( OracleConnection connection = (OracleConnection)frxconn.GetConnection() )
      {
        connection.Open();
        
        using ( OracleCommand command = connection.CreateCommand() )
        { 
          command.CommandType = CommandType.StoredProcedure;
          command.CommandText = &quot;bars.ead_pack.doc_page_count&quot;;
      
          Int64 docId = (Int64)Report.GetParameterValue(&quot;p_doc_id&quot;);
          if ( docId &gt; 0 )
          {  
            command.Parameters.Add(&quot;p_doc_id&quot;, OracleDbType.Int64, docId, ParameterDirection.Input );
            Int32 pageCount = (Int32)Report.GetVariableValue(&quot;TotalPages&quot;);
            command.Parameters.Add(&quot;p_page_count&quot;,OracleDbType.Int32, pageCount, ParameterDirection.Input);
      
            command.ExecuteNonQuery();
          } 
        }  
      }
    }
  }
}
