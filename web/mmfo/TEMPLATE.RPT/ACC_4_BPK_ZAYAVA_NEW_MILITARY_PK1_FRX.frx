        <TableRow Name="Row848" MinHeight="11.34" Height="11.34" AutoSize="true">
          <TableCell Name="Cell6908" Border.Lines="All" Border.Width="0.25" Text="Блок 6: Умови кредитування:" Font="Times New Roman, 7pt, style=Bold" ColSpan="4"/>
