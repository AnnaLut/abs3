        <TableRow Name="Row1030">
          <TableCell Name="Cell7469" Border.Lines="All" Text="Додаткові та супутні послуги третіх осіб, обов'язкові для отримання кредиту:" VertAlign="Center" Font="Times New Roman, 7pt"/>
          <TableCell Name="Cell7470" Border.Lines="All" Text="відсутні" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 7pt, style=Italic"/>
        </TableRow>
        <TableRow Name="Row1031">
          <TableCell Name="Cell7471" Border.Lines="All" Text="послуги нотаріуса" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 7pt"/>
