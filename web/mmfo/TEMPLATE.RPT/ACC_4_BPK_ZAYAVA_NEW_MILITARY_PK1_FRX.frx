        <TableRow Name="Row112" Height="11.34">
          <TableCell Name="Cell457" Border.Lines="All" Border.Width="0.5" Text="Блок 5: Заяви та запевнення Клієнта" HorzAlign="Justify" Font="Times New Roman, 7pt, style=Bold" HtmlTags="true" ColSpan="6"/>
          <TableCell Name="Cell406" HorzAlign="Justify"/>
