          <TableCell Name="Cell455" Border.Lines="All" Border.Width="0.5" Text="Блок 4:  Відкриття вкладного рахунку за допомогою Системи ДБО" HorzAlign="Justify" Font="Times New Roman, 7pt, style=Bold" HtmlTags="true" ColSpan="6"/>
          <TableCell Name="Cell402" HorzAlign="Justify"/>
