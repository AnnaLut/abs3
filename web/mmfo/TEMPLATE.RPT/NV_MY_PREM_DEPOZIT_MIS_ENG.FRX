      <TableDataSource Name="Table6" Alias="COMMON" DataType="System.Int32" Enabled="true" SelectCommand="select F_OURMFO_G as bank_mfo,&#13;&#10;       f_ourokpo as bank_okpo,&#13;&#10;       --F_OURNAME_G as bank_name,&#13;&#10;       --f_translate_kmu(F_OURNAME_G) as bank_name_en,&#13;&#10;       F_OURADDRESS as bank_address,&#13;&#10;       f_translate_kmu(F_OURADDRESS) as bank_address_en,&#13;&#10;      'ТВБВ Банку '||'№'||tvbv_num ||' філії-'||(SELECT val  FROM params  WHERE LOWER(par)=LOWER('NAME'))     as BRANCH_NAME,&#13;&#10;      'Bank Branch '||'№'||tvbv_num ||' of '||(nvl(trim(BRANCH_ATTRIBUTE_UTL.get_value(b.branch,'BRANCH_NAME_ENG')), '____')) BRANCH_NAME_EN,&#13;&#10;       nvl(branch_edit.get_branch_parameter(b.branch, 'ADR_BRANCH' ), '________') as BRANCH_ADDRESS,&#13;&#10;       nvl(branch_edit.get_branch_parameter(b.branch, 'ADR_BRANCH_ENG' ), '________') as BRANCH_ADDRESS_EN,&#13;&#10;       nvl(branch_edit.get_branch_parameter(b.branch, 'DPT_PHON'   ), branch_edit.get_branch_parameter(b.branch, 'BPK_PHONE')) as BRANCH_PHONE,&#13;&#10;       nvl(branch_edit.get_branch_parameter(b.branch, 'TOWN'       ), '________') as BRANCH_TOWN,&#13;&#10;       nvl(branch_edit.get_branch_parameter(b.branch, 'TOWN_ENG'       ), '________') as BRANCH_TOWN_EN,&#13;&#10;       decode(poa_doc, null,'________', poa_doc || ' від ' || to_char(poa_date, 'dd/mm/yyyy')) as STAFF_DOVER,&#13;&#10;       decode(doc_basis, null,'________',doc_basis) as STAFF_DOVER_EN,&#13;&#10;       nvl(fio_r, '________') as STAFF_FIO_R,&#13;&#10;       f_translate_kmu(fio) fio_en,&#13;&#10;       nvl(post_r, '________') as STAFF_STATUS_R,       &#13;&#10;       nvl(post_eng, '________') as STAFF_STATUS_ENG        &#13;&#10;  from branch b,&#13;&#10;       (select *&#13;&#10;          from (select *&#13;&#10;                  from dpt_poa_branches pb, dpt_poas p&#13;&#10;                 where pb.branch = branch_usr.get_branch()&#13;&#10;                   and pb.active = 1&#13;&#10;                   and pb.poa_id = p.id&#13;&#10;                 order by decode(p.staff_id, user_id, 0, pb.ord))&#13;&#10;         where rownum = 1) t&#13;&#10; where b.branch = t.branch(+)&#13;&#10;   and b.branch = branch_usr.get_branch()">
