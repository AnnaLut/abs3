<script>
    $("#splitter").kendoSplitter({
        orientation: "vertical",
        panes: [{ size: "30%" }, {}]       
    })
  
    $("#tabviddstrip").kendoTabStrip({
        animation: {
            close: {
                duration: 0,
                effects: "fadeOut"
            },
            open: {
                duration: 0,
                effects: "fadeIn"
            }
        }
    })

    $("#panelbar").kendoPanelBar()

    var onSelect = function (e) {
        $("#panelbar").data("kendoPanelBar").bind("select", onSelect);
    }

    $("#panelbar").data("kendoPanelBar").bind("select", onSelect);

    $('#wnd_vidd_info').keypress(function (e) {
        if (e.which == 13) {
            $("#buttonSave").click();
        }
        if (e.which == 27) {           
            $('#wnd_vidd_info').close;
        }
    })
</script>