<link rel="stylesheet" href="~/Areas/GDA/Css/GDA/Main.css" />
<div ng-controller="GDACtrlTest">
    <div class="page-header">
        <h1>Портфель депозитів ММСБ фронт-офіс</h1>
    </div>
    @*<hr>*@
    @*Поисковый блок*@
    <div class="well">
        <div class="row">
            <div class="form-group" id="contract_number" style="margin-top: 10px;">
                <a ng-click="GetInfo('dbo')" id="infoBtn" style="margin-right:15px;" title="Підказки користувачу"><i class="pf-icon pf-16 pf-help"></i></a>
                <label><b>Номер ДБО:</b></label>
                <input id="numberDBO" type="text" class="form-control" style="display: inline;width: 180px;margin-right:18px;"
                       ng-model="contract_number" numbers-only />
            </div>

            <div class="form-group" id="rnk">
                <label><b>РНК:</b></label>
                <input id="rnk" type="text" class="form-control" style="display: inline;width: 180px; margin-left:22px;margin-right:18px;"
                       ng-model="rnk" numbers-only />
            </div>

            <div class="form-group" id="okpo">
                <label><b>ЄДРПОУ:</b></label>
                <input id="okpo" class="form-control" style="display: inline;width: 180px;margin-left:18px;margin-right:18px;"
                       ng-model="okpo" numbers-only />
            </div>
            <span id="searchButtonWrap">
                <a class="btn btn-default" ng-click="Find()" id="btnSearch"><i class="pf-icon pf-16 pf-find"></i>Пошук</a>
                <a class="btn btn-default" ng-click="ResetFind()" id="btnResetSearch" title="Очистити пошук"><i class="pf-icon pf-16 pf-filter-remove"></i></a>  @*pf-filter-remove*@
            </span>
        </div>
    </div>

    @*Вкладки Портфель ДБО + Портфель депозитів ММСБ*@
    <div kendo-tab-strip="tabStrip" k-options="tabStripOptions" k-content-urls="[ null, null]" A>
        <ul>
            <li class="k-state-active" id="DBOPorfolio">Портфель ДБО</li>
            <li class="k-item" id="DepositPortfolio">Портфель депозитів ММСБ</li>
            <li class="k-item" id="OperationistDBOPorfolio">Депозити операціоніста</li>
        </ul>
        @*Делаем неактивным поиск, когда стоим на вкладке Портфель Депозитів ММСБ*@
        <script type="text/javascript">
            $('#DepositPortfolio').click(function () {
                $(".well").addClass('disableSearch');

            });
            $("#DBOPorfolio").click(function () {
                $(".well").removeClass('disableSearch');
            });
            $("#OperationistDBOPorfolio").click(function () {
                $(".well").addClass('disableSearch');
                //$("#OperationistDBOGrid").data("kendoGrid").dataSource.read();
                //$("#OperationistDBOGrid").data("kendoGrid").refresh();
            });
        </script>

        @*Вкладка Портфель ДБО*@
        <div style="padding:1px;">
            <div id="DBOGrid" kendo-grid="DBOGrid" k-options="DBOGridOptions"></div>
        </div>



        @*Вкладки портфеля ММСБ*@
        <div style="padding:15px;">
            <div style="margin-bottom:10px;" id="messageDBO">
                <div>
                    @*РНК:<span><br>*@
                    <b> <i class="pf-icon pf-16 pf-user_group" title="РНК клієнта"></i> {{curAccInfo('rnk')}}</b>
                    @*</span>*@
                </div>
                <div>
                    @*  Ім'я клієнта:<br>*@
                    <b> <i class="pf-icon pf-16 pf-user" title="Ім'я клієнта"></i> {{curAccInfo('nmk')}}</b>
                </div>
                <div>
                    @*Номер ДБО:<br>*@
                    <b> <i class="pf-icon pf-16 pf-book" title="Номер ДБО"></i> {{curAccInfo('contract_number')}}</b>
                </div>
                <br>
                <div id="message" ng-show="CheckMessage()">Значень немає.Перейдіть у вкладку "Портфель ДБО/Депозити операціоніста" та оберіть потрібний номер договору</div>
            </div>

            <div style="margin-bottom:10px; display:none;" id="messageOperator">
                <div>
                    @*РНК:<span><br>*@
                    <b> <i class="pf-icon pf-16 pf-user_group" title="РНК клієнта"></i>{{currOperDBO('Customer_id',true)}}</b>
                    @*</span>*@
                </div>
                <div>
                    @*Ім'я клієнта:<br>*@
                    <b> <i class="pf-icon pf-16 pf-user" title="Ім'я клієнта"></i>{{currOperDBO('Customer_name',true)}}</b>
                </div>
                <div>
                    @*Номер ДБО:<br>*@
                    <b> <i class="pf-icon pf-16 pf-book" title="Номер ДБО"></i> {{currOperDBO('Contract_number',true)}}</b>
                </div>
                <div id="message" ng-show="CheckMessage()">Значень немає.Перейдіть у вкладку "Портфель ДБО/Депозити операціоніста" та оберіть потрібний номер договору</div>
            </div>



            @*<div style="margin-bottom:10px; display:none;" id="messageOperator" >
                    <h4><span>РНК:<b> {{currOperDBO('Customer_id')}} </b></span></h4>
                    <h4><span>Ім'я клієнта:<b> {{currOperDBO('Customer_name')}}</b></span> <span id="message" ng-show="CheckMessage()">Значень немає.Перейдіть у вкладку "Портфель ДБО/Депозити операціоніста" та оберіть потрібний номер договору</span></h4>
                    <h4><span>Номер ДБО:<b> {{currOperDBO('Contract_number')}}</b></span></h4>
                </div>*@


            <div kendo-tab-strip="tabStrip" k-options="tabStripOptions" ng-show="!CheckMessage()">
                <ul>
                    <li id="timeTranches" class="k-state-active">Cтрокові транші</li>
                    <li id="depositDemand">Вклади на вимогу</li>
                    <li id="depositAcc">Депозитні рахунки</li>
                </ul>

                <div>
                    <div class="checkbox">
                        <label><input id="showCloseTranches" type="checkbox" ng-model="isShowClosedTranches" ng-change="onShowClosedTranches()"><b>Показати закриті транші</b></label>
                        <a ng-click="GetInfo(timetranche)" id="infoBtn" style="margin-right:15px; margin-left: 15px;" title="Підказки користувачу"><i class="pf-icon pf-16 pf-help"></i></a>
                    </div>


                    <div id="tlbarTimeTranshes" data-kendo-toolbar="toolbarTimeTranshes" k-options="toolbarTimeTranshOptions"> </div>
                    <div id="timetranchesgrid" kendo-grid="TimeTranshesGrid" k-options="TimeTranshesGridOptions"></div>

                </div>

                @*Панель кнопок вклада на требование*@
                <div>
                    <div id="tlbarRequireDeposit" data-kendo-toolbar="toolbarRequireDeposit" k-options="toolbarRequireDepositOptions"> </div>
                    <div id="requireDepositGrid" kendo-grid="RequireDepositGrid" k-options="RequireDepositGridOptions"></div>
                </div>



                @*Розміщення траншу*@
                <div id="placeTranche" data-kendo-window="placementTrancheWindow" data-options="placementTrancheWindowOptions" style="display: none" k-modal="true">
                    <div data-kendo-toolbar="toolbarPlacementTranche" k-options="toolbarPlacementTrancheOptions"> </div>

                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="form-group">
                                <table border="0">
                                    <tr>
                                        <td>
                                            <div id="labelPlaceDBO" style="max-height:100px;">
                                                <div style="display:inline-block; margin-right:10px;">
                                                    <label>ЄДРПОУ:</label>
                                                    <input type="text" class="k-textbox" style="width: 160px;background-color:cornsilk"
                                                           ng-disabled="true" value="{{curAccInfo('okpo')}}" />
                                                </div>

                                                <div style="display:inline-block">
                                                    <label>Назва Клієнта:</label>
                                                    <input type="text" class="k-textbox" style="width: 570px;background-color:cornsilk"
                                                           ng-disabled="true" value="{{curAccInfo('nmk')}}" />
                                                </div>
                                                <hr>
                                            </div>
                                            <div id="labelPlaceOper" style="display:none;max-height:100px;">
                                                <div style="display:inline-block; margin-right:10px;">
                                                    <label>ЄДРПОУ:</label>
                                                    <input type="text" class="k-textbox" style="width: 160px;background-color:cornsilk"
                                                           ng-disabled="true" value="{{currOperDBO('Deposit_id')}}" />
                                                </div>

                                                <div style="display:inline-block">
                                                    <label>Назва Клієнта:</label>
                                                    <input type="text" class="k-textbox" style="width: 270px;background-color:cornsilk"
                                                           ng-disabled="true" value="{{currOperDBO('Customer_name')}}" />
                                                </div>
                                                <hr>
                                            </div>
                                        </td>
                                    </tr>

                                    <tr>
                                        <td>
                                            <label>№ Заяви про розміщ.траншу</label>
                                            <input type="text" class="k-textbox" style="width: 260px;background-color:cornsilk"
                                                   ng-disabled="true" ng-model="placementTranche.kontractPlacement" />

                                            <label>Рег.№:</label>
                                            <input type="text" class="k-textbox" ng-model="placementTranche.CustomerId"
                                                   style="width: 110px;background-color:cornsilk" ng-disabled="true" />


                                            <label>Дата заяви про розміщ.траншу</label>
                                            <input type="text" class="form-control" style="width:120px" kendo-date-picker
                                                   ng-model="placementTranche.StartDate" disabled="disabled"
                                                   k-options="dateOptionsWithoutMin" />

                                        </td>
                                    </tr>


                                    <tr> <td> <hr /> </td> </tr>
                                    <tr>
                                        <td>
                                            <label>Валюта:</label>
                                            <span style="color: red" ng-show="!isValueValid('placementTranche','CurrencyId')">*</span>
                                            <input id="placementCurrency" type="text" class="k-textbox" ng-keypress="checkIfEnterKeyWasPressed($event)" ng-model="placementTranche.CurrencyId" ng-disabled="false" style="width: 40px; background-color:white !important " ng-blur="GetAccounts('placement');" />
                                            <a class='btn btn-default' ng-click="onHandBook('placementTranche','CurrencyId')" ng-disabled="!isEditable('earlyRepaymentTranche')">
                                                <i class='pf-icon pf-16 pf-book'></i>
                                            </a>

                                            <label>Сума траншу:</label>
                                            <span style="color: red" ng-show="!isValueValid('placementTranche','amountTranche')">*</span>
                                            <input class="k-textbox" style="width: 100px"
                                                   k-max="99 999"
                                                   ng-model="placementTranche.amountTranche" ng-disabled="!isEditable('placementTranche')" k-spinners="false" />

                                            <label>Депозитний рахунок:</label>
                                            <input type="text" class="k-textbox" ng-model="placementTranche.PrimaryAccount"
                                                   style="width: 250px;background-color:cornsilk" ng-disabled="true" />
                                        </td>
                                    </tr>
                                    <tr> <td> <hr /> </td> </tr>
                                    <tr>
                                        <td>
                                            <label>Строк траншу:</label>
                                            <span style="color: red" ng-show="!isValueValid('placementTranche','NumberTrancheDays')">*</span>
                                            <input type="text" class="k-textbox" style="width: 40px;"
                                                   ng-model="placementTranche.NumberTrancheDays" ng-disabled="" ng-change="test($event)"
                                                   ng-blur="blurValue('placementTranche', 'NumberTrancheDays')" />

                                            <label>Дата розміщення:</label>
                                            <input id="startDateCalendar" type="text" class="form-control" style="width:140px"
                                                   ng-model="placementTranche.StartDate" kendo-date-picker
                                                   k-options="dateOptionsWithoutMin"
                                                   ng-disabled="true"
                                                   disabled="disabled" />

                                            <label>Дата повернення:</label>
                                            <span style="color: red" ng-show="!isValueValid('placementTranche','ExpiryDate')">*</span>
                                            <input id="expiryDateCalendar" type="text" class="form-control" style="width:140px" ng-disabled="false"
                                                   ng-model="placementTranche.ExpiryDate"
                                                   kendo-date-picker
                                                   k-options="dateOptions"
                                                   ng-blur="blurValue('placementTranche', 'ExpiryDate')" />
                                        </td>
                                    </tr>
                                    <tr> <td> <hr /> </td> </tr>
                                    <tr>
                                        <td>
                                            <label>Рахунок для списання:</label>
                                            <span style="color: red" ng-show="!isValueValid('placementTranche','DebitAccount')">*</span>

                                            <select id="DebitAcc"
                                                    kendo-drop-down-list style="width: 18%"
                                                    k-options="DebitAccOptions"
                                                    ng-model="placementTranche.DebitAccount"></select>


                                            <label>Залишок на рахунку для списання:</label>
                                            <input id="placementOstc" kendo-numeric-text-box k-min="0" style="width:100px;background-color:cornsilk"
                                                   k-max="999999999" ng-disabled="true" k-spinners="false" k-ng-disabled="true" />
                                            <br>
                                            <br>

                                            <label>Рахунок для повернення:</label>
                                            <span style="color: red" ng-show="!isValueValid('earlyRepaymentTranche','returnAccount')">*</span>
                                            <select id="DebitReturn"
                                                    kendo-drop-down-list style="width: 18%"
                                                    k-options="DebitAccOptions"
                                                    ng-model="placementTranche.ReturnAccount"></select>
                                        </td>
                                    </tr>
                                    <tr> <td> <hr /> </td> </tr>
                                    <tr>
                                        <td>
                                            <label>Автопролонгація:</label>
                                            <select kendo-drop-down-list
                                                    k-options="IsProlongationOptions"
                                                    ng-model="placementTranche.IsProlongation" style="width:65px"></select>

                                            <label>Кількість автопролонгацій:</label>
                                            <select id="NumberProlongationList"
                                                    kendo-drop-down-list
                                                    k-options="NumberProlongationOptions"
                                                    style="width: 300px"
                                                    disabled="disabled"
                                                    ng-disabled="isFieldDisabled('placementTranche','NumberProlongation')"
                                                    ng-model="placementTranche.NumberProlongation"></select>


                                            <label>Термін:</label>
                                            <input id="applyprologField" class="k-textbox" style="width:100px;background-color:cornsilk"
                                                   ng-disabled="true" k-spinners="false" k-ng-disabled="true" />
                                        </td>
                                    </tr>
                                    <tr> <td> <hr /> </td> </tr>

                                    <tr>
                                        <td>
                                            <label>Поповнення траншу:</label>
                                            <select id="isReplenishmentTran" kendo-drop-down-list
                                                    k-options="isReplenishmentTrancheOptions"
                                                    ng-model="placementTranche.IsReplenishmentTranche" style="width:55px"></select>

                                            <label>Строк поповнення траншу до:</label>
                                            <span style="color: red" ng-show="!isValueValid('placementTranche','lastReplenishmentDate')">*</span>
                                            @*<input id="returnReplenishment" type="text" ng-model="placementTranche.LastReplenishmentDate" ng-disabled="true" class="k-textbox" style="width:80px;background-color:cornsilk"/>*@
                                            <input id="dateReplenishmentToPlacement"
                                                   type="text"
                                                   class="form-control"
                                                   style="width:140px"
                                                   ng-disabled="true"
                                                   disabled="disabled"
                                                   ng-model="placementTranche.LastReplenishmentDate"
                                                   kendo-date-picker
                                                   k-options="dateOptionsWithoutMin" />

                                            <label>Періодичність виплати</label>
                                            <select kendo-drop-down-list
                                                    k-options="FrequencyListOptions"
                                                    ng-model="placementTranche.FrequencyPayment" style="width:125px"></select>


                                        </td>


                                    </tr>
                                    <tr> <td> <hr /> </td> </tr>
                                    <tr>
                                        <td>
                                            <label>Капіталізація:</label>
                                            <select kendo-drop-down-list
                                                    k-options="IsCapitalizationOptions"
                                                    id="placementIsCapitalizationList"
                                                    ng-model="placementTranche.IsCapitalization"
                                                    ng-disabled="isFieldDisabled('placementTranche','IsCapitalization')"
                                                    style="width:75px"></select>

                                            <label>Індивідуальна ставка:</label>
                                            <select kendo-drop-down-list
                                                    k-options="IsIndividualRateTrancheOptions"
                                                    ng-model="placementTranche.IsIndividualRate" style="width:65px"></select>

                                            <label>% ставка:</label>
                                            <span style="color: red" ng-show="!isValueValid('placementTranche','isIndividualRate')">*</span>
                                            <input kendo-numeric-text-box k-min="0" style="width: 80px"
                                                   k-max="100" @*ng-disabled="placementTranche.IsIndividualRate == 1 ? false : true"*@
                                                   ng-disabled="isFieldDisabled('placementTranche','IndividualInterestRate')"
                                                   k-spinners="false"
                                                   ng-model="placementTranche.IndividualInterestRate" />


                                        </td>
                                    </tr>
                                    <tr> <td> <hr /> </td> </tr>
                                    <tr>
                                        <td>

                                            <label>Розрахункова % ставка:</label>
                                            <input id="interestRate" kendo-numeric-text-box k-min="-100" style="width: 70px; background-color:cornsilk" k-spinners="false"
                                                   k-max="100" ng-disabled="true"
                                                   ng-model="placementTranche.InterestRate" />
                                            <button id="countPercentBtn" class='btn btn-default' ng-click="onClick('placementTranche', 'count')" title="Розрахувати"><i class="pf-icon pf-16 pf-percentage"></i>Розрахувати</button>
                                            <button id="showDetailsPercentBtn" disabled="disabled" class='btn btn-default' ng-click="onClick('placementTranche', 'showDetails')" title="Показати деталі"><i class="pf-icon pf-16 pf-accept_doc"></i>Показати деталі</button>
                                        </td>
                                    </tr>

                                    <tr></tr>
                                    <tr> <td> <hr /> </td> </tr>
                                    <tr>
                                        <td>
                                            <label>Коментар (максимум - 1000 символів):</label>
                                            <span style="color: red" ng-show="!isValueValid('placementTranche','comment')">*</span>
                                            <textarea rows="3" maxlength="1000" ng-model="placementTranche.Comment"
                                                      @*ng-disabled="!placementTranche.IsIndividualRate"*@
                                                      ng-disabled="isFieldDisabled('placementTranche','Comment')"
                                                      class="k-textbox"></textarea>
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>

                    <div style="background-color: #F5F5F5; padding: 10px; width:935px">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><b>Оператор:</b></label>
                                    <input type="text" class="k-textbox" style="width: 250px;background-color:cornsilk"
                                           ng-model="placementTranche.OperatorFullName" ng-disabled="true" />
                                </div>
                            </div>
                            <div class="col-md-1">
                                <div class="form-group">
                                    <label><b>Дата:</b></label>
                                    <input type="text" class="form-control" style="width:110px" ng-disabled="true" disabled="disabled"
                                           ng-model="placementTranche.OperatorSysTime" kendo-date-picker k-options="dateOptionsWithoutMin" />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group" style="margin-left:12px;">
                                    <label><b>Контролер:</b></label>
                                    <input type="text" class="k-textbox" style="width: 250px;background-color:cornsilk"
                                           ng-disabled="true" disabled="disabled" ng-model="placementTranche.ControllerFullName" />
                                </div>
                            </div>
                            <div class="col-md-1">
                                <div class="form-group">
                                    <label><b>Дата:</b></label>
                                    <input type="text" class="form-control" style="width:115px" ng-disabled="true"
                                           ng-model="placementTranche.ControllerSysTime" kendo-date-picker k-options="dateOptionsWithoutMin" />
                                </div>
                            </div>
                            <div class="col-md-1">
                                <div class="form-group" style="margin-left:12px;">
                                    <label><b>Статус:</b></label>
                                    <input type="text" class="k-textbox" style="width: 80px;background-color:cornsilk"
                                           ng-disabled="true" disabled="disabled" ng-model="placementTranche.Status" />
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                @*Кінець розміщення траншу*@

                @*Редагування траншу*@
                <div id='replaceTranche' data-kendo-window="replacementTrancheWindow" data-options="replacementTrancheWindowOptions" style="display: none" k-modal="true">
                    <div data-kendo-toolbar="toolbarReplacementTranche" k-options="toolbarReplacementTrancheOptions"> </div>

                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="form-group">
                                <table border="0">
                                    <tr>
                                        <td>
                                            <div id="labelReplaceDBO">
                                                <div style="display:inline-block; margin-right:10px;">
                                                    <label>ЄДРПОУ:</label>
                                                    <input type="text" class="k-textbox" style="width: 160px;background-color:cornsilk"
                                                           ng-disabled="true" value="{{curAccInfo('okpo')}}" />
                                                </div>

                                                <div style="display:inline-block">
                                                    <label>Назва Клієнта:</label>
                                                    <input type="text" class="k-textbox" style="width: 370px;background-color:cornsilk"
                                                           ng-disabled="true" value="{{curAccInfo('nmk')}}" />
                                                </div>
                                                <div style="display:inline-block">
                                                    <label>Документ підписано:</label>
                                                    <select kendo-drop-down-list
                                                            k-options="IsSignedOptions"
                                                            ng-model="replacementTranche.IsSigned" style="width:65px"></select>
                                                </div>
                                                <hr>
                                            </div>
                                            <div id="labelReplaceOper" style="display:none;">
                                                <div style="display:inline-block; margin-right:10px;">
                                                    <label>ЄДРПОУ:</label>
                                                    <input type="text" class="k-textbox" style="width: 160px;background-color:cornsilk"
                                                           ng-disabled="true" value="{{currOperDBO('Deposit_id')}}" />
                                                </div>

