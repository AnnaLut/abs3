<asp:Button SkinId="bCalc" runat="server" Text="Расчитать" ToolTip="Расчитать" BorderWidth="0" BorderStyle="Solid" Font-Bold="true" BackColor="#D6E7F7" ForeColor="#046695" BorderColor="#046695" />
