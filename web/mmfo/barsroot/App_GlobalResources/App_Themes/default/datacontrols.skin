    <InsertRowStyle BackColor="#E3EAEB" />
    <EmptyDataRowStyle BackColor="#E3EAEB" />
</asp:FormView>

<asp:Image runat="server" SkinId="imgLoading" ImageUrl="Images/loader.gif" />

