<Report ScriptLanguage="CSharp" ReportInfo.Created="10/03/2014 16:02:53" ReportInfo.Modified="10/09/2014 15:00:11" ReportInfo.CreatorVersion="1.7.1.0">
  <Dictionary>
    <OracleDataConnection Name="barsdev" ConnectionString="rijcmlq9SEZoacNJntaKIKp6mVF1+kSHs9h2FEASDb2ziQ1rMUybLG+C6jbSqka/GGQeZKaQSbrDjBY4KltUNuVSQtSEw==">
      <TableDataSource Name="Table" Alias="t" DataType="System.Int32" Enabled="true" SelectCommand="select x.*, c.nmk, c.adr, decode(x.contr_type, 0, 'екс', 'ім') as t1, decode(x.contr_type, 0, '2.3', '3.3') as t6,       &#13;&#10;       decode(x.contr_type, 0, 'Номер та дата МД / Акту', 'Дата платежу') as t7, &#13;&#10;       decode(x.contr_type, 0, 'Сума, що не надійшла', 'Сума заборгованості') as t8,&#13;&#10;       decode(x.contr_type, 0, 'Дата закінчення законодавчо встановлених строків розрахунків', &#13;&#10;                               'Граничний термін поставки продукції') as t9,&#13;&#10;       decode(x.contr_type, 0, 'зарахування виручки за цією операцією на поточний рахунок останнього', &#13;&#10;                               'імпорту товару з увезенням його на територію України') as t2,&#13;&#10;       decode(x.contr_type, 0, 'зарахування виручки', 'надходження товару') as t3,&#13;&#10;       decode(x.contr_type, 0,&#13;&#10;                ' про зарахування виручки за цією операцією на відкритий у цьому банку поточний рахунок резидента', &#13;&#10;                ', якому митний орган надіслав інформацію про цю операцію резидента, про отримання такого реєстру')&#13;&#10;             as t4,&#13;&#10;       decode(x.contr_type, 0, &#13;&#10;                'зазначається вся наявна в реквізитах платежу інформація: номер і дата договору, сума платежу -', &#13;&#10;                'зазначаються номер і дата реєстру, вся наявна в реєстрі інформація про операцію резидента, а також вартість продукції') &#13;&#10;             as t5,   &#13;&#10;       to_char(last_day(to_date(:p_date_end,'dd/mm/yyyy'))+1,'dd.mm.yyyy') as end_date, b.name, b.phone       &#13;&#10;from &#13;&#10;  (select c.branch, c.rnk, c.contr_type, c.num, to_char(c.open_date, 'dd.mm.yyyy') as open_date, &#13;&#10;          b.benef_name, b.benef_adr, null as doc_num, to_char(p.vdat,'dd.mm.yyyy') as dat, &#13;&#10;          v.lcv as v, p.zs_vp as z, to_char(p.control_date,'dd.mm.yyyy') as txt_control_date, p.control_date &#13;&#10;   from v_cim_trade_payments p&#13;&#10;        join cim_contracts c on p.contr_id=c.contr_id and c.contr_type=1&#13;&#10;        join cim_beneficiaries b on c.benef_id=b.benef_id&#13;&#10;        join tabval$global v on p.v_pl=v.kv&#13;&#10;   where (:p_date_begin is null or  p.control_date &gt;= to_date(:p_date_begin,'dd/mm/yyyy')) and p.control_date &lt;= to_date(:p_date_end,'dd/mm/YYYY') and &#13;&#10;    p.zs_vk&gt;0 &#13;&#10;        and c.branch=sys_context('bars_context','user_branch')&#13;&#10;   union all&#13;&#10;   select c.branch, c.rnk, c.contr_type, c.num, to_char(c.open_date, 'dd.mm.yyyy') as open_date,&#13;&#10;          b.benef_name, b.benef_adr, p.num as doc_num, to_char(p.allow_date,'dd.mm.yyyy') as dat, &#13;&#10;          v.lcv as v, p.z_vt as z, to_char(p.control_date,'dd.mm.yyyy') as txt_control_date, p.control_date&#13;&#10;   from v_cim_bound_vmd p&#13;&#10;        join cim_contracts c on p.contr_id=c.contr_id and c.contr_type=0&#13;&#10;        join cim_beneficiaries b on c.benef_id=b.benef_id&#13;&#10;        join tabval$global v on p.vt=v.kv&#13;&#10;   where (:p_date_begin is null or  p.control_date &gt;= to_date(:p_date_begin,'dd/mm/yyyy')) and p.control_date &lt;= to_date(:p_date_end,'dd/mm/yyyy') and&#13;&#10;   p.z_vk&gt;0 &#13;&#10;         and c.branch=sys_context('bars_context','user_branch')&#13;&#10;  ) x     &#13;&#10;  join customer c on c.rnk=x.rnk  &#13;&#10;  join cim_journal_num b on b.branch=x.branch --sys_context('bars_context','user_branch')&#13;&#10;--where x.contr_type=0&#13;&#10;order by x.rnk, x.contr_type, x.open_date, x.num, x.control_date">
