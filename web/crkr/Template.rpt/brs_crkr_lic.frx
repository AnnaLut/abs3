      <TableDataSource Name="Table" Alias="Zap" DataType="System.Int32" Enabled="true" SelectCommand="Select  id, &#13;&#10;           datp, &#13;&#10;           text, &#13;&#10;           decode(dk,0,-1,1)*amount/100 sum_oper, &#13;&#10;           oper_ost/100 oper_ost, &#13;&#10;           dk , &#13;&#10;           first_value(oper_ost-decode(dk,0,-1,1)*amount) over ()/100 ostf, &#13;&#10;          last_value(oper_ost) over ()/100 ost,&#13;&#10;          sum(decode(dk,0, amount,0))  over ()/100 dos,&#13;&#10;          sum(decode(dk,1, amount,0))  over ()/100 kos &#13;&#10; from (&#13;&#10;        select TO_NUMBER(TO_CHAR(visa_date,'j.sssss')) id, trunc(visa_date) datp, text, amount, oper_ost, case when t.type_id in (1,2,4) then 0 else 1 end dk &#13;&#10;         from BARS.COMPEN_PORTFOLIO p &#13;&#10;          join  compen_oper o  on   O.COMPEN_ID = p.id&#13;&#10;          join  COMPEN_OPER_TYPES t on O.OPER_TYPE = T.TYPE_ID &#13;&#10;         where id = :p_id&#13;&#10;           and O.STATE = 20&#13;&#10;           and t.type_id &lt; 5&#13;&#10;           and O.VISA_DATE between :sFdat1 and :sFdat2&#13;&#10;        union all&#13;&#10;        select to_number(AO.IDM), ao.datp, AT.TXT text, ao.sumop, ao.ost, ao.dk&#13;&#10;          from BARS.COMPEN_PORTFOLIO p &#13;&#10;          join  COMPEN_MOTIONS ao  on   AO.ID_COMPEN = p.id&#13;&#10;          join  COMPEN_ASVOTYPO at on AT.TYPO = AO.TYPO&#13;&#10;         where id = :p_id&#13;&#10;           and ao.dk in (0,1)&#13;&#10;           and AO.DATP  between :sFdat1 and :sFdat2&#13;&#10;         order by 2,1  &#13;&#10;       )&#13;&#10;order by 2,1 ">
