      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="SELECT  rownum, S.* FROM &#13;&#10;(&#13;&#10;select &#13;&#10;    &#13;&#10;    case when a.source = 'BARS' then ac.nls else a.nls end nls,&#13;&#10;    a.kv, &#13;&#10;    case a.source when 'BARS' then 'БАРС' when 'АСВО' then 'АСВО'    else 'Інші' end source, &#13;&#10;    a.fio, &#13;&#10;    nvl(ac.ostc/100, 0) ostc , &#13;&#10;    A.OST/100 ost &#13;&#10;from bars.asvo_immobile a, bars.dpt_deposit d, bars.accounts ac&#13;&#10;where &#13;&#10;    a.dptid=d.deposit_id(+)&#13;&#10;    and d.acc=ac.acc(+)&#13;&#10;    and a.fl=4&#13;&#10;    and a.ost &lt;= bars.get_scale_immobile(nvl(a.kv,980))&#13;&#10;    and a.branch like sys_context('bars_context','user_branch_mask')&#13;&#10;ORDER BY    source, A.KV, a.fio ) S">
