        <TableRow Name="Row2" AutoSize="true">
          <TableCell Name="Cell2" Text="Вид та предмет супутніх послуг та обґрунтування їх вартості" HorzAlign="Center" Font="Times New Roman, 10pt, style=Bold" HtmlTags="true"/>
