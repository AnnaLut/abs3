      <TableDataSource Name="Table" Alias="ReestrClosedDPT" DataType="System.Int32" Enabled="true" SelectCommand="select Branch as &quot;Відділення&quot;,&#13;&#10;       nmk as &quot;Клієнт&quot;,&#13;&#10;       to_char(bday, 'dd.mm.yyyy') as &quot;Дата народження&quot;,&#13;&#10;       okpo as &quot;ЗКПО&quot;,&#13;&#10;       pass as &quot;Паспорт&quot;,&#13;&#10;       deposit_id as &quot;Номер депозитного договору&quot;,&#13;&#10;       to_char(dat_begin, 'dd.mm.yyyy') as &quot;Дата початку&quot;, &#13;&#10;       to_char(dat_end, 'dd.mm.yyyy') as &quot;Дата закінчення&quot;, &#13;&#10;       to_char(trunc(bdate), 'dd.mm.yyyy') as &quot;Дата розірвання&quot;, &#13;&#10;       lcv as &quot;Валюта&quot;, &#13;&#10;       fost(acc, trunc(when) - 1) / 100 as &quot;Залишок на момент розірвання&quot;, &#13;&#10;      BARS.get_deal_penalty(deposit_id ) as &quot;Сума штрафу&quot;       &#13;&#10;  from &#13;&#10;       (select dc.deposit_id, dc.datz as dat_begin, dc.dat_end, dc.bdate, dc.when, tv.lcv,dc.acc, dc.ref_dps, dc.branch, c.nmk, p.bday, c.okpo, p.ser || p.numdoc as pass&#13;&#10;          from dpt_deposit_clos dc, customer c, person p, tabval tv&#13;&#10;     where dc.action_id = 5 -- Під «втраченими процентами» мається на увазі сума штрафу утримана з клієнта в момент дострокового розірвання відповідно до умов договору (різні вклади мають різні умови дострокового розірвання) &#13;&#10;          and dc.rnk = c.rnk &#13;&#10;          and c.rnk = p.rnk&#13;&#10;          and dc.branch like :BRANCH&#13;&#10;          and tv.kv = dc.kv&#13;&#10;          and exists &#13;&#10;         (select 1 &#13;&#10;                  from dpt_deposit_clos dc1 &#13;&#10;                 where dc1.deposit_id =dc.deposit_id&#13;&#10;                   and dc1.action_id = 2) -- Під дію даних умов попадають вклади, які були повністю вилучені (залишки по тілу депозиту та відсотків дорівнюють нулю) &#13;&#10;           and fost(dc.acc, sysdate) / 100 = 0 -- 1. Під дію даних умов попадають вклади, які були повністю вилучені (залишки по тілу депозиту та відсотків дорівнюють нулю) &#13;&#10;           and (select bars.fost(ia.acra, sysdate) / 100 as int_bal &#13;&#10;                  from bars.int_accn ia &#13;&#10;                 where ia.acc = dc.acc &#13;&#10;                   and ia.id = 1) = 0 -- 2. Під дію даних умов попадають вклади, які були повністю вилучені (залишки по тілу депозиту та відсотків дорівнюють нулю) &#13;&#10;           and dc.vidd in (select v.vidd &#13;&#10;                             from bars.dpt_vidd v, bars.dpt_types t &#13;&#10;                            where v.type_id = t.type_id &#13;&#10;                              and t.type_id in (1, -- Авансовий &#13;&#10;                                                2, -- Депозитний Ощадного банку &#13;&#10;                                                4, -- Накопичувальний &#13;&#10;                                                5, -- Дитячий &#13;&#10;                                                9, -- Новий відсоток &#13;&#10;                                                10 -- Строковий пенсійний &#13;&#10;                                                -- ??? Майбутнє дітям &#13;&#10;                                                )) -- Акція стосується достроково вилучених вкладів «Депозитний», «Накопичувальний», «Строковий пенсійний», «Новий відсоток», «Авансовий», «Дитячий» та «Майбутнє дітям», які знов залучаються лише за умовою вкладу «Депозитний» &#13;&#10;                    and dc.bdate between&#13;&#10;               to_date(GetGlobalOption('PI_START'), 'dd.mm.yyyy') and&#13;&#10;               to_date(GetGlobalOption('PI_END'), 'dd.mm.yyyy') &#13;&#10;           and dc.branch like &#13;&#10;               sys_context('bars_context', 'user_branch_mask') -- Новий депозит має оформлюватись у тій же установі (бранчі), що і достроково розірваний &#13;&#10;        ) ">
        <Column Name="Дата початку" DataType="System.DateTime"/>
        <Column Name="Дата закінчення" DataType="System.DateTime"/>
        <Column Name="Дата розірвання" DataType="System.DateTime"/>
