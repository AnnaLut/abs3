<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="xmlform">
    <xs:complexType>
      <xs:all>
        <xs:element ref="tabcontainer" minOccurs="0"/>
        <xs:element ref="grids" minOccurs="0"/>
        <xs:element ref="customforms" minOccurs="0"/>
      </xs:all>
      <xs:attribute name="caption" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="minwidth" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:short"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="maxwidth" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:short"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="modalformsrc" type="XmlModalFormSrc" use="optional"/>
      <xs:attribute name="debugxml" type="xs:boolean" use="optional"/>
      <xs:attribute name="version" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="captiontype" use="optional">
        <xs:simpleType>
          <xs:restriction base="XmlCaptionTypes"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="uicontrol">
    <xs:complexType>
      <xs:sequence minOccurs="0">
        <xs:element ref="defaultvalue" minOccurs="0"/>
        <xs:element ref="rels" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="readonly">
        <xs:simpleType>
          <xs:restriction base="xs:boolean"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="align">
        <xs:simpleType>
          <xs:restriction base="XmlHorisontalPosition"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="type" type="XmlUiControlType"/>
      <xs:attribute name="table">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="keyfield">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="valuefield">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="valueformat">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="addemptyitem" type="xs:boolean"/>
      <xs:attribute name="linescount">
        <xs:simpleType>
          <xs:restriction base="xs:byte"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="formname">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="formwidth">
        <xs:simpleType>
          <xs:restriction base="xs:short"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="formheight">
        <xs:simpleType>
          <xs:restriction base="xs:short"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="formfieldname">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="truevalue">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="falsevalue">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="controlwidth">
        <xs:simpleType>
          <xs:restriction base="xs:short"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="titlewidth" type="xs:short"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="sql" type="xs:string"/>
  <xs:element name="rowselection">
    <xs:complexType>
      <xs:attribute name="method" type="XmlRowSelectionMethod" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="rels">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="rel" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="rel">
    <xs:complexType>
      <xs:attribute name="srcname" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="destname" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="sessionVariables">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="variable" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="variable">
    <xs:complexType>
      <xs:attribute name="name" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="field" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>

  <xs:element name="parameters">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="parameter" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="parameter">
    <xs:complexType>
      <xs:sequence minOccurs="0">
        <xs:element ref="defaultvalue" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="name" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="type" use="required">
        <xs:simpleType>
          <xs:restriction base="XmlParameterType"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="datatype" use="optional">
        <xs:simpleType>
          <xs:restriction base="XmlDataType"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="masterid" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="masterkeyfield" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="altname" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="controlid" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="add2successurl" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:boolean"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="link">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="parameters"/>
      </xs:sequence>
      <xs:attribute name="url" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="target" type="XmlTargets" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="grids">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="grid" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="grid" abstract="false">
    <xs:complexType mixed="false">
      <xs:all>
        <xs:element ref="datasource" minOccurs="0"/>
        <xs:element ref="startupcommand" minOccurs="0"/>
        <xs:element ref="filters" minOccurs="0"/>
        <xs:element ref="fields" minOccurs="1"/>
        <xs:element ref="rowselection" minOccurs="0"/>
        <xs:element ref="parameters" minOccurs="0"/>
        <xs:element ref="editform" minOccurs="0"/>
        <xs:element ref="buttons" minOccurs="0"/>
      </xs:all>
      <xs:attribute name="id" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="table" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="title" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="width" type="xs:short"/>
      <xs:attribute name="showpagesizebox" type="xs:boolean" use="optional"/>
      <xs:attribute name="showfilter" type="xs:boolean" use="optional"/>
      <xs:attribute name="pagesize" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:byte"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="tabindex" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:int"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="filters">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="filter" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="caption" type="xs:string" use="optional"/>
      <xs:attribute name="buttontext" type="xs:string" use="required"/>
      <xs:attribute name="width" type="xs:string" use="optional"/>
      <xs:attribute name="columnscount" type="xs:int" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="filter">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="uicontrol" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
      <xs:attribute name="caption" type="xs:string" use="required"/>
      <xs:attribute name="field" type="xs:string" use="required"/>
      <xs:attribute name="id" type="xs:string" use="required"/>
      <xs:attribute name="columnindex" type="xs:int" use="optional"/>
      <xs:attribute name="index" type="xs:int" use="optional"/>
      <xs:attribute name="groupname" type="xs:string" use="optional"/>
      <xs:attribute name="condition" type="XmlFilterConditions">
      </xs:attribute>
      <xs:attribute name="allowemptyvalue" type="xs:boolean" use="optional"/>
      <xs:attribute name="useupperfunc" type="xs:boolean" use="optional"/>      
    </xs:complexType>
  </xs:element>
  <xs:element name="form">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="uicontrol" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="show" type="xs:boolean" use="optional"/>
      <xs:attribute name="rwmode" type="XmlRwMode" use="optional" />
      <xs:attribute name="required" type="xs:boolean" use="optional"/>
      <xs:attribute name="titleindex">
        <xs:simpleType>
          <xs:restriction base="xs:byte"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="groupname" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="columnindex" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:byte"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="index" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:byte"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="fields">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="field" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="field">
    <xs:complexType>
      <xs:all>
        <xs:element ref="column" minOccurs="0"/>
        <xs:element ref="form" minOccurs="0"/>
        <xs:element ref="layout" minOccurs="0"/>
      </xs:all>
      <xs:attribute name="name" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="datatype" type="XmlDataType" use="required"/>
      <xs:attribute name="size">
        <xs:simpleType>
          <xs:restriction base="xs:short"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="sort" type="XmlSortDirection"/>
      <xs:attribute name="key" type="xs:boolean" use="optional"/>
      <xs:attribute name="lobtable" type="xs:string" use="optional"/>
      <xs:attribute name="lobfilename" type="xs:string" use="optional"/>
      <xs:attribute name="lobfilenamefield" type="xs:string" use="optional"/>
      <xs:attribute name="lobrowtext" type="xs:string" use="optional"/>
      <xs:attribute name="sortindex">
        <xs:simpleType>
          <xs:restriction base="xs:byte"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="editform">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="buttons"/>
      </xs:sequence>
      <xs:attribute name="title" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="style" type="XmlFormStyle"/>
      <xs:attribute name="width" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:short"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="height" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:short"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="columnscount" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:byte"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="defaultvalue">
    <xs:complexType>
      <xs:attribute name="value" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="type" type="XmlDefaultValue" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="customfields">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="customfield" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="title" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="oktext" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="canceltext" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="columnscount" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:byte"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="customfield">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="uicontrol" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="name" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="label" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="usecookie" type="xs:boolean"/>
      <xs:attribute name="datasourcefield" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>      
      <xs:attribute name="datatype" type="XmlDataType" use="required"/>
      <xs:attribute name="required" type="xs:boolean" use="optional"/>
      <xs:attribute name="size" type="xs:short"/>
      <xs:attribute name="groupname">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="columnindex">
        <xs:simpleType>
          <xs:restriction base="xs:byte"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="index">
        <xs:simpleType>
          <xs:restriction base="xs:byte"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="titleindex">
        <xs:simpleType>
          <xs:restriction base="xs:byte"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="altname">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="datasource">
    <xs:complexType>
      <xs:sequence minOccurs="0">
        <xs:element ref="sql" minOccurs="0"/>
        <xs:element ref="parameters" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="autofillparams" type="xs:boolean" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="startupcommand">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="1">
        <xs:element ref="datasource" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>  
  <xs:element name="command">
    <xs:complexType>
      <xs:sequence minOccurs="0">
        <xs:element ref="sql" minOccurs="0"/>
        <xs:element ref="parameters" minOccurs="0"/>
        <xs:element ref="customfields" minOccurs="0"/>
        <xs:element ref="validateproc" minOccurs="0"/>
        <xs:element ref="sessionVariables" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="type" type="XmlCommandType" use="required"/>
      <xs:attribute name="datakeynames">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="successmessage">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="successurl">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="checkselectedrow">
        <xs:simpleType>
          <xs:restriction base="xs:boolean"/>
        </xs:simpleType>
      </xs:attribute>      
      <xs:attribute name="url">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="frtemplate">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="target" type="XmlTargets" use="optional"/>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="column">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="link" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="caption" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="align" type="XmlHorisontalPosition" use="optional"/>
      <xs:attribute name="width">
        <xs:simpleType>
          <xs:restriction base="xs:short"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="formatstring">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="usedefaultformat" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:boolean"/>
        </xs:simpleType>
      </xs:attribute>      
      <xs:attribute name="show" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:boolean"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="htmlencode" use="optional">
        <xs:simpleType>
          <xs:restriction base="xs:boolean"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>

  <xs:element name="layout">
    <xs:complexType>
      <xs:sequence minOccurs="1" maxOccurs="1">
        <xs:element ref="colors" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="colors">
    <xs:complexType>
      <xs:sequence minOccurs="0">
        <xs:element ref="color" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="scope" type="XmlLayoutScope" use="optional"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="color">
    <xs:complexType>
      <xs:attribute name="operation" type="XmlMathOperations" use="required"/>
      <xs:attribute name="value" type="xs:string" use="required"/>
      <xs:attribute name="color" type="XmlColors" use="required"/>
      <xs:attribute name="bold" type="xs:boolean" use="optional"/>
      <xs:attribute name="columnindex" type="xs:int" use="optional"/>
    </xs:complexType>
  </xs:element>

  <xs:element name ="tabcontainer">
    <xs:complexType>

      <xs:sequence>
        <xs:element ref="tab" maxOccurs="unbounded"/>
      </xs:sequence>
      
      <xs:attribute name="id" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      
      <xs:attribute name="tabcount" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:int"/>
        </xs:simpleType>
      </xs:attribute>

      <xs:attribute name="title" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>

      <xs:attribute name="visible" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:boolean"/>
        </xs:simpleType>
      </xs:attribute>


    </xs:complexType>
  </xs:element>

  <xs:element name ="tab">
    <xs:complexType>
      <xs:attribute name="id" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>

      <xs:attribute name="index" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:int"/>
        </xs:simpleType>
      </xs:attribute>

      <xs:attribute name="caption" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>

      <xs:attribute name="visible" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:boolean"/>
        </xs:simpleType>
      </xs:attribute>

    </xs:complexType>
  </xs:element>



  <xs:element name="buttons">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="button" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="defaulttype" type="XmlButtonType" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="button">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="command"/>
      </xs:sequence>
      <xs:attribute name="id" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="caption" type="xs:string" use="required"/>
      <xs:attribute name="type" type="XmlButtonType"/>
      <xs:attribute name="hint" type="xs:string" use="optional"/>
      <xs:attribute name="confirmmessage">
        <xs:simpleType>
          <xs:restriction base="xs:string"/>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="linebreak" type="xs:boolean"/>
