      <TableDataSource Name="Table6" Alias="kred_info" DataType="System.Int32" Enabled="true" SelectCommand="select DECODE (&#13;&#10;       s.product_id,&#13;&#10;       'PRD_CONSUMER',decode(b.subproduct_id,&#13;&#10;                             'SBP_CONSUMER_0_5_2_EP','договіру відновлюваної кредитної лінії',&#13;&#10;                             'SBP_CONSUMER_5_10_2_EP','договіру відновлюваної кредитної лінії',&#13;&#10;                             'договору про іпотечний кредит '),&#13;&#10;       'PRD_MORTGAGE',       'договіру про іпотечний кредит',&#13;&#10;       'PRD_MORTGAGE_PR_CON','договіру про іпотечний кредит',&#13;&#10;       'PRD_MORTGAGE_PR_GUA','договіру про іпотечний кредит',&#13;&#10;       'PRD_MORTGAGE_YOUTH', 'договіру про іпотечний кредит',&#13;&#10;       'PRD_HULL_INSURANCE', 'договіру відновлюваної кредитної лінії',&#13;&#10;       'кредитного договору') AS agr_name,  &#13;&#10;       d.cc_id as deal_number, &#13;&#10;       d.sdate crt_date,&#13;&#10;       substr(f_dat_lit(d.sdate),1,25) as   crt_date_pr,&#13;&#10;       b.summ &#13;&#10; from  v_wcs_bids b, cc_deal d, v_wcs_subproducts s &#13;&#10;where  b.bid_id = :p_bid_id&#13;&#10;  and  b.bid_id = d.nd(+)&#13;&#10;  AND b.subproduct_id = s.subproduct_id">
