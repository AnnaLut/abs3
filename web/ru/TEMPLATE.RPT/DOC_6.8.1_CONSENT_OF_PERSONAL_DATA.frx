      <TableDataSource Name="Table7" Alias="list_dog" DataType="System.Int32" Enabled="true" SelectCommand="select INN,MAX(fio) FIO ,MAX(fio_o) fio_o,MAX(fio_r) fio_r,MAX(pasp) PASP,&#13;&#10;       listagg(dog_info, ', ') WITHIN GROUP(ORDER BY n) as list_d&#13;&#10;from(&#13;&#10;select  B.INN , 1 n,&#13;&#10;        wcs_utl.get_answ (b.bid_id,'CL_1') || ' ' || &#13;&#10;        wcs_utl.get_answ (b.bid_id,'CL_2') || ' ' || &#13;&#10;        wcs_utl.get_answ (b.bid_id,'CL_3') AS fio,&#13;&#10;        wcs_utl.get_answ (b.bid_id,'CL_123_O') fio_o,&#13;&#10;        wcs_utl.get_answ (b.bid_id,'CL_123_R') fio_r,                                &#13;&#10;        'паспорт серії: '|| wcs_utl.get_answ (b.bid_id,'CL_7')|| &#13;&#10;        ' № '|| wcs_utl.get_answ (b.bid_id,'CL_8')|| &#13;&#10;        ' виданий '|| trim(wcs_utl.get_answ (b.bid_id,'CL_9'))|| &#13;&#10;        ' '|| wcs_utl.get_answ (b.bid_id,'CL_11')||' р.' AS pasp,&#13;&#10;       decode((select s.PRODUCT_ID from WCS_SUBPRODUCTS s where s.ID=b.SUBPRODUCT_ID),&#13;&#10;       'PRD_MORTGAGE','Договору про іпотечний кредит',&#13;&#10;       'PRD_HULL_INSURANCE','Договору відновлюваної кредитної лінії',&#13;&#10;       case when b.SUBPRODUCT_ID in('SBP_CONSUMER_0_5_2_EP','SBP_CONSUMER_5_10_2_EP') &#13;&#10;            then 'Договору відновлюваної кредитної лінії'&#13;&#10;            else 'Кредитного договору' &#13;&#10;       end)||' №'|| &#13;&#10;       d.cc_id||' від '|| &#13;&#10;--       d.sdate crt_date,&#13;&#10;       substr(f_dat_lit(d.sdate),1,25) dog_info,&#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_8') n_pasp&#13;&#10; from  v_wcs_bids b, cc_deal d &#13;&#10;where  b.bid_id = :p_bid_id&#13;&#10;  and  b.bid_id = d.nd(+)&#13;&#10;union all  &#13;&#10;select distinct &#13;&#10;       decode(nvl(wcs_utl.get_answ(b.bid_id, 'GRT_0', b.ws_id , B.GARANTEE_NUM),0),&#13;&#10;       0,&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CODE_002', b.ws_id , B.GARANTEE_NUM),&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CODE_002')) inn, 2 n,&#13;&#10;       wcs_utl.get_answ(b.bid_id,'CL_1',b.ws_id , B.GARANTEE_NUM)|| ' ' || &#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_2',b.ws_id , B.GARANTEE_NUM)|| ' ' || &#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_3',b.ws_id , B.GARANTEE_NUM) AS fio,&#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_123_O',b.ws_id , B.GARANTEE_NUM) fio_o,&#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_123_R',b.ws_id , B.GARANTEE_NUM) fio_r,                                &#13;&#10;       'паспорт серії: ' || wcs_utl.get_answ (:p_bid_id,'CL_7',b.ws_id , B.GARANTEE_NUM)|| &#13;&#10;       ' № ' || wcs_utl.get_answ (b.bid_id,'CL_8',b.ws_id , B.GARANTEE_NUM)|| &#13;&#10;       ' виданий ' || trim(wcs_utl.get_answ (b.bid_id,'CL_9',b.ws_id , B.GARANTEE_NUM))||&#13;&#10;       ' '|| wcs_utl.get_answ (b.bid_id,'CL_11',b.ws_id , B.GARANTEE_NUM)||' р.' AS pasp,              &#13;&#10;       decode(B.GARANTEE_ID,'GUARANTOR','Договору поруки','VEHICLE','Договору забезпечення','Іпотечного договору')||' №'|| &#13;&#10;       B.AGR_NUMBER||' від '|| &#13;&#10;       substr(f_dat_lit(B.AGR_DATE),1,25) dog_info,&#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_8',b.ws_id , B.GARANTEE_NUM) n_pasp&#13;&#10;from   V_WCS_BID_GARANTEES b&#13;&#10;where  b.bid_id = :p_bid_id  &#13;&#10;union all  &#13;&#10;select distinct B.INN , 1 n,&#13;&#10;        wcs_utl.get_answ (b.bid_id,'CL_1') || ' ' || &#13;&#10;        wcs_utl.get_answ (b.bid_id,'CL_2') || ' ' || &#13;&#10;        wcs_utl.get_answ (b.bid_id,'CL_3') AS fio,&#13;&#10;        wcs_utl.get_answ (b.bid_id,'CL_123_O') fio_o,&#13;&#10;        wcs_utl.get_answ (b.bid_id,'CL_123_R') fio_r,                                &#13;&#10;        'паспорт серії: '|| wcs_utl.get_answ (b.bid_id,'CL_7')|| &#13;&#10;        ' № '|| wcs_utl.get_answ (b.bid_id,'CL_8')|| &#13;&#10;        ' виданий '|| trim(wcs_utl.get_answ (b.bid_id,'CL_9'))|| &#13;&#10;        ' '|| wcs_utl.get_answ (b.bid_id,'CL_11')||' р.' AS pasp,           &#13;&#10;       'Договору поруки №'||Bg.AGR_NUMBER||' від '||substr(f_dat_lit(Bg.AGR_DATE),1,25) dog_info,&#13;&#10;       wcs_utl.get_answ (b.bid_id,'CL_8') n_pasp&#13;&#10;from   v_wcs_bids b,&#13;&#10;       V_WCS_BID_GARANTEES bg&#13;&#10;where  bg.bid_id=B.BID_ID and&#13;&#10;       Bg.GARANTEE_ID='GUARANTOR' and&#13;&#10;       b.bid_id = :p_bid_id  &#13;&#10;) dog&#13;&#10;where dog.inn= wcs_utl.get_answ(:p_bid_id, 'CODE_002', :p_ws_id, :p_ws_number)   &#13;&#10;group by inn,n_pasp">
        <Column Name="INN" DataType="System.String"/>
