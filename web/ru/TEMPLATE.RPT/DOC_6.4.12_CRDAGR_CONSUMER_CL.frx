        <TableColumn Name="Column73" Width="313.74"/>
        <TableColumn Name="Column74" Width="317.52"/>
        <TableRow Name="Row828" Height="37.8" AutoSize="true">
          <TableCell Name="Cell1433" Border.Lines="All" Border.Width="0.5" Text="2.5.1.[Row#]." Font="Times New Roman, 10pt, style=Bold" HtmlTags="true"/>
          <TableCell Name="Cell1434" Border.Lines="All" Border.Width="0.5" Text="&lt;b&gt;[preferential_int.INTEREST_RATE] ([preferential_int.INTEREST_RATE_PR]) &lt;/b&gt;процента(-ів) річних " Font="Times New Roman, 10pt" HtmlTags="true"/>
          <TableCell Name="Cell1435" Border.Lines="All" Border.Width="0.5" Text="в період з [preferential_int.RATE_DATE_BEGIN] по [FormatDateTime([preferential_int.RATE_DATE_END],&quot;Short Date&quot;)]р. включно " Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
      </TableObject>
      <DataHeaderBand Name="DataHeader3" Top="1953.18" Width="718.2" Height="75.6">
        <TableObject Name="Table32" Left="9.45" Width="697.41" Height="75.6">
          <TableColumn Name="Column69"/>
          <TableColumn Name="Column70" Width="313.74"/>
          <TableColumn Name="Column71" Width="317.52"/>
          <TableRow Name="Row826" Height="37.8" AutoSize="true">
            <TableCell Name="Cell1427" Text="&lt;b&gt;2.5.1.&lt;/b&gt; За користування Кредитом Позичальник зобов’язаний сплачувати Банку відповідну плату (проценти) в розмірі:" Padding="20, 1, 2, 1" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="3"/>
            <TableCell Name="Cell1428" HorzAlign="Center" Font="Times New Roman, 10pt" HtmlTags="true"/>
            <TableCell Name="Cell1429" HorzAlign="Center" Font="Times New Roman, 10pt" HtmlTags="true"/>
          </TableRow>
          <TableRow Name="Row827" Height="37.8" AutoSize="true">
            <TableCell Name="Cell1430" Border.Lines="All" Border.Width="0.5" Text="№ під пункту" HorzAlign="Center" Font="Times New Roman, 10pt, style=Bold" HtmlTags="true"/>
            <TableCell Name="Cell1431" Border.Lines="All" Border.Width="0.5" Text="Розмір процентної ставки, що сплачується Позичальником за користування Кредитом" HorzAlign="Center" Font="Times New Roman, 10pt, style=Bold" HtmlTags="true"/>
            <TableCell Name="Cell1432" Border.Lines="All" Border.Width="0.5" Text="Період, протягом якого застосовується процентна ставка за користування  Кредитом" HorzAlign="Center" Font="Times New Roman, 10pt, style=Bold" HtmlTags="true"/>
          </TableRow>
        </TableObject>
      </DataHeaderBand>
      <DataFooterBand Name="DataFooter3" Top="2071.23" Width="718.2" Height="37.8">
        <TableObject Name="Table32" Left="9.45" Width="697.41" Height="37.8">
          <TableColumn Name="Column75"/>
          <TableColumn Name="Column76" Width="313.74"/>
          <TableColumn Name="Column77" Width="317.52"/>
          <TableRow Name="Row829" Height="37.8" AutoSize="true">
            <TableCell Name="Cell1436" Text="Зазначені в цьому пункті Договору процентні ставки є фіксованими та застосовуються у відповідний період дії Договору." Padding="20, 1, 2, 1" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="3"/>
            <TableCell Name="Cell1437" HorzAlign="Center" Font="Times New Roman, 10pt" HtmlTags="true"/>
