        <TableRow Name="Row29" Height="47.25" AutoSize="true">
          <TableCell Name="Cell70" Text="[IIf([Guarantors.GARANTEE_NUM] == 0,&quot;&lt;b&gt;1.1.17. &lt;i&gt;Поручитель&lt;/i&gt;&lt;/b&gt; - &quot;+ IIf(Data2.DataSource.RowCount &gt; 1,&quot;\n&quot;,&quot;&quot;)+ [Guarantors.FIO] + &quot;, реєстраційний номер облікової картки платника податку &quot; + [Guarantors.INN],[Guarantors.FIO] + &quot;, реєстраційний номер облікової картки платника податку&quot; + [Guarantors.INN])][IIf([Guarantors.ROWNUM] == Data2.DataSource.RowCount,&quot;.&quot;,&quot;,&quot;)]" Padding="20, 1, 2, 1" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
