        <TableColumn Name="Column88" Width="313.74"/>
        <TableColumn Name="Column89" Width="317.52"/>
        <TableRow Name="Row865" Height="37.8" AutoSize="true">
          <TableCell Name="Cell1481" Border.Lines="All" Border.Width="0.5" Text="2.4.1.[Row#]." Font="Times New Roman, 10pt, style=Bold" HtmlTags="true"/>
          <TableCell Name="Cell1482" Border.Lines="All" Border.Width="0.5" Text="&lt;b&gt;[preferential_int.INTEREST_RATE] ([preferential_int.INTEREST_RATE_PR]) &lt;/b&gt;процента(-ів) річних " Font="Times New Roman, 10pt" HtmlTags="true"/>
          <TableCell Name="Cell1483" Border.Lines="All" Border.Width="0.5" Text="в період з [preferential_int.RATE_DATE_BEGIN] по [FormatDateTime([preferential_int.RATE_DATE_END],&quot;Short Date&quot;)]р. включно " Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
