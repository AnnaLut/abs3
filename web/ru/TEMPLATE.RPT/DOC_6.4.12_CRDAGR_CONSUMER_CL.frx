    <DataBand Name="Data3" Top="1226.03" Width="718.2" Height="47.25" CanGrow="true" CanShrink="true" CanBreak="true" DataSource="Table9" KeepTogether="true">
      <TableObject Name="Table8" Left="9.45" Width="699.3" Height="47.25">
        <TableColumn Name="Column10" MaxWidth="1134" Width="699.3"/>
        <TableRow Name="Row30" Height="47.25" AutoSize="true">
          <TableCell Name="Cell71" Text="[IIf([Row#] == 1,&quot;&lt;b&gt;1.1.1&quot;+ IIf([Guarantors.ROWNUM]&gt;0,&quot;8&quot;,&quot;7&quot;) +&quot;. &lt;i&gt;Майновий поручитель&lt;/i&gt;&lt;/b&gt; - &quot;+ IIf(Data3.DataSource.RowCount &gt; 1,&quot;\n&quot;,&quot;&quot;)+ [Guarantors_mai.NMK]+ &quot;, реєстраційний номер облікової картки платника податку &quot; + [Guarantors_mai.OKPO], [Guarantors_mai.NMK]+ &quot;, реєстраційний номер облікової картки платника податку &quot; + [Guarantors_mai.OKPO])][IIf([Row#]== Data3.DataSource.RowCount,&quot;.&quot;,&quot;,&quot;)]" Padding="20, 1, 2, 1" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
      </TableObject>
    </DataBand>
    <DataBand Name="Data7" Top="1275.6" Width="718.2" Height="292.95" CanGrow="true" CanShrink="true" CanBreak="true" Guides="0">
      <TableObject Name="Table29" Left="9.45" Width="699.3" Height="292.95">
        <TableColumn Name="Column36" MaxWidth="1134" Width="699.3"/>
        <TableRow Name="Row598" Height="47.25" AutoSize="true">
          <TableCell Name="Cell1148" Text="&lt;b&gt;1.2.&lt;/b&gt; Інші терміни, скорочення їх тлумачення, що використовуються по тексту цього Договору, визначаються чинним Законодавством." HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row599" AutoSize="true">
          <TableCell Name="Cell1149" Fill.Color="LightGray" Text="2. Предмет Договору" HorzAlign="Center" Font="Times New Roman, 12pt, style=Bold" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row600" Height="56.7" AutoSize="true">
          <TableCell Name="Cell1150" Text="&lt;b&gt;2.1&lt;/b&gt; Банк зобов’язується надати Позичальнику на умовах цього Договору, а Позичальник має право отримати та зобов’язується належним чином використати і повернути в передбачені цим Договором строки  Кредит, сплатити проценти за користування Кредитом та інші платежі у порядку та на умовах, визначених цим Договором." HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row601" Height="56.7" AutoSize="true">
          <TableCell Name="Cell1151" Text="&lt;b&gt;2.2.&lt;/b&gt; Кредит надається в розмірі &lt;b&gt;[Format(&quot;{0:N}&quot;, [DataSubProdukt.SUMM])]&lt;/b&gt; (&lt;b&gt;[DataSubProdukt.SSUM_PR]&lt;/b&gt;) гривень на &lt;b&gt;[DataSubProdukt.TERM]&lt;/b&gt; (&lt;b&gt;[DataSubProdukt.TERM_PR]&lt;/b&gt;)  місяців з терміном остаточного повернення Кредиту не пізніше &lt;b&gt;[DataSubProdukt.DAT_DOG_RET_PR]&lt;/b&gt;." HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row602" AutoSize="true">
          <TableCell Name="Cell1152" Text="&lt;b&gt;2.3&lt;/b&gt; Кредит надається на такі цілі: &lt;b&gt;[DataSubProdukt.CL_2_0_01]&lt;/b&gt;" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row603" AutoSize="true">
          <TableCell Name="Cell1153" Text="2.4 &lt;u&gt;Проценти за користування Кредитом&lt;/u&gt;" HorzAlign="Justify" Font="Times New Roman, 10pt, style=Bold" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row604" Height="75.6" AutoSize="true">
          <TableCell Name="Cell1154" Text="[IIf([preferential_int.NROW]== 0,&quot;&lt;b&gt;2.4.1.&lt;/b&gt; За користування Кредитом Позичальник зобов’язаний сплачувати Банку відповідну плату (проценти) в розмірі &lt;b&gt;&quot;+Format(&quot;{0:F}&quot;, [DataSubProdukt.INTEREST_RATE])+&quot;&lt;/b&gt; (&lt;b&gt;&quot;+[DataSubProdukt.INTEREST_RATE_PR]+&quot;&lt;/b&gt;) процента(-ів) річних. Зазначена в цьому пункті Договору процентна ставка є фіксованою.&quot;,&quot;&quot;)]" Padding="20, 1, 2, 1" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
      </TableObject>
    </DataBand>
