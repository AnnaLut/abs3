    private void _StartReport(object sender, EventArgs e)
    {
      OracleDataConnection frxconn = (OracleDataConnection)Report.Dictionary.Connections[0];
      OracleConnection connection = (OracleConnection)frxconn.GetConnection();
      OracleCommand command = connection.CreateCommand();
      command.CommandText = &quot;declare l_Fdat date; l_Irr_kl number;i integer;term integer;COUNT_ALL_ROWS integer; begin bars.wcs_utl.store_gpk(:p_bid_id);select pmt_date into l_Fdat from table(wcs_utl.get_gpk(:p_bid_id)) where rownum = 1;delete from tmp_gpk; delete from TMP_IRR; insert into tmp_gpk(fdat,lim2, sumg, sumo,  SV0K1, SV1RK, SV7RE,  SV6NO, SV2PW, SVBT) select pmt_date,pmt_in_bal*100,pmt_body*100,pmt_total*100,0,0,0,0,sum(NVL(insurance,0)) insurance,0 from(select t.pmt_id,t.pmt_date,t.pmt_in_bal,t.pmt_body,t.pmt_interest,t.pmt_total,t.pmt_out_bal,decode(t.pmt_out_bal,0,0,decode(INS_type,4,decode(RENEW_NEED,1,case when t.pmt_id&lt;36 then s.sum_pl else 0 end ,case when t.pmt_id&lt;round((DATE_OFF-DATE_ON)/365)*12 then s.sum_pl else 0 end),s.sum_pl)) as insurance from table(wcs_utl.get_gpk(:p_bid_id)) t left join (select INS_type,decode(pn_kod,'7',3,'180',6,'360',12,12) per,sum(case when COST_TYPE=0 then b_sum*COST_TARIFF/100 else  COST_SUM end) sum_pl,RENEW_NEED,DATE_ON,DATE_OFF from(select i.INS_TYPE_ID INS_type,wcs_utl.get_answ_refer(bid_id,'INS_PAYMENTS_FREQ',ws_id,INSURANCE_num ) pn_kod, i.SUM b_sum, wcs_utl.get_answ_list(bid_id,'INS_COST_TYPE',ws_id,INSURANCE_num ) COST_TYPE, wcs_utl.get_answ_decimal(bid_id,'INS_COST_TARIFF',ws_id,INSURANCE_num ) COST_TARIFF, wcs_utl.get_answ_decimal(bid_id,'INS_COST_SUM',ws_id,INSURANCE_num ) COST_SUM,wcs_utl.get_answ_bool(bid_id,'INS_RENEW_NEED',ws_id,INSURANCE_num )RENEW_NEED,wcs_utl.get_answ_date(bid_id,'INS_DATE_ON',ws_id,INSURANCE_num ) DATE_ON, wcs_utl.get_answ_date(bid_id,'INS_DATE_OFF',ws_id,INSURANCE_num ) DATE_OFF from V_WCS_BID_INSURANCES i where bid_id=:p_bid_id union select i.INS_TYPE_ID INS_type, wcs_utl.get_answ_refer(bid_id,'INS_PAYMENTS_FREQ',ws_id,INSURANCE_num )pn_kod,i.SUM b_sum, wcs_utl.get_answ_list(bid_id,'INS_COST_TYPE',ws_id,INSURANCE_num ) COST_TYPE, wcs_utl.get_answ_decimal(bid_id,'INS_COST_TARIFF',ws_id,INSURANCE_num ) COST_TARIFF, wcs_utl.get_answ_decimal(bid_id,'INS_COST_SUM',ws_id,INSURANCE_num ) COST_SUM,wcs_utl.get_answ_bool(bid_id,'INS_RENEW_NEED',ws_id,INSURANCE_num )RENEW_NEED,wcs_utl.get_answ_date(bid_id,'INS_DATE_ON',ws_id,INSURANCE_num ) DATE_ON,wcs_utl.get_answ_date(bid_id,'INS_DATE_OFF',ws_id,INSURANCE_num ) DATE_OFF from V_WCS_BID_GRT_INSURANCES i where bid_id=:p_bid_id ) x  group by INS_type,decode(pn_kod,'7',3,'180',6,'360',12,12),RENEW_NEED,DATE_ON,DATE_OFF)s on(mod(t.pmt_id,s.per)=0))z group by pmt_id,pmt_date,pmt_in_bal,pmt_body,pmt_interest,pmt_total,pmt_out_bal order by  pmt_date; update  tmp_gpk tmp set(SV0K1, SV1RK, SV7RE,  SV6NO, SVBT)=(select tmp.SV0K1+x.single_fee, tmp.SV1RK+x.other_bank,tmp.SV7RE+x.services_of_appraisers,tmp.SV6NO+notary_services,tmp.SVBT+other_payments from(select to_number(wcs_utl.get_creditdata(:p_bid_id,'SINGLE_FEE')) as single_fee, (nvl(wcs_utl.get_answ_decimal(:p_bid_id,'CL_0_275'),0) + nvl(wcs_utl.get_answ_decimal(:p_bid_id,'CL_0_277'),0)) as other_bank,(nvl(wcs_utl.get_answ_decimal(:p_bid_id,'CL_0_258'),0)) as services_of_appraisers, (nvl(wcs_utl.get_answ_decimal(:p_bid_id,'CL_0_260'),0)) as notary_services, (nvl(wcs_utl.get_answ_decimal(:p_bid_id,'CL_0_262'),0) + nvl(wcs_utl.get_answ_decimal(:p_bid_id,'CL_0_264'),0) + nvl(wcs_utl.get_answ_decimal(:p_bid_id,'CL_0_272'),0) ) as other_payments from dual)x) where rownum = 1; i:=0;term := wcs_utl.get_answ_numb(:p_bid_id,'CREDIT_TERM');SELECT COUNT(*) into COUNT_ALL_ROWS FROM tmp_gpk;for c in (select tmp.FDAT from tmp_gpk tmp order by tmp.FDAT) loop if COUNT_ALL_ROWS &gt; i+1 then update tmp_gpk tmp set (SV1RK,SVBT)=(select tmp.SV1RK+decode(s.type_pl,0,case when per=0 then decode(i,0,sum_pl,0) else case when mod(i,s.per)=0 then round(s.sum_pl/term*s.per,2) else 0 end end,0),tmp.SVBT +decode(s.type_pl,1,case when per=0 then decode(i,0,sum_pl,0) else case when mod(i,s.per)=0 then round(s.sum_pl/term*s.per,2) else 0 end end,0) from(select decode(wcs_utl.get_answ_list(:p_bid_id, 'CL_0_282'),0,0,1,1,2,3,3,6,4,12,12) per,nvl(wcs_utl.get_answ_decimal(:p_bid_id, 'CL_0_283'),0) sum_pl,nvl(wcs_utl.get_answ_list(:p_bid_id, 'CL_0_284'),0) type_pl from dual) s) where tmp.FDAT=c.FDAT; update tmp_gpk tmp set (SV1RK,SVBT)=(select tmp.SV1RK+decode(s.type_pl,0,case when per=0 then decode(i,0,sum_pl,0) else case when mod(i,s.per)=0 then round(s.sum_pl/term*s.per,2) else 0 end end,0),tmp.SVBT +decode(s.type_pl,1,case when per=0 then decode(i,0,sum_pl,0) else case when mod(i,s.per)=0 then round(s.sum_pl/term*s.per,2) else 0 end end,0)from(select decode(wcs_utl.get_answ_list(:p_bid_id, 'CL_0_282_1'),0,0,1,1,2,3,3,6,4,12,12) per,nvl(wcs_utl.get_answ_decimal(:p_bid_id, 'CL_0_283_1'),0) sum_pl,nvl(wcs_utl.get_answ_list(:p_bid_id, 'CL_0_284_1'),0) type_pl from dual) s) where tmp.FDAT=c.FDAT; update tmp_gpk tmp set (SV1RK,SVBT)=(select tmp.SV1RK+decode(s.type_pl,0,case when per=0 then decode(i,0,sum_pl,0) else case when mod(i,s.per)=0 then round(s.sum_pl/term*s.per,2) else 0 end end,0),tmp.SVBT +decode(s.type_pl,1,case when per=0 then decode(i,0,sum_pl,0) else case when mod(i,s.per)=0 then round(s.sum_pl/term*s.per,2) else 0 end end,0) from(select decode(wcs_utl.get_answ_list(:p_bid_id, 'CL_0_282_2'),0,0,1,1,2,3,3,6,4,12,12) per,nvl(wcs_utl.get_answ_decimal(:p_bid_id, 'CL_0_283_2'),0) sum_pl, nvl(wcs_utl.get_answ_list(:p_bid_id, 'CL_0_284_2'),0) type_pl from dual) s) where tmp.FDAT=c.FDAT; end if; i:=i+1; end loop; insert into TMP_IRR (n, s)(select fdat - l_fdat,case when rownum = 1 then -lim2 + sumo + (SV0K1 + SV1RK + SV7RE + SV2PW + SV6NO + SVBT)*100 else sumo + (SV0K1 + SV1RK + SV7RE + SV2PW + SV6NO + SVBT)*100 end from tmp_gpk); select XIRR(10) * 100 into l_Irr_kl from dual; update tmp_gpk set irr_kl = trunc(l_irr_kl, 4) where ROWNUM = 1; end;&quot;;
      
      Parameter par = Report.GetParameter(&quot;ND&quot;);
      command.Parameters.Add(&quot;p_nd&quot;, par.Value);
      
      connection.Open();
      try
      {
        command.ExecuteNonQuery();
      }
      finally
      {
        connection.Close();
      }
    }
  }
}
</ScriptText>
