.head 4 -  Call SalTblPopulate ( tblRezArcN, hSql(),
"  select coalesce(dat1,dat2),nvl(dat3,dat4), nvl(name2,name1),nvl(id2,id1) , rezform
   into :Archive_VRM.tblRezArcN.Dat,
          :Archive_VRM.tblRezArcN.Dat_1,
          :Archive_VRM.tblRezArcN.User,
          :Archive_VRM.tblRezArcN.Id,
          :Archive_VRM.tblRezArcN.Rezform
   from ( select distinct t.dat dat1,dat_n dat3,s.id id1,s.fio name1 
          from tmp_rez t, staff s
          where t.id=s.id and s.logname =user and dat_n is not null
        ) a
   full outer join
       ( select r.dat dat2,r.dat_n dat4, s.id id2, s.fio name2, 't' rezform
         from rez_protocol r, staff s
         where r.userid = s.id and dat_n is not null
       ) b
   on (a.dat1=b.dat2)
   order by 1 desc "
,TBL_FillAll )
