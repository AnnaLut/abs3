.head 7 -  Set sTmp=
 "SELECT a.nls, a.ostb, s.S_ISP  INTO :S_FORM, :ost_S_FORM , :S_ISP
  FROM srezerv s, accounts a 
  WHERE s.S_FOND=:sAccA and a.kv=:n980 and a.nls=s.S_FORM "
.head 7 -  Set ost_Q_1=ost_S_1
.head 7 -  Set sTmp1=
  "SELECT sum(a.ostb)  INTO  :ost_Q_All
