.head 6 +  Else
.head 7 -  Set sTmp=
"SELECT a.nls, a.ostb, gl.p_icurval(:nKv, :ost_S_1, :bDAT), s.S_ISPV
 INTO  :S_FORM, :ost_S_FORM , :ost_Q_1, :S_ISP
 FROM srezerv s, accounts a 
 WHERE s.S_FOND=:sAccA and a.kv=:n980 and a.nls=s.S_FORMV "
.head 7 -  Set sTmp1=
  "SELECT sum(gl.p_icurval(a.kv,a.ostb,:bDAT))  INTO  :ost_Q_All
