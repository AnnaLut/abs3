.head 4 -  Set Rezrash.strSqlPopulate =
" select ak.nls,ak.kv,az.nls,az.kv,s/100,spriv/100,t.proc,to_char(t.PAWN) ||'('||p.NAME||')',t2.s080
  into :Rezrash.colNlsk,:Rezrash.colKvk,:Rezrash.colNlsz,:Rezrash.colKvz,:Rezrash.colS,
       :Rezrash.colSpriv,:Rezrash.colProc,:Rezrash.colPawn,:Rezrash.colS080
  from accounts az, accounts ak, tmp_rez_risk2 t,tmp_rez_risk t2, cc_pawn p
  where az.acc=t.accz
    and ak.acc=t.accs
    and t.pawn = p.pawn
    and t.userid = :Rezrash.nUid
    and t.DAT = :Rezrash.dBd
    and t2.id = :Rezrash.nUid
    and t2.DAT = :Rezrash.dBd
    and t2.acc = ak.acc 
  order by ak.nls,ak.kv,t.pawn  "
