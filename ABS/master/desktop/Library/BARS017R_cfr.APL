.head 4 -  Set SVR.strSqlPopulate=
"SELECT s.id, 
                s.CUSTTYPE, 
                s.S080, 
                s.S_FORM, 
                s.S_RASF, 
                s.S_ISP, 
                s.S_FOND,
                s.S_FONDNR, 
                s.S_FORMV,
                s.S_RASFV, 
                s.S_ISPV, 
                r.name,  
                c.name, 
                i.name,
                s.otvisp,
                s.otvispval   
 INTO  :SVR.ID, :SVR.CUSTTYPE, :SVR.S080,
       :SVR.S_FORM, :SVR.S_RASF, :SVR.S_ISP, :SVR.S_FOND,:SVR.S_FONDNR,
       :SVR.S_FORMV , :SVR.S_RASFV, :SVR.S_ISPV, 
       :SVR.sS080, :SVR.sCUSTTYPE,:SVR.sID, :SVR.OTVISP, :SVR.OTVISPVAL 
 FROM srezerv s, crisk r, custtype c, srez_id i
 WHERE s.s080=r.crisk and s.custtype=c.custtype and i.id=s.id 
 union all
 SELECT s.id, s.CUSTTYPE, s.S080,  s.S_FORM, s.S_RASF, s.S_ISP, s.S_FOND, s.S_FONDNR,
        s.S_FORMV,  s.S_RASFV, s.S_ISPV,  'Сомнит. и просроч(>31д) %',  c.name, i.name ,s.otvisp , s.otvispval        
 FROM srezerv s, custtype c, srez_id i
 WHERE s.custtype=c.custtype and s.S080=9 and i.id=s.id 
 ORDER BY 1, 2, 3 "         
