.head 3 +  Function: Encript
.head 4 -  Description: RC4 algorithm encription
.head 4 +  Returns
.head 5 -  String: strRet
.head 4 +  Parameters
.head 5 -  String: strInVal
.head 4 -  Static Variables
.head 4 +  Local variables
.head 5 -  String: strOriginalKey
.head 5 -  String: strOutVal
.head 5 -  Number: OriginalKey[256]
.head 5 -  Number: Key[256]
.head 5 -  Number: InVal[*]
.head 5 -  Number: OutVal[*]
.head 5 -  Number: i
.head 5 -  Number: j
.head 5 -  Number: k
.head 5 -  Number: t
.head 5 -  Number: nDataLen
.head 4 +  Actions
.head 5 -  ! Copying data to arrays
.head 5 -  Set strOriginalKey='ThisisMyKey.Itisgood,isn`tit?'
.head 5 -  Call InitKey( OriginalKey, strOriginalKey )
.head 5 -  Call InitKey( Key, strOriginalKey )
.head 5 -  Call SalStrToArray( strInVal, OutVal )
.head 5 -  Set nDataLen=SalStrToArray( strInVal, InVal )
.head 5 -  ! Operating on data
.head 5 -  Set i=0
.head 5 -  Set j=0
.head 5 -  Set k=0
.head 5 +  Loop
.head 6 +  If k<=nDataLen
.head 7 -  Set i=(i+1) & 0xFF
.head 7 -  Set j=( j+Key[i] ) & 0xFF
.head 7 -  Set t=Key[i]
.head 7 -  Set Key[i]=Key[j]
.head 7 -  Set Key[j]=t
.head 7 -  Set t=( Key[i]+Key[j] ) & 0xFF
.head 7 -  Set OutVal[k]=XOR ( InVal[k], Key[t] )
.head 6 +  Else
.head 7 -  Break
.head 6 -  Set k=k+1
.head 5 -  ! Returning data
.head 5 -  Call SalArrayToStr( OutVal, strOutVal )
.head 5 -  Return strOutVal
.head 3 +  Function: SalStrToArray
.head 4 -  Description:
.head 4 +  Returns
.head 5 -  Number: nRet
.head 4 +  Parameters
.head 5 -  String: strSource
.head 5 -  Receive Number: arrTarget[*]
.head 4 -  Static Variables
.head 4 +  Local variables
.head 5 -  Number: i
.head 5 -  Number: nUpperBnd
.head 4 +  Actions
.head 5 -  Set nUpperBnd=SalStrLength( strSource )-1
.head 5 -  Call SalArraySetUpperBound( arrTarget, 1, nUpperBnd )
.head 5 -  Set i=0
.head 5 +  Loop
.head 6 +  If i<=nUpperBnd
.head 7 -  Set arrTarget[i]=SalStrLop( strSource )
.head 6 +  Else
.head 7 -  Break
.head 6 -  Set i=i+1
.head 5 -  Return nUpperBnd
.head 3 +  Function: SalArrayToStr
.head 4 -  Description:
.head 4 +  Returns
.head 5 -  Number: nRet
.head 4 +  Parameters
.head 5 -  Number: arrSource[*]
.head 5 -  Receive String: strTarget
.head 4 -  Static Variables
.head 4 +  Local variables
.head 5 -  Number: i
.head 5 -  Number: nUpperBnd
.head 4 +  Actions
.head 5 -  Call SalArrayGetUpperBound( arrSource, 1, nUpperBnd )
.head 5 -  Set i=0
.head 5 -  Set strTarget=''
.head 5 +  Loop
.head 6 +  If i<=nUpperBnd
.head 7 -  Set strTarget=strTarget || SalNumberToChar ( arrSource[i] )
.head 6 +  Else
.head 7 -  Break
.head 6 -  Set i=i+1
.head 5 -  Return nUpperBnd
.head 3 +  Function: InitKey
.head 4 -  Description:
.head 4 +  Returns
.head 5 -  Boolean: bOk
.head 4 +  Parameters
.head 5 -  Receive Number: Key[256]
.head 5 -  String: strKey
.head 4 -  Static Variables
.head 4 +  Local variables
.head 5 -  Number: i
.head 5 -  Number: k
.head 5 -  Number: j
.head 5 -  Number: t
.head 5 -  Number: nLen
.head 5 -  String: strWKey
.head 5 -  Number: xKey[256]
.head 4 +  Actions
.head 5 -  ! Checking key length
.head 5 -  Set nLen=SalStrLength( strKey )
.head 5 +  If nLen>255
.head 6 -  Return FALSE
.head 5 -  ! Filling xKey with StrKey
.head 5 -  Set i=0
.head 5 -  Set k=1
.head 5 -  Set strWKey=strKey
.head 5 +  Loop
.head 6 +  If i<=255
.head 7 +  If k>nLen
.head 8 -  Set k=1
.head 8 -  Set strWKey=strKey
.head 7 -  Set xKey[i]=SalStrLop( strWKey )
.head 7 -  Set Key[i]=i
.head 6 +  Else
.head 7 -  Break
.head 6 -  Set i=i+1
.head 6 -  Set k=k+1
.head 5 -  ! Codding Key
.head 5 -  Set i=0
.head 5 -  Set j=0
.head 5 +  Loop
.head 6 +  If i<=255
.head 7 -  Set j=((j+Key[i]+xKey[i]) & 0xFF)
.head 7 -  Set t=Key[i]
.head 7 -  Set Key[i]=Key[j]
.head 7 -  Set Key[j]=t
.head 6 +  Else
.head 7 -  Break
.head 6 -  Set i=i+1
.head 5 -  Return TRUE
.head 3 +  Function: XOR
