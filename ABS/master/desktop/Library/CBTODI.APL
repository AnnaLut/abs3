.head 5 -  ! DBMS Type
.head 5 -  Set strSrvType=GetDBMS()
.head 5 -  Call SalStrUpper( strSrvType, strSrvType )
.head 5 -  ! Composing connection parameter string
.head 5 -  Set strConParams='('||strSrvType||')'||strUser||'/'||strPass||'@'||strDB
.head 5 -  ! Encripting Conection params
.head 5 -  Set strEncConParams=Encript(strConParams)
