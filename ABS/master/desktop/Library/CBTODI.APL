.head 4 +  Actions
.head 5 -  Set bInited=FALSE
.head 5 -  ! For direct debug
.head 5 -  !
.head 5 -  ! Set strDB='BARS'
.head 5 -  ! Set strUser='bars'
.head 5 -  ! Set strPass='bars'
.head 5 -  ! Set strPath='D:\\Borland\\Delphi4\\Projects\\Current\\DToCB\\'
.head 5 -  ! Set strSrvType='ORACLE'
.head 5 -  !
.head 5 -  ! To work in ABS 
.head 5 -  !
.head 5 -  ! Connection Params
.head 5 -  Call XConnectGetParams( cTmp )
.head 5 -  Set strDB=cTmp.strDBName
.head 5 -  Set strUser=cTmp.strUserName
.head 5 -  Set strPass=cTmp.strPassword
.head 5 -  ! Path to Bin
.head 5 -  Set strPath=GetDir()
.head 5 -  Call SalStrMid( strPath, 0, 1, strPath )
.head 5 -  Call SalStrUpper( strPath, strPath )
