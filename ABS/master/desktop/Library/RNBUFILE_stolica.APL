		Message Actions
			On SAM_Create
				Call SalCenterWindow( hWndForm )
				Call SalSetWindowText( hWndForm, sTitl )
				!
				Set nLength =  SalStrLength( SalStrTrimX( T4.ID ) )
				If nLength < 5
					If nLength = 1
						Set nCol =  SalColorFromRGB( 255,255,180)  !  желтій
					Else If nLength = 2
						Set nCol =  SalColorFromRGB( 230,255,230 ) ! бледнозеленый
					Else If nLength = 3
						Set nCol= SalColorFromRGB( 235,235,235 ) ! серый
					Else If nLength = 4
						Set nCol= SalColorFromRGB( 250,250,250 ) ! серый
					Call SalColorSet( NAME, COLOR_IndexWindow, nCol )
				!
				Call SalTblReset( tD )
				If nRep = 611 
					Set sSql =
							"SELECT NAME,
							        D0 ||'',V0 ,NORM, D1 ||'',V1 ,NORM, D2 ||'',V2 ,NORM, D3||'',V3,NORM, D4||'',V4,NORM,
							        D5 ||'',V5 ,NORM, D6 ||'',V6 ,NORM, D7 ||'',V7 ,NORM, D8||'',V8,NORM, D9||'',V9,NORM,
							        D10||'',V10,NORM, D11||'',V11,NORM, D12||'',V12,NORM    
							 INTO :NAME, 
							      :D[0],:V[0],:G[0], :D[1],:V[1],:G[1], :D[2],:V[2],:G[2], :D[3],:V[3],:G[3], :D[4],:V[4],:G[4],
							      :D[5],:V[5],:G[5], :D[6],:V[6],:G[6], :D[7],:V[7],:G[7], :D[8],:V[8],:G[8], :D[9],:V[9],:G[9],
							      :D[10],:V[10],:G[10], :D[11],:V[11],:G[11], :D[12],:V[12],:G[12]    
							 FROM anb_611s WHERE :T4.ID like '___'||id "
					Call SalTblSetColumnTitle( GR0.tD.SS2, 
							"Встан.
							норма" )
				Else
					Set sSql =
							"SELECT NAME, 
							        D0||'',V0,G0, D1||'',V1,G1, D2||'',V2,G2, D3||'',V3,G3, D4||'',V4,G4,
							        D5||'',V5,G5, D6||'',V6,G6, D7||'',V7,G7, D8||'',V8,G8, D9||'',V9,G9,
							        D10||'',V10,G10, D11||'',V11,G11, D12||'',V12,G12      
							 INTO :NAME, 
							      :D[0],:V[0],:G[0], :D[1],:V[1],:G[1], :D[2],:V[2],:G[2], :D[3],:V[3],:G[3], :D[4],:V[4],:G[4],
