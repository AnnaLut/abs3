		Functions
			Function: FunMakeColSetNSQLStr
				Description: Создать набор колонок для расшифровки показателей
						и строки "населения" и сохранения таблицы
				Returns
					Boolean:
				Parameters
					Receive Number: nColInKey
					Receive String: strPopulate
				Static Variables
				Local variables
					Number: nNum
					String: strColName
					String: strColVal
					String: strIsCode
					Number: nAtr
					!
					Number: nColId
					String: strSelect
					String: strInto
					String: strOrderBy
				Actions
					Set nNum = 0
					Set strSelect = ''
					Set strInto = ''
					Call SalTblDestroyColumns( hWndForm )
					If SqlPrepareAndExecute( hSql(), 
							"SELECT natr, name, val, iscode
							 INTO :nAtr, :strColName, :strColVal, :strIsCode
							 FROM form_stru
							 WHERE kodf = :sFCode ORDER BY natr")
						While SqlFetchNext( hSql(), nFetchRes )
							Set nNum = nNum + 1
							If SalStrToNumber( strIsCode ) != 0
								Set strColName = strColName || PutCrLf() || '(код)'
							Else
								Set nColInKey = nNum - 1
							!
							If NOT SalTblCreateColumn( hWndForm, nNum, 1.8, 25, strColName )
								Return FALSE
							!
							If strSelect != ''
								Set strSelect = strSelect || ', '
							Set strSelect = strSelect || strColVal
							!
							If SalStrToNumber( strIsCode ) != 0
								If strOrderBy != ''
									Set strOrderBy = strOrderBy || ', '
								Set strOrderBy = strOrderBy || SalNumberToStrX( nNum, 0 )
							!
							If strInto != ''
								Set strInto = strInto || ', '
							Set strInto = strInto || ':T00#' || SalNumberToStrX(nNum, 0)
						If nNum = 0
							Return FALSE
						Set strPopulate = 'SELECT '||strSelect||' INTO '||strInto||
								' FROM anb_xau WHERE file_id='||Str(FILE_ID)||' ORDER BY '||strOrderBy
						Return TRUE
		Window Parameters
