		Window Parameters
			Number: nPar
			String: strPar01
			String: strPar02
		Window Variables
			String: sTmp
			String: sMETOD
			String: sMFO
			String: sNB
			String: aMFO
			String: sFiltr
			Number: nRow
			Date/Time: bDat
		Message Actions
			On SAM_Create
				Set LIM.strPrintFileName = 'LIM'
				Set LIM.nTabInstance = 1
				Set LIM.strFilterTblName = 'ANB_MFO_LIM'
				Call SalTblSetLockedColumns( hWndForm, 9 )
				Call SalGetWindowText( hWndForm, sTmp, 250 )
				If SqlPrepareAndExecute( hSql(),
						"SELECT val into :sMETOD from params where par ='ANB_2' ")
						and not SqlFetchNext( hSql(), nFetchRes )
					Set sMETOD ='AGIO'
				Call SalSetWindowText( hWndForm, sTmp ||' Методика '||sMETOD )
				If sMETOD = "STOLICA"
					Call SalTblSetColumnTitle( LIM.K2, "Сума
							балів" )
					Call SalTblSetColumnTitle( LIM.K3, "Фін.
							стан" )
				Else If sMETOD = 'AGIO'
					Call SalHideWindow( LIM.sK3 )
				! Call SalDisableWindow( pb_Lim )
				! Set LIM.fFilterAtStart = 1
				!
				! дата встановлення лiмiту     : D1                    ( 01.07.2004
						баланси i нормативи за дату    D0 = D1-1мiс          ( 01.06.2004
				Set bDat = GetBankDate()
				Set aMFO = GetBankMfo()
				If SqlPrepareAndExecute( hSql()," SELECT last_day(:bDat)+1 into :D1 from dual ")
						and SqlFetchNext( hSql(), nFetchRes )
					Call SalSendMsg( D1, SAM_Validate, 0, 0 )
				!
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On SAM_FetchRowDone
				! 13. Лiмiт розрахований    - голубой
				Call XSalTblSetCellBackColor( LIM.LROZ, COLOR_Sky )
				! 9. Пропозицii з лiмiту (дилера) - желтый
				Call XSalTblSetCellBackColor( LIM.PROP, COLOR_Maize)
				! 8. Лiмiт дiючий
				Call XSalTblSetCellBackColor( LIM.DLIM, COLOR_LightGray)
				! 14. Лiмiт затверджений
				If LIM.VYSN = NUMBER_Null
					Set LIM.VYSN = SalNumberMin( LIM.PROP, LIM.LROZ )
				! меньшее из 8 и 13 - слабо зеленое
				If LIM.VYSN = SalNumberMin( LIM.PROP, LIM.LROZ )
					Call XSalTblSetCellBackColor( LIM.VYSN, COLOR_LightGreen)
				! Иначе (изменено вручную)  - зеленое
				Else
					Call XSalTblSetCellBackColor( LIM.VYSN, COLOR_Green)
				Call XSalTblSetCellBackColor( LIM.K1, SalColorFromRGB( 220,255,255 ) )
				Call XSalTblSetCellBackColor( LIM.K2, SalColorFromRGB( 220,255,255 ) )
				Call XSalTblSetCellBackColor( LIM.K3, SalColorFromRGB( 220,255,255 ) )
				If sMETOD = "STOLICA"
				If SqlPrepareAndExecute( hSqlAux2(),"SELECT name into :sK3 from stan_fin where fin =:K3 ")
						and not SqlFetchNext(  hSqlAux2(), nFetchRes )
			On UM_Populate
				Call SalWaitCursor(TRUE)
				Set sFiltr = ""
				Set sFiltr = cF.GetFilterWhereClause( FALSE )
				If sFiltr !=  ""
					Set sFiltr = " and " || sFiltr
				Set LIM.strSqlPopulate =
						"SELECT 1, b.MFO ,   l.ID  ,   b.nb  ,   l.name,   l.KOEF,   
						           (select max(fdat) from anb_mfo_lim where mfo=b.mfo and id=l.id and fdat<:D1 ),
						           ANB_MFO_LIM.LIM ,   ANB_MFO_LIM.PROP,   ANB_MFO_LIM.VYSN, 
						           ANB_MFO_LIM.K1  ,   ANB_MFO_LIM.K2,     ANB_MFO_LIM.K3  ,   ANB_MFO_LIM.LROZ,
						           nvl( ANB_MFO_LIM.H1, anb.NORMativ(2, anb.BASE_FILE_ID(b.mfo,:D0, 'G' ) ) ) 
						   INTO :LIM.ROW_, :LIM.MFO ,:LIM.ID  ,:LIM.NB  ,:LIM.NLIM,:LIM.KOEF,:LIM.FDAT,  
						        :LIM.DLIM,:LIM.PROP,:LIM.VYSN,:LIM.K1  ,:LIM.K2  ,:LIM.K3  ,:LIM.LROZ, :LIM.H1  
						 from  ANB_MFO_LIM,  anb_bank b, anb_lim l
						 where ANB_MFO_LIM.mfo=b.mfo AND b.prp>0 AND ANB_MFO_LIM.id=l.id AND ANB_MFO_LIM.fdat=:D1 " || sFiltr ||" 
						 UNION all 
						 SELECT 0, MFO, ID, NB, NAME, KOEF, FDAT, LIM, PROP, VYSN, K1, K2, K3, LROZ, H1
						 FROM ( SELECT  b.MFO ,   l.ID  ,   b.nb  ,   l.name,   l.KOEF,    
						          (select max(fdat) from anb_mfo_lim where mfo=b.mfo and id=l.id and fdat<:D1 ) FDAT,
						          to_number(null) LIM, to_number(null) PROP, to_number(null) VYSN, 
						          to_number(null) K1, to_number(null) K2, to_number(null) K3, to_number(null) LROZ,
						          anb.NORMativ(2, anb.BASE_FILE_ID(b.mfo,:D0, 'G' ) ) H1
						        FROM  anb_bank b, anb_lim l 
						        where b.prp>0 AND (b.mfo,l.id) not in (select mfo,id from anb_mfo_lim where fdat=:D1 ) 
						       ) ANB_MFO_LIM 
						  WHERE MFO is not null " || sFiltr ||" 
						 order by 2, 3  " 
				! Call Debug(LIM.strSqlPopulate)
				Call SalTblDefineSplitWindow(hWndForm, 0, FALSE)
				Call SalTblPopulate(hWndForm, hSql(),  LIM.strSqlPopulate, TBL_FillAll)
				Call SalTblSetContext(hWndForm, 0)
				Call SalPostMsg(hWndForm, SAM_TblDoDetails, 0, 0)
				Call SalWaitCursor(FALSE)
