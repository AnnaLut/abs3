			Function: SVERNUT
				Description: Свернуть ветку
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					If OP =1 
						Set OP = 0
						Set yLike =SalStrTrimX( ID )
						Set nLength =  SalStrLength( yLike )
						Set nRow = SalTblQueryContext ( hWndForm ) 
						Call SalTblFetchRow( hWndForm, nRow )
						While SalTblFindNextRow( hWndForm, nRow, 0, 0)
							Call SalTblFetchRow( hWndForm, nRow )
							If Subs( ID, 1, nLength ) = yLike
								Call SalTblDeleteRow( hWndForm, nRow, TBL_NoAdjust ) 
								Set nRow = nRow -1 
							Else
								Return TRUE
					Return TRUE 
			Function: RAZ_All
				Description: Розгорнути все "дерево" звіту '
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Set nRow = TBL_MinRow
					While SalTblFindNextRow( hWndForm, nRow, 0, 0)
						Call SalTblFetchRow( hWndForm, nRow )
						Call RAZVERN()
					Return TRUE 
			Function: RAZVERN
				Description: Развернуть  ветку
				Returns
				Parameters
				Static Variables
				Local variables
					String: yLike1
					Number: V[13]
					Number: G[13]
				Actions
					If OP =0 
						Set yLike =SalStrTrimX( ID )
						Set nLength =  SalStrLength( yLike )
						If nLength > 1
							Set OP =1 
							If nLength = 2
								Set yLike1 = yLike || '_ '
							Else If nLength = 3
								Set yLike1 = yLike || '_'
							Set nLike = ID
							Set sTmp =
									"SELECT id,NAME,SID, V0, G0, V1, G1, V2, G2, V12, G12, 
									  V3, G3, V4, G4, V5, G5, V6, G6, V7, G7, V8, G8, V9, G9, V10, G10, V11, G11
									 INTO :ID_,:NAME_,:SIM_,
									 :V[0],:G[0],:V[1],:G[1],:V[2],:G[2],:V[12],:G[12],
									 :V[3],:G[3],:V[4],:G[4],:V[5],:G[5],:V[6], :G[6], 
									 :V[7],:G[7],:V[8],:G[8],:V[9],:G[9],:V[10],:G[10],:V[11],:G[11]
									 FROM anb1 
									 WHERE  nrep="||Str(nRep) ||" and id like '"||yLike1||"' and id >'"||nLike|| "' 
									 ORDER BY id "
							If SqlPrepareAndExecute(  hSqlAux(), sTmp )
								While SqlFetchNext( hSqlAux(), nFetchRes)
									Set nRow = SalTblQueryContext ( hWndForm )
									Set nRow = SalTblInsertRow( hWndForm, nRow + 1)
									Call SalTblSetContext(hWndForm, nRow )
									Set ID  = ID_
									Set NAME= NAME_ 
									Set SIM = SIM_
									Set V0 = V[0] 
									Set G0 = G[0]
									Set V1 = V[1] 
									Set G1 = G[1]
									Set V2 = V[2] 
									Set G2 = G[2]
									Set V3 = V[3] 
									Set G3 = G[3]
									Set V4 = V[4] 
									Set G4 = G[4]
									Set V5 = V[5] 
									Set G5 = G[5]
									Set V6 = V[6] 
									Set G6 = G[6]
									Set V7 = V[7] 
									Set G7 = G[7]
									Set V8 = V[8] 
									Set G8 = G[8]
									Set V9 = V[9] 
									Set G9 = G[9]
									Set V10= V[10] 
									Set G10= G[10]
									Set V11= V[11] 
									Set G11= G[11]
									Set V12= V[12] 
									Set G12= G[12]
									Set OP = 0 
									Call Col_SUM ( nLength + 1 )
									Call SalTblSetRowFlags( hWndForm, nRow, ROW_New, FALSE )
					Return TRUE 
			Function: Col_SUM
				Description:
				Returns
				Parameters
					Number: nLength
				Static Variables
				Local variables
					Number: nCol
					Number: nV
					Number: nG
				Actions
					If nRep != 611 
						Set NAME = SalStrRepeatX( ' ',  (nLength -1) * 4 ) || NAME
						If nLength < 5
							If nLength = 1
								Set nCol =  SalColorFromRGB( 255,255,180)  !  желтій
							Else If nLength = 2
								Set nCol =  SalColorFromRGB( 170,255,170 ) 
							Else If nLength = 3
								Set nCol= SalColorFromRGB( 215,255,215 )
							Else If nLength = 4
								Set nCol= SalColorFromRGB( 230,250,230 ) 
					Else
						If GS>7 and GS<13 or GS>19
							Set nCol= SalColorFromRGB( 215,255,215 )
						Else
							Set nCol =  SalColorFromRGB( 170,255,170 ) 
					!
					Call XSalTblSetCellBackColor( ID, nCol )
					Call XSalTblSetCellBackColor( NAME, nCol ) 
					Call XSalTblSetCellBackColor( SIM, nCol ) 
					!
					If sTIP = 'У' or sTIP = 'Н'
						If T4.V1 ! = 0 
							Set G0 = (V0-V1)*100 /V1
						If T4.V2 ! = 0 
							Set G1 = (V1-V2)*100 /V2
						If T4.V3 ! = 0 
							Set G2 = (V2-V3)*100 /V3
						If T4.V4 ! = 0 
							Set G3 = (V3-V4)*100 /V4
						If T4.V5 ! = 0 
							Set G4 = (V4-V5)*100 /V5
						If T4.V6 ! = 0 
							Set G5 = (V5-V6)*100 /V6
						If T4.V7 ! = 0 
							Set G6 = (V6-V7)*100 /V7
						If T4.V8 ! = 0 
							Set G7 = (V7-V8)*100 /V8
						If T4.V9 ! = 0 
							Set G8 = (V8-V9)*100 /V9
						If T4.V10 ! = 0 
							Set G9 = (V9-V10)*100 /V10
						If T4.V11 ! = 0 
							Set G10 = (V10-V11)*100 /V11
						If T4.V12 ! = 0 
							Set G11 = (V11-V12)*100 /V12
						If sTIP = 'Н'
							Call NH_611 (T4.GS, T4.V0, T4.V0, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V1, T4.V1, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V2, T4.V2, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V3, T4.V3, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V4, T4.V4, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V5, T4.V5, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V6, T4.V6, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V7, T4.V7, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V8, T4.V8, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V9, T4.V9, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V10, T4.V10, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V11, T4.V11, FALSE, NUMBER_Null )
							Call NH_611 (T4.GS, T4.V12, T4.V12, FALSE, NUMBER_Null )
							Set ID = Subs(Str(100+GS),2,2)||'-'||ID 
					!
					! Else
					If sTIP = 'У' or sTIP = 'Б'
						Set nV = V0/2 + V1 + V2 + V3+ V4+ V5+ V6+ V7+ V8+ V9+ V10+ V11+ V12/2
						Set VS = nV /T4.kT4
						Set nG = G0/2 + G1 + G2 + G3+ G4+ G5+ G6+ G7+ G8+ G9+ G10+ G11+ G12/2
						Set GS = nG /T4.kT4
						!
						Call Col_SUM1 (VS, VS, GS, GS)
						Call Col_SUM1 (V0, V0, G0, G0)
						Call Col_SUM1 (V1, V1, G1, G1)
						Call Col_SUM1 (V2, V2, G2, G2)
						Call Col_SUM1 (V3, V3, G3, G3)
						Call Col_SUM1 (V4, V4, G4, G4)
						Call Col_SUM1 (V5, V5, G5, G5)
						Call Col_SUM1 (V6, V6, G6, G6)
						Call Col_SUM1 (V7, V7, G7, G7)
						Call Col_SUM1 (V8, V8, G8, G8)
						Call Col_SUM1 (V9, V9, G9, G9)
						Call Col_SUM1 (V10,V10,G10,G10)
						Call Col_SUM1 (V11,V11,G11,G11)
						Call Col_SUM1 (V12,V12,G12,G12)
			Function: Col_SUM1
				Description:
				Returns
				Parameters
					Window Handle: hV
					Receive Number: nV
					Window Handle: hG
					Receive Number: nG
				Static Variables
				Local variables
					Number: nCol
				Actions
					!
					If nV = 0 or nV = NUMBER_Null
						Set nV = NUMBER_Null
					Else If nV > 0
						Call XSalTblSetCellBackColor(  hV ,  nColP )
					Else
						! Set nV = - nV 
						Call XSalTblSetCellBackColor(  hV ,  nColA )
					!
					If nG = 0 or nG = NUMBER_Null
						Set nG = NUMBER_Null
					Else If nG > 0
						Call XSalTblSetCellBackColor(  hG ,  nColP )
					Else
						! Set nG = - nG
						Call XSalTblSetCellBackColor(  hG ,  nColA )
					!
