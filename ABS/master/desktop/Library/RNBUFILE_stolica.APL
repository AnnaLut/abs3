		Functions
			Function: F_txt
				Description:
				Returns
				Parameters
					Window Handle: hW
					String: NAME
					Number: i
				Static Variables
				Local variables
					Number: j
				Actions
					Set j= SalNumberMod( i-1, 6 ) * 40
					Call SalTblSetColumnTitle( hW, Subs(NAME, j+1,10)||'
							'||Subs(NAME,j+11,10)||'
							'||Subs(NAME,j+21,10)||'
							'||Subs(NAME,j+31,10)||'
							'||Str(i) )
			Function: F_MFO
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Call SalEnableWindow( pbIns )
					If MFON = NUMBER_Null
						Set MFOS= STRING_Null
						Set sNB = "Всі банки-позичальники" 
					Else
						Set MFOS= Str(MFON)
						If SqlPrepareAndExecute( hSql(),"SELECT NB INTO :sNB from anb_bank where mfo=:MFOS")
								and not SqlFetchNext( hSql(), nFetchRes )
							Set sNB = "Банк не зареєстровано !" 
							Call SalDisableWindow( pbIns )
		Window Parameters
		Window Variables
			String: NAME6
			String: NAME12
			String: NAME18
			String: NAME24
			String: NAME30
			String: NAME36
			String: NAME42
			String: NAME48
			String: NAME54
			String: NAME60
			String: NAME66
			String: NAME72
			String: MFOS
			String: sFiltr
		Message Actions
			On SAM_Create
				Set AR.strPrintFileName = 'AR'
				Set AR.nTabInstance = 1
				Set DAT = GetBankDate() - Day( GetBankDate() ) + 1 
				Set MFON = NUMBER_Null
				Set sNB = "Всі банки-позичальники" 
				Call SalTblSetLockedColumns( hWndForm, 3 )
				If SqlPrepareAndExecute( hSql(), 
						  "select NAME6, NAME12, NAME18, NAME24, NAME30, NAME36,
						          NAME42,NAME48, NAME54, NAME60, NAME66, NAME72  
						   into  :NAME6,:NAME12,:NAME18,:NAME24,:NAME30,:NAME36,
						        :NAME42,:NAME48,:NAME54,:NAME60,:NAME66,:NAME72  
