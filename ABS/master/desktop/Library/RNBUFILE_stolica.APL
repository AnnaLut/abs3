			On SAM_Create
				Set TR.strPrintFileName = 'TR'
				Set nCol_MFO =  SalColorFromRGB( 230,250,230 ) 
				Set TR.strSqlPopulate =
						"SELECT a.mfo, a.nb, substr(b.sab,2,3)
						 INTO :TR.MFO,:TR.NB, :TR.SAB3 
						 FROM banks b, anb_bank a WHERE a.mfo=b.mfo  "
				Set TR.nTabInstance = 1
				Call LH_611 ()
				Set DAT = GetBankDate()
				Call SalSendMsg( DAT, SAM_Validate, 0, 0 )
				!
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On SAM_CreateComplete
				Call SalWaitCursor(FALSE)
			On UM_Populate
				Call SalWaitCursor(TRUE)
				Call SalTblDefineSplitWindow(hWndForm, 0, FALSE)
				Call SalTblPopulate(hWndForm, hSql(), TR.strSqlPopulate, TBL_FillAll)
				Call SalTblSetContext(hWndForm, 0)
				Call SalPostMsg(hWndForm, SAM_TblDoDetails, 0, 0)
				Call SalWaitCursor(FALSE)
			On SAM_FetchRowDone
				If SqlPrepareAndExecute( hSqlAux2(),
						  "SELECT c.nd, c.RNK, c.NMK, -sum(a.ostc)/100000000 into :ND, :RNK, :NMK, :OST
						   FROM customer c, custbank u, accounts a , cust_acc cu
						   WHERE u.mfo=:MFO and u.rnk=c.rnk and u.rnk =cu.rnk and cu.acc=a.acc and a.ostc<0 
						   GROUP BY c.nd, c.RNK, c.NMK" )
						and SqlFetchNext( hSqlAux2(), nFetchRes )
				Set FAS='S'||SAB3||MMYY||'%'
				If SqlPrepareAndExecute( hSqlAux2(),
						  "SELECT  FILE_ID into :nFileID FROM anb_611 where fa like :FAS " )
						and SqlFetchNext( hSqlAux2(), nFetchRes )
						and SqlPrepareAndExecute( hSqlAux2(),
						"SELECT 
						anb.NORMativ(2, :nFileID),anb.NORMativ(3, :nFileID),anb.NORMativ(4, :nFileID),anb.NORMativ(5, :nFileID), 
						anb.NORMativ(6, :nFileID),anb.NORMativ(7, :nFileID),anb.NORMativ(8, :nFileID),anb.NORMativ(11,:nFileID), 
						anb.NORMativ(13,:nFileID),anb.NORMativ(14,:nFileID),anb.NORMativ(16,:nFileID),anb.NORMativ(17,:nFileID),
						anb.NORMativ(19,:nFileID),anb.NORMativ(20,:nFileID),anb.NORMativ(21,:nFileID),anb.NORMativ(22,:nFileID) 
						 INTO :G02,:G03,:G04,:G05,:G06,:G07,:G08,:G11,:G13,:G14,:G16,:G17,:G19,:G20,:G21,:G22 
						 FROM dual") 
						and  SqlFetchNext( hSqlAux2(), nFetchRes )
					! Set nTmp = nFileID
					Set nTmp = NUMBER_Null
					Call NH_611 ( 2, TR.G02, TR.G02, TRUE, nTmp ) 
					Call NH_611 ( 3, TR.G03, TR.G03, TRUE, nTmp ) 
					Call NH_611 ( 4, TR.G04, TR.G04, TRUE, nTmp ) 
					Call NH_611 ( 5, TR.G05, TR.G05, TRUE, nTmp ) 
					Call NH_611 ( 6, TR.G06, TR.G06, TRUE, nTmp ) 
					Call NH_611 ( 7, TR.G07, TR.G07, TRUE, nTmp ) 
					Call NH_611 ( 8, TR.G08, TR.G08, TRUE, nTmp )  
					Call NH_611 (11, TR.G11, TR.G11, TRUE, nTmp ) 
					Call NH_611 (13, TR.G13, TR.G13, TRUE, nTmp ) 
					Call NH_611 (14, TR.G14, TR.G14, TRUE, nTmp ) 
					Call NH_611 (16, TR.G16, TR.G16, TRUE, nTmp ) 
					Call NH_611 (17, TR.G17, TR.G17, TRUE, nTmp ) 
					Call NH_611 (19, TR.G19, TR.G19, TRUE, nTmp ) 
					Call NH_611 (20, TR.G20, TR.G20, TRUE, nTmp ) 
					Call NH_611 (21, TR.G21, TR.G21, TRUE, nTmp ) 
					Call NH_611 (22, TR.G22, TR.G22, TRUE, nTmp ) 
				Call XSalTblSetCellBackColor( MFO, nCol_MFO )
				Call XSalTblSetCellBackColor( NB, nCol_MFO )
			On SAM_DoubleClick
				If nFileID > 0
					Call ANB ( 400 + 611, nFileID, 'S'||SAB3||MMYY||' '||MFO ||' '|| NB, SalFmtFormatDateTime( DAT, "dd/MM/yyyy" ) )
