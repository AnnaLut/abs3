			Date/Time: dDAT1
			String: DD1
			Number: n980
		Message Actions
			On SAM_Create
				Set MFO = Val(sMFO)
				Call SalMapEnterToTab( TRUE )
				Set NEXT_DATE = dDAT
				Set FA  = sFA
				Set NB  = sNB
				Set FILE_ID = nFileID 
				Call SalSetWindowText( hWndForm, "Файл звітності " || sFA  )
				Call LH_611 ()
				Set T611.nTabInstance = 1
				Set n980 = GetBaseVal()
				!
				If sFA = STRING_Null 
						and SqlPrepareAndExecute( hSql(),"SELECT sab into :SAB from banks where MFo=:MFO ")
						and SqlFetchNext( hSql(), nFetchRes )
					! 611 еще нет - вставим
					Set DDMMYYYY = '01'|| SalFmtFormatDateTime( NEXT_DATE, 'MMyyyy' )
					Set FA='S'||Subs(SAB,2,3)|| Subs(DDMMYYYY,3,2) ||  Subs(DDMMYYYY,7,2) ||'.not'
					Set dDAT1 = AddMonths( NEXT_DATE, -1 )
					Set dDAT1 = SalDateConstruct ( SalDateYear( dDAT1 ), SalDateMonth( dDAT1), 1, 0, 0, 0 )
					If SqlPrepareAndExecute( hSql(), "INSERT INTO ANB_611 ( file_id, fa ) values (:nFileID, :FA )" ) 
							and SqlPrepareAndExecute( hSql(),"INSERT INTO ANB_611t ( file_id, G01 ) values (:nFileID, 'Середні ' )" ) 
							and SqlPrepareAndExecute( hSql(),"INSERT INTO ANB_611t ( file_id, G01 ) 
							    SELECT :nFileID, to_char( :dDAT1 + (NUM-1) ,'dd/mm/yy') FROM conductor WHERE num < 32 and
							           to_char(:dDAT1 + (NUM-1),'mmyyyy') = to_char(:dDAT1,'mmyyyy') and 
							           :dDAT1 + (NUM-1) not in 
							    (select HOLIDAY from HOLIDAY where kv=:n980 and 
							           to_char(HOLIDAY,'mmyyyy')=to_char(:dDAT1,'mmyyyy')) " )
						Call SqlPrepareAndExecute( hSql(), 'commit')
						Set T1.FAS = FA
				Else
					! прочитать из архива этот FA
					If SqlPrepareAndExecute( hSql(), 
							  "SELECT sTiket INTO :sTiket FROM anb_611 where UPPER(fa)=:sFA ")
							 and SqlFetchNext( hSql(), nFetchRes )
						Set sTiket = VisStrSubstitute ( StrDosToWinX( sTiket ), 'НС', '--')
						Set sTiket = VisStrSubstitute (sTiket, 'НН', '--')
						Set sTiket = VisStrSubstitute (sTiket, 'і', '|')
						Set sTiket = VisStrSubstitute (sTiket, 'Ш', '+')
						Set sTiket = VisStrSubstitute (sTiket, 'Н-', '--')
						Set sTiket = VisStrSubstitute (sTiket, 'Ж-', '|-')
						Set sTiket = VisStrSubstitute (sTiket, 'Нµ', '-|')
						Set mlEditor = VisStrSubstitute (sTiket, 'Н+', '-+') 
				!
				If sTiket != STRING_Null
					! из импорченного текста - только просмотр
					Call SalDisableWindow( pbIns )
					Call SalDisableWindow( pbDel )
					Call SalDisableWindow( pbUpdate )
				Else
					! из ручного ввода  - ввод, корректировка, просмотр
					Set T611.strSqlDelete = "DELETE FROM anb_611t WHERE FILE_ID= :T611.nFileID and G01= :T611.G01o "
					Set T611.strSqlInsert = "INSERT INTO anb_611t 
							 (FILE_ID,G01,G02,G03,G04,G05,G06,G07,G08,G11,G13,G14,G16,G17,G19,G20,G21,G22) 
							 values (:T611.nFileID,:T611.G01,:T611.G02,:T611.G03,:T611.G04,:T611.G05,:T611.G06,:T611.G07,:T611.G08,
							   :T611.G11,:T611.G13,:T611.G14,:T611.G16,:T611.G17,:T611.G19,:T611.G20,:T611.G21,:T611.G22) "
					Set T611.strSqlUpdate = "UPDATE anb_611t set 
							    G01=:T611.G01, G02=:T611.G02, G03=:T611.G03, G04=:T611.G04, G05=:T611.G05, G06=:T611.G06, 
							    G07=:T611.G07, G08=:T611.G08, G11=:T611.G11, G13=:T611.G13, G14=:T611.G14, G16=:T611.G16,
							    G17=:T611.G17, G19=:T611.G19, G20=:T611.G20, G21=:T611.G21, G22=:T611.G22
							  WHERE FILE_ID = :T611.nFileID and G01= :T611.G01o "
				!
				Set T611.strSqlPopulate =
						"SELECT G01, G02, G03, G04, G05, G06, G07, G08, G11, G13, G14, G16, G17, G19, G20, G21, G22 
						 INTO  :T611.G01, :T611.G02, :T611.G03, :T611.G04, :T611.G05, :T611.G06, :T611.G07, :T611.G08,
						       :T611.G11, :T611.G13, :T611.G14, :T611.G16, :T611.G17, :T611.G19, :T611.G20, :T611.G21, :T611.G22
						 FROM anb_611t     
						 WHERE FILE_ID = :T611.nFileID 
						 ORDER BY G01 "
				!
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On SAM_FetchRowDone
				Set G01o = G01 
