		Functions
			! !!!!!!! 
			Function: F_BAL
				Description:
				Returns
				Parameters
					String: u1
					Number: n2
				Static Variables
				Local variables
				Actions
					Call SalWaitCursor(TRUE)
					Set Ntmp =  IifN( n2>4,4, n2)
					Set Utmp =  u1 || "%" 
					Set dA = 0
					Set dP = 0
					Set nA = 0
					Set Stmp = 
							"SELECT X.NBS, SUBSTR(P.NAME,1,40), x.SA/100, x.SP/100
							 INTO :tA.BS, :tA.NN, :tA.SA, :tA.SP
							 FROM PS p, (SELECT SUBSTR(KODP,3," || Str(Ntmp) || ") NBS,
							  SUM(decode(SUBSTR(KODP,1,1),'1',ZNAP,0)) SA, SUM(decode(SUBSTR(KODP,1,1),'2',ZNAP,0)) SP
							  FROM anb_XAU  WHERE FILE_ID = :FILE_ID AND
							   SUBSTR(KODP,1,1) IN ('1','2') AND SUBSTR(KODP,2,1) = '0' AND
							   SUBSTR(KODP,3,4) LIKE :Utmp   AND substr(kodp,3,1)" || IifS( pb_Bal, "='9'", "<'8'" ) || "           
							   GROUP BY SUBSTR(KODP,3," || Str(Ntmp) || ") ) X
							 WHERE LTRIM(RTRIM(P.NBS(+))) =X.NBS   ORDER by x.nbs"
					! Call Debug(Stmp)
					Call SalTblPopulate( tA, hSql(), Stmp, TBL_FillAll)
					Call SalHideWindow( ccGrafD )
					Call SalHideWindow( tD )
					Call SalHideWindow( dBS )
					Call SalHideWindow( dNN )
					Call SalWaitCursor(FALSE)
			Function: Redraw2
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Call WaitCursorOn( )
					If nA >1
						Call ccGrafD.Draw()
						! Set ccGrafD.Graph.GraphType = 6
						Set ccGrafD.Graph.YAxisStyle[0] = 2
						Set ccGrafD.Graph.YAxisTicks[0] = 4
						Set ccGrafD.Graph.Background[0] = 7
						Set ccGrafD.Graph.Background[1] = 7
						Set ccGrafD.Graph.Background[6] = 7
						Set ccGrafD.Graph.DrawMode = 2
						Call ccGrafD.Draw()
						Call SalBringWindowToTop ( ccGrafD )
					Call WaitCursorOff( )
		Window Parameters
			Number: nMode !   1 - по структуре баланса
			Number: FILE_ID
			String: MFO_NB
			String: tDAT
		Window Variables
			Number: nN2
			Number: nA
			Number: nM
			String: Stmp
			Number: Ntmp
			String: Utmp
			String: MFO
			Number: nGod
			Number: nGod1
			String: M0
			String: MM[7]
			String: MMYYYY
		Message Actions
			On SAM_Create
				Call SalCenterWindow( hWndForm )
				Call SalSetWindowText( hWndForm, "Порівняння Балансу по Файлам звітності " || MFO_NB || " від "|| tDAT )
				Set MFO = Subs(MFO_NB,1,6) 
				Set nGod = Val(Subs(tDAT,7,4))
				Set nGod1 = nGod - 1
				Set MMYYYY = Subs(tDAT,4,2) || Subs(tDAT,7,4) 
				Set pb_Bal = FALSE
				Set nN2 = 1
				Call F_BAL('', nN2 )
				Set Stmp = 
						 "SELECT distinct r.mfo, b.nb INTO :tM.MFO1,:tM.NB1
						 FROM anb_IN_FILES r, anb_bank b         
						 WHERE SUBSTR(r.file_name,2,2) IN ('01','02') AND r.mfo=b.mfo  and
						 b.mfo<>:MFO and substr(r.NEXT_DATE,3,6)= :MMYYYY
						 ORDER BY 1"
				! Call Debug(Stmp)
				Set nM = 1
				Set MM[1] = MFO
				Call SalTblSetColumnTitle(  tD.S1, MM[1] )
				Call SalTblPopulate( tM, hSql(), Stmp, TBL_FillAll)
				Call WaitCursorOff( )
				Call SalHideWindow( ccGrafD )
