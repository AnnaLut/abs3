			Function: Col_
				Description:
				Returns
				Parameters
					Window Handle: hV
					Receive Number: nV
				Static Variables
				Local variables
				Actions
					!
					If nV = 0 or nV = NUMBER_Null
						Set nV = NUMBER_Null
					Else If nV > 0
						Call XSalTblSetCellBackColor(  hV ,  nColP )
					Else
						! Set nV = - nV 
						Call XSalTblSetCellBackColor(  hV ,  nColA )
					!
		Window Parameters
			Number: nRep
			String: sG
		Window Variables
			String: sSql2
			String: sID
			String: sSIM
			String: sMFO
			String: sTIP
			String: sRep
			String: NB
			String: FAS
			String: FA
			Number: NA0
			Number: NA1
			Number: NA2
			Number: NA12 
			Number: nV[13]
			Number: nCol_MFO
			Number: nColP
			Number: nColA
			Number: nRow1
			Number: nRow2
			!
			Number: nKv
			Number: nNBCU
		Message Actions
			On SAM_Create
				Set TP.strPrintFileName = 'TP'
				Set nCol_MFO =  SalColorFromRGB( 230,250,230 ) 
				Set nColA = SalColorFromRGB( 255,240,255 )  ! розовій
				Set nColP = SalColorFromRGB( 220,255,255 ) ! голубой
				Set TP.strSqlPopulate =
						"SELECT mfo, nb, anb.BASE_FILE_ID( MFO,:DAT, :sG ) 
						 INTO :sMFO, :NB, :NA0  FROM anb_bank ORDER by mfo"
				Set TP.nTabInstance = 1
				! Call LH_611 ()
				Set DAT = GetBankDate()
				Call SalSendMsg( DAT, SAM_Validate, 0, 0 )
				If SqlPrepareAndExecute( hSql(), "SELECT name,tip INTO :sRep,:sTIP FROM anb0 WHERE nrep=:nRep ")
						 and SqlFetchNext( hSql(), nFetchRes )  
					Call SalSetWindowText( hWndForm, sRep  )
				!
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On SAM_CreateComplete
				Call SalWaitCursor(FALSE)
			On UM_Populate
				Call SalWaitCursor(TRUE)
				Call SalTblReset( hWndForm )
				If SqlPrepareAndExecute( hSqlAux2(), TP.strSqlPopulate)
					While SqlFetchNext( hSqlAux2(), nFetchRes )
						If NA0 > 0
							Set nRow1 = SalTblInsertRow(hWndForm, TBL_MaxRow)
							Call SalTblSetContext( hWndForm, nRow1)
							Call SalTblSetRowFlags(hWndForm, nRow1, ROW_New, FALSE)
							Set MFO = sMFO
							Set NAME = NB
							Call XSalTblSetCellBackColor( MFO, nCol_MFO )
							Call XSalTblSetCellBackColor( NAME, nCol_MFO )
							Call XSalTblSetCellBackColor( SIM, nCol_MFO )
							Call XSalTblSetCellBackColor( ID, nCol_MFO )
							Call XSalTblSetCellBackColor( VS, nCol_MFO )
							Call XSalTblSetCellBackColor( V12, nCol_MFO )
							Set nFileID = NA0
							If SqlPrepareAndExecute( hSql(), 
									  "SELECT anb.NEW_FILE_ID(:NA0,-1), anb.NEW_FILE_ID(:NA0,-2 ), anb.NEW_FILE_ID(:NA0,-12) 
									   INTO :NA1,:NA2,:NA12  FROM dual ")
									 and SqlFetchNext( hSql(), nFetchRes ) 
								If SqlPLSQLCommand(hSql(), "ANB.U4 ( nRep, nNBCU, sTIP, Str(nKv), NA0, NA1 ,NA2, NA12) ")
									! есть ли хоть что-нибудь
									If SqlPrepareAndExecute( hSql(),
											  "SELECT 1 from anb1 where Nvl(V0,0)<>0 and nrep=:nRep") 
											and SqlFetchNext( hSql(), nFetchRes )
										If sTIP = 'У' 
											Set sSql2 = 
													"SELECT a.id, a.NAME, a.SID, 
													        a.V0, a.V1, a.V2, a.V3, a.V4, a.V5, a.V6, a.V7, a.V8, a.V9, a.V10, a.V11, a.V12
													   INTO :sID, :NB, :sSIM, :nV[0], :nV[1], :nV[2], :nV[3], :nV[4], 
													    :nV[5], :nV[6],:nV[7], :nV[8], :nV[9], :nV[10],:nV[11],:nV[12]
													 FROM anb1 a, anb1_form f
													 where a.id=f.id  ORDER BY f.ord "
										Else
											Set sSql2 = 
													"SELECT id, NAME, SID, V0,V1,V2,V3,V4,V5,V6,V7,V8,V9,V10,V11,V12
													   INTO :sID, :NB, :sSIM, :nV[0], :nV[1], :nV[2], :nV[3], :nV[4], 
													    :nV[5], :nV[6],:nV[7], :nV[8], :nV[9], :nV[10],:nV[11],:nV[12]
													 FROM anb1 where nrep=:nRep and substr(id,3,1)=' ' 
													 ORDER BY id"
										If SqlPrepareAndExecute( hSql(),  sSql2 ) 
											While SqlFetchNext( hSql(), nFetchRes )
												! If nV[0] != 0 and  nV[0]!= NUMBER_Null
												Set nRow2 = SalTblInsertRow(hWndForm, TBL_MaxRow)
												Call SalTblSetContext( hWndForm, nRow2)
												Call SalTblSetRowFlags(hWndForm, nRow2, ROW_New, FALSE)
												Set ID = sID
												Set MFO = sMFO
												Set NAME = NB
												Set SIM  = sSIM
												Set V0 = nV[0]
												Set V1 = nV[1]
												Set V2 = nV[2]
												Set V3 = nV[3]
												Set V4 = nV[4]
												Set V5 = nV[5]
												Set V6 = nV[6]
												Set V7 = nV[7]
												Set V8 = nV[8]
												Set V9 = nV[9]
												Set V10= nV[10]
												Set V11= nV[11]
												Set V12= nV[12]
												Set VS = (V0/2 + V1 + V2 + V3+ V4+ V5+ V6+ V7+ V8+ V9+ V10+ V11+ V12/2) /12
												Call Col_ (VS, VS)
												Call Col_ (V0, V0)
												Call Col_ (V1, V1)
												Call Col_ (V2, V2)
												Call Col_ (V3, V3)
												Call Col_ (V4, V4)
												Call Col_ (V5, V5)
												Call Col_ (V6, V6)
												Call Col_ (V7, V7)
												Call Col_ (V8, V8)
												Call Col_ (V9, V9)
												Call Col_ (V10,V10)
												Call Col_ (V11,V11)
												Call Col_ (V12,V12)
				Call SalWaitCursor(FALSE)
