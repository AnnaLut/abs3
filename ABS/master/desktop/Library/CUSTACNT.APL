.head 10 -  Set sNlsSD2 = Subs(sNlsSD, i+1, n-i )
.head 10 -  Break
.head 9 -  Set i=i+1
.head 8 +  If i > n
.head 9 -  Set sNlsSD0 = sNlsSD
.head 9 -  Set sNlsSD2 = STRING_Null
.head 7 +  Else
.head 8 -  Set sNlsSD0 = STRING_Null
.head 8 -  Set sNlsSD2 = STRING_Null
