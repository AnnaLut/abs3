.head 7 +  Loop
.head 8 +  If nTmp = -1
.head 9 -  Break
.head 8 -  Set sStrFilter = SalStrReplaceX(sStrFilter, nTmp, 4, Str(nAcc))
.head 8 -  Set nTmp = SalStrScan(sStrFilter, ':ACC')
.head 7 -  Set nTmp = SalStrScan(sStrFilter, ':RNK')
.head 7 +  Loop
