.head 7 +  Else If sAction = 'DELETE'
.head 8 -  Set sQuery = sQuery || ' FROM ' || sTableList
.head 8 +  If sWhereClause != ''
.head 9 -  Set sQuery = sQuery || ' WHERE ' || sNewWhere
.head 7 +  Else
.head 8 -  Set sQuery = ''
.head 7 -  Return sQuery
