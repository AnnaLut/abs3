.head 3 -  String: sFn	!! - имя файла
.head 3 -  !
.head 2 +  Window Variables
.head 3 -  String: sTabn
.head 3 -  String: sFilt
.head 3 -  String: sFldn
.head 3 -  String: sOtm
.head 3 -  String: sOtmInto
.head 3 -  Boolean: bTab
.head 3 -  !
.head 3 -  String: Items[*,5]
.head 3 -  String: Condition[*, 2]
.head 3 -  Number: nNumItems
.head 3 -  !
.head 3 -  String: strTHeader
.head 2 +  Message Actions
.head 3 +  On SAM_Create
.head 4 -  Set winReest.strSqlUpdate = 
"UPDATE ree_tmp SET otm=:winReest.cOtm WHERE rowid = :winReest.cRowid"
.head 4 +  If nMod = REE_FILE_QUITED
.head 5 -  Set winReest.nFlags = GT_ReadOnly
.head 5 -  Set sTabn = "REE_ACC"
.head 5 -  Set sFldn = "a.nmk"
.head 5 -  !
.head 5 -  Set sOtm = ""
.head 5 -  Set sOtmInto = ""
.head 5 -  Set winReest.strTitleAux = "Заквитованые"
.head 5 -  Call SalHideWindow(winReest.cOtm)
.head 4 +  Else
.head 5 -  Set winReest.nFlags = GT_Normal
.head 5 -  Call SalEnableWindow( winReest.pbUpdate )
.head 5 -  Set sTabn = "REE_TMP"
.head 5 -  Set sFldn = "a.nmkw"
.head 5 -  !
.head 5 -  Set sOtm = ", a.otm "
.head 5 -  Set sOtmInto = ", :winReest.cOtm "
.head 5 -  Set winReest.strTitleAux = "Незаквитованые"
.head 5 -  Call SalShowWindow(winReest.cOtm)
.head 4 -  !
.head 4 +  If nDir = REE_FILE_IN
.head 5 -  Set sFilt=" a.Fn_i=:winReest.sFn "
.head 4 +  Else If nDir = REE_FILE_OUT
.head 5 -  Set sFilt=" a.Fn_o=:winReest.sFn "
.head 4 +  Else
.head 5 -  Set sFilt = ""
.head 4 +  If winReest.strTitleAux != "" AND sFn != "" 
.head 5 -  Set winReest.strTitleAux =  winReest.strTitleAux || " - "
.head 4 -  Set winReest.strTitleAux =  winReest.strTitleAux || sFn
.head 4 -  !
