! ДОЛЖНА БЫТЬ ВЫЗВАНА ПРИ ОБРАБОТКЕ SAM_CreateComplete !
! СООБЩЕНИЯ ОКНА РОДИТЕЛЯ !
.head 6 +  Returns
.head 7 -  Boolean:
.head 6 +  Parameters
.head 7 -  Window Handle: hWnd
.head 7 -  Number: nMenuItem
.head 6 -  Static Variables
.head 6 -  Local variables
.head 6 +  Actions
.head 7 -  Set fActivated = FALSE
.head 7 -  Set fInit      = TRUE
.head 7 -  !
.head 7 -  Set hWndParent = hWnd
.head 7 -  Set hWndChild  = hWndNULL
.head 7 -  Set nItem      = nMenuItem
.head 7 -  Set nItemId    = GetMenuItemID( VisMenuGetHandle( hWndParent ), nMenuItem )
.head 7 -  Set strDefaultItemName = XSalGetMenuItemTextByID( VisMenuGetHandle( hWndParent ), nItemId )
.head 7 -  Call EnableMenuItem( VisMenuGetHandle( hWndParent ), nItemId, MFS_GRAYED )
.head 7 -  Call DrawMenuBar( hWndParent )
.head 7 -  Return TRUE
.head 5 +  Function: SetItem
.head 6 -  Description: Установить название (и активировать пунтк меню)

! ДОЛЖНА БЫТЬ ВЫЗВАНА ПРИ ОБРАБОТКЕ UM_MenuComponentSet !
! СООБЩЕНИЯ РОДИТЕЛЬСКОГО ОКНА !
.head 6 +  Returns
.head 7 -  Boolean:
.head 6 +  Parameters
.head 7 -  Number: nHWindow
.head 7 -  Number: nHString
.head 6 -  Static Variables
.head 6 +  Local variables
.head 7 -  Window Handle: hWndFrom
.head 7 -  String: strItemName
.head 6 +  Actions
.head 7 +  If fInit
.head 8 -  Set hWndFrom = SalNumberToWindowHandle( nHWindow )
.head 8 -  Set strItemName = ''
.head 8 -  !
.head 8 -  Set hWndChild = hWndFrom
.head 8 -  Set strItemName = SalNumberToHString( nHString )
.head 8 -  Call XSalSetMenuItemTextByID( VisMenuGetHandle( hWndParent ), nItemId, strItemName )
.head 8 -  Return Enable( )
.head 7 +  Else
.head 8 -  Return FALSE
.head 5 +  Function: Enable
.head 6 -  Description: Активизировать пункт меню
.head 6 +  Returns
.head 7 -  Boolean:
.head 6 -  Parameters
.head 6 -  Static Variables
.head 6 -  Local variables
.head 6 +  Actions
.head 7 +  If fInit
.head 8 -  Call EnableMenuItem( VisMenuGetHandle( hWndParent ), nItemId, MFS_ENABLED )
.head 8 -  Call DrawMenuBar( hWndParent )
.head 8 -  Set fActivated = TRUE
.head 8 -  Return TRUE
.head 7 +  Else
.head 8 -  Return FALSE
.head 5 +  Function: Disable
.head 6 -  Description: Деактивировать пункт меню

! ДОЛЖНА БЫТЬ ВЫЗВАНА ПРИ ОБРАБОТКЕ UM_MenuComponentDisable !
! СООБЩЕНИЯ РОДИТЕЛЬСКОГО ОКНА !
.head 6 +  Returns
.head 7 -  Boolean:
.head 6 -  Parameters
.head 6 -  Static Variables
.head 6 -  Local variables
.head 6 +  Actions
.head 7 +  If fInit
.head 8 -  Call EnableMenuItem( VisMenuGetHandle( hWndParent ), nItemId, MFS_GRAYED )
.head 8 -  Call DrawMenuBar( hWndParent )
.head 8 -  Set fActivated = FALSE
.head 8 -  Set hWndChild = hWndNULL
.head 8 -  Return TRUE
.head 7 +  Else
.head 8 -  Return FALSE
.head 5 +  Function: Kill
.head 6 -  Description: Вернуть пункту меню изначальное состояние

! ДОЛЖНА БЫТЬ ВЫЗВАНА ПРИ ОБРАБОТКЕ UM_MenuComponentKill !
! СООБЩЕНИЯ РОДИТЕЛЬСКОГО ОКНА !
.head 6 +  Returns
.head 7 -  Boolean:
.head 6 -  Parameters
.head 6 -  Static Variables
.head 6 -  Local variables
.head 6 +  Actions
.head 7 +  If NOT fActivated AND fInit
.head 8 -  Call XSalSetMenuItemTextByID( VisMenuGetHandle( hWndParent ), nItemId, strDefaultItemName )
