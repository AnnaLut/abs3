.head 3 +  On SAM_Create
.head 4 -  Call PrepareWindow(hWndForm)
.head 4 -  Call SetWindowFullSize(hWndForm)
.head 4 -  Call SalSetWindowText(hWndForm, 'КРЕДИТОВЫЙ символ для документов РЦК')
.head 4 -  Call SalTblSetLockedColumns(hWndForm, 4)
.head 4 -  Call BU.cF.Init('Oper', 'o')
.head 4 -  !
.head 4 -  Set nKV   = GetBaseVal()
.head 4 -  Set dDATA = GetBankDate()
.head 4 -  Set sdat  = SalFmtFormatDateTime(dDATA,'MM-dd-yyyy')
.head 4 -  Set nDKSimv = 1
.head 4 -  Call SalMapEnterToTab(TRUE)
.head 4 -  Call SalSendClassMessage(SAM_Create, 0, 0)
.head 4 -  Call SalEnableWindow(pbFilter)
.head 3 +  On UM_Populate
.head 4 -  Call SalWaitCursor(TRUE)
.head 4 -  Set fs = cF.GetFilterWhereClause(FALSE)
.head 4 -  Set BU.strSqlPopulate =
"SELECT o.nd, o.tt, o.ref, o.kv, o.kv2, o.dk, o.s/100, 
        o.nlsa, o.nam_a, o.nlsb, o.nam_b, o.nazn, 
        p.acc, a.nls, a.kv, c.rnkp, 
        w.value, w.value, t.name    
   INTO :BU.colND,  :BU.colTT,   :BU.colREF,  :BU.colKVA, 
        :BU.colKVB, :BU.colDK,   :BU.colS,    :BU.colNLSA, 
        :BU.colNAMA,:BU.colNLSB, :BU.colNAMB, :BU.colNAZN, 
        :BU.colAcc, :BU.colNLS,  :BU.colKV,   :BU.IDP, 
        :BU.KEKB,   :BU.KEKBold, :BU.colTTName 
   FROM oper o, accounts a, opldok p, cust_acc u, customer c, 
        rck_custmap r, v_rck_accounts rck_a, 
        tts t, (select ref, VALUE from operw where tag='RCK_K')  w
  WHERE o.ref=p.ref AND o.tt=t.tt
    AND a.nbs LIKE '26__' 
    AND a.kv IN (980, 840, 978, 643) 
    AND a.acc = p.acc 
    AND p.sos > 0 
    AND p.fdat = :dDATA
    AND p.dk = :nDKSimv  
    AND a.acc = u.acc 
    AND u.rnk = c.rnk 
    AND c.rnk = r.rnk 
    AND w.ref(+) = o.ref 
    AND a.acc = rck_a.acc " || IifS(fs!="", " AND " || fs, "") || "
  ORDER BY o.ref"
.head 4 -  Call SalTblPopulate(hWndForm, hSql(), strSqlPopulate, TBL_FillAll)
.head 4 -  Call SalTblSetContext(hWndForm, 0)
.head 4 -  Call SalTblSetRowFlags(hWndForm, 0, ROW_Selected, TRUE)
.head 4 -  Call SalSendMsg(hWndForm, SAM_TblDoDetails, 0, 0)
.head 4 -  Call SalWaitCursor(FALSE)
.head 3 +  On UM_Update
.head 4 -  Set nRow = TBL_MinRow
.head 4 +  While SalTblFindNextRow(hWndForm, nRow, ROW_Edited, 0)
.head 5 -  Call SalTblSetContext(hWndForm, nRow)
.head 5 +  If BU.KEKB
.head 6 -  Call SqlPrepareAndExecute(hSql(),
"SELECT 1 FROM operw WHERE ref=:BU.colREF AND tag='RCK_K'")
.head 6 +  If SqlFetchNext(hSql(), nFetchRes)
.head 7 +  If NOT SqlPrepareAndExecute(hSql(), 
"UPDATE operw SET value=:BU.KEKB WHERE ref=:BU.colREF AND tag='RCK_K' ")
.head 8 -  Call SqlRollback(hSql())
.head 6 +  Else 
.head 7 +  If NOT SqlPrepareAndExecute(hSql(), 
"INSERT INTO operw(ref,tag,value) VALUES(" || Str(BU.colREF) || ", 'RCK_K', '" || BU.KEKB || "')")
.head 8 -  Call SqlRollback(hSql())
.head 4 -  Call SqlCommit(hSql())
.head 4 -  Call SalSendMsg(hWndForm, UM_Populate, 0, 0)
