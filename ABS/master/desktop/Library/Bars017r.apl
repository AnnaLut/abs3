.head 4 -  Call SalTblPopulate ( tblRezArc, hSql(),
"  select coalesce(dat1,dat2), nvl(name2,name1),nvl(id2,id1) , rezform
   into :Archive.tblRezArc.Dat,
        :Archive.tblRezArc.User,
        :Archive.tblRezArc.Id,
        :Archive.tblRezArc.Rezform
   from ( select distinct t.dat dat1,s.id id1,s.fio name1 
          from tmp_rez_risk t, staff s
          where t.id=s.id and s.logname =user 
        ) a
   full outer join
       ( select r.dat dat2, s.id id2, s.fio name2, 't' rezform
         from rez_protocol r, staff s
         where r.userid = s.id
       ) b
   on (a.dat1=b.dat2)
   order by 1 desc "
,TBL_FillAll )
