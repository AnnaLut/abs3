.head 5 +  On WM_Size
.head 6 -  Call GetClientRect( hWndForm, nDlgX, nDlgY, nDlgWidth, nDlgHeight )
.head 6 -  !
.head 6 -  Set nXSel = DTL_Delta
.head 6 -  Set nYSel = DTL_Delta
.head 6 -  Set nTabW = (nDlgWidth - (DTL_ButtonWidth + 4 * DTL_Delta)) / 2
.head 6 -  Set nTabH = nDlgHeight - 2 * DTL_Delta
.head 6 -  Set nXAvl = nTabW + 3 * DTL_Delta + DTL_ButtonWidth
.head 6 -  Set nYAvl = DTL_Delta
.head 6 -  !
.head 6 -  Set nXBtn = nTabW + 2 * DTL_Delta
.head 6 -  Set nYAdd = (nDlgHeight - 2 * (DTL_ButtonHeight + DTL_Delta)) / 2
.head 6 -  Set nYDrp = nYAdd + DTL_ButtonHeight + 2 * DTL_Delta
.head 6 -  !
.head 6 -  Set nXSel = SalPixelsToFormUnits( hWndForm, nXSel, FALSE )
.head 6 -  Set nYSel = SalPixelsToFormUnits( hWndForm, nYSel, TRUE  )
.head 6 -  Set nXAvl = SalPixelsToFormUnits( hWndForm, nXAvl, FALSE )
.head 6 -  Set nYAvl = SalPixelsToFormUnits( hWndForm, nYAvl, TRUE  )
.head 6 -  Set nXBtn = SalPixelsToFormUnits( hWndForm, nXBtn, FALSE )
.head 6 -  Set nYAdd = SalPixelsToFormUnits( hWndForm, nYAdd, TRUE  )
.head 6 -  Set nYDrp = SalPixelsToFormUnits( hWndForm, nYDrp, TRUE  )
.head 6 -  Set nTabW = SalPixelsToFormUnits( hWndForm, nTabW, FALSE )
.head 6 -  Set nTabH = SalPixelsToFormUnits( hWndForm, nTabH, TRUE  )
.head 6 -  Set nBtnW = SalPixelsToFormUnits( hWndForm, DTL_ButtonWidth, FALSE )
.head 6 -  Set nBtnH = SalPixelsToFormUnits( hWndForm, DTL_ButtonHeight, TRUE  )
.head 6 -  !
.head 6 -  Call SalSetWindowLoc( tblSelected, nXSel, nYSel )
.head 6 -  Call SalSetWindowSize( tblSelected, nTabW, nTabH )
.head 6 -  Call SalSetWindowLoc( tblAvailable, nXAvl, nYAvl )
.head 6 -  Call SalSetWindowSize( tblAvailable, nTabW, nTabH )
.head 6 -  Call SalSetWindowLoc( pbAdd, nXBtn, nYAdd )
.head 6 -  Call SalSetWindowSize( pbAdd, nBtnW, nBtnH )
.head 6 -  Call SalSetWindowLoc( pbDrop, nXBtn, nYDrp )
