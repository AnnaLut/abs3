      <TableDataSource Name="Table" Alias="DOCS_DATA" DataType="System.Int32" Enabled="true" SelectCommand="select rownum as rwn, t.*&#13;&#10;  from (select d.id,&#13;&#10;               d.crt_branch,&#13;&#10;               b.name as branch_name,&#13;&#10;               d.rnk,&#13;&#10;               c.nmk as fio,&#13;&#10;               d.ea_struct_id,&#13;&#10;               sc.name as ea_struct_name,&#13;&#10;               d.crt_date,&#13;&#10;               d.crt_staff_id,&#13;&#10;               sb.fio as crt_staff_fio,&#13;&#10;               nvl(d.page_count, 1) as page_count&#13;&#10;          from (select *&#13;&#10;                  from ead_docs&#13;&#10;                 where type_id = 'SCAN'&#13;&#10;                   and :sFdat1 &lt;= crt_date&#13;&#10;                   and crt_date &lt; :sFdat2 + 1&#13;&#10;                  and crt_staff_id = decode(:Param0, '%', crt_staff_id, to_number(:Param0))&#13;&#10;                  and crt_branch like   decode(:BRANCH, 'Поточне',  sys_context ('bars_context', 'user_branch'),:BRANCH)||'%'&#13;&#10;                       ) d,&#13;&#10;               branch b,&#13;&#10;               customer c,&#13;&#10;               ead_struct_codes sc,&#13;&#10;               staff$base sb&#13;&#10;         where d.crt_branch = b.branch&#13;&#10;           and d.ea_struct_id in (143,221,146,224)&#13;&#10;           and d.rnk = c.rnk&#13;&#10;           and d.ea_struct_id = sc.id&#13;&#10;           and d.crt_staff_id = sb.id&#13;&#10;         order by d.id desc) t">
