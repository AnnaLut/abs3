      <TableDataSource Name="Table27" Alias="list_5_1" DataType="System.Int32" Enabled="true" SelectCommand="select G.BID_ID, listagg(list_mort, ', ') WITHIN GROUP(ORDER BY BID_ID) as list_mort,count(*) cunt_mort&#13;&#10;from (&#13;&#10;select bid_id, 'іпотекою &lt;b&gt;'||name||'&lt;/b&gt;, що знаходиться за адресою: '||&#13;&#10;        '&lt;b&gt;'||case when key_mort=1 then a_adres else l_adres end||'&lt;/b&gt;'||&#13;&#10;        ', загальною площею &lt;b&gt;'||case when key_mort=1 then a_total_area else l_total_area end||&#13;&#10;        '&lt;/b&gt; що надається в іпотеку &lt;b&gt;'||case when is_mort_borrower=1 then 'Позичальником' else 'Майновим поручителем' end||'&lt;/b&gt;' as list_mort&#13;&#10;from(        &#13;&#10;  select bg.bid_id,  &#13;&#10;         decode(bg.garantee_id,'MORTGAGE',1,2) key_mort,&#13;&#10;         wcs_utl.get_answ_BOOL(bg.bid_id, 'GRT_0', bg.ws_id, bg.garantee_num) is_mort_borrower,    &#13;&#10;         wcs_utl.get_answ(bg.bid_id, 'GRT_2_7', bg.ws_id, bg.garantee_num) name,&#13;&#10;         wcs_utl.get_answ_formated(bg.bid_id, 'GRT_4_3', 0, bg.ws_id, bg.garantee_num) a_total_area,&#13;&#10;         wcs_utl.get_answ(bg.bid_id, 'GRT_4_6', bg.ws_id, bg.garantee_num) a_adres,&#13;&#10;         wcs_utl.get_answ(bg.bid_id, 'GRT_5_1', bg.ws_id, bg.garantee_num) l_total_area,&#13;&#10;         wcs_utl.get_answ(bg.bid_id, 'GRT_5_21', bg.ws_id, bg.garantee_num) l_adres&#13;&#10;  from v_wcs_bid_garantees bg&#13;&#10;  where bg.bid_id = :p_bid_id&#13;&#10;  and bg.garantee_id in('MORTGAGE_LAND','MORTGAGE')&#13;&#10;  )x)g&#13;&#10;group by BID_ID">
