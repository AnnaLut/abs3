        <TableRow Name="Row142" Height="18.51">
          <TableCell Name="Cell415" Text="&lt;b&gt;4.4. &lt;u&gt;Права Позичальника&lt;/u&gt;&lt;/b&gt;" HorzAlign="Justify" Font="Times New Roman, 10pt, style=Bold" HtmlTags="true" ColSpan="5"/>
          <TableCell Name="Cell416"/>
          <TableCell Name="Cell417"/>
          <TableCell Name="Cell418"/>
          <TableCell Name="Cell419"/>
        </TableRow>
        <TableRow Name="Row143" Height="37.41" AutoSize="true">
          <TableCell Name="Cell420" Text="&lt;b&gt;4.4.1.&lt;/b&gt; В порядку, передбаченому цим Договором, одержувати від Банку інформацію про стан заборгованості за цим Договором. " Padding="20, 1, 2, 1" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="5"/>
          <TableCell Name="Cell421" Padding="20, 1, 2, 1"/>
          <TableCell Name="Cell422" Padding="20, 1, 2, 1"/>
          <TableCell Name="Cell423" Padding="20, 1, 2, 1"/>
          <TableCell Name="Cell424" Padding="20, 1, 2, 1"/>
        </TableRow>
        <TableRow Name="Row144" Height="37.41" AutoSize="true">
          <TableCell Name="Cell425" Text="&lt;b&gt;4.4.2.&lt;/b&gt; Порушувати перед Банком питання про перенесення строку повернення Кредиту (його частини) у разі виникнення тимчасових фінансових або інших ускладнень з незалежних від нього (Позичальника) причин." Padding="20, 1, 2, 1" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="5"/>
          <TableCell Name="Cell426" Padding="20, 1, 2, 1"/>
          <TableCell Name="Cell427" Padding="20, 1, 2, 1"/>
          <TableCell Name="Cell428" Padding="20, 1, 2, 1"/>
          <TableCell Name="Cell429" Padding="20, 1, 2, 1"/>
        </TableRow>
        <TableRow Name="Row145" Height="18.51" AutoSize="true">
          <TableCell Name="Cell430" Text="&lt;b&gt;4.4.3.&lt;/b&gt; Достроково погасити Кредит в порядку, передбаченому цим Договором." Padding="20, 1, 2, 1" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="5"/>
          <TableCell Name="Cell431" Padding="20, 1, 2, 1"/>
          <TableCell Name="Cell432" Padding="20, 1, 2, 1"/>
          <TableCell Name="Cell433" Padding="20, 1, 2, 1"/>
          <TableCell Name="Cell434" Padding="20, 1, 2, 1"/>
        </TableRow>
        <TableRow Name="Row146" Height="46.86" AutoSize="true">
          <TableCell Name="Cell435" Text="&lt;b&gt;4.4.4.&lt;/b&gt; Після здійснення дострокового часткового повернення Кредиту протягом 3-х (трьох) Банківських днів з дати здійснення такого повернення, укласти з Банком додатковий договір до цього Договору відповідно до пп. 3.8.6 цього Договору." Padding="20, 1, 2, 1" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="5"/>
          <TableCell Name="Cell436" Padding="20, 1, 2, 1"/>
