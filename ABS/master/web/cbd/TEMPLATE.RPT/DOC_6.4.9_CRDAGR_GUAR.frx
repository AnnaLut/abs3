      <TableDataSource Name="Table7" Alias="Marital_status" DataType="System.Int32" Enabled="true" SelectCommand="select decode(wcs_utl.get_answ(b.bid_id, 'CL_34', p.ws_id, p.ws_number),&#13;&#10;              0,&#13;&#10;              1,&#13;&#10;              4,&#13;&#10;              1,&#13;&#10;              0) as is_married,&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CL_1', p.ws_id, p.ws_number) || ' ' ||&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CL_2', p.ws_id, p.ws_number) || ' ' ||&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CL_3', p.ws_id, p.ws_number) as cl_fio,&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CL_123_O', p.ws_id, p.ws_number) as cl_fio_o,&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CL_0_176', p.ws_id, p.ws_number) as spouse_fio,&#13;&#10;       'серії ' ||&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CL_0_179', p.ws_id, p.ws_number) ||&#13;&#10;       ' № ' ||&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CL_0_179_1', p.ws_id, p.ws_number) ||&#13;&#10;       ', виданий ' ||&#13;&#10;       trim(wcs_utl.get_answ(b.bid_id, 'CL_0_180', p.ws_id, p.ws_number)) || ' ' ||&#13;&#10;       trim(f_dat_lit(wcs_utl.get_answ_date(b.bid_id,&#13;&#10;                                            'CL_0_181',&#13;&#10;                                            p.ws_id,&#13;&#10;                                            p.ws_number))) as spouse_passp,&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'CL_0_178', p.ws_id, p.ws_number) as spouse_inn&#13;&#10;  from v_wcs_bids b,&#13;&#10;       (select :p_ws_id as ws_id, :p_ws_number as ws_number from dual) p&#13;&#10; where b.bid_id = :p_bid_id">
