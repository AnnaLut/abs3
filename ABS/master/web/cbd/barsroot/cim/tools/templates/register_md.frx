      <TableDataSource Name="Table" Alias="reg959" DataType="System.Int32" Enabled="true" SelectCommand="select fn||' '||to_char(d.dat,'DD.MM.YYYY') as dat_vmd, &#13;&#10;        to_char(cnum_cst,'fm999999999999')||'/'||to_char(cnum_year,'fm9999')||'/'||to_char(cnum_num,'fm000000') as n_vmd,&#13;&#10;       decode(ctype,'цЊ','Ім.','…Љ','Ек.')  as type_vmd, to_char(cdat,'DD.MM.YYYY') as dt_create,&#13;&#10;       s_okpo||'; '||TranslateDos2Win(s_name)|| ' / ' ||TranslateDos2Win(s_adres)||' / ' ||s_type|| ' / ' ||s_taxid as sender,&#13;&#10;       r_okpo||'; '||TranslateDos2Win(r_name)||' / ' ||TranslateDos2Win(r_adres)||' / ' ||r_type|| ' / ' ||r_taxid as recever, &#13;&#10;       f_okpo||'; '||TranslateDos2Win(f_name)||'/ ' ||TranslateDos2Win(f_adres)||'/ ' ||f_type|| ' / ' ||f_taxid||' / '||f_country  as agent, &#13;&#10;       d.kv as kv, round(kurs/100000000,8) as kurs, round(s/100,2) as sum, to_char(allow_dat,'DD.MM.YYYY') as allow_dat, cmode_code as regim, &#13;&#10;       TranslateDos2Win(doc)||' / '||to_char(sdate,'DD.MM.YYYY') as doc, to_char(fdate,'DD.MM.YYYY') as end_dat,&#13;&#10;       case when cim_id is null then 'Дубль'&#13;&#10;              when cim_id is not null and d.cim_original=1 then 'Нова'&#13;&#10;              else 'Зміна'&#13;&#10;       end as stan       &#13;&#10;  from bars.customs_decl d  &#13;&#10; where ((TranslateDos2Win(d.s_name) like '%'||:p_name||'%') or (TranslateDos2Win(d.r_name) like '%'||:p_name||'%') or &#13;&#10;       TranslateDos2Win(d.f_name) like '%'||:p_name||'%' or :p_name is null) and&#13;&#10;       (d.s_okpo=:p_okpo or d.r_okpo=:p_okpo or d.f_okpo=:p_okpo or :p_okpo is null) and&#13;&#10;       (to_number(:p_mdtype,'9')=0 and d.ctype='цЊ' or to_number(:p_mdtype,'9')=1 and d.ctype='…Љ' or :p_mdtype is null) and &#13;&#10;       0&lt;(select count(*) from accounts &#13;&#10;           where dazs is null and &#13;&#10;                 (nls like '2600%' or nls like '2542%' or nls like '2545%' or nls like '2650%' or nls like '2603%') and&#13;&#10;                 branch like sys_context('bars_context','user_branch_mask') and (:p_branch = branch or :p_branch is null) and&#13;&#10;                 rnk in (select rnk from customer where okpo=nvl(d.s_okpo,d.r_okpo) or okpo=d.f_okpo)) and&#13;&#10;       (to_char(d.kv,'fm999')=:p_kv or :p_kv is null) and to_date(:p_fdat1,'dd/mm/yyyy')&lt;=dat and dat-0.9999&lt;=to_date(:p_fdat2,'dd/mm/yyyy')&#13;&#10;order by d.dat">
