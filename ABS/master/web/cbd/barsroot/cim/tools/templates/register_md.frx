        <Column Name="END_DAT" DataType="System.String"/>
        <Column Name="KV" DataType="System.String"/>
        <Column Name="STAN" DataType="System.String"/>
        <CommandParameter Name="p_fdat1" DataType="126" Expression="[p_fdat1]"/>
        <CommandParameter Name="p_fdat2" DataType="126" Expression="[p_fdat2]"/>
        <CommandParameter Name="p_name" DataType="126" Expression="[p_name]"/>
        <CommandParameter Name="p_okpo" DataType="126" Expression="[p_okpo]"/>
        <CommandParameter Name="p_mdtype" DataType="126" Expression="[p_mdtype]"/>
        <CommandParameter Name="p_kv" DataType="126" Expression="[p_kv]"/>
        <CommandParameter Name="p_branch" DataType="126" Expression="[p_branch]"/>
      </TableDataSource>
      <TableDataSource Name="Table2" Alias="ofise" DataType="System.Int32" Enabled="true" SelectCommand="select sys_context('bars_context','user_branch_mask') as user_branch from dual">
