        <Column Name="N_VMD" DataType="System.String"/>
        <Column Name="TYPE_VMD" DataType="System.String"/>
        <Column Name="DT_CREATE" DataType="System.String"/>
        <Column Name="SENDER" DataType="System.String"/>
        <Column Name="RECEVER" DataType="System.String"/>
        <Column Name="AGENT" DataType="System.String"/>
