      <TableDataSource Name="Table1" Alias="CRD_DATA" DataType="System.Int32" Enabled="true" SelectCommand="select cd.cc_id,&#10;       f_dat_lit(cd.sdate) as sdate_pr,&#10;       cd.sdog,&#10;       trim(f_sumpr(cd.sdog, null, null)) as sdog_pr,&#10;       round(acrn.fprocn(a.acc, 0, a.daos), 2) as ir,&#10;       f_sumpr(round(acrn.fprocn(a.acc, 0, a.daos), 2), null, null) as ir_pr,&#10;       cd.wdate,&#10;       f_dat_lit(cd.wdate) as wdate_pr,&#10;       cck_app.pay_day_sn_to_nd(cd.nd) as pay_day&#10;  from cc_deal cd, accounts a, nd_acc n&#10; where cd.nd = :p_bid_id&#10;   and n.nd = cd.nd&#10;   and n.acc = a.acc&#10;   and a.tip = 'LIM'">
