      <TableDataSource Name="Table" Alias="BID_DATA" DataType="System.Int32" Enabled="true" SelectCommand="with pars as&#13;&#10; (select decode(wcs_utl.get_answ_bool(bg.bid_id,&#13;&#10;                                      'GRT_0',&#13;&#10;                                      bg.ws_id,&#13;&#10;                                      bg.garantee_num),&#13;&#10;                1,&#13;&#10;                'MAIN',&#13;&#10;                bg.ws_id) as ws_id,&#13;&#10;         decode(wcs_utl.get_answ_bool(bg.bid_id,&#13;&#10;                                      'GRT_0',&#13;&#10;                                      bg.ws_id,&#13;&#10;                                      bg.garantee_num),&#13;&#10;                1,&#13;&#10;                0,&#13;&#10;                bg.garantee_num) as ws_number&#13;&#10;    from v_wcs_bid_garantees bg&#13;&#10;   where bg.bid_id = :p_bid_id&#13;&#10;     and bg.garantee_id = 'MORTGAGE'&#13;&#10;     and bg.garantee_num = :p_ws_number)&#13;&#10;select bg.agr_number,&#13;&#10;       bg.agr_date,&#13;&#10;       f_dat_lit(bg.agr_date) as agr_date_pr,&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_1', pars.ws_id, pars.ws_number) || ' ' ||&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_2', pars.ws_id, pars.ws_number) || ' ' ||&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_3', pars.ws_id, pars.ws_number) as fio,&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_123_O', pars.ws_id, pars.ws_number) as fio_o,&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_7', pars.ws_id, pars.ws_number) as passp_ser,&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_8', pars.ws_id, pars.ws_number) as passp_num,&#13;&#10;       trim(wcs_utl.get_answ(bg.bid_id, 'CL_9', pars.ws_id, pars.ws_number)) as passp_organ,&#13;&#10;       wcs_utl.get_answ_date(bg.bid_id, 'CL_11', pars.ws_id, pars.ws_number)as passp_date,&#13;&#10;       case&#13;&#10;         when 1 = wcs_utl.get_answ(bg.bid_id,&#13;&#10;                                   'CL_0_7',&#13;&#10;                                   pars.ws_id,&#13;&#10;                                   pars.ws_number) then&#13;&#10;          decode(wcs_utl.get_answ(bg.bid_id,&#13;&#10;                                  'CL_13',&#13;&#10;                                  pars.ws_id,&#13;&#10;                                  pars.ws_number),&#13;&#10;                 26,&#13;&#10;                 null,&#13;&#10;                 27,&#13;&#10;                 null,&#13;&#10;                 'обл. ' ||&#13;&#10;                 wcs_utl.get_answ_refer_text(bg.bid_id,&#13;&#10;                                             'CL_13',&#13;&#10;                                             pars.ws_id,&#13;&#10;                                             pars.ws_number) || ', ') ||&#13;&#10;          decode(wcs_utl.get_answ(bg.bid_id,&#13;&#10;                                  'CL_14',&#13;&#10;                                  pars.ws_id,&#13;&#10;                                  pars.ws_number),&#13;&#10;                 null,&#13;&#10;                 null,&#13;&#10;                 'р. ' || wcs_utl.get_answ(bg.bid_id,&#13;&#10;                                           'CL_14',&#13;&#10;                                           pars.ws_id,&#13;&#10;                                           pars.ws_number) || ', ') ||&#13;&#10;          wcs_utl.get_answ(bg.bid_id, 'CL_15', pars.ws_id, pars.ws_number) || ', ' ||&#13;&#10;          wcs_utl.get_answ(bg.bid_id, 'CL_16', pars.ws_id, pars.ws_number)&#13;&#10;         else&#13;&#10;          decode(wcs_utl.get_answ(bg.bid_id,&#13;&#10;                                  'CL_18',&#13;&#10;                                  pars.ws_id,&#13;&#10;                                  pars.ws_number),&#13;&#10;                 26,&#13;&#10;                 null,&#13;&#10;                 27,&#13;&#10;                 null,&#13;&#10;                 'обл. ' ||&#13;&#10;                 wcs_utl.get_answ_refer_text(bg.bid_id,&#13;&#10;                                             'CL_18',&#13;&#10;                                             pars.ws_id,&#13;&#10;                                             pars.ws_number) || ', ') ||&#13;&#10;          decode(wcs_utl.get_answ(bg.bid_id,&#13;&#10;                                  'CL_19',&#13;&#10;                                  pars.ws_id,&#13;&#10;                                  pars.ws_number),&#13;&#10;                 null,&#13;&#10;                 null,&#13;&#10;                 'р. ' || wcs_utl.get_answ(bg.bid_id,&#13;&#10;                                           'CL_19',&#13;&#10;                                           pars.ws_id,&#13;&#10;                                           pars.ws_number) || ', ') ||&#13;&#10;          wcs_utl.get_answ(bg.bid_id, 'CL_20', pars.ws_id, pars.ws_number) || ', ' ||&#13;&#10;          wcs_utl.get_answ(bg.bid_id, 'CL_21', pars.ws_id, pars.ws_number)&#13;&#10;       end as adr,&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CODE_002', pars.ws_id, pars.ws_number) as inn,&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_1') || ' ' ||&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_2') || ' ' ||&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_3') as cl_fio&#13;&#10;  from v_wcs_bid_garantees bg, pars&#13;&#10; where bg.bid_id = :p_bid_id&#13;&#10;   and bg.garantee_id = 'MORTGAGE'&#13;&#10;   and bg.garantee_num = :p_ws_number">
        <Column Name="AGR_NUMBER" DataType="System.String"/>
        <Column Name="AGR_DATE" DataType="System.DateTime"/>
        <Column Name="AGR_DATE_PR" DataType="System.String"/>
        <Column Name="FIO" DataType="System.String"/>
        <Column Name="PASSP_SER" DataType="System.String"/>
        <Column Name="PASSP_NUM" DataType="System.String"/>
        <Column Name="PASSP_ORGAN" DataType="System.String"/>
        <Column Name="PASSP_DATE" DataType="System.DateTime"/>
        <Column Name="ADR" DataType="System.String"/>
        <Column Name="INN" DataType="System.String"/>
        <Column Name="CL_FIO" DataType="System.String"/>
        <Column Name="FIO_O" DataType="System.String"/>
        <CommandParameter Name="p_bid_id" DataType="107" Expression="[p_bid_id]" DefaultValue="-1"/>
        <CommandParameter Name="p_ws_number" DataType="107" Expression="[p_ws_number]" DefaultValue="0"/>
