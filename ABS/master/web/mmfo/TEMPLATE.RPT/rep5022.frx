      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="with dates as&#13;&#10; (select holiday as date_f, dat_next_u (holiday, 0) as date_d&#13;&#10;    from holiday&#13;&#10;   where kv = 980&#13;&#10;  union all&#13;&#10;  select fdat, fdat from fdat)  &#13;&#10;select m.name, &#13;&#10;       round(nvl(mfo_ost.s2600_2604, 0))/100 as AVG_2600_4, &#13;&#10;       round(nvl(mfo_ost.s2600_2604_v, 0))/100 as AVG_2600_4_V,&#13;&#10;       round(nvl(mfo_ost.suma2603, 0))/100 as AVG_2603,&#13;&#10;       round(nvl(mfo_ost.suma2610, 0))/100 as AVG_2610,&#13;&#10;       round(nvl(mfo_ost.suma2605, 0))/100 as AVG_2605 &#13;&#10;from&#13;&#10;clim_mfo m&#13;&#10;left join&#13;&#10;(SELECT SUM(case when cd.nbs in ('2600', '2604') then cd.ostq-cd.obkrq+cd.obdbq else 0 end) / count (distinct (d.date_f)) as s2600_2604,&#13;&#10;        SUM(case when cd.nbs in ('2600', '2604') and kv&lt;&gt;980 then cd.ostq-cd.obkrq+cd.obdbq else 0 end) / count (distinct (d.date_f)) as s2600_2604_v, &#13;&#10;        SUM(case when cd.nbs = '2603' then cd.ostq-cd.obkrq+cd.obdbq else 0 end) / count (distinct (d.date_f)) as suma2603,&#13;&#10;        SUM(case when cd.nbs = '2610' then cd.ostq-cd.obkrq+cd.obdbq else 0 end) / count (distinct (d.date_f)) as suma2610,  &#13;&#10;        SUM(case when cd.nbs = '2605' then cd.ostq-cd.obkrq+cd.obdbq else 0 end) / count (distinct (d.date_f)) as suma2605,               &#13;&#10;        CD.kf as kf&#13;&#10; FROM ob_corp_data_acc cd&#13;&#10; join dates d on d.date_d=cd.fdat &#13;&#10; join v_ob_corp_rep_nbs q on q.nbs = cd.nbs and Q.CORP_ID = cd.corp_id &#13;&#10; WHERE cd.is_last = 1 &#13;&#10;       AND q.rep_id = 5022 &#13;&#10;       AND CD.CORP_ID = 17&#13;&#10;       AND cd.nbs in ('2600', '2604', '2603', '2610', '2605') &#13;&#10;       AND d.date_f between :sFdat1 AND :sFdat2&#13;&#10;       AND cd.fdat between :sFdat1 and :sFdat2+10&#13;&#10; group by cd.kf) mfo_ost on  mfo_ost.kf = m.mfo             &#13;&#10;order by m.ord">
