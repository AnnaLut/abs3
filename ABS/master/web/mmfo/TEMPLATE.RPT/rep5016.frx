      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="SELECT t1.name, t1.ord, t2.kf ourmfo, nvl(t2.sum_post,0)sum_post, nvl(t2.sum_gu_pf,0)sum_gu_pf,t1.kod_s&#13;&#10;    FROM clim_mfo t1&#13;&#10;         LEFT JOIN&#13;&#10;         (  SELECT a.kf, SUM (case when substr(d.nlsa,1,4) = '2560' AND substr(d.nlsb,1,4) = '2604' then d.sq else 0 end) / 100 sum_post,&#13;&#10;                          SUM (case when okpob in (select corporation_code from ob_corporation where parent_id=1)&#13;&#10;                                      and nlsb in (select nls from ob_corp_data_acc q where a.kf=q.kf and q.fdat = a.fdat and kod_analyt=40 and corp_id = 1)            &#13;&#10;                                                   then d.sq else 0 end) / 100 sum_gu_pf&#13;&#10;              FROM ob_corp_data_acc a&#13;&#10;              join ob_corp_data_doc d on A.ACC = d.acc and A.SESS_ID = d.sess_id and a.kf = d.kf&#13;&#10;              WHERE a.corp_id = 1&#13;&#10;              and a.is_last = 1&#13;&#10;              AND a.fdat = :sFdat1&#13;&#10;          GROUP BY a.kf) t2&#13;&#10;            ON t1.mfo = t2.kf&#13;&#10;ORDER BY t1.ord  ">
        <Column Name="NAME" DataType="System.String"/>
        <Column Name="ORD" DataType="System.Int16"/>
        <Column Name="OURMFO" DataType="System.String"/>
        <Column Name="SUM_POST" DataType="System.Decimal"/>
        <Column Name="SUM_GU_PF" DataType="System.Decimal"/>
        <Column Name="KOD_S" DataType="System.String"/>
        <CommandParameter Name="sFdat1" DataType="106" Expression="[sFdat1]" DefaultValue="01.01.1900"/>
      </TableDataSource>
    </OracleDataConnection>
    <Parameter Name="sFdat1" DataType="System.DateTime"/>
    <Total Name="Total_Post" Expression="[Table.SUM_POST]" Evaluator="Data1" PrintOn="ReportSummary1"/>
    <Total Name="Total_GU_PF" Expression="[Table.SUM_GU_PF]" Evaluator="Data1" PrintOn="ReportSummary1"/>
  </Dictionary>
