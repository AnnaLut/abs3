          <TableCell Name="Cell1699" Border.Lines="Left, Right, Top" Border.Width="0.25" Text=" [ACC_BPK.ACC_BPK_PGROUP]" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 7pt, style=Bold" ColSpan="6"/>
          <TableCell Name="Cell1700" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 7pt, style=Bold"/>
          <TableCell Name="Cell1701" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 7pt, style=Bold"/>
