        <TableRow Name="Row110" MinHeight="11.34" Height="18" AutoSize="true">
          <TableCell Name="Cell455" Border.Lines="All" Border.Width="0.5" Text="Блок 4: Заяви та запевнення Клієнта" HorzAlign="Justify" Font="Times New Roman, 7pt, style=Bold" HtmlTags="true" ColSpan="6"/>
          <TableCell Name="Cell402" HorzAlign="Justify"/>
