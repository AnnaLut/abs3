    private void Data1_BeforePrint(object sender, EventArgs e)
    {    
      if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.KVED&quot;)) == &quot;1&quot;)
      {Text250.Text = &quot;1. Сільське господарство, лісове господарство та рибне господарство:  секція A (розділи 01 – 03)&quot;;}
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.KVED&quot;)) == &quot;2&quot;)
      {Text250.Text =  &quot;2. Виробництво харчових продуктів, напоїв та тютюнових виробів: секція С (розділи 10 – 12)&quot;;} 
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.KVED&quot;)) == &quot;3&quot;)
      {Text250.Text =  &quot;3. Переробна промисловість: секція С (розділи 13 – 18, 31, 32)&quot;;} 
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.KVED&quot;)) == &quot;4&quot;)
      {Text250.Text =  &quot;4. Добувна промисловість і розроблення кар’єрів: секція  В (розділи 05 – 09); переробна промисловість: секція С (розділи 19 – 30,  33); постачання електроенергії, газу: секція D (розділ 35); водопостачання, каналізація: секція  E (розділи 36 –  39)&quot;;} 
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.KVED&quot;)) == &quot;5&quot;)
      {Text250.Text =  &quot;5. Будівництво: секція F (розділи 41 – 43)&quot;;} 
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.KVED&quot;)) == &quot;6&quot;)
      {Text250.Text =  &quot;6. Оптова та роздрібна торгівля, ремонт автотранспортних засобів:  секція G (розділи 45 – 47); тимчасове розміщування й організація харчування: секція І (розділи 55, 56)&quot;;} 
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.KVED&quot;)) == &quot;7&quot;)
      {Text250.Text =  &quot;7. Транспорт, складське господарство, поштова та кур’єрська діяльність: секція Н (розділи 49 – 53); електрозв’язок: секція J (розділ 61) &quot;;} 
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.KVED&quot;)) == &quot;8&quot;)
      {Text250.Text =  &quot;8. Фінансова та страхова діяльність (крім банків): секція К (розділи 64 – 66)&quot;;} 
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.KVED&quot;)) == &quot;9&quot;)
      {Text250.Text =  &quot;9. Інформація та телекомунікації: секція J (розділи 58 – 60, 62, 63);  інші операції та послуги: секції L – U (розділи 68 – 99)&quot;;} 
      
      
     /* if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.OBS_M&quot;)) == &quot;1&quot;)
      {Text259.Text = &quot;I категорія&quot;;}
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.OBS_M&quot;)) == &quot;2&quot;)
      {Text259.Text = &quot;II категорія&quot;;}
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.OBS_M&quot;)) == &quot;3&quot;)
      {Text259.Text = &quot;III категорія&quot;;}
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.OBS_M&quot;)) == &quot;4&quot;)
      {Text259.Text = &quot;IV категорія&quot;;}
      else if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.OBS_M&quot;)) == &quot;5&quot;)
      {Text259.Text = &quot;V категорія&quot;;}
      */
      
      // Юридична адреса
      if ( String.IsNullOrEmpty ((String)Report.GetColumnValue(&quot;client_dani.ADR_Y&quot;)) )
      {Text39.Text=((String)Report.GetColumnValue(&quot;client_dani.ADR&quot;));}
      else
      {Text39.Text=((String)Report.GetColumnValue(&quot;client_dani.ADR_Y&quot;));}
      
      // Фактична адреса
      if ( String.IsNullOrEmpty ((String)Report.GetColumnValue(&quot;client_dani.ADR_F&quot;)) )
      {Text248.Text=((String)Report.GetColumnValue(&quot;client_dani.ADR&quot;));}
      else
      {Text248.Text=((String)Report.GetColumnValue(&quot;client_dani.ADR_F&quot;));}
     
        
      
      
      
      if  ( Convert.ToDecimal((Decimal)Report.GetColumnValue(&quot;client_dani.KOL&quot;)) &gt; Convert.ToDecimal(1)&amp;&amp; ((String)Report.GetColumnValue(&quot;client_dani.VIS&quot;)) == &quot;Поточний&quot;  )
      { Text41.Text = &quot;Так&quot;;}
      else if   ( Convert.ToDecimal((Decimal)Report.GetColumnValue(&quot;client_dani.KOL&quot;)) &gt; Convert.ToDecimal(1)&amp;&amp; ((String)Report.GetColumnValue(&quot;client_dani.VIS&quot;)) != &quot;Поточний&quot;&amp;&amp; Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.SOS&quot;))==&quot;0&quot; )
      { Text41.Text = &quot;Так&quot;;}
      else if   ( Convert.ToDecimal((Decimal)Report.GetColumnValue(&quot;client_dani.KOL&quot;)) &gt;= Convert.ToDecimal(1)&amp;&amp; ((String)Report.GetColumnValue(&quot;client_dani.VIS&quot;)) != &quot;Поточний&quot;&amp;&amp; Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.SOS&quot;))==&quot;-1&quot; )
      { Text41.Text = &quot;Так&quot;;}
      else 
      { Text41.Text = &quot;Ні&quot;;} 
      
   /*   Наявність інших зобовязань в ощадбанку
      if (  ((Decimal)Report.GetColumnValue(&quot;client_dani.ZB0&quot;)) &gt;  ((Decimal)Report.GetColumnValue(&quot;client_dani.SUM_E&quot;)))
      { Text41.Text = &quot;Так&quot;;}
      else 
      { Text41.Text = &quot;Ні&quot;;}
   */   
      
       
      if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.KOD_INV&quot;)) == &quot;1&quot;)
      {Text297.Visible = false;
        Text298.Visible = false;}
       
        
                                  
      if (Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.KOD_VIDD&quot;)) != &quot;3&quot;)
      {  
             if ( ((String)Report.GetColumnValue(&quot;client_dani.VAL&quot;))==&quot;980&quot;) {Text261.Text = &quot;&quot;;Text262.Text= &quot;&quot;;  Text260.Text=&quot;UAH&quot;;} 
        else if ( ((String)Report.GetColumnValue(&quot;client_dani.VAL&quot;))==&quot;840&quot;) {Text47.Text = &quot;&quot;; Text262.Text= &quot;&quot;;  Text260.Text=&quot;USD&quot;;}
        else if ( ((String)Report.GetColumnValue(&quot;client_dani.VAL&quot;))==&quot;978&quot;) {Text47.Text = &quot;&quot;; Text261.Text = &quot;&quot;; Text260.Text=&quot;EUR&quot;;}       
      }
      else
      {
             if ( ((String)Report.GetColumnValue(&quot;client_dani.VAL&quot;))==&quot;980&quot;) { Text260.Text=&quot;UAH&quot;;} 
        else if ( ((String)Report.GetColumnValue(&quot;client_dani.VAL&quot;))==&quot;840&quot;) { Text260.Text=&quot;USD&quot;;}
        else if ( ((String)Report.GetColumnValue(&quot;client_dani.VAL&quot;))==&quot;978&quot;) { Text260.Text=&quot;EUR&quot;;} 
      }
          
      
      if (  Convert.ToString((Decimal)Report.GetColumnValue(&quot;client_dani.VIS1&quot;)) != &quot;1&quot;)
          {
                    Data5.Visible = false;
          }
      
            
      }

   

