    private void skip_band(object sender, EventArgs e)
    {
      if (((Decimal)Report.GetColumnValue(&quot;fin_110.RES&quot;)) == 1 )
      {
        DataHeader1.Visible = false;
        Data2.Visible = false;
        DataHeader3.Visible = false;
        Data3.Visible = false;
        DataFooter1.Visible = false;
      }
      else
      {
        DataHeader1.Visible = true;
        Data2.Visible = true;
        DataHeader3.Visible = true;
        Data3.Visible = true;
        DataFooter1.Visible = true;
      }
    }
    
    public decimal GetDyn(decimal Sn1, decimal Sn2)
    {
      if (Sn2!=0)
      {
        return Round((Sn1-Sn2)/Abs(Sn2),4);
      }
      else 
      {
      return 0;
      }
    }
    
