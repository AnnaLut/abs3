 = 'RK2') &#13;&#10;  union all&#13;&#10;  SELECT '3'||idf||lpad(ord,4,'0')   ord, to_char(idf),kod, name, fin_obu.ZN_P_ND_REPL (kod,  idf , DAT_ ,  ND_ ,  RNK_)    as repl, &#13;&#10;          null repl2&#13;&#10;    FROM FIN_QUESTION f, (select :rnk as rnk_,:nd as nd_ ,  to_date(:dat_f,'dd.mm.yyyy') as dat_ from dual) d&#13;&#10;   WHERE idf IN (54)   &#13;&#10;  union all&#13;&#10; SELECT '3'||'54'||lpad('21',5,'0')   ord, to_char(idf),kod, name, to_char(fin_nbu.ZN_P_ND_HIST(kod,  idf , DAT_ ,  ND_ ,  RNK_) )      as repl, &#13;&#10;          null repl2&#13;&#10;    FROM FIN_kod f, (select :rnk as rnk_,:nd as nd_ ,  to_date(:dat_f,'dd.mm.yyyy') as dat_ from dual) d&#13;&#10;   WHERE idf IN (51)     and kod in ('CRES','PRES') &#13;&#10;  union all&#13;&#10; SELECT '3'||'55'||lpad(decode(kod,'VD0','0','1'),4,'0')   ord, to_char('55'),kod, name,  case when fin_nbu.ZN_P_ND_HIST (kod,  idf , DAT_ ,  ND_ ,  RNK_) = 1 then 'Так' else null end     as repl, &#13;&#10;          case when kod = 'VD0' and  to_char(fin_nbu.ZN_P_ND_HIST(kod,  idf , DAT_ ,  ND_ ,  RNK_) ) = 1 then '№'||to_char(fin_nbu.ZN_P_ND_HIST('VDN1',  56 , DAT_ ,  ND_ ,  RNK_) ) ||' від '||to_char(FIN_NBU.ZN_P_ND_DATE_HIST('VDD1', 56, dat_, nd_,rnk_),'DD-MM-YYYY') &#13;&#10;               else null&#13;&#10;          end  repl2&#13;&#10;    FROM FIN_QUESTION f, (select :rnk as rnk_,:nd as nd_ ,  to_date(:dat_f,'dd.mm.yyyy') as dat_ from dual) d&#13;&#10;   WHERE idf IN (56)     and kod in ('VD0')&#13;&#10;  union all&#13;&#10;  SELECT '3'||idf||lpad(ord,4,'0')   ord, to_char(idf),kod, name,  case  to_char(fin_nbu.ZN_P_ND_HIST (kod,  idf , DAT_ ,  ND_ ,  RNK_)) when  '1' then 'Так'  when '0' then 'Ні' else 'X' end     as repl, &#13;&#10;          case when kod = 'ZD3' and  to_char(fin_nbu.ZN_P_ND_HIST(kod,  idf , DAT_ ,  ND_ ,  RNK_) ) = 1 then '№'||to_char(fin_nbu.ZN_P_ND_HIST('ZDN1',  57 , DAT_ ,  ND_ ,  RNK_) ) ||' від '||to_char(FIN_NBU.ZN_P_ND_DATE_HIST('ZDD1', 57, dat_, nd_,rnk_),'DD-MM-YYYY')&#13;&#10;               else null&#13;&#10;          end repl2&#13;&#10;    FROM FIN_QUESTION f, (select :rnk as rnk_,:nd as nd_ ,  to_date(:dat_f,'dd.mm.yyyy') as dat_ from dual) d&#13;&#10;   WHERE ( idf = 56 and kod = 'RK3')    &#13;&#10;    union all&#13;&#10;  SELECT '4'||idf||lpad(ord,4,'0')     ord, to_char(idf),kod, name, case to_char(fin_nbu.ZN_P_ND_HIST (kod,  idf , DAT_ ,  ND_ ,  RNK_)) when '1' then 'Так' when '0' then 'Ні' else 'X' end  as repl,&#13;&#10;          case when kod = 'ZD3' and to_char(fin_nbu.ZN_P_ND_HIST(kod,  idf , DAT_ ,  ND_ ,  RNK_) ) = 1 then '№'||to_char(fin_nbu.ZN_P_ND_HIST('ZDN1',  57 , DAT_ ,  ND_ ,  RNK_) ) ||' від '||to_char(FIN_NBU.ZN_P_ND_DATE_HIST('ZDD1', 57, dat_, nd_,rnk_),'DD-MM-YYYY')&#13;&#10;               else null&#13;&#10;          end repl2&#13;&#10;    FROM FIN_QUESTION f, (select :rnk as rnk_,:nd as nd_ ,  to_date(:dat_f,'dd.mm.yyyy') as dat_ from dual) d&#13;&#10;   WHERE idf IN (55)  and kod != 'ZD5'       &#13;&#10;ORDER BY 1&#13;&#10;">
