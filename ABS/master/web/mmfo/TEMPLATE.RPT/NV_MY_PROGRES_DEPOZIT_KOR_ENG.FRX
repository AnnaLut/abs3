      <TableDataSource Name="Table5" Alias="DPT" DataType="System.Int32" Enabled="true" SelectCommand="SELECT NVL(d.nd, TO_CHAR(d.deposit_id)) AS id,&#13;&#10;       f_dat_lit(d.datz) AS dealdate,&#13;&#10;       f_dat_lit(d.datz, 'ENG') AS dealdate_en,&#13;&#10;       f_dat_lit(d.dat_end) AS end_date,&#13;&#10;       f_dat_lit(d.dat_end, 'ENG') AS end_date_en,&#13;&#10;       a.nls AS nlsd,&#13;&#10;       UPPER(bt.type_name) AS type_name,&#13;&#10;       f_translate_kmu(UPPER(bt.type_name)) AS type_name_en,&#13;&#10;       d.LIMIT / 100 AS summ,&#13;&#10;       f_sumpr(d.LIMIT, d.kv, 'F') AS sum_w,&#13;&#10;       replace(sumpr_en.toword_currency(d.LIMIT / 100, d.kv),&#13;&#10;               'hryvnia',&#13;&#10;               'hryvnias') AS sum_w_en,&#13;&#10;       DECODE(d.kv, 980, 'грн.', t.lcv) AS kv,&#13;&#10;       DECODE(d.kv, 980, 'UAH', t.lcv) AS kv_en,&#13;&#10;       f_doc_dpt(d.deposit_id, 3, 1) AS term_calc,&#13;&#10;       DECODE(NVL(MONTHS_BETWEEN(TO_DATE(d.dat_end, 'dd.mm.yyyy'),&#13;&#10;                                 TO_DATE(d.dat_begin, 'dd.mm.yyyy')),&#13;&#10;                  0),&#13;&#10;              0,&#13;&#10;              '',&#13;&#10;              TO_CHAR(TRUNC(MONTHS_BETWEEN(TO_DATE(d.dat_end, 'dd.mm.yyyy'),&#13;&#10;                                           TO_DATE(d.dat_begin, 'dd.mm.yyyy')),&#13;&#10;                            0)) || ' months') ||&#13;&#10;       DECODE(NVL(GREATEST(TO_DATE(d.dat_end, 'dd.mm.yyyy') -&#13;&#10;                           dpt.f_duration(TO_DATE(d.dat_begin, 'dd.mm.yyyy'),&#13;&#10;                                          TRUNC(MONTHS_BETWEEN(TO_DATE(d.dat_end,&#13;&#10;                                                                       'dd.mm.yyyy'),&#13;&#10;                                                               TO_DATE(d.dat_begin,&#13;&#10;                                                                       'dd.mm.yyyy'))),&#13;&#10;                                          0),&#13;&#10;                           0),&#13;&#10;                  0),&#13;&#10;              0,&#13;&#10;              '',&#13;&#10;              TO_CHAR(GREATEST(TO_DATE(d.dat_end, 'dd.mm.yyyy') -&#13;&#10;                               dpt.f_duration(TO_DATE(d.dat_begin,&#13;&#10;                                                      'dd.mm.yyyy'),&#13;&#10;                                              TRUNC(MONTHS_BETWEEN(TO_DATE(d.dat_end,&#13;&#10;                                                                           'dd.mm.yyyy'),&#13;&#10;                                                                   TO_DATE(d.dat_begin,&#13;&#10;                                                                           'dd.mm.yyyy'))),&#13;&#10;                                              0),&#13;&#10;                               0) || ' days')) AS term_calc_EN,&#13;&#10;       f_doc_dpt(d.deposit_id, 3, 2) AS term_calc_w,&#13;&#10;       DECODE(NVL(MONTHS_BETWEEN(TO_DATE(d.dat_end, 'dd.mm.yyyy'),&#13;&#10;                                 TO_DATE(d.dat_begin, 'dd.mm.yyyy')),&#13;&#10;                  0),&#13;&#10;              0,&#13;&#10;              '',&#13;&#10;              sumpr_en.ToWord(TRUNC(MONTHS_BETWEEN(TO_DATE(d.dat_end,&#13;&#10;                                                           'dd.mm.yyyy'),&#13;&#10;                                                   TO_DATE(d.dat_begin,&#13;&#10;                                                           'dd.mm.yyyy')))) ||&#13;&#10;              ' months') ||&#13;&#10;       DECODE(NVL(GREATEST(TO_DATE(d.dat_end, 'dd.mm.yyyy') -&#13;&#10;                           dpt.f_duration(TO_DATE(d.dat_begin, 'dd.mm.yyyy'),&#13;&#10;                                          TRUNC(MONTHS_BETWEEN(TO_DATE(d.dat_end,&#13;&#10;                                                                       'dd.mm.yyyy'),&#13;&#10;                                                               TO_DATE(d.dat_begin,&#13;&#10;                                                                       'dd.mm.yyyy'))),&#13;&#10;                                          0),&#13;&#10;                           0),&#13;&#10;                  0),&#13;&#10;              0,&#13;&#10;              '',&#13;&#10;              sumpr_en.ToWord(GREATEST(TO_DATE(d.dat_end, 'dd.mm.yyyy') -&#13;&#10;                                       dpt.f_duration(TO_DATE(d.dat_begin,&#13;&#10;                                                              'dd.mm.yyyy'),&#13;&#10;                                                      TRUNC(MONTHS_BETWEEN(TO_DATE(d.dat_end,&#13;&#10;                                                                                   'dd.mm.yyyy'),&#13;&#10;                                                                           TO_DATE(d.dat_begin,&#13;&#10;                                                                                   'dd.mm.yyyy'))),&#13;&#10;                                                      0),&#13;&#10;                                       0)) || ' days') AS term_calc_w_en,&#13;&#10;       DECODE(bv.duration,&#13;&#10;              '0',&#13;&#10;              bv.duration_days || ' днів',&#13;&#10;              bv.duration || ' міс.') AS term_calc_like,&#13;&#10;       DECODE(bv.duration,&#13;&#10;              '0',&#13;&#10;              bv.duration_days || ' days',&#13;&#10;              bv.duration || ' months') AS term_calc_like_en,&#13;&#10;       DPT_WEB.GET_DPTRATE(d.acc, d.kv, d.LIMIT, bankdate) AS rate,&#13;&#10;       f_sumpr(DPT_WEB.GET_DPTRATE(d.acc, d.kv, d.LIMIT, bankdate),&#13;&#10;               '',&#13;&#10;               'M',&#13;&#10;               3) AS rate_w,&#13;&#10;       case&#13;&#10;           when mod(DPT_WEB.GET_DPTRATE(d.acc, d.kv, d.LIMIT, bankdate), 1) &gt; 0 then&#13;&#10;            lower(to_char(to_timestamp(lpad(trunc(DPT_WEB.GET_DPTRATE(d.acc,&#13;&#10;                                                                      d.kv,&#13;&#10;                                                                      d.LIMIT,&#13;&#10;                                                                      bankdate)),&#13;&#10;                                            9,&#13;&#10;                                            '0'),&#13;&#10;                                       'FF9'),&#13;&#10;                          'Ffsp')) || ' point ' ||&#13;&#10;            lower(to_char(to_timestamp(lpad((DPT_WEB.GET_DPTRATE(d.acc,&#13;&#10;                                                                 d.kv,&#13;&#10;                                                                 d.LIMIT,&#13;&#10;                                                                 bankdate) -&#13;&#10;                                            trunc(DPT_WEB.GET_DPTRATE(d.acc,&#13;&#10;                                                                       d.kv,&#13;&#10;                                                                       d.LIMIT,&#13;&#10;                                                                       bankdate))) *&#13;&#10;                                            power(10,&#13;&#10;                                                  length(substr(to_char(DPT_WEB.GET_DPTRATE(d.acc,&#13;&#10;                                                                                            d.kv,&#13;&#10;                                                                                            d.LIMIT,&#13;&#10;                                                                                            bankdate)),&#13;&#10;                                                                instr(to_char(DPT_WEB.GET_DPTRATE(d.acc,&#13;&#10;                                                                                                  d.kv,&#13;&#10;                                                                                                  d.LIMIT,&#13;&#10;                                                                                                  bankdate)),&#13;&#10;                                                                      '.') + 1))),&#13;&#10;                                            9,&#13;&#10;                                            '0'),&#13;&#10;                                       'FF9'),&#13;&#10;                          'Ffsp'))&#13;&#10;           else&#13;&#10;            lower(to_char(to_timestamp(lpad(trunc(DPT_WEB.GET_DPTRATE(d.acc,&#13;&#10;                                                                      d.kv,&#13;&#10;                                                                      d.LIMIT,&#13;&#10;                                                                      bankdate)),&#13;&#10;                                            9,&#13;&#10;                                            '0'),&#13;&#10;                                       'FF9'),&#13;&#10;                          'Ffsp'))&#13;&#10;       end AS rate_w_en,&#13;&#10;       DECODE(d.freq,&#13;&#10;              5,&#13;&#10;              'щомісячно',&#13;&#10;              7,&#13;&#10;              'щоквартально',&#13;&#10;              360,&#13;&#10;              'щорічно',&#13;&#10;              400,&#13;&#10;              'разом з поверненням Депозиту',&#13;&#10;              (SELECT name FROM freq WHERE freq = d.freq)) AS freq_w,&#13;&#10;       NVL(TRIM(d.mfo_p), '_________________') AS paym_mfo,&#13;&#10;       NVL(TRIM(d.mfo_d), '_________________') AS mfo_d,&#13;&#10;       NVL(TRIM(b.nb), '____') AS paym_nb,&#13;&#10;       NVL(TRIM(f_translate_kmu(b.nb)), '_________________') AS paym_nb_en,&#13;&#10;       NVL(TRIM(BRANCH_ATTRIBUTE_UTL.get_value(d.BRANCH, 'NAME_PRINT')),&#13;&#10;           '____') AS nb_d,&#13;&#10;       NVL(TRIM(BRANCH_ATTRIBUTE_UTL.get_value(d.BRANCH, 'BRANCH_NAME_ENG')),&#13;&#10;           '____') AS nb_d_eng,&#13;&#10;       NVL(TRIM(d.nls_p), '_________________') AS paym_nls,&#13;&#10;       NVL(TRIM(d.nls_d), '_________________') AS nls_d&#13;&#10;  FROM dpt_deposit d,&#13;&#10;       tabval      t,&#13;&#10;       accounts    a,&#13;&#10;       banks       b,&#13;&#10;       dpt_vidd    bv,&#13;&#10;       dpt_types   bt,&#13;&#10;       banks       bd&#13;&#10; WHERE d.acc = a.acc&#13;&#10;   AND d.vidd = BV.VIDD&#13;&#10;   AND bv.type_id = bt.type_id&#13;&#10;   AND d.kv = t.kv&#13;&#10;   AND d.mfo_p = b.mfo(+)&#13;&#10;   AND d.mfo_d = bd.mfo(+)&#13;&#10;   AND d.deposit_id = :p_agr_id">
