      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="SELECT t1.kod_s,t1.name, &#13;&#10;       t1.ord, t2.kf ourmfo, &#13;&#10;       nvl(t2.s_debet,0) s_debet, &#13;&#10;       nvl(t2.s_debet_pens,0) s_debet_pens, &#13;&#10;       nvl(t2.s_credit,0) s_credit, &#13;&#10;       nvl(t2.s_credit_pfu,0) s_credit_pfu, &#13;&#10;       nvl(t2.s_credit_return,0) s_credit_return&#13;&#10;    FROM clim_mfo t1&#13;&#10;         LEFT JOIN&#13;&#10;         (  SELECT a.kf,&#13;&#10;                   SUM (case when d.dk=0 AND SUBSTR (d.nlsa, 1, 4) = '2604' then d.sq  else 0 end) / 100 s_debet,&#13;&#10;                   SUM (case when d.dk=0 AND SUBSTR (d.nlsb, 1, 3) = '100'  &#13;&#10;                                         and SUBSTR (d.nlsa, 1, 4) = '2604' then d.sq else 0 end) / 100 s_debet_pens,&#13;&#10;                   SUM (case when d.dk=1 AND SUBSTR (d.nlsb, 1, 4) = '2604' then d.sq else 0 end) / 100 s_credit,&#13;&#10;                   SUM (case when d.dk=1 AND SUBSTR (d.nlsa, 1, 4) = '2560' &#13;&#10;                                         and SUBSTR (d.nlsb, 1, 4) = '2604' then d.sq else 0 end) / 100 s_credit_pfu,&#13;&#10;                   SUM (case when d.dk=1 AND SUBSTR (d.nlsa, 1, 3) = '100'  &#13;&#10;                                         and SUBSTR (d.nlsb, 1, 4) = '2604' then d.sq else 0 end) / 100 s_credit_return&#13;&#10;              FROM ob_corp_data_acc a&#13;&#10;              join ob_corp_data_doc d on A.ACC = d.acc and A.SESS_ID = d.sess_id and a.kf = d.kf&#13;&#10;              WHERE a.corp_id = 2&#13;&#10;                   AND a.kod_analyt in ('1','01')&#13;&#10;                   AND a.fdat = :sFdat1&#13;&#10;          GROUP BY a.kf) t2&#13;&#10;            ON t1.mfo = t2.kf&#13;&#10;            ORDER BY t1.ord">
        <Column Name="NAME" DataType="System.String"/>
        <Column Name="ORD" DataType="System.Int16"/>
        <Column Name="OURMFO" DataType="System.String"/>
        <Column Name="S_DEBET" DataType="System.Decimal"/>
        <Column Name="S_DEBET_PENS" DataType="System.Decimal"/>
        <Column Name="S_CREDIT" DataType="System.Decimal"/>
        <Column Name="S_CREDIT_PFU" DataType="System.Decimal"/>
        <Column Name="S_CREDIT_RETURN" DataType="System.Decimal"/>
        <Column Name="KOD_S" DataType="System.String"/>
        <CommandParameter Name="sFdat1" DataType="106" Expression="[sFdat1]" DefaultValue="01.01.1900"/>
      </TableDataSource>
    </OracleDataConnection>
    <Parameter Name="sFdat1" DataType="System.DateTime"/>
