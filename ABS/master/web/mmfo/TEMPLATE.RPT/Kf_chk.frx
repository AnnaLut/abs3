      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="select t3.name,&#13;&#10;       t2.corporation_name,&#13;&#10;       t1.*,&#13;&#10;       to_char(:sFdat1)repdt &#13;&#10;       from (select case when a.corp_id is null then b.corp_id else a.corp_id end corp_id,&#13;&#10;                    case when a.corp_id is null then b.kf else a.kf end kf,&#13;&#10;                    case when a.corp_id is null then b.okpo else a.okpo end okpo,&#13;&#10;                    case when a.corp_id is null then b.nls else a.nls end nls,&#13;&#10;                    case when a.corp_id is null then b.kv else a.kv end kv,&#13;&#10;                    case when a.corp_id is null then b.kod_analyt &#13;&#10;                         when nvl (a.kod_analyt, 0) &lt;&gt; nvl (b.kod_analyt, 0) then a.kod_analyt&#13;&#10;                         else a.kod_analyt end kod_analyt,&#13;&#10;                    case when a.corp_id is null then b.kod_ustan else a.kod_ustan end kod_ustan,&#13;&#10;                    case when a.corp_id is null then b.fdat else a.fdat end fdat,&#13;&#10;                   (case when a.corp_id is null then  b.ostq-b.obkrq+b.obdbq  when b.corp_id is null then a.ostq-a.obkrq+a.obdbq else b.ostq end)/100 ostq_in,&#13;&#10;                   (case when a.corp_id is null then b.obdbq else a.obdbq end)/100 obdbq,&#13;&#10;                   (case when a.corp_id is null then b.obkrq else a.obkrq end)/100 obkrq,&#13;&#10;                   (case when a.corp_id is null then b.ostq else a.ostq end)/100 ostq,&#13;&#10;                    case when a.corp_id is null then 'Рахунки минулого дня, відсутні в поточному'&#13;&#10;                         when a.ostq-a.obkrq+a.obdbq&lt;&gt;b.ostq then 'Вхідний залишок не співпадає з залишком поточного дня'&#13;&#10;                         when nvl (a.kod_analyt, 0) &lt;&gt; nvl (b.kod_analyt, 0) then 'Змінено код аналітичного обліку'  &#13;&#10;                         else 'Рахунки поточного дня, відсутні в минулому' end notes&#13;&#10;                    from&#13;&#10;                       (select okpo,kod_ustan,kod_analyt,corp_id,fdat,kf,nls,kv,obdb,obdbq,obkr,obkrq,ost,ostq from ob_corp_data_acc cd&#13;&#10;                         where  cd.corp_id=to_number(:Param1) and cd.is_last  = 1 and cd.fdat = :sFdat1) a&#13;&#10;                          full join &#13;&#10;                       (select  okpo,kod_ustan,kod_analyt,corp_id,fdat,kf,nls,kv,obdb,obdbq,obkr,obkrq,ost,ostq from ob_corp_data_acc cd&#13;&#10;                         where cd.corp_id = to_number(:Param1) and cd.is_last  = 1 &#13;&#10;                           and cd.fdat = DAT_PREV_U (:sFdat1, 1)) b&#13;&#10;                            on  a.kf=b.kf and  b.nls=A.NLS and a.kv=b.kv                                   &#13;&#10;              where a.corp_id is null or b.corp_id is null or a.ostq-a.obkrq+a.obdbq&lt;&gt;b.ostq &#13;&#10;              or nvl (a.kod_analyt, 0) &lt;&gt; nvl (b.kod_analyt, 0)&#13;&#10;              ) t1&#13;&#10;left join V_ROOT_CORPORATION t2 on t2.external_id=t1.corp_id&#13;&#10;join clim_mfo t3 on t3.mfo=t1.kf&#13;&#10;order by t3.ord,notes">
