    <grid id="Id2" title="Введені форми кліента за звітні періоди" showpagesizebox="true" pagesize="6" width="100">
            <datasource>
        <sql>
          select okpo, fdat, fm, decode(fin_nbu.LOGK_read(fdat, okpo, 1,1), 0, 'Без помилок', 2, 'З помилками', 1, 'Не введені дані') f1 ,
          decode(fin_nbu.LOGK_read(fdat, okpo, 2,1), 0, 'Без помилок', 2, 'З помилками', 1, 'Не введені дані') f2,
          decode(fin_formaf3.LOGK_read(fdat, okpo, 3,1), 0, 'Без помилок', 2, 'З помилками', 1, 'Не введені дані') f3,
          decode(fin_formaf3.LOGK_read(fdat, okpo, 4,1), 0, 'Без помилок', 2, 'З помилками', 1, 'Не введені дані') f3n,
          (fin_nbu.LOGK_read(fdat, okpo, 1,1)+fin_nbu.LOGK_read(fdat, okpo, 2,1)+sign(fin_formaf3.f_prot_kol( okpo,fdat, 3)+fin_formaf3.f_prot_kol(okpo,fdat, 4))) as col, :VED as ved, fin_nbu.ZN_P('IPB', 6, fdat, okpo) as IPB,
          fin_zp.f_get_fin_nd (:RNK ,  0 ,  '110' , fdat,  sysdate ) kl_110,
          fin_zp.f_get_fin_nd (:RNK ,  0 ,  '080' , fdat,  sysdate ) kl_080,
          fin_zp.f_get_fin_nd (:RNK ,  0 ,  'OBS' , fdat,  sysdate ) kl_OBS,
          :RNK as rnk,
          to_char(fdat,'dd/mm/yyyy') dat,
          fin_nbu.ZN_P('KPB', 9, fdat, okpo, null) as KPB,
          --fin_nbu.GET_KVED(:RNK, fdat,0) as kved,
          fin_nbu.GET_VED (:RNK, fdat) as ved1,
          -- 351
          fin_nbu.GET_KVED(:RNK, fdat,1) as kved351,
          FIN_NBU.ZN_P('GVED', 6, fdat, okpo, null) as ved351,
          FIN_NBU.ZN_P('PIPB', 6, fdat, okpo, null) PIPB351,
          FIN_NBU.ZN_P('CLAS', 6, fdat, okpo, null) cls351
          from fin_fm
          where okpo = :okpo
          order by fdat desc

        </sql>
        <parameters>
          <parameter name="RNK" altname="rnk" datatype="Int64" masterid="Id1" masterkeyfield="RNK" type="MasterGrid"></parameter>
          <parameter name="OKPO" altname="okpo" datatype="Int64" masterid="Id1" masterkeyfield="OKPO" type="MasterGrid"></parameter>
          <parameter name="VED" altname="ved" datatype="String" masterid="Id1" masterkeyfield="VED" type="MasterGrid"></parameter>
        </parameters>
