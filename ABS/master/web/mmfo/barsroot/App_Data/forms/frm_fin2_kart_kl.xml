    <grid id="Id3" title="Кредитні угоди клієнта" pagesize="5" showpagesizebox="true" >
      <datasource>
        <sql>
          select  c.*, :okpo as okpo, :rnk1 as rnk1 ,
                  fin_zp.zn_vncrr(rnk, nd) as vncrr,
                  FIN_NBU.ZN_P_ND('KKDP', 56, sysdate, ND, RNK, null) KKDP,
                  FIN_NBU.ZN_P_ND('CLAS', 56, sysdate, ND, RNK, null) clas,
