      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="select t.contr_id,&#13;&#10;ct.contr_type_name,&#13;&#10;t.num,&#13;&#10;t.kv,&#13;&#10;t.open_date,&#13;&#10;t.rnk,&#13;&#10;t.okpo,&#13;&#10;c.nmkk,&#13;&#10;b.benef_name,&#13;&#10;(select co.name from country co where co.country = b.country_id) as country_name,&#13;&#10;t.branch,&#13;&#10;v.allow_date,&#13;&#10;v.s_vt&#13;&#10;from cim_contracts t &#13;&#10;join customer c on c.rnk=t.rnk&#13;&#10;join cim_contract_types ct on ct.contr_type_id=t.contr_type&#13;&#10;join cim_beneficiaries b on b.benef_id=t.benef_id&#13;&#10;join (select d.* from           (select 0 as d_k, 0 as type_id, d.bound_id, d.contr_id, (d.s+d.comiss) as s_vt, d.s_cv as s_vk, NVL( (SELECT MAX (fdat) FROM opldok WHERE REF = o.REF ), o.vdat ) as allow_date&#13;&#10;                                 from cim_payments_bound d JOIN oper o ON o.REF = d.REF&#13;&#10;                                where d.delete_date is null and&#13;&#10;                                      d.contr_id is not null and contr_id != 0&#13;&#10;                                  and d.branch like sys_context('bars_context', 'user_mfo_mask')&#13;&#10;                                  and decode(d.is_doc, 1, 'Так', 'Ні') = 'Ні'&#13;&#10;                               union all&#13;&#10;                               select 0 as d_k, f.payment_type as type_id, d.bound_id, d.contr_id, (d.s+d.comiss) as s_vt, d.s_cv as s_vk, f.val_date as allow_date&#13;&#10;                                 from cim_fantoms_bound d JOIN cim_fantom_payments f ON f.fantom_id = d.fantom_id&#13;&#10;                                where d.delete_date is null and&#13;&#10;                                       f.payment_type in (1, 4) and d.contr_id is not null and contr_id != 0&#13;&#10;                                  and d.branch like sys_context('bars_context', 'user_mfo_mask')&#13;&#10;                                  and decode(d.is_doc, 1, 'Так', 'Ні') = 'Ні'&#13;&#10;                               union all&#13;&#10;                               select 1 as d_k, 0 as type_id, d.bound_id, d.contr_id, d.s_vt as s_vt, d.s_vk, v.allow_dat as allow_date&#13;&#10;                                 from cim_vmd_bound d join customs_decl v on v.cim_id=d.vmd_id&#13;&#10;                                where d.delete_date is null&#13;&#10;                                  and d.branch like sys_context('bars_context', 'user_mfo_mask')&#13;&#10;                                  and decode(d.is_doc, 1, 'Так', 'Ні') = 'Ні'&#13;&#10;                               union all&#13;&#10;                               select 1 as d_k, v.act_type as type_id, d.bound_id, d.contr_id, d.s_vt as s_vt, d.s_vk, v.allow_date as allow_date&#13;&#10;                                 from cim_act_bound d join cim_acts v on v.act_type in (1, 3, 4) and v.act_id=d.act_id&#13;&#10;                                where d.delete_date is null&#13;&#10;                                  and d.branch like sys_context('bars_context', 'user_mfo_mask') &#13;&#10;                                  and decode(d.is_doc, 1, 'Так', 'Ні') = 'Ні'&#13;&#10;                                  ) d&#13;&#10;                                  where d.allow_date between to_date(:p_fdat1,'dd/mm/yyyy') and to_date(:p_fdat2,'dd/mm/yyyy')&#13;&#10;                                  ) v&#13;&#10;on (t.contr_id = v.contr_id and t.contr_type in (0,3))   &#13;&#10;order by t.contr_id">
