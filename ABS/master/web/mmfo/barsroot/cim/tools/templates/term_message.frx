  <Dictionary>
    <OracleDataConnection Name="barsdev" ConnectionString="rijcmlq9SEZoacNJntaKIKp6mVF199XSGu7xEui53MkZZGtMQsHBmgMZ86UEB9QvrDXTeoFv/vnz1tqv0qdiCfq6/lCoA==">
      <TableDataSource Name="Table" Alias="t" DataType="System.Int32" Enabled="true" SelectCommand="select y.rnk, y.contr_type, y.num, to_char(y.open_date, 'dd.mm.yyyy') as open_date, c.nmk, cim_mgr.get_address(y.rnk) as adr, b.benef_name, b.benef_adr, &#13;&#10;       case when y.doc_type=0 then decode(y.doc_kind, 1, v.cnum_cst || '/' || v.cnum_year || '/' || LPAD (v.cnum_num, 6, '0'), null) &#13;&#10;                              else y.doc_num end as doc_num,  &#13;&#10;       to_char( case when y.doc_type=0 then decode(y.doc_kind, 1, v.allow_dat, p.vdat) &#13;&#10;                                       else y.dat end, 'dd.mm.yyyy') as dat,&#13;&#10;       t.lcv as v, y.z,to_char(y.control_date,'dd.mm.yyyy') as txt_control_date, to_char(last_day(to_date(:p_date_end,'dd/mm/yyyy'))+1,'dd.mm.yyyy') as end_date,&#13;&#10;       u.name, u.phone,                               &#13;&#10;       decode(y.contr_type, 0, 'екс', 'ім') as t1, decode(y.contr_type, 0, '2.3', '3.3') as t6,       &#13;&#10;       decode(y.contr_type, 0, 'Номер та дата МД / Акту', 'Дата платежу') as t7, &#13;&#10;       decode(y.contr_type, 0, 'Сума, що не надійшла', 'Сума заборгованості') as t8,&#13;&#10;       decode(y.contr_type, 0, 'Дата закінчення законодавчо встановлених строків розрахунків', &#13;&#10;                               'Граничний термін поставки продукції') as t9,&#13;&#10;       decode(y.contr_type, 0, 'зарахування виручки за цією операцією на поточний рахунок останнього', &#13;&#10;                               'імпорту товару з увезенням його на територію України') as t2,&#13;&#10;       decode(y.contr_type, 0, 'зарахування виручки', 'надходження товару') as t3,&#13;&#10;       decode(y.contr_type, 0,&#13;&#10;                ' про зарахування виручки за цією операцією на відкритий у цьому банку поточний рахунок резидента', &#13;&#10;                ', якому митний орган надіслав інформацію про цю операцію резидента, про отримання такого реєстру')&#13;&#10;             as t4,&#13;&#10;       decode(y.contr_type, 0, &#13;&#10;                'зазначається вся наявна в реквізитах платежу інформація: номер і дата договору, сума платежу -', &#13;&#10;                'зазначаються номер і дата реєстру, вся наявна в реєстрі інформація про операцію резидента, а також вартість продукції') &#13;&#10;             as t5                         &#13;&#10;  from   &#13;&#10;  ( select x.contr_id, x.contr_type, x.branch, x.rnk, x.benef_id, x.num, x.open_date, round(x.s_vt*(x.s_vk-x.ls)/x.s_vk/100, 2) as z,  &#13;&#10;           cim_mgr.get_control_date(1, x.doc_type, x.bound_id) as control_date, 1 as doc_kind, x.doc_type, x.doc_id, x.doc_num, x.dat, x.kv&#13;&#10;      from   &#13;&#10;      ( with c as ( select contr_id, contr_type, branch, rnk, benef_id, num, open_date&#13;&#10;                    from cim_contracts where branch=sys_context('bars_context','user_branch') and contr_type=0 ),&#13;&#10;             l as (select vmd_id, act_id, sum(s) as ls from cim_link where delete_date is null group by vmd_id, act_id )  &#13;&#10;        select c.contr_id, c.contr_type, c.branch, c.rnk, c.benef_id, c.num, c.open_date, b.bound_id, b.vmd_id as doc_id, 0 as doc_type, b.s_vk, b.s_vt, nvl(l.ls, 0) as ls, &#13;&#10;               null as doc_num, null as dat, null as kv&#13;&#10;        from c&#13;&#10;             join cim_vmd_bound b on b.delete_date is null and b.contr_id=c.contr_id&#13;&#10;             left outer join l on l.vmd_id=b.bound_id              &#13;&#10;        union all&#13;&#10;        select c.contr_id, c.contr_type, c.branch, c.rnk, c.benef_id, c.num, c.open_date, b.bound_id, b.act_id as doc_id, a.act_type as doc_type, b.s_vk, b.s_vt, nvl(l.ls, 0) as ls,&#13;&#10;               a.num as doc_num, a.allow_date as dat, a.kv&#13;&#10;        from c&#13;&#10;             join cim_act_bound b on b.delete_date is null and b.contr_id=c.contr_id&#13;&#10;             left outer join l on l.act_id=b.bound_id &#13;&#10;             join cim_acts a on a.act_id=b.act_id ) x&#13;&#10;      left outer join customs_decl v on v.cim_id=x.doc_id              &#13;&#10;     where x.s_vk&gt;x.ls &#13;&#10;    union all&#13;&#10;    select x.contr_id, x.contr_type, x.branch, x.rnk, x.benef_id, x.num, x.open_date, round( x.s*(x.s_cv-x.ls)/x.s_cv/100,2) as z, &#13;&#10;           cim_mgr.get_control_date(0, x.doc_type, x.bound_id) as control_date, 0 as doc_kind, x.doc_type, x.doc_id, x.doc_num, x.dat, x.kv  &#13;&#10;      from   &#13;&#10;      ( with c as ( select contr_id, contr_type, branch, rnk, benef_id, num, open_date&#13;&#10;                    from cim_contracts where branch=sys_context('bars_context','user_branch') and contr_type=1 ),&#13;&#10;             l as (select payment_id, fantom_id, sum(s) as ls from cim_link where delete_date is null group by payment_id, fantom_id )  &#13;&#10;        select c.contr_id, c.contr_type, c.branch, c.rnk, c.benef_id, c.num, c.open_date, b.bound_id, b.ref as doc_id, 0 as doc_type, b.s_cv, b.s, nvl(l.ls, 0) as ls,&#13;&#10;               null as doc_num, null as dat, null as kv &#13;&#10;        from c&#13;&#10;             join cim_payments_bound b on b.delete_date is null and b.pay_flag=0 and b.contr_id=c.contr_id&#13;&#10;             left outer join l on l.payment_id=b.bound_id&#13;&#10;        union all&#13;&#10;        select c.contr_id, c.contr_type, c.branch, c.rnk, c.benef_id, c.num, c.open_date, b.bound_id, b.fantom_id as doc_id, p.payment_type as doc_type, b.s_cv, b.s, nvl(l.ls, 0) as ls,&#13;&#10;               null as doc_num, p.val_date as dat, p.kv&#13;&#10;        from c&#13;&#10;             join cim_fantoms_bound b on b.delete_date is null and b.pay_flag=0 and b.contr_id=c.contr_id&#13;&#10;             left outer join l on l.fantom_id=b.bound_id &#13;&#10;             join cim_fantom_payments p on p.fantom_id=b.fantom_id ) x        &#13;&#10;     where x.s_cv&gt;x.ls ) y&#13;&#10;  left outer join customs_decl v on v.cim_id=y.doc_id and doc_type=0 and doc_kind=1&#13;&#10;  left outer join oper p on p.ref=y.doc_id and doc_type=0 and doc_kind=0   &#13;&#10;  join customer c on c.rnk=y.rnk&#13;&#10;  join cim_beneficiaries b on b.benef_id=y.benef_id  &#13;&#10;  left outer join cim_journal_num u on u.branch=y.branch  &#13;&#10;  left outer join tabval$global t on t.kv=case when y.doc_type=0 then decode(y.doc_kind, 0, p.kv, v.kv) else y.kv end&#13;&#10; where (:p_date_begin is null or y.control_date &gt;= to_date(:p_date_begin,'dd/mm/yyyy')) and y.control_date &lt;= to_date(:p_date_end,'dd/mm/yyyy')">
