      <TableDataSource Name="Table" Alias="t" DataType="System.Int32" Enabled="true" SelectCommand="select b.bound_id, c.okpo, c.nmkk, c.adr, decode(c.custtype,3,'ДРФО','ЄДРПОУ') as kod_name, k.num, to_char(k.open_date,'dd/mm/yyyy') as open_date&#13;&#10;from cim_act_bound b, cim_contracts k, customer c&#13;&#10;where c.rnk=k.rnk&#13;&#10;and k.contr_id=b.contr_id&#13;&#10;and b.bound_id=to_number(:p_mail_id)&#13;&#10;&#13;&#10;">
