      <TableDataSource Name="Table1" Alias="b" DataType="System.Int32" Enabled="true" SelectCommand="select * from (&#13;&#10;select l.act_id, to_char(o.vdat,'dd/mm/yyyy') as vdat, o.kv, to_char(round(o.s/100,2),'999999990.00') as s, &#13;&#10;to_char(round(l.s*b.s/b.s_cv/100,2),'999999990.00') as bound_s,&#13;&#10;o.vdat as dat&#13;&#10;from cim_link l, cim_payments_bound b, oper o&#13;&#10;where l.delete_date is null and o.ref=b.ref and b.bound_id=l.payment_id &#13;&#10;union all&#13;&#10;select l.act_id, to_char(o.val_date,'dd/mm/yyyy') as vdat, o.kv, to_char(round(o.s/100,2),'999999990.00') as s,&#13;&#10;to_char(round(l.s*b.s/b.s_cv/100,2),'999999990.00') as bound_s,   &#13;&#10; o.val_date as dat&#13;&#10;from cim_link l, cim_fantoms_bound b, cim_fantom_payments o&#13;&#10;where l.delete_date is null and o.fantom_id=b.fantom_id and b.bound_id=l.fantom_id )&#13;&#10;order by dat">
