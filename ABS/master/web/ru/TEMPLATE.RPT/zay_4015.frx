  <Dictionary>
    <OracleDataConnection Name="oBMMFOD" ConnectionString="rijcmlqHytUMhhPg9gG4fal5Q6rRPGZvppyEtPf+9m46lsPv8nFK4fN2mC7+1pBOkn1ZBvqc52xIiMvWxdrK/ytodPRSw==">
      <TableDataSource Name="Table" Alias="QMain" DataType="System.Int32" Enabled="true" SelectCommand="SELECT z.nd num_doc,&#13;&#10;         z.id,&#13;&#10;         z.dk,&#13;&#10;         f_dat_lit (z.fdat) begin_date,&#13;&#10;         z.s2 / 100 amount_in,&#13;&#10;         ROUND (z.s2 / 100 * z.KURS_F, 2) base_amount_in,&#13;&#10;         z.kv2 currency,&#13;&#10;         z.kv_conv currency_conv,&#13;&#10;         tg.name currency_name,&#13;&#10;         (select tv.name from tabval$global tv where tv.kv = z.kv_conv) currency_conv_name,&#13;&#10;         z.KURS_F kurs_diler,&#13;&#10;         z.comm AS details,&#13;&#10;         z.kom commiss,&#13;&#10;         cz.fio,&#13;&#10;         cz.tel,&#13;&#10;         c.nmk name_customer,&#13;&#10;         c.okpo,&#13;&#10;         (SELECT    ca.ZIP&#13;&#10;          || ' '&#13;&#10;          || (SELECT name&#13;&#10;              FROM country&#13;&#10;             WHERE country = CA.COUNTRY)&#13;&#10;          FROM customer_address ca&#13;&#10;          WHERE ca.rnk = c.rnk AND type_id = 1)||', '||c.adr as adr,&#13;&#10;         cz.nls29,&#13;&#10;         (SELECT p.telw&#13;&#10;            FROM person p&#13;&#10;           WHERE p.rnk = c.rnk)&#13;&#10;            phone,&#13;&#10;         (SELECT t.val&#13;&#10;            FROM params /*params$base*/ t&#13;&#10;           WHERE t.par = 'NAME')&#13;&#10;            bank_name,&#13;&#10;         z.kf bank_mfo,&#13;&#10;         (SELECT t.val&#13;&#10;            FROM branch_parameters t&#13;&#10;           WHERE t.tag = 'ADR_BRANCH' AND t.branch = '/' || z.kf || '/')&#13;&#10;            adr_branch,&#13;&#10;         ac_$.account_customer,&#13;&#10;         ac_$.bank_mfo_customer,&#13;&#10;         ac_$.bank_name_customer,&#13;&#10;         ac_UAH.account_customer_UAH,&#13;&#10;         ac_UAH.bank_mfo_customer_UAH,&#13;&#10;         ac_UAH.bank_name_customer_UAH,&#13;&#10;         vk.vk_fio,&#13;&#10;         vk.VK_date,&#13;&#10;         vk.VK_time,&#13;&#10;         vv.VV_fio,&#13;&#10;         vv.VV_date,&#13;&#10;         vv.VV_time,&#13;&#10;         TO_CHAR (z.datedokkb, 'dd.mm.yyyy') POST_KB_date,&#13;&#10;         TO_CHAR (z.datedokkb, 'HH24:MI') POST_KB_time,&#13;&#10;         (SELECT sb.fio&#13;&#10;            FROM zay_track zt, staff$base sb&#13;&#10;           WHERE     zt.userid = sb.id&#13;&#10;                 AND zt.old_viza is null&#13;&#10;                 AND zt.new_viza = 0&#13;&#10;                 AND zt.id = z.id)&#13;&#10;            POST_KB_fio,&#13;&#10;         zm.sign01,&#13;&#10;         zm.sign02&#13;&#10;    FROM zayavka z,&#13;&#10;         customer c,&#13;&#10;         cust_zay cz,&#13;&#10;         tabval$global tg,&#13;&#10;         (SELECT acc.acc, acc.nls account_customer, acc.kf bank_mfo_customer, b.val bank_name_customer&#13;&#10;            FROM accounts acc, branch_parameters b&#13;&#10;           WHERE b.branch = '/' || acc.kf || '/' AND b.tag = 'NAME_BRANCH') ac_$,&#13;&#10;         (SELECT acc.acc, acc.nls account_customer_UAH, acc.kf bank_mfo_customer_UAH, b.val bank_name_customer_UAH&#13;&#10;            FROM accounts acc, branch_parameters b&#13;&#10;           WHERE b.branch = '/' || acc.kf || '/' AND b.tag = 'NAME_BRANCH') ac_UAH,&#13;&#10;         (SELECT sb.fio vk_fio, zt.id, TO_CHAR (zt.change_time, 'dd.mm.yyyy') VK_date, TO_CHAR (zt.change_time, 'HH24:MI') VK_time&#13;&#10;            FROM zay_track zt, staff$base sb&#13;&#10;           WHERE zt.userid = sb.id AND zt.track_id = (select max(zt1.track_id) from zay_track zt1 where zt1.old_viza in (0, -1) and zt1.new_viza = 1 and zt1.id = zt.id)) vk,&#13;&#10;         (SELECT sb.fio vv_fio, zt.id, TO_CHAR (zt.change_time, 'dd.mm.yyyy') VV_date, TO_CHAR (zt.change_time, 'HH24:MI') VV_time&#13;&#10;            FROM zay_track zt, staff$base sb&#13;&#10;           WHERE zt.userid = sb.id AND zt.old_sos = 1 AND zt.new_sos = 2) vv,&#13;&#10;           barsaq.zayavka_id_map zm&#13;&#10;   WHERE     z.rnk = c.rnk&#13;&#10;         AND cz.rnk = c.rnk&#13;&#10;         AND z.kv2 = tg.kv&#13;&#10;         AND z.dk = 2&#13;&#10;         AND z.acc0 = ac_UAH.acc&#13;&#10;         AND z.acc1 = ac_$.acc&#13;&#10;         AND vk.id(+) = z.id&#13;&#10;         AND vv.id(+) = z.id&#13;&#10;         AND z.identkb IS NOT NULL&#13;&#10;         AND z.fdat BETWEEN :p_begin_date AND :p_end_date&#13;&#10;         and z.id = zm.idz&#13;&#10;         and z.sos &gt;= 0&#13;&#10;ORDER BY z.dk, z.fdat">
