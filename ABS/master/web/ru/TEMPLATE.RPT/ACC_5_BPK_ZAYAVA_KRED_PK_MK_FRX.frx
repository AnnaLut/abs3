        <TableRow Name="Row484" Height="41.92" AutoSize="true">
          <TableCell Name="Cell3584" Border.Lines="Left, Top" Border.Width="0.25"/>
          <TableCell Name="Cell3585" Border.Lines="Top" Border.Width="0.25" Text="Для заповнення тільки працівником Банку&#13;&#10;[ACC_BPK.ACC_BPK_POSADA_KR]&#13;&#10;[ACC_BPK.ACC_BPK_FIO_KR] &#13;&#10;який діє на підставі &lt;b&gt;[ACC_BPK.ACC_BPK_DOVER_KR]&lt;/b&gt;" Font="Times New Roman, 6.5pt, style=Bold" HtmlTags="true"/>
          <TableCell Name="Cell3586" Border.Lines="Top" Border.Width="0.25" Text="Дата заповнення та підписання Заяви &#13;&#10;Клієнтом/Довіреною особою Клієнта &#13;&#10;&lt;b&gt;[ACC_BPK.BANK_SYSDATE]&lt;/b&gt;" Font="Times New Roman, 6.5pt" HtmlTags="true"/>
          <TableCell Name="Cell3587" Border.Lines="Right, Top" Border.Width="0.25"/>
        </TableRow>
        <TableRow Name="Row486" Height="59.45" AutoSize="true">
          <TableCell Name="Cell3592" Border.Lines="Left" Border.Width="0.25"/>
          <TableCell Name="Cell3593" Text="М.П.&#13;&#10;&#13;&#10;&lt;b&gt;Підпис уповноваженого працівника Банку&lt;/b&gt;" VertAlign="Center" Font="Times New Roman, 6.5pt" HtmlTags="true"/>
          <TableCell Name="Cell3594" Text="&lt;b&gt;Підпис Клієнта ____________/[ACC_BPK.ACC_BPK_NMKK]/&lt;/b&gt;&#13;&#10;&#13;&#10;Або&#13;&#10;&#13;&#10;&lt;b&gt;Підпис Довіреної особи ____________/______________/&#13;&#10;                                                                          (ПІБ)&lt;/b&gt;" VertAlign="Center" Font="Times New Roman, 6.5pt" HtmlTags="true"/>
          <TableCell Name="Cell3595" Border.Lines="Right" Border.Width="0.25"/>
