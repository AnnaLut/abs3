  <TfrxDataPage Name="Data" Height="1000" Left="0" Top="0" Width="1000">
    <TfrxODACQuery Name="ODACQuery1" UserName="ODACQuery1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="     select last_day(:Dat)+1 dat1, k.kod, k.name, c.c c_open, nvl(d.c,0) c_turn, f_month_lit((:Dat),1,4)    &#13;&#10;  from ( select 1 kod, 'Клієнти, відкриті на  ПЦ Way4 валюта 980' name from dual&#13;&#10;          union all&#13;&#10;         select 2, 'Клієнти, відкриті на  ПЦ Way4 валюта 840, 978' from dual&#13;&#10;          union all&#13;&#10;         select 3, 'Клієнти, що мають Зарплатні та пенсійні рахунки, відкриті на  ПЦ Way4  в т.ч. Нацкарта' from dual&#13;&#10;          union all&#13;&#10;         select 4, 'Клієнти, відкриті на  УПЦ валюта 980' from dual&#13;&#10;          union all&#13;&#10;         select 5, 'Клієнти, відкриті на  УПЦ валюта 840' from dual&#13;&#10;          union all&#13;&#10;         select 6, 'Клієнти, що мають Зарплатні та пенсійні рахунки, відкриті на  УПЦ' from dual) k,&#13;&#10;       ( select kod, count(acc) c&#13;&#10;           from ( select case when a.kv = 980&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('25','26','28','34') then a.acc&#13;&#10;                              when a.kv in (840, 978)&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('25','26','28','34') then a.acc&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('22','23','24','27','29','30','31','32','33') then a.acc&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk = 'A' then a.acc&#13;&#10;                              when a.kv = 840&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk = 'A' then a.acc&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk &#60;&#62; 'A' then a.acc&#13;&#10;                              else null&#13;&#10;                         end acc,&#13;&#10;                         case when a.kv = 980&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('25','26','28','34') then 1&#13;&#10;                              when a.kv in (840, 978)&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('25','26','28','34') then 2&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('22','23','24','27','29','30','31','32','33') then 3&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk = 'A' then 4&#13;&#10;                              when a.kv = 840&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk = 'A' then 5&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk &#60;&#62; 'A' then 6&#13;&#10;                              else null&#13;&#10;                         end kod          &#13;&#10;                    from bpk_all_accounts o, accounts a, specparam_int i&#13;&#10;                   where o.acc_pk = a.acc&#13;&#10;                     and a.nls like '2625%'&#13;&#10;                     and a.daos &#60;= last_day(:Dat)&#13;&#10;                     and a.dazs is null&#13;&#10;                     and a.acc = i.acc )&#13;&#10;          where kod is not null&#13;&#10;          group by kod ) c,&#13;&#10;       ( select kod, count(unique acc) c&#13;&#10;           from ( select case when a.kv = 980&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('25','26','28','34')&#13;&#10;                               and d.tt in ('OW1','PKF') then a.acc&#13;&#10;                              when a.kv in (840, 978)&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('25','26','28','34')&#13;&#10;                               and (d.tt = 'PKF' or d.tt like 'OW%') then a.acc&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('22','23','24','27','29','30','31','32','33')&#13;&#10;                               and d.tt in ('OW1','PKF') then a.acc&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk = 'A'&#13;&#10;                               and d.tt in ('PK1','PKF') then a.acc&#13;&#10;                              when a.kv = 840&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk = 'A'&#13;&#10;                               and d.tt in ('PK1','PKF') then a.acc&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk &#60;&#62; 'A'&#13;&#10;                               and d.tt in ('PK1','PKF') then a.acc&#13;&#10;                             else null&#13;&#10;                         end acc,&#13;&#10;                         case when a.kv = 980&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('25','26','28','34')&#13;&#10;                               and d.tt in ('OW1','PKF') then 1&#13;&#10;                              when a.kv in (840, 978)&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('25','26','28','34')&#13;&#10;                               and (d.tt = 'PKF' or d.tt like 'OW%') then 2&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'W4%'&#13;&#10;                               and a.ob22 in ('22','23','24','27','29','30','31','32','33')&#13;&#10;                               and d.tt in ('OW1','PKF') then 3&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk = 'A'&#13;&#10;                               and d.tt in ('PK1','PKF') then 4&#13;&#10;                              when a.kv = 840&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk = 'A'&#13;&#10;                               and d.tt in ('PK1','PKF') then 5&#13;&#10;                              when a.kv = 980&#13;&#10;                               and a.tip like 'PK%'&#13;&#10;                               and i.demand_kk &#60;&#62; 'A'&#13;&#10;                               and d.tt in ('PK1','PKF') then 6&#13;&#10;                             else null&#13;&#10;                         end kod&#13;&#10;                    from bpk_all_accounts o, accounts a, specparam_int i, opldok d, oper p&#13;&#10;                   where o.acc_pk = a.acc&#13;&#10;                     and a.nls like '2625%'&#13;&#10;                     and a.daos &#60;= last_day(:Dat)&#13;&#10;                     and a.dazs is null&#13;&#10;                     and a.acc = i.acc&#13;&#10;                     and a.acc = d.acc&#13;&#10;                     and d.dk = 0 and (d.tt in ('PK1', 'PKF') or d.tt like 'OW%')&#13;&#10;                     and d.fdat between trunc(:Dat, 'mm') and last_day(:Dat)&#13;&#10;                     and d.sos = 5&#13;&#10;                     and d.ref = p.ref and p.nlsb like '292%' )&#13;&#10;          where kod is not null&#13;&#10;          group by kod ) d&#13;&#10;where k.kod = c.kod(+)&#13;&#10;   and k.kod = d.kod(+)&#13;&#10;order by k.kod&#13;&#10;    " PropData=""/>
