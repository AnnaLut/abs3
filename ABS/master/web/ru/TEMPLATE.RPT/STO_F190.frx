    <OracleDataConnection Name="sberdevo" ConnectionString="rijcmlqvvbGhv0rJ/ulNhGM40hQI48foykP3yfjCTOuXnUwIvqnBbeCUu10fLPHOpnSPjhRYbWWXiWGcarq43NCYeLzvA==">
      <TableDataSource Name="Table" Alias="RegularSTO" DataType="System.Int32" Enabled="true" SelectCommand="select o.id, o.priority,&#13;&#10;       a.nls account_number,&#13;&#10;       case when o.order_type_id = 1 then sep.purpose&#13;&#10;            when o.order_type_id = 2 then sbonf.purpose&#13;&#10;            when o.order_type_id = 3 then upper(substr(trim(sp.payment_name), 1, 1)) || substr(trim(sp.payment_name), 2)&#13;&#10;            when o.order_type_id = 4 then upper(substr(trim(sp.payment_name), 1, 1)) || substr(trim(sp.payment_name), 2)&#13;&#10;            else null&#13;&#10;       end payment_name,&#13;&#10;       case when o.order_type_id = 1 then sep.receiver_account&#13;&#10;            when o.order_type_id = 2 then sbonf.receiver_account&#13;&#10;            when o.order_type_id = 3 then sp.receiver_account&#13;&#10;            when o.order_type_id = 4 then sp.receiver_account&#13;&#10;            else null&#13;&#10;       end receiver_account,&#13;&#10;       case when o.order_type_id = 1 then sep.receiver_mfo&#13;&#10;            when o.order_type_id = 2 then sbonf.receiver_mfo&#13;&#10;            when o.order_type_id = 3 then sp.receiver_mfo&#13;&#10;            when o.order_type_id = 4 then sp.receiver_mfo&#13;&#10;            else null&#13;&#10;       end receiver_mfo,&#13;&#10;       case when o.order_type_id = 1 then sep.receiver_name&#13;&#10;            when o.order_type_id = 2 then sbonf.receiver_name&#13;&#10;            when o.order_type_id = 3 then sp.receiver_name&#13;&#10;            when o.order_type_id = 4 then sp.receiver_name&#13;&#10;            else null&#13;&#10;       end receiver_name,&#13;&#10;       case when o.order_type_id = 1 then sep.receiver_edrpou&#13;&#10;            when o.order_type_id = 2 then sbonf.receiver_edrpou&#13;&#10;            when o.order_type_id = 3 then sp.receiver_edrpou&#13;&#10;            when o.order_type_id = 4 then sp.receiver_edrpou&#13;&#10;            else null&#13;&#10;       end receiver_edrpou,&#13;&#10;       case when o.order_type_id = 1 then null&#13;&#10;            when o.order_type_id = 2 then null&#13;&#10;            when o.order_type_id = 3 then sbonc.customer_account&#13;&#10;            when o.order_type_id = 4 then sbonnc.customer_account&#13;&#10;            else null&#13;&#10;       end customer_account,&#13;&#10;       (select f.name from freq f where f.freq = o.payment_frequency) payment_frequency,&#13;&#10;       case when o.order_type_id = 1 then to_char(sep.regular_amount, 'FM9999999990.00')&#13;&#10;            when o.order_type_id = 2 then to_char(sbonf.regular_amount, 'FM9999999990.00')&#13;&#10;            when o.order_type_id = 3 then&#13;&#10;                 case when sbonc.regular_amount is null or sbonc.regular_amount = 0 then&#13;&#10;                           'не фіксована, але не більше ' || to_char(sbonc.ceiling_amount, 'FM9999999990.00')&#13;&#10;                      else to_char(sbonc.regular_amount, 'FM9999999990.00')&#13;&#10;                 end&#13;&#10;            when o.order_type_id = 4 then to_char(sbonnc.regular_amount, 'FM9999999990.00')&#13;&#10;            else null&#13;&#10;       end amount,&#13;&#10;       sto_ui.date_to_char_in_genitive(min(o.start_date) over (order by a.acc)) first_pay_date,&#13;&#10;       to_char(o.start_date, 'dd.mm.yyyy') || ' - ' || nvl(to_char(o.stop_date, 'dd.mm.yyyy'), 'без обмеження') period,&#13;&#10;       sto_ui.date_to_char_in_genitive(bankdate()) current_date,&#13;&#10;       coalesce (w.nd, b.nd, a.acc) contract_number,&#13;&#10;       to_char(a.daos, 'dd.mm.yyyy') contract_date,&#13;&#10;       c.nmk customer_name,&#13;&#10;       w1.value as EMAIL,&#13;&#10;       w2.value as MOB_PHONE,&#13;&#10;       (select bp.val from branch_parameters bp where bp.branch = a.branch and bp.tag = 'NAME_BRANCH') branch_name,&#13;&#10;       (select bp.val from branch_parameters bp where bp.branch = a.branch and bp.tag = 'ADR_BRANCH') branch_address,&#13;&#10;       case when nvl(o.SEND_SMS,'N') = 'N' then 'Ні' else 'Так' end as send_sms&#13;&#10;  from bars.sto_order o&#13;&#10;  join bars.accounts a on a.acc = o.payer_account_id&#13;&#10;  join bars.customer c on c.rnk = a.rnk&#13;&#10;  left &#13;&#10;  join bars.customerw w1 on (w1.rnk = C.RNK and w1.tag='EMAIL' )&#13;&#10;  left &#13;&#10;  join bars.customerw w2 on (w2.rnk = C.RNK and w2.tag='MPNO'  )&#13;&#10;  left &#13;&#10;  join bars.sto_sep_order sep on sep.id = o.id&#13;&#10;  left join bars.sto_sbon_order_free sbonf on sbonf.id = o.id&#13;&#10;  left join bars.sto_sbon_order_contr sbonc on sbonc.id = o.id&#13;&#10;  left join bars.sto_sbon_order_no_contr sbonnc on sbonnc.id = o.id&#13;&#10;  left join bars.w4_acc w on w.acc_pk = o.payer_account_id&#13;&#10;  left join bars.bpk_acc b on b.acc_pk = o.payer_account_id&#13;&#10;  left join bars.sto_product p on p.id = o.product_id&#13;&#10;  left join bars.sto_sbon_product sp on sp.id = p.id&#13;&#10; where a.acc = (select od.payer_account_id&#13;&#10;               from   bars.sto_order od&#13;&#10;               where  od.id = :order_id) and&#13;&#10;      o.cancel_date is null and&#13;&#10;      (o.stop_date is null or o.stop_date &gt;= bankdate())&#13;&#10;order by 1&#13;&#10;">
