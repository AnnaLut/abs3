    <TfrxODACQuery Name="ODACQuery1" UserName="ODACQuery1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select substr(a.branch,1,15) branch, c.nmk, a.nls, c.okpo, a.category,&#13;&#10;       (select d.nls from bpk_acc k, accounts d where k.acc_w4 = a.acc and k.acc_pk = d.acc) nls_pk&#13;&#10;  from ( -- счета, на которые на протяжении года зачислялась пенсия&#13;&#10;         select a.acc, a.nls, a.kv, a.branch, a.rnk, decode(s.ob22,'22','ПФУ','военная пенсия') category&#13;&#10;           from w4_acc o, accounts a, oper r, accounts s&#13;&#10;          where o.acc_pk = a.acc&#13;&#10;            and a.nls like '2625%'&#13;&#10;            and a.ob22 in ('29','30')&#13;&#10;             -- есть проводки за последний месяц&#13;&#10;            and a.nls = decode(r.dk,1,r.nlsb,r.nlsa)&#13;&#10;            and a.kv  = decode(r.dk,1,nvl(r.kv2,r.kv),r.kv)&#13;&#10;            and s.nls = decode(r.dk,1,r.nlsa,r.nlsb)&#13;&#10;            and s.kv  = decode(r.dk,1,r.kv,nvl(r.kv2,r.kv)) &#13;&#10;            and s.nbs = '2909' and s.ob22 in ('22','25')&#13;&#10;            and r.sos = 5&#13;&#10;            and r.mfoa = r.mfob&#13;&#10;            and r.vdat between trunc(trunc(:dat,'mm')-1,'mm') and trunc(:dat,'mm')-1&#13;&#10;             -- есть проводки за первый месяц&#13;&#10;             --   по счету Way4&#13;&#10;            and ( exists ( select 1&#13;&#10;                             from oper p, accounts b&#13;&#10;                            where a.nls = decode(p.dk,1,p.nlsb,p.nlsa)&#13;&#10;                              and a.kv  = decode(p.dk,1,nvl(p.kv2,p.kv),p.kv)&#13;&#10;                              and b.nls = decode(p.dk,1,p.nlsa,p.nlsb)&#13;&#10;                              and b.kv  = decode(p.dk,1,p.kv,nvl(p.kv2,p.kv)) &#13;&#10;                              and b.nbs = '2909' and b.ob22 in ('22','25')&#13;&#10;                              and p.vdat between trunc(add_months(:dat,-12),'mm') and last_day(add_months(:dat,-12))&#13;&#10;                              and p.sos = 5&#13;&#10;                              and p.mfoa = p.mfob )&#13;&#10;               -- по счету CONTS&#13;&#10;               or exists ( select 1&#13;&#10;                             from bpk_acc k, accounts d, oper p, accounts b&#13;&#10;                            where k.acc_w4 = a.acc&#13;&#10;                              and k.acc_pk = d.acc&#13;&#10;                              and d.nls = decode(p.dk,1,p.nlsb,p.nlsa)&#13;&#10;                              and d.kv  = decode(p.dk,1,nvl(p.kv2,p.kv),p.kv)&#13;&#10;                              and b.nls = decode(p.dk,1,p.nlsa,p.nlsb)&#13;&#10;                              and b.kv  = decode(p.dk,1,p.kv,nvl(p.kv2,p.kv)) &#13;&#10;                              and b.nbs = '2909' and b.ob22 in ('22','25')&#13;&#10;                              and p.vdat between trunc(add_months(:dat,-12),'mm') and last_day(add_months(:dat,-12))&#13;&#10;                              and p.sos = 5&#13;&#10;                              and p.mfoa = p.mfob ) )&#13;&#10;             -- минус счета, по которым были выборочные списания за период&#13;&#10;            and not exists ( select 1&#13;&#10;                               from obpc_acct c, obpc_tran_hist h, oper p&#13;&#10;                              where c.lacct = a.nls&#13;&#10;                                and c.card_acct = h.card_acct&#13;&#10;                                and h.ref = p.ref&#13;&#10;                                and p.vdat between trunc(add_months(:dat,-12),'mm') and trunc(:dat,'mm')-1&#13;&#10;                                and h.tran_type in ('05', '07', '21', '28', '2F') )&#13;&#10;            and not exists ( select 1 from oper p, ow_rep_tt t&#13;&#10;                              where a.nls = decode(p.dk,0,p.nlsb,p.nlsa)&#13;&#10;                                and a.kv  = decode(p.dk,0,nvl(p.kv2,p.kv),p.kv)&#13;&#10;                                and p.vdat between trunc(add_months(:dat,-12),'mm') and trunc(:dat,'mm')-1&#13;&#10;                                and p.sos = 5&#13;&#10;                                and p.tt = t.tt&#13;&#10;                                and p.nazn like  t.nazn ) &#13;&#10;       ) a, customer c&#13;&#10;where a.rnk = c.rnk&#13;&#10;order by a.branch, a.category&#13;&#10;  &#13;&#10;  " PropData=""/>
