        <TableRow Name="Row122" MinHeight="11.34" Height="11.34" AutoSize="true">
          <TableCell Name="Cell524" Border.Lines="All" Border.Width="0.25" Text="Блок 6: Умови кредитування:" Font="Times New Roman, 7pt, style=Bold" ColSpan="4"/>
