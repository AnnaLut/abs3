        </TableRow>
        <TableRow Name="Row124" MinHeight="11.34" Height="13.2" AutoSize="true">
          <TableCell Name="Cell534" Border.Lines="Left" Border.Width="0.25" Font="Times New Roman, 7pt" HtmlTags="true"/>
          <TableCell Name="Cell535" Border.Lines="All" Border.Width="0.25" Text="	Бажаний розмір Кредиту" Font="Times New Roman, 7pt, style=Bold" HtmlTags="true"/>
          <TableCell Name="Cell536" Border.Lines="All" Border.Width="0.25" Text="[ACC_BPK.ACC_BPK_DESIRED_CREDIT_SUM] грн." HorzAlign="Center" Font="Times New Roman, 7pt, style=Bold" HtmlTags="true"/>
          <TableCell Name="Cell537" Border.Lines="Right" Border.Width="0.25"/>
        </TableRow>
        <TableRow Name="Row125" MinHeight="11.34" Height="13.94" AutoSize="true">
          <TableCell Name="Cell539" Border.Lines="Left" Border.Width="0.25" Font="Times New Roman, 7pt" HtmlTags="true"/>
          <TableCell Name="Cell540" Border.Lines="All" Border.Width="0.25" Text="	Максимальний розмір Кредиту" Font="Times New Roman, 7pt, style=Bold" HtmlTags="true"/>
          <TableCell Name="Cell541" Border.Lines="All" Border.Width="0.25" Text="[ACC_BPK.ACC_BPK_MAX_CREDIT_SUM] грн." HorzAlign="Center" Font="Times New Roman, 7pt, style=Bold" HtmlTags="true"/>
