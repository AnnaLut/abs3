        <TableRow Name="Row894">
          <TableCell Name="Cell1960" Border.Lines="Left, Right, Bottom" Border.Color="Gray" Text="м.п." Font="Times New Roman, 10pt, style=Bold" HtmlTags="true"/>
          <TableCell Name="Cell1961" Border.Lines="Left, Right, Bottom"/>
