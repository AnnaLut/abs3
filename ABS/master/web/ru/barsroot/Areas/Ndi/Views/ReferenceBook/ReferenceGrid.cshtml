@{
  Layout = "~/Views/Shared/_LayoutExt.cshtml";
}
@using BarsWeb.Areas.Ndi.Models.ViewModels
@model MainOptionsViewModel

<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<script>
 
    window.tableId = '@Model.TableId';
    /* Table Modes List by META_ACCESS_CODES table
    0 - Update + Insert + Delete
    1 - Read Only
    2 - Update
    3 - Insert
    4 - Delete
    5 - Update + Insert
    6 - Update + Delete
    7 - Insert + Delete
    */
    
    window.tableMode = '@Model.TableMode';
    window.conditions = '@Model.Conditions';
    window.sqlProc = '@Model.SqlProc';
    window.base64jsonSqlProcParams = '@Model.Base64jsonSqlProcParams';
    window.CodeOper = '@Model.CodeOper';
    window.sParColumn = '@Model.SParColumn';
    window.isFuncOnly = '@Model.IsFuncOnly';
    window.nativeTabelId = '@Model.NativeTabelId';
    window.nsiTableId = '@Model.NsiTableId';
    window.nsiFuncId = '@Model.NsiFuncId';
    window.filterCode = '@Model.FilterCode';
    window.hasCallbackFunction = '@Model.HasCallbackFunction';
    window.getFiltersOnly = '@Model.GetFiltersOnly';
    window.baseCodeOper = "@Model.BaseCodeOper";
    window.saveFilterLocal = "@Model.SaveFilterLocal";
    window.Code = "@Model.Code";
    window.Base64InsertDefParamsString = "@Model.DefParamModel.Base64InsertDefParamsString"
    
</script>
<script src="@Url.Content("~/Areas/Ndi/Scripts/ReferenceGrid.js")"></script>


