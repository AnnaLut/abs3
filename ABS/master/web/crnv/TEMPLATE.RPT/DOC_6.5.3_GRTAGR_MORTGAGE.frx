      <TableDataSource Name="Table16" Alias="target_dog" DataType="System.Int32" Enabled="true" SelectCommand="select  wcs_utl.get_answ(:p_bid_id, 'CL_2_0_01', :p_ws_id , :p_ws_number ) as   target_dog,&#10;          wcs_utl.get_answ(:p_bid_id, 'CL_2_0_02', :p_ws_id , :p_ws_number ) as   description_dog,&#10;          wcs_utl.get_answ(:p_bid_id, 'CL_2_0_03', :p_ws_id , :p_ws_number ) as   num_description_dog,&#10;          wcs_utl.get_answ(:p_bid_id, 'CL_2_0_04', :p_ws_id , :p_ws_number ) as   date_description_dog&#10;  from dual">
