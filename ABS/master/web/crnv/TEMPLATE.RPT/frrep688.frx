      <TableDataSource Name="Table" Alias="q" DataType="System.Int32" Enabled="true" SelectCommand="           select  o.nlsb, o.nam_b ,        sum(CASE &#13;&#10;                                                   WHEN p.tt='$09'and p.dk = 0 THEN p.s/100&#13;&#10;                                                   ELSE 0&#13;&#10;                                                END) as kol,&#13;&#10;                                            sum(CASE &#13;&#10;                                                   WHEN p.tt='$00'and p.dk = 0 THEN p.s/100&#13;&#10;                                                   ELSE 0&#13;&#10;                                                END)   as sum, K.CP_ID, K.CENA_KUP, k.cena                                    &#13;&#10;            from oper o, opldok p, operw w, cp_kod k&#13;&#10;            where o.vdat =:fdat and&#13;&#10;                  p.fdat = :fdat and&#13;&#10;                  p.sos  = 5&#13;&#10;              and o.tt in ('$00')&#13;&#10;              and o.ref = p.ref &#13;&#10;              and o.ref = w.ref and w.tag = 'KODCP'&#13;&#10;              and to_number(value) = K.ID &#13;&#10;              and (exists (select 1 from v_gl where nbs = '2801' and ob22 = '04' and acc = p.acc and p.dk = 0)&#13;&#10;                or exists (select 1 from v_gl where nbs = '9812' and ob22 = '64' and acc = p.acc and p.dk = 0))&#13;&#10;              group by  o.nlsb, o.nam_b , K.CP_ID, K.CENA_KUP , k.cena  &#13;&#10;              order by  o.nlsb">
