        <CommandParameter Name="P1" DataType="126" Expression="[p_bid_id]"/>
      </TableDataSource>
      <TableDataSource Name="Table7" Alias="Marital_status" DataType="System.Int32" Enabled="true" SelectCommand="select (case  when nvl(wcs_utl.get_answ(:p_bid_id, 'CL_34', :p_ws_id , :p_ws_number ), -1) != 0 and nvl(wcs_utl.get_answ(:p_bid_id, 'CL_34', :p_ws_id , :p_ws_number ), -1) != 4   then&#10;--'Холостой'  &#10;'      Я, ' || wcs_utl.get_answ(:p_bid_id, 'CL_1', :p_ws_id , :p_ws_number )||' '|| wcs_utl.get_answ(:p_bid_id, 'CL_2', :p_ws_id , :p_ws_number )||' '|| wcs_utl.get_answ(:p_bid_id, 'CL_3', :p_ws_id , :p_ws_number )|| ', цим засвідчую, що на момент укладення цього Кредитного договору  в шлюбі не перебуваю та однією сім’єю з будь-якою особою без реєстрації шлюбу не проживаю.&#10;'        &#10;          else &#10;      -- 'Женат'&#10;          '      Я, ' ||wcs_utl.get_answ(:p_bid_id, 'CL_0_176', :p_ws_id , :p_ws_number )||', паспорт громадянина України серії '||wcs_utl.get_answ(:p_bid_id, 'CL_0_179', :p_ws_id , :p_ws_number ) ||' № '||wcs_utl.get_answ(:p_bid_id, 'CL_0_179_1', :p_ws_id , :p_ws_number )||', виданий '||wcs_utl.get_answ(:p_bid_id, 'CL_0_180', :p_ws_id , :p_ws_number )||' '||substr(f_dat_lit(wcs_utl.get_answ_date(:p_bid_id, 'CL_0_181', :p_ws_id , :p_ws_number )),1,25)||', реєстраційний номер облікової картки платника податку № '||wcs_utl.get_answ(:p_bid_id, 'CL_0_178', :p_ws_id , :p_ws_number )||', надаю свою згоду на укладання моїм(-єю) чоловіком (дружиною) '|| wcs_utl.get_answ(:p_bid_id, 'CL_1', :p_ws_id , :p_ws_number )||' '|| wcs_utl.get_answ(:p_bid_id, 'CL_2', :p_ws_id , :p_ws_number )||' '|| wcs_utl.get_answ(:p_bid_id, 'CL_3', :p_ws_id , :p_ws_number )||' цього Кредитного договору. Підтверджую, що з умовами цього Кредитного договору ознайомлений(-на), ніяких заперечень щодо його положень та укладання не маю. &#10;      Надання згоди на укладення моїм(-єю)  чоловіком (дружиною) цього Кредитного договору засвідчую власним підписом &#10;'    &#10;      end) maried&#10;            from dual&#10;">
        <Column Name="MARIED" DataType="System.String"/>
        <CommandParameter Name="P1" DataType="126" Expression="[p_bid_id]"/>
        <CommandParameter Name="P2" DataType="126" Expression="[p_ws_id]"/>
        <CommandParameter Name="P3" DataType="126" Expression="[p_ws_number]"/>
      </TableDataSource>
      <TableDataSource Name="Table8" Alias="Target_Many" DataType="System.Int32" Enabled="true" SelectCommand="select ( &#10; case  when to_number(wcs_utl.get_answ(:p_bid_id, 'PI_FREE_SELECTED'))=1 then &#10;' шляхом перерахування коштів в національній валюті з рахунку наданого кредиту на поточний (вкладний) рахунок Продавця '||&#10;wcs_utl.get_answ(:p_bid_id, 'CL_2_0_02', 'MAIN' , 0 )||' - '||&#10;wcs_utl.get_answ(:p_bid_id, 'PI_FREE_NAME')||' (ПІБ Продавця) № '||&#10;wcs_utl.get_answ(:p_bid_id, 'PI_FREE_NLS')||&#10;' відкритий в '||&#10;nvl((select trim(fullName)  from rcukru where mfo=wcs_utl.get_answ(:p_bid_id, 'PI_FREE_MFO') ),'')||&#10;' Банку,'||    &#10;' МФО '||wcs_utl.get_answ(:p_bid_id, 'PI_FREE_MFO')||&#10;' на підставі платіжного доручення Позичальника'&#10;else&#10;' готівкою шляхом надання Позичальнику готівки з каси Банку'&#10;end) target from dual&#10;&#10;">
        <Column Name="TARGET" DataType="System.String"/>
        <CommandParameter Name="P1" DataType="126" Expression="[p_bid_id]"/>
      </TableDataSource>
      <TableDataSource Name="Table9" Alias="Mortage" DataType="System.Int32" Enabled="true" SelectCommand="select concatstr('&#10; --   з прийняттям в іпотеку нерухомого майна, яке  '||(case when val_bool!=1    &#10; then&#10; 'є власністю  Майнового поручителя – '||&#10; wcs_utl.get_answ(bid_id, 'CL_1', ws_id , ws_number )||' '||&#10; wcs_utl.get_answ(bid_id, 'CL_2', ws_id , ws_number )||' '||&#10; wcs_utl.get_answ(bid_id, 'CL_3', ws_id , ws_number )||' і '&#10; else &#10; ' '&#10; end)||&#10;'знаходиться за адресою : '||&#10; wcs_utl.get_answ(bid_id, 'GRT_4_6', ws_id , ws_number )||&#10; ' буд. '||(wcs_utl.get_answ(bid_id, 'GRT_4_8', ws_id , ws_number ))||&#10;' кв. '||(wcs_utl.get_answ(bid_id, 'GRT_4_2', ws_id , ws_number ))||'; ' )&#10;MORTGAGE &#10;from WCS_ANSWERS where ws_id='GRT_MORTGAGE' and question_id='GRT_0' &#10;and  bid_id=:p_bid_id&#10;">
        <Column Name="MORTGAGE" DataType="System.String"/>
        <CommandParameter Name="P1" DataType="126" Expression="[p_bid_id]"/>
      </TableDataSource>
      <TableDataSource Name="Table11" Alias="pidpus" DataType="System.Int32" Enabled="true" SelectCommand="select (case  when nvl(wcs_utl.get_answ(:p_bid_id, 'CL_34', :p_ws_id , :p_ws_number ), -1) != 0 and nvl(wcs_utl.get_answ(:p_bid_id, 'CL_34', :p_ws_id , :p_ws_number ), -1) != 4   then&#13;&#10;--'Холостой'  &#13;&#10;' _________________________________ &#13;&#10;(ініціали та прізвище Позичальника &#13;&#10;  власноручний підпис та напис)'                  else &#13;&#10;      -- 'Женат'&#13;&#10;  ' ____________________________________&#13;&#10; (ініціали та прізвище чоловіка/дружини &#13;&#10;Позичальника  власноручний підпис та напис)'    &#13;&#10;      end) mar&#13;&#10;            from dual">
        <Column Name="MAR" DataType="System.String"/>
        <CommandParameter Name="P1" DataType="126" Expression="[p_bid_id]"/>
        <CommandParameter Name="P2" DataType="126" Expression="[p_ws_id]"/>
        <CommandParameter Name="P3" DataType="126" Expression="[p_ws_number]"/>
      </TableDataSource>
    </OracleDataConnection>
    <Parameter Name="p_bid_id" DataType="System.Int32" Expression="113502"/>
    <Parameter Name="p_ws_id" DataType="System.String" Expression="&quot;MAIN&quot;"/>
    <Parameter Name="p_ws_number" DataType="System.Int32" Expression="0"/>
  </Dictionary>
  <ReportPage Name="Page2">
    <DataBand Name="Data2" Width="718.2" Height="6628.61" CanGrow="true" CanShrink="true" CanBreak="true">
      <TableObject Name="Table6" Left="9.45" Top="9.45" Width="693.66" Height="6602.42" GrowToBottom="true">
        <TableColumn Name="Column1" Width="346.83"/>
        <TableColumn Name="Column2" Width="346.83"/>
        <TableRow Name="Row1" Height="28.56">
          <TableCell Name="Cell1" Text="ДОГОВІР ПРО ІПОТЕЧНИЙ КРЕДИТ №[DataSubProdukt.DEAL_NUMBER]&#13;&#10;" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 12pt, style=Bold" ColSpan="2"/>
          <TableCell Name="Cell2" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 12pt, style=Bold"/>
        </TableRow>
        <TableRow Name="Row2" Height="19.11">
          <TableCell Name="Cell6" Text="[Branch_Info.BR_LOCATION]" HorzAlign="Center"/>
          <TableCell Name="Cell7" Text="[DataSubProdukt.DAT_DOG_PR]" HorzAlign="Center"/>
        </TableRow>
        <TableRow Name="Row3" Height="141.96" AutoSize="true">
          <TableCell Name="Cell11" Text="      Цей Кредитний договір (далі - Договір) укладений між: &lt;i&gt;Публічним акціонерним товариством «Державний ощадний банк України»&lt;/i&gt;(далі - Банк), що є юридичною особою за законодавством України, в особі &lt;b&gt;[Branch_Info.POST_R]&lt;/b&gt; &lt;b&gt;[Branch_Info.BR_BOSS_R]&lt;/b&gt;, повноваження якого підтверджені &lt;b&gt;[Branch_Info.BR_BOSS_DOC_R]&lt;/b&gt; від &lt;b&gt;[FormatDateTime([Branch_Info.BR_BOSS_DOC_D], &quot;Short Date&quot;)] &lt;/b&gt;року, посвідченою &lt;b&gt;[Branch_Info.BR_BOSS_DOC_N]&lt;/b&gt;, зареєстрованою в реєстрі за № &lt;b&gt;[Branch_Info.BR_BOSS_DOC_N_R]&lt;/b&gt;, з однієї сторони, та фізичною особою    &lt;b&gt;[klient_info.FIO_O]&lt;/b&gt; (далі - Позичальник), з другої сторони, (разом за текстом Договору - Сторони), свідчить що Сторони досягли згоди щодо врегулювання всіх майнових та організаційних аспектів на нижченаведених умовах і уклали цей Договір про наступне: " HorzAlign="Justify" VertAlign="Bottom" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell12" HorzAlign="Justify"/>
        </TableRow>
        <TableRow Name="Row4" Height="28.56">
          <TableCell Name="Cell16" Text="       1.ПРЕДМЕТ ДОГОВОРУ" VertAlign="Bottom" Font="Times New Roman, 10pt, style=Bold, Underline"/>
          <TableCell Name="Cell17" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row5" Height="113.61" AutoSize="true">
          <TableCell Name="Cell21" Text="      1.1. Банк зобов’язується надати Позичальнику на умовах цього Договору грошові кошти в сумі &lt;b&gt;[DataSubProdukt.SUMM] ([DataSubProdukt.SSUM_PR])&lt;/b&gt; гривень (надалі – Кредит), а Позичальник зобов’язується прийняти, належним чином використати та повернути Кредит в сумі &lt;b&gt;[DataSubProdukt.SUMM] ([DataSubProdukt.SSUM_PR])&lt;/b&gt;  гривень, сплатити проценти за користування Кредитом в розмірі &lt;b&gt;[DataSubProdukt.INTEREST_RATE] ( [DataSubProdukt.INTEREST_RATE_PR] )&lt;/b&gt; процентів річних та комісійну винагороду в порядку, на умовах та в строки, визначені цим Договором. Зазначена в цьому пункті процентна ставка за користування Кредитом є фіксованою" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell22" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row6" Height="66.36" AutoSize="true">
          <TableCell Name="Cell23" Text="      1.2. Кредит надається на &lt;b&gt;[DataSubProdukt.TERM] ( [DataSubProdukt.TERM_PR])&lt;/b&gt; місяців з терміном остаточного погашення Кредиту не пізніше &lt;b&gt;[DataSubProdukt.DAT_DOG_RET_PR]&lt;/b&gt; на &lt;b&gt;[target_dog.TARGET_DOG] [target_dog.DESCRIPTION_DOG]&lt;/b&gt;  за договором купівлі-продажу № &lt;b&gt;[target_dog.NUM_DESCRIPTION_DOG]&lt;/b&gt; від &lt;b&gt;[target_dog.DATE_DESCRIPTION_DOG]&lt;/b&gt;. &#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell24" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row7" Height="94.71" AutoSize="true">
          <TableCell Name="Cell25" Text="      1.3. Кредит надається безготівково протягом 1 (одного) робочого дня з моменту виконання Позичальником умов, що зазначені в п. 1.4. цього Договору шляхом перерахування коштів в національній валюті з рахунку наданого кредиту на поточний (вкладний) рахунок Продавця квартири (або __поверхового жилого будинку та земельної ділянки або нерухомого майна) -________________________________ (ПІБ Продавця) №__________________, відкритий в ________________________ Банку, МФО __________ на підставі платіжного доручення Позичальника (або готівкою шляхом надання Позичальнику готівки з каси Банку). &#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell26" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row8" Height="65.01" AutoSize="true">
          <TableCell Name="Cell27" Text="      Детальний розпис сукупної вартості кредиту (у процентному значенні та грошовому виразі), графік платежів, вид і предмет кожної супутньої послуги, пов’язаної з одержанням, обслуговуванням, погашенням Кредиту та укладенням цього Договору та їх обґрунтування визначені в додатках №№ 1-3 до цього Договору, що є його невід’ємною частиною.&#13;&#10;" Font="Arial, 10pt, style=Italic" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell28" Font="Arial, 10pt, style=Italic"/>
        </TableRow>
        <TableRow Name="Row9" Height="46.11" AutoSize="true">
          <TableCell Name="Cell29" Text="      1.4 Сторони погоджуються з тим, що зобов’язання Банку надати Кредит протягом 1 (одного) робочого дня в повній сумі виникає з моменту виконання всіх та кожної з наведених нижче умов (надалі по тексту використовуватиметься скорочення – «Загальні умови надання Кредиту»):&#13;&#10;" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell30" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row10" Height="32.88" AutoSize="true">
          <TableCell Name="Cell31" Text="      1.4.1. Укладення в забезпечення виконання зобов’язань за цим Договором Іпотечних договорів: &lt;b&gt;[Mortage.MORTGAGE]&lt;/b&gt;.&#13;&#10;" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell32"/>
        </TableRow>
        <TableRow Name="Row11" Height="32.88" AutoSize="true">
          <TableCell Name="Cell33" Text="      1.4.2. Укладення на користь Банку договору страхування Предмету іпотеки, договору страхування життя Позичальника.&#13;&#10;" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell34" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row12" Height="17.76" AutoSize="true">
          <TableCell Name="Cell35" Text="      1.4.3. Сплати Позичальником (Майновим поручителем) страхових платежів за договорами страхування." Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell36"/>
        </TableRow>
        <TableRow Name="Row13" Height="32.88" AutoSize="true">
          <TableCell Name="Cell37" Text="      1.4.4. Відкриття Позичальником поточного рахунку у Банку № &lt;b&gt;[Current_Accounts.CURRENT_ACC]&lt;/b&gt; для здійснення розрахунково-касового обслуговування, в тому числі з обслуговування  кредиту.&#13;&#10;" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell38" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row14" Height="112.26" AutoSize="true">
          <TableCell Name="Cell39" Text="      1.5. Позичальник зобов’язується здійснювати погашення Кредиту рівними частинами в сумі &lt;b&gt;[DATA_LIM.SUMG] ([DATA_LIM.SUMG_PR])&lt;/b&gt;  гривень та сплату процентів, нарахованих Банком на залишок заборгованості за Кредитом, щомісячно до [DATA_LIM.DAY_SS] числа місяця наступного за звітним, починаючи з &lt;b&gt;[DATA_LIM.FIRST_DAT_G_PR]&lt;/b&gt; шляхом внесення коштів на поточний рахунок, зазначений в п. 1.4.4., які Банк надалі списує в рахунок погашення кредиту відповідно до п. 1.7. цього Договору. Останній платіж в рахунок погашення Кредиту та сплати нарахованих Банком процентів здійснити не пізніше  &lt;b&gt;[DataSubProdukt.DAT_DOG_RET_PR]&lt;/b&gt;.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell40" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row15" Height="17.76">
          <TableCell Name="Cell41" Text="      1.6. За користування Кредитом Позичальник зобов’язаний сплачувати Банку відповідну плату (проценти) в порядку, розмірах та строки, визначені в цьому Договорі." HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell42" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row16" Height="168.96" AutoSize="true">
          <TableCell Name="Cell43" Text="      1.6.1. При нарахуванні та сплаті процентів за користування Кредитом Сторони повинні керуватись наступним:&#13;&#10;      1.6.1.1. На період з дня видачі Кредиту та до моменту закінчення строку, на який було надано Кредит, а саме: до &lt;b&gt;[DataSubProdukt.DAT_DOG_RET_PR]&lt;/b&gt; включно, проценти за користування Кредитом нараховуються Банком на підставі процентної ставки в розмірі &lt;b&gt;[DataSubProdukt.INTEREST_RATE] ([DataSubProdukt.INTEREST_RATE_PR]) &lt;/b&gt;процентів річних. &#13;&#10;      1.6.1.2. При нарахуванні процентів за користування Кредитом враховується перший і не враховується останній день користування Кредитом. &#13;&#10;      1.6.1.3. Проценти нараховуються Банком щомісячно за методом факт/факт на фактичний залишок заборгованості за Кредитом, що був отриманий Позичальником, починаючи з дати видачі Кредиту до моменту закінчення строку, на який надано Кредит. Сплата процентів здійснюється Позичальником  згідно з Графіком платежів або достроково відповідно до порядку визначеному Розділом 3 цього Договору.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell44" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row17" Height="82.78" AutoSize="true">
          <TableCell Name="Cell45" Text="      1.6.1.4. При здійсненні Позичальником останнього платежу в погашення заборгованості за Кредитом Позичальник та Банк здійснюють звірку погашення заборгованості за Кредитом та сплати процентів за користування ним в повному обсязі та визначають остаточну суму залишку заборгованості за Кредитом, починаючи з першого дня видачі включно, та до дня повернення кредитних коштів, що були фактично отримані Позичальником.  " HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell46"/>
        </TableRow>
        <TableRow Name="Row18" Height="47.25">
          <TableCell Name="Cell47" Text="      1.6.1.5. Всі платежі за цим Договором (погашення Кредиту, плата процентів за його користування, комісійної винагороди, штрафних санкцій тощо) здійснюються шляхом списання Банком коштів з поточного рахунку, зазначеного у п. 1.4.4." Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell48" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row28" MaxHeight="689.09" Height="311.85" AutoSize="true">
          <TableCell Name="Cell79" Text="      1.6.1.6. Сторони погоджуються з тим, що Банк не дає жодних гарантій щодо незмінності розміру процентів за користування Кредитом, а тому розмір процентів за користування Кредитом за цим Договором внаслідок настання події, яка має безпосередній вплив на вартість кредитних ресурсів, в тому числі,  залежно від зміни облікової ставки Національного банку України або процентних ставок за активними і пасивними операціями Національного банку України, зміни процентних ставок на кредитному ринку України, внаслідок прийняття компетентними державними органами України рішень, що безпосередньо впливають на стан кредитного ринку України, може бути змінений. Банк має право ініціювати зміну розміру  процентів за користування Кредитом по цьому Договору. В такому разі Банк направляє Позичальнику та Майновому поручителю (за наявності), майно якого передано в заставу (іпотеку) Банку як забезпечення виконання Зобов’язання Позичальника, а також Поручителю (за наявності)Позичальника відповідне повідомлення з зазначенням нової процентної ставки, а також причин, з якими Банк пов’язує необхідність зміни  процентів за користування Кредитом та пропозицію укласти додатковий  договір до цього Договору та договорів забезпечення виконання Зобов’язання. &#13;&#10;      1.6.1.7. Якщо запропонований Банком  розмір процентів за користування кредитом задовольнить Позичальника, та Майновий поручитель/Поручитель погодиться залишити в іпотеці Банку майно на умовах зміненої процентної ставки за користування Кредитом, то Позичальник та Майновий поручитель/Поручитель у 15 (п’ятнадцятиденний) строк з моменту отримання повідомлення, що вказане в п. 1.6.1.6. Договору, підписують з Банком додаткові договори до цього Договору та до Іпотечного договору/Договору поруки стосовно нового розміру процентної ставки. &#13;&#10;      1.6.2. Сплата нарахованих  процентів здійснюється Позичальником в національній валюті та в порядку передбаченому цим Договором.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell80"/>
        </TableRow>
        <TableRow Name="Row29" MaxHeight="689.09" Height="66.15">
          <TableCell Name="Cell81" Text="      1.6.3. Якщо сума платежу, що надійшла в погашення кредиту, менша розміру, вказаного у п. 1.5. цього Договору та Графіку платежів, то платіж та нараховані проценти, що мають бути сплаченими, вважаються простроченими. Наступна сума платежу повинна додатково включати погашення простроченого платежу та нарахованої пені за прострочені платежі з урахуванням вимог п. 5.2. цього Договору." HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell82" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row30" Height="105.84" AutoSize="true">
          <TableCell Name="Cell83" Text="      1.7. Цим Договором Позичальник доручає Банку проводити договірне списання коштів з його поточного рахунку № [Current_Accounts.CURRENT_ACC], відкритого відповідно до п. 1.4.4. цього Договору:&#13;&#10;	· коштів, що підлягають сплаті Позичальником на користь Банку за цим Договором в дату погашення згідно з Графіком платежів;&#13;&#10;	· коштів, що підлягають сплаті Позичальником на користь Банку за цим Договором при достроковому погашенні Кредиту згідно заяви Позичальника, зазначеній у п. 3.2.3. та/або при простроченні виконання зобов’язань.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell84"/>
        </TableRow>
        <TableRow Name="Row31" Height="156.87" AutoSize="true">
          <TableCell Name="Cell85" Text="      Цим Договором Позичальник також доручає Банку проводити договірне списання коштів з поточних рахунків № &lt;b&gt;[Current_Accounts.CURRENT_ACC]&lt;/b&gt;, відкритих в &lt;b&gt;[Branch_Info.NAME_BRANCH]&lt;/b&gt; у разі невиконання Позичальником своїх зобов’язань за цим Договором.&#13;&#10;      У разі, якщо поточний рахунок клієнта використовується для обслуговування двох та більше кредитів черговість списання коштів з рахунку здійснюється у порядку настання планової дати погашення кредитів у черговості, визначеній п. 1.8. цього Договору. Якщо на одну планову дату припадає погашення за двома та більше кредитами, то порядок списання здійснюється у черговості, визначеній п. 1.8. цього Договору відповідно у порядку укладення кредитних договорів. Якщо на одну планову дату припадає погашення за двома та більше кредитами, дата укладання кредитних договорів, за якими співпадає, в першу чергу списуються кошти з рахунку для погашення заборгованості за тим кредитним договором, строк дії якого/строк погашення траншу за яким коротший.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell86"/>
        </TableRow>
        <TableRow Name="Row32" Height="154.98" AutoSize="true">
          <TableCell Name="Cell87" Text="       1.8. При надходженні від Позичальника коштів у рахунок погашення Кредиту та сплати процентів за користування ним, виконання Позичальником грошових зобов’язань за цим Договором здійснюється у наступній черговості:&#13;&#10;	· погашення прострочених процентів за користування Кредитом (якщо прострочення буде мати місце);&#13;&#10;	· погашення простроченої заборгованості за Кредитом (якщо прострочення буде мати місце);&#13;&#10;	· погашення нарахованих строкових процентів за користування Кредитом ;&#13;&#10;	· погашення строкової заборгованості за Кредитом;&#13;&#10;	· сплата пені за непогашення в строк платежів по Кредиту (якщо непогашення буде мати місце);&#13;&#10;	· сплата штрафів, передбачених цим Договором;&#13;&#10;	· сплата інших платежів відповідно до умов цього Договору.&#13;&#10;" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell88"/>
        </TableRow>
        <TableRow Name="Row87" Height="122.85" AutoSize="true">
          <TableCell Name="Cell197" Text="      1.9. Банк має право самостійно без отримання попередньої згоди Позичальника змінювати послідовність погашення Зобов’язань Позичальником за цим Договором і спрямовувати кошти, отримані від Позичальника/Поручителя/Майнового поручителя та/або від реалізації майна, переданого Позичальником/Майновим поручителем/ будь-якою третьою особою в заставу/іпотеку Банку в забезпечення виконання зобов’язань Позичальника за цим Договором, на виконання Зобов’язань Позичальника без  дотримання послідовності, передбаченої  умовами п. 1.8. цього Договору. Про зміну порядку (послідовності) виконання Зобов’язання, Банк повідомляє Позичальника шляхом надання Позичальникові письмового повідомлення у відповідь на його (Позичальника) письмове звернення до Банку." HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell198" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row35" Height="185.22" AutoSize="true">
          <TableCell Name="Cell93" Text="      1.10. Погашення заборгованості, внаслідок отримання Банком грошових коштів за результатами виконання виконавчого документа, здійснюється в порядку, визначеному таким виконавчим документом.&#13;&#10;      1.11. У випадку неналежного виконання зобов’язань за цим Договором Позичальник надає згоду на розкриття Банком інформації щодо діяльності та фінансового стану Позичальника, яка стала відомою Банку у процесі обслуговування Позичальника та є банківською таємницею в обсязі, необхідному для захисту інтересів Банку та стягнення заборгованості, а також  в обсязі, необхідному для відступлення майнових прав за цим Договором та Договором застави (іпотеки) на користь інших осіб. Розкриття Банком інформації, що становить банківську таємницю, відповідно до цього пункту Договору, може бути здійснено на користь будь-яких осіб, в тому числі, але не виключно, правоохоронних органів, а також юридичних осіб, що надаватимуть послуги Банку, пов’язані з поверненням простроченої заборгованості за цим Договором.&#13;&#10;      1.12. Кредит вважається погашеним в момент зарахування грошової суми в повному обсязі на відповідний рахунок Банку.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell94" HorzAlign="Justify" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row36">
          <TableCell Name="Cell95"/>
          <TableCell Name="Cell96"/>
        </TableRow>
        <TableRow Name="Row34">
          <TableCell Name="Cell91" Text="2. ЗАБЕЗПЕЧЕННЯ ВИКОНАННЯ ЗОБОВ’ЯЗАНЬ ЗА ДОГОВОРОМ" HorzAlign="Right" Font="Arial, 10pt, style=Bold, Underline"/>
          <TableCell Name="Cell92"/>
        </TableRow>
        <TableRow Name="Row33" Height="94.5" AutoSize="true">
          <TableCell Name="Cell89" Text="      2.1. Виконання Позичальником зобов’язань за цим Договором (в тому числі і додатковими угодами до нього) забезпечується: &#13;&#10;	·нотаріально посвідченим(и) Іпотечним(и) договором (договорами) нерухомого майна – Предмету іпотеки, що розташоване за адресою: &lt;b&gt;[Mortage.MORTGAGE]&lt;/b&gt;.&#13;&#10;      2.2. Іпотечний договір (іпотечні договори)  укладається (укладаються) Банком та Позичальником (та/або Майновим поручителем) одночасно із укладанням цього Договору.&#13;&#10;" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell90" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row37">
          <TableCell Name="Cell97"/>
          <TableCell Name="Cell98"/>
        </TableRow>
        <TableRow Name="Row38">
          <TableCell Name="Cell99" Text="      3. ДОСТРОКОВЕ ПОГАШЕННЯ КРЕДИТУ" Font="Arial, 10pt, style=Bold, Underline"/>
          <TableCell Name="Cell100"/>
        </TableRow>
        <TableRow Name="Row39" Height="415.8" AutoSize="true">
          <TableCell Name="Cell101" Text="      3.1 Позичальник має право в будь-який час провести дострокове погашення всієї суми або частини суми наданого Кредиту, за умови, що нараховані Банком проценти за користування Кредитом, та всі інші суми, в тому числі і комісійні винагороди, які повинні бути сплачені, будуть сплачені Позичальником в той же час. В разі дострокового повернення Позичальником частини суми заборгованості за Кредитом Банк зобов’язаний здійснити відповідне коригування кредитних зобов’язань Позичальника в бік її зменшення.&#13;&#10;      3.2. Сторони домовились встановити наступний порядок дострокового погашення Кредиту:&#13;&#10;      3.2.1. Достроковим повним погашенням вважається повернення Позичальником суми Кредиту та нарахованих процентів за його користуванням в повному обсязі на дату погашення до настання кінцевого терміну погашення за цим Договором.&#13;&#10;      3.2.2. Достроковим частковим погашенням Кредиту вважається сплата Позичальником чергових платежів у розмірах більших, ніж це передбачено Графіком платежів.&#13;&#10;      3.2.3. В разі прийняття Позичальником рішення здійснити дострокове повне або часткове погашення Кредиту, він зобов’язується письмово попередити Банк про намір здійснити дострокове погашення не менш ніж за 5 (п’ять) календарних днів. &#13;&#10;      3.2.4. При достроковому погашенні Кредиту нарахування та сплата процентів за користування Кредитом здійснюється у відповідності до положень п. 1.6. цього Договору. &#13;&#10;      3.2.5. Здійснення дострокового часткового погашення Кредиту у поточному платіжному періоді не звільняє Позичальника від сплати чергового платежу у наступному платіжному періоді.&#13;&#10;      3.2.6. Впродовж 1 (одного) календарного дня після здійснення Позичальником дострокового часткового погашення  Кредиту Сторони, залежно від змісту поданої Позичальником відповідно до п. 3.2.3. заяви, вносять наступні зміни до цього Договору:&#13;&#10;	· в частині зміни терміну остаточного погашення Кредиту (в бік зменшення) із встановленням відповідного нового Графіку платежів – у разі прийняття Позичальником рішення збереження розміру чергових платежів по Кредиту, що визначені у п. 1.5. цього Договору; &#13;&#10;	· в частині перерахунку розміру чергових платежів по Кредиту із встановленням відповідного нового Графіку платежів – у разі прийняття Позичальником рішення збереження первинного терміну остаточного погашення Кредиту. &#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell102"/>
        </TableRow>
        <TableRow Name="Row91" Height="62.37">
          <TableCell Name="Cell205" Text="      3.2.7. У разі, якщо Позичальником, після здійснення дострокового часткового погашення  Кредиту, не укладено з Банком додатковий договір відповідно до п. 3.2.6. цього Договору, у будь-якому разі Банк здійснює перерахунок Графіка платежів із скороченням первинного терміну остаточного погашення Кредиту, новий Графік платежів надається Позичальнику за першою вимогою." Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell206" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row40" Height="64.26" AutoSize="true">
          <TableCell Name="Cell103" Text="      3.3. При достроковому погашенні Кредиту або будь-якої його частини платежі за цим Договором вносяться Позичальником в такій послідовності: &#13;&#10;	· нараховані  проценти за час фактичного користування Кредитом, &#13;&#10;	· основна сума Кредиту." HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell104" HorzAlign="Justify" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row41" Height="325.08" AutoSize="true">
          <TableCell Name="Cell105" Text="      3.4. Банк має право вимагати від Позичальника дострокового повернення суми Кредиту в цілому, або у визначеній Банком частині, сплати процентів за його користування та інших платежів, що належать до сплати за цим Договором, у випадку невиконання або неналежного виконання Позичальником будь-яких зобов’язань за цим Договором або невиконання чи неналежного виконання Позичальником та або Майновим поручителем будь-яких зобов’язань за Іпотечним договором, в тому числі, але не виключно, у випадку:&#13;&#10;	· затримання сплати частини Кредиту та/або процентів за користування Кредитом  та інших платежів за цим Договором на три календарні місяці;&#13;&#10;	· використання Кредиту не за цільовим призначенням;&#13;&#10;	· виникнення обставин, які за обґрунтованим висновком Банку призведуть до того, що Позичальник не виконає свої зобов’язання по цьому Договору;&#13;&#10;	· втрати Предметом іпотеки ліквідності, позбавлення Позичальника прав на отримання Предмету іпотеки у власність або права власності на Предмет іпотеки відповідно до умов Іпотечного договору, виникнення обставин, що унеможливлюють звернення стягнення на Предмет іпотеки; &#13;&#10;	· неподання Позичальником, на вимогу Банку, даних, що стосуються його фінансового стану; &#13;&#10;	· в разі порушення Позичальником умов Іпотечного договору, в тому числі в разі несплати страхових платежів, порушення умов зберігання Предмету іпотеки, що призвело до пошкодження, знищення та втрати його вартості, що обумовлена Іпотечним договором;&#13;&#10;	· у разі невиконання Позичальником обов’язку визначеного п. 2.2. цього Договору. &#13;&#10;      3.5. Виконання Позичальником вимоги Банку щодо дострокового повернення суми Кредиту належних до сплати процентів, та інших платежів відповідно до умов цього Договору повинно бути проведено Позичальником протягом 60 (шістдесяти) календарних днів з дати одержання такої   вимоги від Банку" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell106"/>
        </TableRow>
        <TableRow Name="Row42">
          <TableCell Name="Cell107"/>
          <TableCell Name="Cell108"/>
        </TableRow>
        <TableRow Name="Row43">
          <TableCell Name="Cell109" Text="      4. ПРАВА ТА ЗОБОВ’ЯЗАННЯ СТОРІН " Font="Arial, 10pt, style=Bold, Underline"/>
          <TableCell Name="Cell110"/>
        </TableRow>
        <TableRow Name="Row44">
          <TableCell Name="Cell111" Text="      4.1. Зобов’язання Банку:&#13;&#10;" Font="Arial, 10pt, style=Bold"/>
          <TableCell Name="Cell112"/>
        </TableRow>
        <TableRow Name="Row45" Height="321.3" AutoSize="true">
          <TableCell Name="Cell113" Text="      4.1.1. При умові виконання «Загальних умов надання Кредиту», надати Позичальнику Кредит у порядку і на умовах, викладених в цьому Договорі.&#13;&#10;      4.1.2. Відкрити Позичальнику рахунок для обліку наданого Кредиту та обліку нарахованих процентів за Кредитом відповідно до правил, що діють у Банку.&#13;&#10;      4.1.3. Відкрити Позичальнику поточний рахунок згідно з п. 1.4.4.&#13;&#10;      4.1.4. Здійснити видачу Кредиту безготівково шляхом перерахування коштів у національній валюті з рахунку наданого кредиту  на поточний (вкладний) рахунок Продавця на підставі платіжного доручення Позичальника (або готівкою  шляхом надання Позичальнику  готівки з каси Банку)  на цілі, передбачені п. 1.2. цього Договору.&#13;&#10;      4.1.5. Забезпечити Позичальника консультативними послугами з питань укладення та виконання цього Договору.&#13;&#10;      4.1.6.Здійснювати нарахування процентів за користування Кредитом з моменту його видачі та до терміну, зазначеного п. 1.6.1.1. цього Договору.&#13;&#10;      4.1.7. За вимогою Позичальника надавати йому довідку щодо стану заборгованості за  цим Договором. Вартість послуг Банку щодо надання довідки щодо стану заборгованості за цим Договором визначена в Додатку №3, що є невід’ємною його частиною.&#13;&#10;      4.1.8. Не розголошувати інформацію щодо діяльності та фінансового стану Позичальника, яка становить банківську таємницю, за винятком випадків, коли розкриття банківської таємниці без погодження з Позичальником є обов’язковим для Банку у відповідності з чинним законодавством України та у випадках, передбачених п. 1.11., п. 4.2.7. та п. 7.6. цього Договору.&#13;&#10;      4.1.9. У разі передачі Банком своїх прав за цим Договором третій стороні - завчасно повідомити Позичальника про таку передачу.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell114"/>
        </TableRow>
        <TableRow Name="Row46">
          <TableCell Name="Cell115" Text="      4.2. Права Банку:" Font="Arial, 10pt, style=Bold"/>
          <TableCell Name="Cell116"/>
        </TableRow>
        <TableRow Name="Row86" Height="110.75" AutoSize="true">
          <TableCell Name="Cell195" Text="      4.2.1. Вимагати від Позичальника належного виконання останнім взятих на себе зобов’язань по цьому Договору.&#13;&#10;      4.2.2. У разі порушення Позичальником умов цього Договору та/або Іпотечного договору стягнути Кредит та суму нарахованих процентів, та інших платежів, що достроково підлягають сплаті.&#13;&#10;      4.2.3. При виникненні простроченої заборгованості за Кредитом чи процентами, а також в інших випадках, передбачених цим Договором, вимагати дострокового повернення Кредиту, нарахованих процентів та інших платежів за цим Договором, та стягнути заборгованість за цим Договором в примусовому порядку, в тому числі, шляхом звернення стягнення на Предмет іпотеки за Іпотечним(-и) договором(-ами). " HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell196"/>
        </TableRow>
        <TableRow Name="Row92" Height="66.15">
          <TableCell Name="Cell207" Text="      4.2.4. Вимагати від Позичальника укладення нових договорів забезпечення  виконання зобов’язань за цим Договором у разі, якщо укладені договори не в повному обсязі відповідно до правил Банку забезпечують повернення отриманого Позичальником Кредиту згідно з умовами цього Договору або Предмет іпотеки втратив ліквідність. Висновок Банку щодо необхідності укласти нові договори є безспірним і не підлягає оскарженню." HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell208" HorzAlign="Justify" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row89" Height="66.15" AutoSize="true">
          <TableCell Name="Cell201" Text="      4.2.5. У разі відмови Позичальника щодо укладення договорів та/або додаткових угод, у випадках, передбачених п.           4.2.4. цього Договору, а також неукладення таких договорів (додаткових угод) в строки, визначені Банком, Банк має право вимагати від Позичальника дострокового повернення Кредиту, нарахованих процентів та інших платежів, що достроково підлягають сплаті." HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell202" HorzAlign="Justify" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row47" Height="217.35" AutoSize="true">
          <TableCell Name="Cell117" Text="      4.2.6. Банк має право вимагати від Позичальника дострокового повернення Кредиту, нарахованих процентів та інших платежів, що достроково підлягають сплаті у випадку невиконання Позичальником зобов’язань щодо сплати страхових платежів за договором страхування, в порядку, передбаченому п. 3.5. цього Договору.  &#13;&#10;      4.2.7. У випадку неналежного виконання зобов’язань за цим Договором Позичальник надає згоду на розкриття Банком інформації щодо діяльності та фінансового стану Позичальника, яка стала відомою Банку у процесі обслуговування Позичальника та є банківською таємницею в обсязі, необхідному для захисту інтересів Банку та стягнення заборгованості, а також в обсязі, необхідному для відступлення майнових прав за цим Договором та Іпотечним договором на користь інших осіб.&#13;&#10;      4.2.8. Банк має право припинити видачу Кредиту у разі істотної зміни кон’юнктури кредитного ринку або інших фінансових ринків, якщо така зміна може спричинити істотне погіршення фінансового результату від виконання кредитних операцій за цим Договором для Банку відносно очікуваного фінансового результату на момент укладення цього Договору.&#13;&#10;      4.2.9. У випадку настання страхових випадків за договорами страхування, отримати страхове відшкодування для покриття суми заборгованості за цим Договором." HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell118" HorzAlign="Justify" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row48">
          <TableCell Name="Cell119" Text="      4.3. Зобов’язання Позичальника:" Font="Arial, 10pt, style=Bold"/>
          <TableCell Name="Cell120"/>
        </TableRow>
        <TableRow Name="Row49" Height="47.25" AutoSize="true">
          <TableCell Name="Cell121" Text="      4.3.1. Належним чином виконувати взяті на себе зобов’язання за цим Договором.&#13;&#10;      4.3.2. Використовувати Кредит на цілі та у строки, визначені цим Договором.&#13;&#10;      4.3.3. Повернути Кредит в сумі &lt;b&gt;[DataSubProdukt.SUMM] ([DataSubProdukt.SSUM_PR])&lt;/b&gt; гривень." HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell122"/>
        </TableRow>
        <TableRow Name="Row50" Height="122.85" AutoSize="true">
          <TableCell Name="Cell123" Text="      4.3.4. Точно в строки, обумовлені цим Договором, погашати Кредит та своєчасно сплачувати нараховані проценти за користування Кредитом, комісійні винагороди, а у випадку неналежного виконання взятих на себе зобов’язань по цьому Договору на першу вимогу Банку сплатити штрафні санкції, як це передбачено в Договорі.&#13;&#10;      4.3.5. У разі порушення умов цього Договору та/або Іпотечного договору на вимогу Банку достроково повернути Кредит з одночасною сплатою процентів за фактичний час користування кредитними ресурсами, комісійних винагород та штрафних санкцій в порядку, передбаченому п. 3.4., п. 3.5. цього Договору відповідно до чинного законодавства України. Сторони домовилися, що розмір збитків визначатиметься Банком самостійно, на що Позичальник цим надає свою згоду. " HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell124" HorzAlign="Justify" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row88" Height="122.85" AutoSize="true">
          <TableCell Name="Cell199" Text="      4.3.6. Відповідати всіма власними коштами та майном по своїх зобов’язаннях, що випливають з цього Договору.&#13;&#10;      4.3.7. Позичальник зобов’язується з метою можливості реалізації Банком наданого йому права договірного списання коштів з поточних/карткових рахунків Позичальника,  відкритих в інших банках, передбачати в укладених з іншими банківськими установами право Банку здійснювати договірне списання коштів з його поточних/карткових рахунків у випадку невиконання Позичальником грошових зобов’язань за цим Договором відповідно до вимог Інструкції НБУ «Про безготівкові розрахунки в Україні в національній валюті», затвердженої Постановою Правління НБУ № 22 від 21.01.2004 року (із змінами та доповненнями) та надавати належним чином завірені копії таких договорів Банку протягом 2 (двох) робочих днів з моменту їх укладення." HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell200"/>
        </TableRow>
        <TableRow Name="Row51" Height="170.1" AutoSize="true">
          <TableCell Name="Cell125" Text="      4.3.8. При сплаті останнього платежу за Кредитом на підставі звірки нарахованих і сплачених процентів сплатити суму недоотриманих Банком процентів. У разі якщо Позичальник сплатив суму, що перевищує нараховані проценти, то надлишок сплачених авансом процентів направляється у погашення основного боргу за Кредитом.&#13;&#10;      4.3.9. З метою здійснення Банком контролю за фінансовим станом та кредитоспроможністю Позичальника, за станом забезпечення повернення Кредиту протягом дії цього Договору, надавати Банку кожні 12 (дванадцять) місяців з моменту укладення цього Договору, або на першу вимогу Банку, документи, що підтверджують фінансовий стан Позичальника, а в разі прострочення Позичальником строку повернення будь-якої частини Кредиту та/або сплати процентів за його користування, негайно подати Банку документи, що підтверджують фінансовий стан Позичальника або документи, що підтверджують причини, що об’єктивно унеможливлюють виконання зобов’язань Позичальника за цим Договором. В разі настання будь-яких страхових випадків, визначених договорами страхування письмово повідомити Кредитора протягом 2 (двох) робочих днів.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell126" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row52" Height="113.4" AutoSize="true">
          <TableCell Name="Cell127" Text="      4.3.10. У 30 (тридцяти) денний строк повідомити Банк про зміну реєстрації свого постійного місця проживання (реєстрації),  зміни місця працевлаштування, прізвища, імені, по-батькові, та інші обставини, що можуть вплинути на виконання зобов’язань за цим Договором.&#13;&#10;      4.3.11. Впродовж строку дії цього Договору своєчасно та самостійно ознайомлюватись із Тарифами Банку, що пов’язані із обслуговуванням та погашенням Кредиту, в [Branch_Info.NAME_BRANCH]  установі Банку.&#13;&#10;      4.3.12. Завчасно забезпечувати наявність коштів для сплати чергових платежів за цим Кредитним договором на поточному рахунку, визначеному п. 1.4.4. цього Договору до платіжної дати, визначеної Графіком платежів.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell128" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row93" Height="66.15">
          <TableCell Name="Cell209" Text="      4.3.13. До моменту отримання кредитних коштів надати документи, що підтверджують виконання «Загальних умов надання Кредиту».&#13;&#10;      4.3.14. Після здійснення дострокового часткового погашення  Кредиту впродовж 1 (одного) календарного дня укласти з Банком Додатковий договір до цього Договору відповідно до п. 3.2.6. цього Договору." Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell210" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row53" Height="91.85" AutoSize="true">
          <TableCell Name="Cell129" Text="      4.3.15. Протягом строку дії цього Договору без попередньої письмової згоди Банку:&#13;&#10;	• не укладати будь-які інші кредитні договори чи договори позики, договори поруки, фінансової допомоги (їх різновидів) або інші договори, що встановлюють зобов’язання для Позичальника з повернення грошових коштів;&#13;&#10;	• не відчужувати, не продавати, не здавати в оренду (найм), не встановлювати інші обтяження щодо власного майна.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell130" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row54">
          <TableCell Name="Cell131" Text="      4.4. Права Позичальника:" VertAlign="Bottom" Font="Arial, 10pt, style=Bold"/>
          <TableCell Name="Cell132"/>
        </TableRow>
        <TableRow Name="Row56" Height="81.27" AutoSize="true">
          <TableCell Name="Cell135" Text="      4.4.1. В порядку, передбаченому цим Договором, одержувати від Банку інформацію про стан заборгованості за Кредитом, процентами за його користування, комісійними винагородами та іншими платежами за цим Договором. &#13;&#10;      4.4.2. Порушувати перед Банком питання про перенесення строку погашення Кредиту у разі виникнення тимчасових фінансових або інших ускладнень з незалежних від нього (Позичальника) причин.&#13;&#10;      4.4.3. Достроково погасити Кредит або будь-яку його частину в порядку, передбаченому цим Договором.   &#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell136"/>
        </TableRow>
        <TableRow Name="Row57">
          <TableCell Name="Cell137"/>
          <TableCell Name="Cell138"/>
        </TableRow>
        <TableRow Name="Row58">
          <TableCell Name="Cell139" Text="      5. ВІДПОВІДАЛЬНІСТЬ СТОРІН" Font="Arial, 10pt, style=Bold, Underline"/>
          <TableCell Name="Cell140"/>
        </TableRow>
        <TableRow Name="Row60" Height="189" AutoSize="true">
          <TableCell Name="Cell143" Text="      5.1. За невиконання або неналежне виконання взятих на себе зобов’язань по цьому Договору, Сторони несуть відповідальність в порядку та на умовах, обумовлених в цьому Договорі та чинним законодавством України та відшкодовують завдані збитки понад неустойку (пеню, штраф). &#13;&#10;      5.2. За порушення взятих на себе зобов’язань по поверненню Кредиту та своєчасній сплаті процентів за користування Кредитом, комісійної винагороди та інших платежів за цим Договором, Позичальник зобов’язується сплатити на користь Банку пеню в розмірі подвійної облікової ставки Національного банку України, яка діяла в період, за який сплачується пеня, від суми платежу за кожний день прострочення.&#13;&#10;      5.3. У разі документального підтвердження використання Позичальником Кредиту не за цільовим призначенням, Позичальник повинен сплатити на користь Банку штраф в розмірі 5 (п’ять) процентів від суми Кредиту, використаного не за цільовим призначенням, обумовленим цим Договором.&#13;&#10;      5.4. У разі ненадання Позичальником документів, що передбачені умовами цього Договору, Позичальник повинен сплатити на користь Банку неустойку в розмірі 0,05 (нуль цілих п’ять сотих) процентів від суми Кредиту.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell144" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row61">
          <TableCell Name="Cell145"/>
          <TableCell Name="Cell146"/>
        </TableRow>
        <TableRow Name="Row62">
          <TableCell Name="Cell147" Text="      6. ПОРЯДОК РОЗГЛЯДУ СПОРІВ" Font="Arial, 10pt, style=Bold, Underline"/>
          <TableCell Name="Cell148"/>
        </TableRow>
        <TableRow Name="Row63" Height="66.15" AutoSize="true">
          <TableCell Name="Cell149" Text="      6.1. У разі виникнення між Сторонами спорів за цим Договором або у зв’язку з ним, Сторони зроблять все необхідне для врегулювання вказаних спорів  шляхом переговорів.&#13;&#10;      6.2. Спори за цим Договором, врегулювання яких неможливо досягнути шляхом переговорів протягом 30 (тридцяти) календарних днів, вирішуються в судовому порядку відповідно до чинного законодавства України.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell150" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row64">
          <TableCell Name="Cell151"/>
          <TableCell Name="Cell152"/>
        </TableRow>
        <TableRow Name="Row65">
          <TableCell Name="Cell153" Text="      7. ОСОБЛИВІ ПОЛОЖЕННЯ ДОГОВОРУ" Font="Arial, 10pt, style=Bold, Underline"/>
          <TableCell Name="Cell154"/>
        </TableRow>
        <TableRow Name="Row66" Height="109.62" AutoSize="true">
          <TableCell Name="Cell155" Text="      7.1. Цей Договір може бути змінений або доповнений за взаємною згодою Сторін. &#13;&#10;      7.2. Всі зміни та доповнення до цього Договору викладаються в письмовій формі та набувають чинності з моменту їх підписання.&#13;&#10;      7.3. Будь-які повідомлення, які направляються Сторонами одна одній в рамках цього Договору, повинні бути здійснені в письмовій формі та будуть вважатись поданими належним чином, якщо вони надіслані листом на замовлення або доставлені особисто на адресу Сторін, окрім випадків, що передбачені окремими положеннями цього Договору. &#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell156" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row67" Height="124.74" AutoSize="true">
          <TableCell Name="Cell157" Text="      7.4. Додаткові договори до цього Договору, які можуть бути укладені в майбутньому, є його невід’ємною частиною.&#13;&#10;      7.5.	При здійсненні Позичальником останнього платежу в погашення заборгованості за Кредитом останній (Позичальник), спільно з Банком здійснюють звірку проведених розрахунків, уточнюють наявність чи відсутність заборгованості перед Банком по Кредиту, процентах, комісійній винагороді та інших платежах.&#13;&#10;      7.6.	Банк повідомляє Позичальника, а Позичальник цим надає згоду на те, що інформація про Позичальника зберігатиметься в центральній базі даних Національного банку України, а також в інших базах даних, що передбачені чинним законодавством України.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell158" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row90" Height="66.15" AutoSize="true">
          <TableCell Name="Cell203" Text="      7.7. Позичальник надає Банку необмежену строком письмову згоду на обробку його персональних даних в обсязі, що міститься в цьому Договорі та інших документах, підписаних/засвідчених та поданих ним Банку для отримання Кредиту згідно цього Договору, та/або буде отримано Банком під час обслуговування Кредиту від Позичальника або третіх осіб, у т.ч. змінювати його персональні дані за інформацією від третіх осіб. " HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell204" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row68" Height="113.4" AutoSize="true">
          <TableCell Name="Cell159" Text="      Також, Позичальник надає Банку згоду поширювати його персональні дані та/або здійснювати їх передачу чи надання доступу до них третім особам у випадках, передбачених законодавством України, цим договором та/або договорами, що укладені (будуть укладені) між Позичальником та Банком. Без отримання додаткової письмової згоди Позичальник надає право Банку передавати його персональні данні для обробки третім особам та здійснювати відносно його персональних даних будь-які інші дії, якщо це пов’язано із виконанням укладених між Позичальником та Банком договорів та/або із захистом прав Банку за ними, або необхідно для реалізації повноважень Банку за законом. &#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell160" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row69" Height="156.87" AutoSize="true">
          <TableCell Name="Cell161" Text="      7.8. Банк повідомляє Позичальника, а Позичальник цим надає згоду на те, що інформація про Позичальника надаватиметься товариству з обмеженою  відповідальністю «Перше всеукраїнське бюро кредитних історій», яке знаходиться за адресою: 02002, м. Київ, вул. М.Раскової, 11, в обсязі, необхідному для формування кредитної історії відповідно до Закону України «Про організацію формування та обігу кредитних історій».&#13;&#10;      7.9. Сторони домовились про збільшення строків позовної давності відповідно до ч.1 ст. 259 Цивільного кодексу України до 3 (трьох) років для всіх грошових зобов’язань Позичальника (в тому числі, але не виключно, щодо повернення суми Кредиту, сплати процентів за його користування, комісійної винагороди, штрафів, пені), що передбачені умовами цього Договору.&#13;&#10;      7.10. Цей Договір може бути розірваний за взаємною згодою Сторін шляхом підписання додаткового договору про розірвання цього Договору або в порядку встановленому чинним законодавством України.&#13;&#10;&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell162" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row70">
          <TableCell Name="Cell163"/>
          <TableCell Name="Cell164"/>
        </TableRow>
        <TableRow Name="Row71">
          <TableCell Name="Cell165" Text="      8. ЗАКЛЮЧНІ ПОЛОЖЕННЯ" Font="Arial, 10pt, style=Bold, Underline"/>
          <TableCell Name="Cell166"/>
        </TableRow>
        <TableRow Name="Row72" Height="122.85" AutoSize="true">
          <TableCell Name="Cell167" Text="      8.1. Цей Договір набуває чинності з моменту його підписання та діє до повного виконання Сторонами взятих на себе зобов’язань по цьому Договору. &#13;&#10;      8.2. У випадках, не передбачених цим Договором, Сторони керуються чинним законодавством України.&#13;&#10;      8.3. Цей Договір складений у 2 (двох) оригінальних примірниках (один для Банку, один для Позичальника), що мають однакову юридичну силу та є автентичним за змістом.&#13;&#10;      8.4. Сторони зобов’язані вчасно повідомляти одна одну про зміни адреси, місця проживання, номерів телефонів, всі інші зміни, які здатні вплинути на реалізацію Договору та виконання зобов’язань по ньому.&#13;&#10;      8.5. Невід’ємною частиною цього договору є Додаток №1, №2, №3.&#13;&#10;" HorzAlign="Justify" Font="Times New Roman, 10pt" ColSpan="2"/>
          <TableCell Name="Cell168" Font="Times New Roman, 10pt"/>
        </TableRow>
        <TableRow Name="Row73">
          <TableCell Name="Cell169"/>
          <TableCell Name="Cell170"/>
        </TableRow>
        <TableRow Name="Row74">
          <TableCell Name="Cell171" Text="      9. РЕКВІЗИТИ ТА ПІДПИСИ СТОРІН" Font="Arial, 10pt, style=Bold, Underline"/>
          <TableCell Name="Cell172"/>
        </TableRow>
        <TableRow Name="Row75" Height="37.8">
          <TableCell Name="Cell173" Text="Банк:" VertAlign="Center" Font="Arial, 10pt, style=Bold"/>
          <TableCell Name="Cell174" Text="Позичальник:&#13;&#10;Фізична особа (ПІБ) " Font="Arial, 10pt, style=Bold"/>
        </TableRow>
        <TableRow Name="Row76" Height="113.4">
          <TableCell Name="Cell175" Text="Адреса: [Branch_Info.ADR_BRANCH],&#13;&#10;МФО: [Branch_Info.MFO],&#13;&#10;код ЄДРПОУ: [Branch_Info.OKPO]. " Font="Arial, 10pt, style=Bold"/>
          <TableCell Name="Cell176" Text="[klient_info.FIO]&#13;&#10;[klient_info.PASPORT]&#13;&#10;[klient_info.ADRESS]&#13;&#10;Реєстраційний номер облікової картки платника податку [klient_info.IDCODE]" Font="Arial, 10pt, style=Bold"/>
        </TableRow>
        <TableRow Name="Row77">
          <TableCell Name="Cell177"/>
          <TableCell Name="Cell178"/>
        </TableRow>
        <TableRow Name="Row78">
          <TableCell Name="Cell179"/>
