        <CommandParameter Name="p_ws_id" DataType="126" Expression="[p_ws_id]" DefaultValue="&quot;MAIN&quot;"/>
        <CommandParameter Name="p_ws_number" DataType="107" Expression="[p_ws_number]" DefaultValue="0"/>
        <CommandParameter Name="p_bid_id" DataType="107" Expression="[p_bid_id]" DefaultValue="-1"/>
      </TableDataSource>
      <TableDataSource Name="Table8" Alias="Target_Many" DataType="System.Int32" Enabled="true" SelectCommand="select ( &#10; case  when to_number(wcs_utl.get_answ(:p_bid_id, 'PI_FREE_SELECTED'))=1 then &#10;' шляхом перерахування коштів в національній валюті з рахунку наданого кредиту на поточний (вкладний) рахунок Продавця '||&#10;wcs_utl.get_answ(:p_bid_id, 'CL_2_0_02', 'MAIN' , 0 )||' - '||&#10;wcs_utl.get_answ(:p_bid_id, 'PI_FREE_NAME')||' (ПІБ Продавця) № '||&#10;wcs_utl.get_answ(:p_bid_id, 'PI_FREE_NLS')||&#10;' відкритий в '||&#10;nvl((select trim(fullName)  from rcukru where mfo=wcs_utl.get_answ(:p_bid_id, 'PI_FREE_MFO') ),'')||&#10;' Банку,'||    &#10;' МФО '||wcs_utl.get_answ(:p_bid_id, 'PI_FREE_MFO')||&#10;' на підставі платіжного доручення Позичальника'&#10;else&#10;' готівкою шляхом надання Позичальнику готівки з каси Банку'&#10;end) target from dual&#10;&#10;">
        <Column Name="TARGET" DataType="System.String"/>
        <CommandParameter Name="P1" DataType="126" Expression="[p_bid_id]"/>
      </TableDataSource>
      <TableDataSource Name="Table9" Alias="Garantees" DataType="System.Int32" Enabled="true" SelectCommand="select concatstr(&#13;&#10;--select&#13;&#10;  (case when ws_id='GRT_MORTGAGE' then  &#13;&#10; 'з прийняттям в іпотеку нерухомого майна, яке  '||(case when val_bool!=1    &#13;&#10; then&#13;&#10; 'є власністю  Майнового поручителя – '||&#13;&#10; wcs_utl.get_answ(bid_id, 'CL_1', ws_id , ws_number )||' '||&#13;&#10; wcs_utl.get_answ(bid_id, 'CL_2', ws_id , ws_number )||' '||&#13;&#10; wcs_utl.get_answ(bid_id, 'CL_3', ws_id , ws_number )||' і '&#13;&#10; else &#13;&#10; ' '&#13;&#10; end)||&#13;&#10;'знаходиться за адресою : '||&#13;&#10; wcs_utl.get_answ(bid_id, 'GRT_4_6', ws_id , ws_number )||&#13;&#10; ' буд. '||(wcs_utl.get_answ(bid_id, 'GRT_4_8', ws_id , ws_number ))||&#13;&#10;' кв. '||(wcs_utl.get_answ(bid_id, 'GRT_4_2', ws_id , ws_number ))--||'; ' --)&#13;&#10;||'; із укладенням договору страхування предмету іпотеки на користь Банку;'&#13;&#10;when ws_id='GRT_VEHICLE' then --GRT_VEHICLE&#13;&#10;'договору застави рухомого майна, а саме: '||&#13;&#10;wcs_utl.get_answ(bid_id, 'GRT_2_7', 'GRT_VEHICLE' ,  ws_number)||' '||&#13;&#10;', Марка, модель ТЗ: '||wcs_utl.get_answ(bid_id, 'GRT_3_2', 'GRT_VEHICLE' ,  ws_number)||' '||&#13;&#10;', Реєстраційний номер ТЗ: '||wcs_utl.get_answ(bid_id, 'GRT_2_7', 'GRT_VEHICLE' ,  ws_number)||' '||&#13;&#10;'; із укладенням договору страхування предмету застави на користь Банку;'&#13;&#10;when ws_id='GRT_DEPOSIT' then --GRT_VEHICLE&#13;&#10;'договору застави депозиту, а саме: '||&#13;&#10;'; із укладенням договору страхування предмету застави на користь Банку;'&#13;&#10;when ws_id='GRT_MORTGAGE_LAND' then -- GRT_MORTGAGE_LAND&#13;&#10;'договору застави земельної ділянки, а саме: '||&#13;&#10;'що  знаходиться за адресою : '||&#13;&#10; wcs_utl.get_answ(bid_id, 'GRT_4_6', ws_id , ws_number )||&#13;&#10; ' буд. '||(wcs_utl.get_answ(bid_id, 'GRT_4_8', ws_id , ws_number ))||&#13;&#10;' кв. '||(wcs_utl.get_answ(bid_id, 'GRT_4_2', ws_id , ws_number ))|| --)&#13;&#10;'; із укладенням договору страхування предмету застави на користь Банку;'&#13;&#10;when ws_id='GRT_GUARANTOR' then -- GRT_GUARANTOR-&#13;&#10;'договору поруки :'||&#13;&#10; wcs_utl.get_answ(bid_id, 'CL_1', ws_id , ws_number )||' '||&#13;&#10; wcs_utl.get_answ(bid_id, 'CL_2', ws_id , ws_number )||' '||&#13;&#10; wcs_utl.get_answ(bid_id, 'CL_3', ws_id , ws_number )||'  '&#13;&#10;when ws_id='GRT_PRODUCT' then -- GRT_PRODUCT&#13;&#10;'договору застави промислових товарів; '&#13;&#10;when ws_id='GRT_VALUABLES' then -- GRT_VALUABLES&#13;&#10;'договору застави цінностей; '&#13;&#10;else &#13;&#10;bid_id||'  '||ws_id&#13;&#10;end))&#13;&#10;GARANTEES&#13;&#10;--MORTGAGE &#13;&#10;from WCS_ANSWERS where --ws_id='GRT_MORTGAGE' &#13;&#10;--and &#13;&#10;question_id='GRT_0' &#13;&#10;and  &#13;&#10;bid_id=:p_bid_id">
        <Column Name="GARANTEES" DataType="System.String"/>
        <CommandParameter Name="P1" DataType="126" Expression="[p_bid_id]"/>
      </TableDataSource>
      <TableDataSource Name="Table11" Alias="Guarantors" DataType="System.Int32" Enabled="true" SelectCommand="select bg.garantee_num,&#13;&#10;       wcs_utl.get_answ_text(bg.bid_id, 'CL_1', bg.ws_id, bg.garantee_num) || ' ' ||&#13;&#10;       wcs_utl.get_answ_text(bg.bid_id, 'CL_2', bg.ws_id, bg.garantee_num) || ' ' ||&#13;&#10;       wcs_utl.get_answ_text(bg.bid_id, 'CL_3', bg.ws_id, bg.garantee_num) as fio,&#13;&#10;       wcs_utl.get_answ_text(bg.bid_id,&#13;&#10;                             'CL_123_R',&#13;&#10;                             bg.ws_id,&#13;&#10;                             bg.garantee_num) as fio_r,&#13;&#10;       wcs_utl.get_answ_text(bg.bid_id,&#13;&#10;                             'CL_123_O',&#13;&#10;                             bg.ws_id,&#13;&#10;                             bg.garantee_num) as fio_o&#13;&#10;  from v_wcs_bid_garantees bg&#13;&#10; where bg.bid_id = :p_bid_id&#13;&#10;   and bg.garantee_id = 'GUARANTOR'&#13;&#10; order by bg.garantee_num">
        <Column Name="GARANTEE_NUM" DataType="System.Decimal"/>
        <Column Name="FIO_R" DataType="System.String"/>
        <Column Name="FIO" DataType="System.String"/>
        <Column Name="FIO_O" DataType="System.String"/>
        <CommandParameter Name="p_bid_id" DataType="107" Expression="[p_bid_id]" DefaultValue="-1"/>
      </TableDataSource>
    </OracleDataConnection>
