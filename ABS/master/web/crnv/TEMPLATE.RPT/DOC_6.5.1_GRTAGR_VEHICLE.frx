      <TableDataSource Name="Table14" Alias="DataDogStrah" DataType="System.Int32" Enabled="true" SelectCommand="select &#10;(case when  nvl(a.bid_id,0)&lt;&gt;0 then &#10;'та Договір відновлюваної кредитної лінії від '|| &#10;substr(f_dat_lit(  d.sdate ),1,25) ||&#10;' № '||to_char(a.bid_id)||', '||&#10;'укладений між Заставодержателем та Заставодавцем, а також усі додаткові угоди, додатки, '||&#10;'зміни та доповнення до нього, які чинні на момент укладання цього Договору та можуть ' || &#10;'бути укладені після його (цього Договору) укладання.'&#10;else&#10;' нет договора ' &#10;end) Pu1,&#10;(case when  nvl(a.bid_id,0)&lt;&gt;0 then &#10;'&#10;- повернути Заставодержателю кредит у розмірі ' ||&#10;to_char(b.summ,'999999999999.99')  || ' ( ' || f_sumpr(b.summ*100,t.kv, T.GENDER) ||&#10;' ) гривень у строк та на умовах, зазначених у Договорі відновлюваної кредитної лінії;'&#10;else&#10;' нет договора ' &#10;end) Pu2,&#10;(case when  nvl(a.bid_id,0)&lt;&gt;0 then &#10;'та Договором відновлюваної кредитної лінії  № '||to_char(a.bid_id)||' від '||substr(f_dat_lit(  d.sdate ),1,25)&#10;else&#10;' нет договора ' &#10;end) Pu3&#10;from  WCS_ANSWERS a, v_wcs_bids b, tabval t, cc_deal d  &#10;where    &#10;b.bid_id=d.nd(+)  and B.CREDIT_CURRENCY =T.lcv and&#10;a.bid_id=b.bid_id  AND   a.question_id= 'CL_MAIN_CRD_ID' and a.val_numb=:p_bid_id">
        <Column Name="PU1" DataType="System.String"/>
        <Column Name="PU2" DataType="System.String"/>
        <Column Name="PU3" DataType="System.String"/>
        <CommandParameter Name="P1" DataType="126" Expression="[p_bid_id]"/>
      </TableDataSource>
