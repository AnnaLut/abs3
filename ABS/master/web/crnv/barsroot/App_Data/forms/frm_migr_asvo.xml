<xmlform captiontype="OnlyTitle">
  <grids>
    <grid table="v_migr_asvo" id="gv" pagesize="10" showpagesizebox="true" title="Замигровани АСВО" showfilter="true">
     <rowselection method="SingleRow"/>
      <fields>
        <field name="TVBV" key="true" sort="Default" datatype="String">
          <column show="true" caption="Код ТВБВ" align="Center"/>
           <form show="true">
            <uicontrol type="TextBox"/>
          </form>
        </field>
        <field name="KV" key="false" datatype="Int64" sort="default">
          <column show="true" caption="Код валюти" align="Center" />
          <form show="true">
            <uicontrol type="IntegerInput" readonly="true" />
          </form>
        </field>
        <field name="KOL" key="false" datatype="Int64" sort="default">
          <column show="true" caption="Кількість" align="Right"/>
          <form show="true">
            <uicontrol type="IntegerInput" readonly="true" />
          </form>
        </field>
        <field name="SUMA" key="false" datatype="Decimal" sort="default">
          <column show="true" caption="Сума" align="Right" />
          <form show="true">
            <uicontrol type="DecimalInput" readonly="true"/>
          </form>
        </field>
        <field name="BRANCH" key="false" datatype="String" sort="default">
          <column show="true" caption="Бранч" align="Center" />
          <form show="true">
            <uicontrol type="TextBox" readonly="true" />
          </form>
        </field>
        <field name="NAME" key="false" datatype="String" sort="default">
