          <form show="true">
            <uicontrol type="TextBox" readonly="true"/>
          </form>
        </field>
        <field name="DATZ" key="false" datatype="DateTime" sort="default">
          <column show="true" caption="Дата міграції" align="Center" />
          <form show="true">
            <uicontrol type="DateInput" readonly="true"/>
          </form>
        </field>
        <field name="VER" key="false" datatype="String" sort="default">
          <column show="true" caption="Версія АСВО" align="Center" />
          <form show="true">
            <uicontrol type="TextBox" readonly="true"/>
          </form>
        </field>
       </fields>
    </grid>
  </grids>
