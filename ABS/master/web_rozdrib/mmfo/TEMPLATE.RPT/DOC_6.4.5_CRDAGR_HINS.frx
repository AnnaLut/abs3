        <TableColumn Name="Column8" Width="349.61"/>
        <TableRow Name="Row11">
          <TableCell Name="Cell47" Fill.Color="LightGray" Text="Договір відновлюваної кредитної лінії  № &lt;b&gt;[DataSubProdukt.DEAL_NUMBER]&lt;/b&gt;" HorzAlign="Center" Font="Times New Roman, 12pt, style=Bold" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell48"/>
        </TableRow>
        <TableRow Name="Row12">
          <TableCell Name="Cell52" Text="&lt;b&gt;[Branch_Info.BR_LOCATION]&lt;/b&gt;" Font="Times New Roman, 10pt" HtmlTags="true"/>
          <TableCell Name="Cell53" Text="&lt;b&gt;[DataSubProdukt.DAT_DOG_PR]&lt;/b&gt;" HorzAlign="Right" VertAlign="Center" Font="Times New Roman, 10pt" HtmlTags="true"/>
