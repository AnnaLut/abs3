      <TableDataSource Name="Table" Alias="BID_DATA" DataType="System.Int32" Enabled="true" SelectCommand="select bg.agr_number,&#13;&#10;       bg.agr_date,&#13;&#10;       f_dat_lit(bg.agr_date) as agr_date_pr,&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_1', bg.ws_id, bg.garantee_num) || ' ' ||&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_2', bg.ws_id, bg.garantee_num) || ' ' ||&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_3', bg.ws_id, bg.garantee_num) as grt_fio,&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_7', bg.ws_id, bg.garantee_num) as grt_passp_ser,&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_8', bg.ws_id, bg.garantee_num) as grt_passp_num,&#13;&#10;       wcs_utl.get_answ(bg.bid_id, 'CL_9', bg.ws_id, bg.garantee_num) as grt_passp_organ,&#13;&#10;       wcs_utl.get_answ_date(bg.bid_id, 'CL_11', bg.ws_id, bg.garantee_num) as grt_passp_date,&#13;&#10;       case&#13;&#10;         when 1 = wcs_utl.get_answ(bg.bid_id,&#13;&#10;                                   'CL_0_7',&#13;&#10;                                   bg.ws_id,&#13;&#10;                                   bg.garantee_num) then&#13;&#10;          decode(wcs_utl.get_answ(bg.bid_id,&#13;&#10;                                  'CL_13',&#13;&#10;                                  bg.ws_id,&#13;&#10;                                  bg.garantee_num),&#13;&#10;                 26,&#13;&#10;                 null,&#13;&#10;                 27,&#13;&#10;                 null,&#13;&#10;                 'обл. ' ||&#13;&#10;                 wcs_utl.get_answ_refer_text(bg.bid_id,&#13;&#10;                                             'CL_13',&#13;&#10;                                             bg.ws_id,&#13;&#10;                                             bg.garantee_num) || ', ') ||&#13;&#10;          decode(wcs_utl.get_answ(bg.bid_id,&#13;&#10;                                  'CL_14',&#13;&#10;                                  bg.ws_id,&#13;&#10;                                  bg.garantee_n