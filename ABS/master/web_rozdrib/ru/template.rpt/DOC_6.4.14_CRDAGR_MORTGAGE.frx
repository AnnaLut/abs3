        <TableColumn Name="Column53" Width="349.65"/>
        <TableColumn Name="Column54" Width="349.65"/>
        <TableRow Name="Row740">
          <TableCell Name="Cell1217" Fill.Color="Gainsboro" Text="14. Реквізити та підписи Сторін" Padding="0, 0, 0, 0" HorzAlign="Center" Font="Times New Roman, 12pt, style=Bold" HtmlTags="true" ColSpan="2"/>
          <TableCell Name="Cell1218"/>
        </TableRow>
        <TableRow Name="Row741">
          <TableCell Name="Cell1219" Padding="0, 0, 0, 0"/>
          <TableCell Name="Cell1220" Padding="0, 0, 0, 0"/>
        </TableRow>
        <TableRow Name="Row742">
          <TableCell Name="Cell1221" Fill.Color="Gainsboro" Text="Банк:" Padding="0, 0, 0, 0" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 12pt, style=Bold" HtmlTags="true"/>
          <TableCell Name="Cell1222" Fill.Color="Gainsboro" Text="Позичальник:&#10;" Padding="0, 0, 0, 0" HorzAlign="Center" VertAlign="Center" Font="Times New Roman, 12pt, style=Bold" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row743" Height="56.7" AutoSize="true">
          <TableCell Name="Cell1223" Text="публічне акціонерне товариство&#13;&#10;„Державний ощадний банк України”&#13;&#10;Філія - [Branch_Info.NAME_BRANCH]" Padding="0, 0, 0, 0" HorzAlign="Center" Font="Times New Roman, 10pt, style=Bold"/>
          <TableCell Name="Cell1224" Text="[klient_info.FIO]" Padding="0, 0, 0, 0" HorzAlign="Center" Font="Times New Roman, 10pt, style=Bold"/>
        </TableRow>
        <TableRow Name="Row744" Height="75.6" AutoSize="true">
          <TableCell Name="Cell1225" Text="Адреса: &lt;b&gt;[Branch_Info.ADR_BRANCH]&lt;/b&gt;,&#13;&#10;МФО: &lt;b&gt;[Branch_Info.MFO]&lt;/b&gt;,&#13;&#10;код ЄДРПОУ: &lt;b&gt;[Branch_Info.OKPO]&lt;/b&gt;.   &#10;" Padding="0, 0, 0, 0" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
          <TableCell Name="Cell1226" Text="Паспорт: &lt;b&gt;[klient_info.PASPORT]&lt;/b&gt;&#13;&#10;Реєстраційний номер облікової картки платника податку: &lt;b&gt;[klient_info.IDCODE]&lt;/b&gt;&#13;&#10;Адреса: &lt;b&gt;[Adress.ADR_LIVE]&lt;/b&gt;" Padding="0, 0, 0, 0" HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
        </TableRow>
        <TableRow Name="Row745">
          <TableCell Name="Cell1227" Padding="0, 0, 0, 0"/>
          <TableCell Name="Cell1228" Padding="0, 0, 0, 0"/>
