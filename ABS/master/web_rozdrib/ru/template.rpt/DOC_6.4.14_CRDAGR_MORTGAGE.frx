          <TableCell Name="Cell1708" Border.Lines="All" Border.Width="0.5" Text="в сумі &lt;b&gt;[preferential_int.SIZE_ANNUITIES]&lt;/b&gt; грн. " HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
          <TableCell Name="Cell1709" Border.Lines="All" Border.Width="0.5" Text="починаючи з &lt;b&gt;[preferential_int.RATE_DATE_BEGIN]&lt;/b&gt; р. " HorzAlign="Justify" Font="Times New Roman, 10pt" HtmlTags="true"/>
