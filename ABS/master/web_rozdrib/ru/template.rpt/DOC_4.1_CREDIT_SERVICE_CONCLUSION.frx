      <TableDataSource Name="Table42" Alias="list_kzb" DataType="System.Int32" Enabled="true" SelectCommand="select  listagg(KZB, ';&#13;&#10;') &#13;&#10;                  WITHIN GROUP(ORDER BY kzb) as val&#13;&#10;from (&#13;&#10;select case when t.group_id = 3&#13;&#10;            then wcs_utl.get_answ_list_text(bg.bid_id, 'GRT_4_0', bg.ws_id, bg.garantee_num)||' (&quot;'||to_char(wcs_utl.get_answ_list(bg.bid_id, 'GRT_4_0', bg.ws_id, bg.garantee_num)+1)||'&quot;)' &#13;&#10;            when garantee_id = 'VEHICLE'&#13;&#10;            then case&#13;&#10;                 when wcs_utl.get_answ_list(bg.bid_id,'GRT_3_4_0',bg.ws_id,bg.garantee_num) = 1 then 'Легкові та вантажні автомобілі, які були у використанні («2»)'&#13;&#10;                 when wcs_utl.get_answ_list(bg.bid_id,'GRT_3_4_0',bg.ws_id,bg.garantee_num) = 0 and wcs_utl.get_answ_list(bg.bid_id,'GRT_3_1',bg.ws_id,bg.garantee_num) = 1 then 'Нові вантажні автомобілі («4»)'&#13;&#10;                 when wcs_utl.get_answ_list(bg.bid_id,'GRT_3_4_0',bg.ws_id,bg.garantee_num) = 0 and wcs_utl.get_answ_list(bg.bid_id,'GRT_3_1',bg.ws_id,bg.garantee_num) = 0 then 'Нові легкові автомобілі («5»)'&#13;&#10;                 else ''&#13;&#10;                end&#13;&#10;            when garantee_id in ('DEPOSIT','VALUABLES','PROPERTY_DEPOSIT')&#13;&#10;            then 'Вироби з дорогоцінних металів, державні цінні папери, депозитні вклади (в т.ч. у банківських металах) («7»).'&#13;&#10;            when garantee_id in ('PRODUCT') &#13;&#10;            then 'Нові побутові товари прийняті під заклад або в заставу, фінансова порука («3»)'  &#13;&#10;            when garantee_id in ('GUARANTOR') &#13;&#10;             and wcs_utl.get_answ_list(bg.bid_id, 'GRT_2_19', bg.ws_id, bg.garantee_num)=0 &#13;&#10;             and wcs_utl.get_score(bg.bid_id,ss.scoring_id,'KPP',bg.ws_id,bg.garantee_num) &gt; 1.3&#13;&#10;             and wcs_utl.get_score(bg.bid_id,ss.scoring_id,'P4DP',bg.ws_id,bg.garantee_num) &gt; 1&#13;&#10;     