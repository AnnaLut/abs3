      <TableDataSource Name="Table8" Alias="preferential_int" DataType="System.Int32" Enabled="true" SelectCommand="select 1 nrow,&#13;&#10;       wcs_utl.get_answ_decimal(b.bid_id, 'RATE_WORKER_BANK') as INTEREST_RATE,&#13;&#10;       substr(f_sumpr(wcs_utl.get_answ_decimal(b.bid_id, 'RATE_WORKER_BANK'), '', 'M'), 1, 100) as INTEREST_RATE_PR,&#13;&#10;       'дати отримання кредитних коштів' as RATE_DATE_BEGIN,&#13;&#10;              case when  wcs_utl.get_answ_date(b.bid_id, 'RATE_WORKER_DATE1')-1 &gt; D.WDATE&#13;&#10;            then D.WDATE&#13;&#10;            else wcs_utl.get_answ_date(b.bid_id, 'RATE_WORKER_DATE1')-1 end as RATE_DATE_END,&#13;&#10;       case when b.REPAYMENT_METHOD='4' and wcs_utl.get_answ_refer(b.bid_id, 'WORKER_BANK')= 1  &#13;&#10;            then (select pmt_total &#13;&#10;                  from table(wcs_utl.get_gpk(:p_bid_id)) gpk &#13;&#10;                  where pmt_date&lt;nvl(wcs_utl.get_answ_date(b.bid_id, 'RATE_WORKER_DATE1'),wcs_utl.get_answ_date(b.bid_id, 'DATE_RATE_BEGIN'))&#13;&#10;                  group by pmt_total&#13;&#10;                  having count(*) &gt;= ALL(select count(*) &#13;&#10;                                         from table(wcs_utl.get_gpk(b.bid_id)) gpk &#13;&#10;                                         where pmt_date&lt;nvl(wcs_utl.get_answ_date(b.bid_id, 'RATE_WORKER_DATE1'),wcs_utl.get_answ_date(b.bid_id, 'DATE_RATE_BEGIN')) &#13;&#10;                                         group by pmt_total))&#13;&#10;            else 0 end Size_annuities&#13;&#10;  from v_wcs_bids b, cc_deal d&#13;&#10; where b.bid_id = :p_bid_id   &#13;&#10;   and b.bid_id = d.nd(+)&#13;&#10;   and wcs_utl.get_answ_refer(b.bid_id, 'WORKER_BANK') = 1&#13;&#10;   and wcs_utl.get_answ_date(b.bid_id, 'RATE_WORKER_DATE1') &lt; D.WDATE&#13;&#10;union            &#13;&#10;select 2 nrow,           &#13;&#10;       wcs_utl.get_answ_decimal(b.bid_id, 'RATE_WORKER_BANK1') as INTEREST_RATE,&#13;&#10;       substr(f_sumpr(wcs_utl.get_answ_decimal(b.bid_id, 'RATE_WORKER_BANK1'), '', 'M'), 1, 100) as INTEREST_RATE_PR,&#13;&#10;       wcs_utl.get_answ(b.bid_id, 'RATE_WORKER_DATE1') as RATE_DATE_BEGIN,&#13;&#10;       case when  wcs_utl.get_answ_date(b.bid_id, 'DATE_RATE_BEGIN')-1 &gt; D.WDATE&#13;&#10;            then D.WDATE&#13;&#10;            else wcs_utl.get_answ_date(b.bid_id, 'DATE_RATE_BEGIN')-1 end as RATE_DATE_END,&#13;&#10;       case when b.REPAYMENT_METHOD='4' and wcs_utl.get_answ_refer(b.bid_id, 'WORKER_BANK')= 1 and wcs_utl.get_answ_decimal(b.bid_id, 'RATE_WORKER_BANK1') &gt; 0 &#13;&#10;            then (select pmt_total &#13;&#10;                  from table(wcs_utl.get_gpk(:p_bid_id)) gpk &#13;&#10;                  where pmt_date&gt;wcs_utl.get_answ_date(b.bid_id, 'RATE_WORKER_DATE1') and pmt_date&lt; wcs_utl.get_answ_date(b.bid_id, 'DATE_RATE_BEGIN')&#13;&#10;                  group by pmt_total&#13;&#10;                  having count(*) &gt;= ALL(select count(*) &#13;&#10;                                         from table(wcs_utl.get_gpk(b.bid_id)) gpk &#13;&#10;                                         where pmt_date&gt;wcs_utl.get_answ_date(b.bid_id, 'RATE_WORKER_DATE1') and pmt_date&lt; wcs_utl.get_answ_date(b.bid_id, 'DATE_RATE_BEGIN') group by pmt_total))&#13;&#10;            else 0 end Size_annuities&#13;&#10;  from v_wcs_bids b, cc_deal d&#13;&#10; where b.bid_id = :p_bid_id  &#13;&#10;   and b.bid_id = d.nd(+)&#13;&#10;   and wcs_utl.get_answ_refer(b.bid_id, 'WORKER_BANK') = 1 &#13;&#10;   and wcs_utl.get_answ_date(b.bid_id, 'RATE_WORKER_DATE1') &lt; D.WDATE&#13;&#10;union              &#13;&#10;select 3 nrow,                       &#13;&#10;       wcs_utl.get_answ_decimal(b.bid_id, 'INTEREST_RATE') as INTEREST_RATE,&#13;&#10;       substr(f_sumpr(wcs_utl.get_answ_decimal(b.bid_id, 'INTEREST_RATE'), '', 'M'), 1, 100) as INTEREST_RATE_PR,                               &#13;&#10;       wcs_utl.get_answ(b.bid_id, 'DATE_RATE_BEGIN') RATE_DATE_BEGIN,&#13;&#10;       D.WDATE as RATE_DATE_END,&#13;&#10;       case when b.REPAYMENT_METHOD='4' and wcs_utl.get_answ_refer(b.bid_id, 'WORKER_BANK')= 1  &#13;&#10;            then (select pmt_total &#13;&#10;                  from table(wcs_utl.get_gpk(:p_bid_id)) gpk &#13;&#10;                  where pmt_date&gt;wcs_utl.get_answ_date(b.bid_id, 'DATE_RATE_BEGIN')&#13;&#10;                  group by pmt_total&#13;&#10;                  having count(*) &gt;= ALL(select count(*) from table(wcs_utl.get_gpk(b.bid_id)) gpk where pmt_date&gt;wcs_utl.get_answ_date(b.bid_id, 'DATE_RATE_BEGIN') group by pmt_total))&#13;&#10;            else 0 end Size_annuities &#13;&#10;  from v_wcs_bids b, cc_deal d&#13;&#10; where b.bid_id = d.nd(+)&#13;&#10;   and b.bid_id = :p_bid_id   &#13;&#10;   and wcs_utl.get_answ_refer(b.bid_id, 'WORKER_BANK') = 1 &#13;&#10;   and wcs_utl.get_answ_date(b.bid_id, 'DATE_RATE_BEGIN') &lt; D.WDATE">
