						! Set sSql = 
								"SELECT substr(id,3,2) NPP, name, v0, 
								        0 f01, 0 f02, 0 f03, 0 f04, 0 f05, 0 f06, 0 f07 
								 INTO :FD.NPP, :FD.NAME,:FD.F00,
								      :FD.F01, :FD.F02, :FD.F03, :FD.F04, :FD.F05, :FD.F06, :FD.F07 
