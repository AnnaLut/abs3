			On SAM_Create
				!
				Set sTitl = "Тест дерева звіту "|| sRep 
				Call SalSetWindowText( hWndForm, sTitl  )
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On SAM_FetchRowDone
				If KOL = 0
					Call XSalTblSetRowBackColor( hWndForm, lParam, SalColorFromRGB( 255,240,255 )  ) 
			On UM_Populate
				Set D0.strSqlPopulate =
						"SELECT nvl(d.K,0), r.r020, r.txt, r.D_CLOSE  INTO :D0.KOL, :D0.NBS, :D0.NAME, :D0.D_CLOSE
						 FROM kl_r020 r,
						    (select s.nbs,count(*) K from anb1 d,anb1_ps s 
						     where s.id=d.id and d.id in (select id from anb2 where nrep="||Str(nRep)||" )
						      group by s.nbs ) d
						 WHERE r.PREM='КБ ' and substr(r.r020,4,1)<>' ' and r.r020=d.nbs(+) 
						 ORDER BY 1,2 "
				Call SalWaitCursor(TRUE)
				Call SalTblDefineSplitWindow(hWndForm, 0, FALSE)
				Call SalTblPopulate(hWndForm, hSql(), D0.strSqlPopulate, TBL_FillAll)
				Call SalTblSetContext(hWndForm, 0)
				Call SalPostMsg(hWndForm, SAM_TblDoDetails, 0, 0)
				Call SalWaitCursor(FALSE)
