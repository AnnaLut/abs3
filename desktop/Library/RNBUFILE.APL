			On UM_Populate
				Call SalWaitCursor(TRUE)
				Set AL.strSqlPopulate =
						"SELECT ANB_MFO_LIM.fdat, b.MFO ,   l.ID  , b.nb,   l.name, l.KOEF, 
						        ANB_MFO_LIM.LIM,ANB_MFO_LIM.PROP, ANB_MFO_LIM.VYSN, ANB_MFO_LIM.K1, ANB_MFO_LIM.K2, 
						        ANB_MFO_LIM.K3, ANB_MFO_LIM.LROZ, ANB_MFO_LIM.H1 
						   INTO :AL.FDAT, :AL.MFO, :AL.ID, :AL.NB, :AL.NLIM, :AL.KOEF,
						        :AL.DLIM, :AL.PROP,:AL.VYSN,:AL.K1,:AL.K2  , :AL.K3  , :AL.LROZ, :AL.H1  
						 from  ANB_MFO_LIM,  anb_bank b, anb_lim l
						 where ANB_MFO_LIM.mfo=b.mfo AND b.prp>0 AND ANB_MFO_LIM.id=l.id   
						 order by "||IifS(rb1,"1,2,3" ,"2,1,3")
				Call SalTblDefineSplitWindow(hWndForm, 0, FALSE)
				Call SalTblPopulate(hWndForm, hSql(),  AL.strSqlPopulate, TBL_FillAll)
				Call SalTblSetContext(hWndForm, 0)
				Call SalPostMsg(hWndForm, SAM_TblDoDetails, 0, 0)
				Call SalWaitCursor(FALSE)
			On SAM_FetchRowDone
				! 13. Лiмiт розрахований    - голубой
				Call XSalTblSetCellBackColor( AL.LROZ, COLOR_Sky )
				! 9. Пропозицii з лiмiту (дилера) - желтый
				Call XSalTblSetCellBackColor( AL.PROP, COLOR_Maize)
				! 8. Лiмiт дiючий
				Call XSalTblSetCellBackColor( AL.DLIM, COLOR_LightGray)
				! 14. Лiмiт затверджений
				If AL.VYSN = NUMBER_Null
					Set LIM.VYSN = SalNumberMin( LIM.PROP, LIM.LROZ )
				! меньшее из 8 и 13 - слабо зеленое
				If AL.VYSN = SalNumberMin( AL.PROP, AL.LROZ )
					Call XSalTblSetCellBackColor( AL.VYSN, COLOR_LightGreen)
				! Иначе (изменено вручную)  - зеленое
				Else
					Call XSalTblSetCellBackColor( AL.VYSN, COLOR_Green)
				Call XSalTblSetCellBackColor( AL.K1, SalColorFromRGB( 220,255,255 ) )
				Call XSalTblSetCellBackColor( AL.K2, SalColorFromRGB( 220,255,255 ) )
				Call XSalTblSetCellBackColor( AL.K3, SalColorFromRGB( 220,255,255 ) )
