			Function: RunGetRNBUFile !__exported
				Description:
				Returns
				Parameters
					Window Handle: hwndPurrent
				Static Variables
				Local variables
					Number: nMode
					Number: nPar
					String: strPar01
					String: strPar02
					Window Handle: hW
				Actions
					Set nMode = SalWindowHandleToNumber( hwndPurrent )
					! !
					If nMode = 15 ! Кред картка
						If not IsWindow(hW_15)
							Set hW_15 = SalCreateWindow( KK, hWndMDI, nMode, nPar, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( KK )
						Return TRUE
					! !! 
					If sMETOD = STRING_Null
						If SqlPrepareAndExecute( hSql(),
								"SELECT val into :sMETOD from params where par ='ANB_2' ")
								and not SqlFetchNext( hSql(), nFetchRes )
							Set sMETOD ='AGIO'
					Call SqlPrepareAndExecute( hSql(),"alter session set nls_date_format='DD-MON-YY'" )
					If ANB_F13 = STRING_Null 
						If SqlPrepareAndExecute( hSql(),
								  "SELECT Substr(pul.Get_Mas_Ini_Val('ANB_F13'),1,13)
								   INTO :ANB_F13  from dual " )
								and SqlFetchNext( hSql(), nFetchRes )
						If ANB_F13 = STRING_Null 
							Set ANB_F13 ='1111111111111'
						Else If Subs(ANB_F13,1,1) != '1'
							Set ANB_F13 ='1' || Subs(ANB_F13,2,12)
					If SqlPLSQLCommand(hSql(), "ANB.ANB_F13 (ANB_F13) ")
					! Set hW = SalNumberToWindowHandle( 10 )
					! Для манипуляции разніми функциями в качестве параметра используем hW <-> nMode
					! ВСТАНОВЛЕННЯ ЛIМIТIВ на банки по АКТ операціям                
					If nMode = 10
						If not IsWindow(hW_10)
							Set hW_10 = SalCreateWindow( LIM, hWndMDI, nPar, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( LIM )
					! Архів коефіцієтнів та показників
					Else If nMode = 11
						If not IsWindow(hWinAR)
							Set hWinAR = SalCreateWindow( AR, hWndMDI )
						Else
							Call SalBringWindowToTop ( AR )
					! Історія лімітів на банки по АКТ операціям       
					Else If nMode = 12
						If not IsWindow(hWinAL)
							Set hWinAL = SalCreateWindow( AL, hWndMDI, nMode, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( AL )
					! Лiмити вiд банкiв з активних операцiй
					Else If nMode = 13
						If not IsWindow(hWinPAS)
							Set hWinPAS = SalCreateWindow( PAS, hWndMDI, nMode, nPar, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( PAS )
					! Лiмiти НА(ВIД) банкiв та стан їх використання 
					Else If nMode = 14
						If not IsWindow(hWinLAP)
							Set hWinLAP = SalCreateWindow( LAP, hWndMDI, nMode, nPar, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( LAP )
					! Таблиця прийнятих файлів
					Else
						If not IsWindow(hWin)
							Set hWin = SalCreateWindow( T1, hWndMDI, nMode, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( T1 )
			Function: ANB 
				Description:
				Returns
				Parameters
					Number: nMode
					Number: nPar
					String: strPar01
					String: strPar02
				Static Variables
				Local variables
				Actions
					If nMode > 400  ! Виконати Звіт УПРАВЛ. обліку
						If not IsWindow(hWin4)
							Set hWin4 = SalCreateWindow( T4, hWndMDI, nMode-400, nPar, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( T4 )
					Else
						! Call DebugN(nMode)
						Select Case nMode
							Case 0 ! Перегляд файлу
								If not IsWindow(hWin0)
									Set hWin0 = SalCreateWindow( T00, hWndMDI, nMode, nPar, strPar01, strPar02 )
								Else
									Call SalBringWindowToTop ( T00 )
								Break
							Case 1 ! Структура балансу вибраного файлу
								If not IsWindow(hWin1)
									Set hWin1 = SalCreateWindow( F1, hWndMDI, nMode, nPar, strPar01, strPar02 )
								Else
									Call SalBringWindowToTop ( F1 )
								Break
							Case 2 ! Динаміка балансу вибраного МФО
								If not IsWindow(hWin2)
									Set hWin2 = SalCreateWindow( F2, hWndMDI, nMode, nPar, strPar01, strPar02 )
								Else
									Call SalBringWindowToTop ( F2 )
								Break
							Case 3 ! Порівняння балансів за вибрану дату
								If not IsWindow(hWin3)
									Set hWin3 = SalCreateWindow( F3, hWndMDI, nMode, nPar, strPar01, strPar02 )
								Else
									Call SalBringWindowToTop ( F3 )
								Break
							Case 4 ! Фондирование
								! Call DebugN(nPar)
								Call SalCreateWindow( FD, hWndMDI, nMode, nPar, strPar01, strPar02 )
								Break
							! Case 13 ! Лимиты на банки в широком формате
								                                     If not IsWindow(hWin3)
									                                     Set hWin3 = SalCreateWindow( F3, hWndMDI, nMode, nPar, strPar01, strPar02 )
								                                     Else 
									                                     Call SalBringWindowToTop ( F3 )
								                                     Break 
							! Case 611 ! F9. Виконати Звіт-611 (нормативів)
								                                                                                                                         If not IsWindow(hWin_611)
									                                                                                                                         Set hWin_611 = SalCreateWindow( N4, hWndMDI, nMode, nPar, strPar01, strPar02 )
								                                                                                                                         Else 
									                                                                                                                         Call SalBringWindowToTop ( N4 )
								                                                                                                                         Break 
			Function: AddMont
				Description: Возвращает Дату + n Месяцев
				Returns
					Date/Time:
				Parameters
					Date/Time: dDat
					Number: nN
				Static Variables
				Local variables
					Number: nNextM
					! Number: nDay
					! Number: nDayL
					Number: nYear
				Actions
					! Set nNextM = SalDateMonth(dDat) + nN - 1
					! Set nDay = SalDateDay(dDat)
					! Set nDayL=SalDateDay(SalDateConstruct(
							    SalDateYear(dDat)+SalNumberTruncate((nNextM+1)/12,15,0),
							    SalNumberMod(nNextM+1,12)+1,1,0,0,0)-1)
					! If nDay > nDayL
						                                                                                                  Set nDay = nDayL
					! Return SalDateConstruct(SalDateYear(dDat)+SalNumberTruncate(nNextM/12,10,0),
							       SalNumberMod(nNextM,12)+1, nDay, 0, 0, 0)
					Set nNextM = SalDateMonth(dDat) + nN
					Set nYear = SalDateYear(dDat)
					Loop
						If nNextM > 12
							Set nNextM = nNextM - 12
							Set nYear = nYear + 1
						Else
							Break
					Loop
						If nNextM <= 0
							Set nNextM = nNextM + 12
							Set nYear = nYear - 1
						Else
							Break
					Return SalDateConstruct(nYear, nNextM, SalDateDay(dDat), 0, 0, 0)
			Function: NH_611
				Description: Проверка логики нормативов
				Returns
					Boolean:
				Parameters
					Number: i
					Window Handle: hG
					Number: nG
					Boolean: bZAG
					Number: FILE_ID
				Static Variables
				Local variables
					Number: nTmp
				Actions
					If i >0 and i<23 
						! Проверка логики
						If ZNAKo[i] !=STRING_Null and NORMo[i] != NUMBER_Null and nG != NUMBER_Null 
							If ZNAKo[i]= '>=' and nG < NORMo[i] or 
									   ZNAKo[i]= '<=' and nG > NORMo[i] 
								Call XSalTblSetCellBackColor(  hG , SalColorFromRGB( 255,210,250 ) )
						If ZAGo[i] = 0 and bZAG
							Set ZAGo[i] = 1
							Call SalTblSetColumnTitle( hG ,  IDo[i]||"
									"||Subs(NAMEo[i], 1,8)||"
									"||Subs(NAMEo[i], 9,8)||"
									"||Subs(NAMEo[i],17,8)||"
									"||Subs(NAMEo[i],25,8)||"
									"||Subs(NAMEo[i],33,8) ||"
									"||Subs(Str(100+i),2,2)  )
						If FILE_ID > 0 
							If SqlPrepareAndExecute( hSqlAux3(),
									   "SELECT anb.NORMATIV(:i,:FILE_ID) into :nTmp from dual" ) 
									and SqlFetchNext( hSqlAux3(), nFetchRes ) 
									and nG = nTmp
								Call XSalTblSetCellBackColor(  hG ,  SalColorFromRGB( 170,255,170 ) )
			Function: LH_611
				Description: Считвание логики нормативов
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					String: ID
					String: NAME
					Number: NORM
					String: ZNAK
					Number: ORD
				Actions
					If SqlPrepareAndExecute( hSql(), 
							  "SELECT ID, NAME,NORM,ZNAK,ORD into :ID,:NAME,:NORM,:ZNAK,:ORD from anb_611s where ord>0 and ord<23" )
						While SqlFetchNext( hSql(), nFetchRes ) 
							Set IDo [ORD] = ID
							Set NAMEo [ORD] = NAME
							Set NORMo [ORD] = NORM
							Set ZNAKo [ORD] = ZNAK
							Set ZAGo [ORD] = 0 
					Return TRUE
