					Function: F_RAW
						Description:
						Returns
						Parameters
							Number: nAkt
						Static Variables
						Local variables
						Actions
							If nAkt = 1
								Set nCol = SalColorFromRGB( 230,255,230 )
							Else
								Set nCol = COLOR_Sky 
							!
							Call SalShowWindow( dBS )
							Call SalShowWindow( dNN )
							Call SalShowWindow( tD )
							If tA.BS != STRING_Null
								Set nA = 0 
								Set dBS = tA.BS ||'%'
								Set dNN = tA.NN
								Set Stmp = 
										"SELECT substr(f.NEXT_DATE,5,4) || '-'|| substr(f.NEXT_DATE,3,2),
										        round(SUM(decode(f.mfo,:MM[1],x.ZNAP,0))/100000,0),
										        round(SUM(decode(f.mfo,:MM[2],x.ZNAP,0))/100000,0),
										        round(SUM(decode(f.mfo,:MM[3],x.ZNAP,0))/100000,0),
										        round(SUM(decode(f.mfo,:MM[4],x.ZNAP,0))/100000,0), 
										        round(SUM(decode(f.mfo,:MM[5],x.ZNAP,0))/100000,0),
										        round(SUM(decode(f.mfo,:MM[6],x.ZNAP,0))/100000,0)
										 INTO  :tD.DAT, :tD.S1, :tD.S2, :tD.S3, :tD.S4, :tD.S5, :tD.S6
										 FROM anb_IN_FILES f, anb_XAU x
										 WHERE f.file_id=x.file_id AND  f.mfo in 
										  ( :MM[1], :MM[2], :MM[3], :MM[4], :MM[5],:MM[6] ) AND 
										  to_number(substr(f.NEXT_DATE,5,4)) in ( :nGod, :nGod1 ) AND
										       SUBSTR(x.KODP,1,1)=:nAkt AND SUBSTR(x.KODP,2,1) = '0' AND
										       SUBSTR(KODP,3,4) LIKE :dBS 
										 GROUP by f.NEXT_DATE 
										 ORDER by substr(f.NEXT_DATE,5,4) || substr(f.NEXT_DATE,3,2)"
								Call SalTblPopulate( tD, hSql(), Stmp, TBL_FillAll)
								If nA > 1 
									Call SalShowWindow( ccGrafD )
									Call Redraw2()
