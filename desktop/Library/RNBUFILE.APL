			Number: nKol_P
		Message Actions
			On SAM_Create
				If nMode=100 or nMode=102 !! пользователь-просмотр всего
					Call SalDisableWindow( pbIns )
					Call SalDisableWindow( pbInsS )
					Call SalDisableWindow( pbDel )
					Call SalDisableWindow( pb_Der )
					Call SalDisableWindow( pb_Arc )
				Else If nMode=101 or nMode=103 !! только импорт и просмотр импорченного
					Call SalDisableWindow( pb_F1 )
					Call SalDisableWindow( pb_F2 )
					Call SalDisableWindow( pb_F3 )
					Call SalDisableWindow( pb_F4 )
					Call SalDisableWindow( cmb_Rep )
					Call SalDisableWindow( pb_Rez )
					Call SalDisableWindow( pb_Rez4 )
				! Филиалі
				If nMode=102 or nMode=103 or nMode=104
					Set sGxx = "C"
				! Банки- ЮЛ
				Else
					Set sGxx = "G"
				If SqlPrepareAndExecute( hSql(),
						"SELECT to_number(val) into :nKol_P from params where par ='ANB_FILE' ")
						and not SqlFetchNext( hSql(), nFetchRes )
					Set nKol_P = 10
				Call SalSetWindowText( hWndForm, "Файли-звіти "||
						IifS(sGxx = "C", "Філій банку","Інших Банків") )
				Set nCol[1]= SalColorFromRGB( 220,255,255 ) ! голубой
				Set nCol[2]= SalColorFromRGB( 255,255,220)  !  желтый
				Set nCol[3]= SalColorFromRGB( 230,245,240 ) ! бледнофиолет
				Set nCol[4]= SalColorFromRGB( 230,255,230 ) ! бледнозеленый
				Set nCol[5]= SalColorFromRGB( 250,250,250 ) ! серый
				Set nCol[6]= SalColorFromRGB( 255,240,255 )  ! розовый
				Set nCol[7]= nCol[1]
				Set nCol[8]= nCol[2]
				Set nCol[9]= nCol[3]
				Set nCol[10]=nCol[4]
				Set nCol[11]=nCol[5]
				Set nCol[12]=nCol[6]
				! Call LH_611 ()
				!
				Set T1.strPrintFileName = 'T1'
				Set T1.nTabInstance = 1
				Set T1.strFilterTblName = 'ANB_IN_FILES'
				Set T1.fFilterAtStart = 1
				! Call SalTblSetLockedColumns( hWndForm, 6 )
				Set T1.strSqlPopulate =
						"SELECT to_date(ANB_IN_FILES.next_date,'ddmmyyyy'), UPPER(a.FA),
						        UPPER(ANB_IN_FILES.FILE_NAME), ANB_IN_FILES.mfo, b.nb, ANB_IN_FILES.FILE_ID, 
						        b.PRP 
						 INTO  :T1.DAT, :T1.FAS, :T1.FA, :T1.MFO, :T1.NB, :T1.FILE_ID, :T1.BP 
						 FROM ANB_IN_FILES, anb_bank b, anb_611 a      
						 WHERE SUBSTR(ANB_IN_FILES.file_name,1,3) IN ('#01','#02') AND ANB_IN_FILES.mfo=b.mfo and 
						   ANB_IN_FILES.FILE_ID = a.FILE_ID (+) 
						   AND UPPER( SUBSTR(ANB_IN_FILES.file_name,10,1)) =:T1.sGxx 
						 ORDER BY to_date(ANB_IN_FILES.next_date,'ddmmyyyy') desc, ANB_IN_FILES.mfo "
				!
				If SqlPrepareAndExecute( hSql(), "delete from anb1_tmp" )
						and SqlPrepareAndExecute( hSql(),
						 "insert into anb1_tmp(ID,NAME,N0,SID,PR_99,PR_SR,PR_AU)
						  select ID,NAME,N0,SID,PR_99,PR_SR,PR_AU from anb1" )
					Call SalSendClassMessage(SAM_Create, 0, 0)
			On SAM_DoubleClick
				Set ANB_DAT0 = DAT
				If FILE_ID > 0 
					Call ANB ( 0, FILE_ID, FA, MFO ||' ~ '|| NB || ' за '|| SalFmtFormatDateTime( DAT, "dd/MM/yyyy" ) )
			On SAM_FetchRowDone
				Call XSalTblSetRowBackColor( hWndForm, lParam,  nCol[SalDateMonth( DAT )]) 
				! If SqlPrepareAndExecute( hSqlAux2(), "SELECT 1 into :BP from anb_mfo_lim where mfo=:MFO and rownum=1" )
						and SqlFetchNext( hSqlAux2(), nFetchRes )
				Set MMYY = SalFmtFormatDateTime( DAT, 'MMyy' )
				Set SAB3 = Subs(FA,4,3)
			On SAM_RowSetContext
				Set ANB_DAT0 = DAT
				If FAS != STRING_Null
					! Call SalEnableWindow( pbDetailsS )
					Call SalEnableWindow( pb_F9 )
					! Call SalDisableWindow( pb_611 )
				Else
					! Call SalDisableWindow( pbDetailsS )
					Call SalDisableWindow( pb_F9 )
					! Call SalEnableWindow( pb_611 )
