		Message Actions
			On SAM_Create
				Call SalCenterWindow( hWndForm )
				Call SalSetWindowText( hWndForm, "Порівняння Балансу по Файлам звітності " || MFO_NB || " від "|| tDAT )
				Set MFO = Subs(MFO_NB,1,6) 
				Set nGod = Val(Subs(tDAT,7,4))
				Set nGod1 = nGod - 1
				Set MMYYYY = Subs(tDAT,4,2) || Subs(tDAT,7,4) 
				Set pb_Bal = FALSE
				Set nN2 = 1
				Call F_BAL('', nN2 )
				Set Stmp = 
						 "SELECT distinct r.mfo, b.nb INTO :tM.MFO1,:tM.NB1
						 FROM anb_IN_FILES r, anb_bank b         
						 WHERE SUBSTR(r.file_name,2,2) IN ('01','02') AND r.mfo=b.mfo  and
						 b.mfo<>:MFO and substr(r.NEXT_DATE,3,6)= :MMYYYY
						 ORDER BY 1"
				! Call Debug(Stmp)
				Set nM = 1
				Set MM[1] = MFO
				Call SalTblSetColumnTitle(  tD.S1, MM[1] )
				Call SalTblPopulate( tM, hSql(), Stmp, TBL_FillAll)
				Call WaitCursorOff( )
				Call SalHideWindow( ccGrafD )
