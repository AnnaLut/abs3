		Functions
			Function: FunMakeColSetNSQLStr
				Description: Создать набор колонок для расшифровки показателей
						и строки "населения" и сохранения таблицы
				Returns
					Boolean:
				Parameters
					Receive Number: nColInKey
					Receive String: strPopulate
				Static Variables
				Local variables
					! Number: nNum
					Window Handle: hWnd
					String: strColName
					String: strColVal
					String: strIsCode
					Number: nAtr
					!
					Number: nColId
					String: strSelect
					! String: strInto
					String: strOrderBy
				Actions
					Set nNum = 0
					Set strSelect = ''
					Call SalTblDestroyColumns( hWndForm )
					If SqlPrepareAndExecute( hSql(), 
							"SELECT natr, name, val, iscode
							 INTO :nAtr, :strColName, :strColVal, :strIsCode
							 FROM form_stru
							 WHERE kodf = :sFCode ORDER BY natr")
						While SqlFetchNext( hSql(), nFetchRes )
							Set nNum = nNum + 1
							If SalStrToNumber( strIsCode ) != 0
								Set strColName = strColName || PutCrLf() || '(код)'
							Else
								Set nColInKey = nNum - 1
							!
							! If NOT SalTblCreateColumn( hWndForm, nNum, 1.8, 25, strColName )
								                                      Return FALSE
							If nNum = 1
								Call SalTblSetColumnTitle( T00.GR1, strColName )
							Else If nNum = 2
								Call SalTblSetColumnTitle( T00.GR2, strColName )
							Else If nNum = 3
								Call SalTblSetColumnTitle( T00.GR3, strColName )
							Else If nNum = 4
								Call SalTblSetColumnTitle( T00.GR4, strColName )
							Else If nNum = 5
								Call SalTblSetColumnTitle( T00.GR5, strColName )
							Else If nNum = 6
								Call SalTblSetColumnTitle( T00.GR6, strColName )
							!
							If strSelect != ''
								Set strSelect = strSelect || ', '
							Set strSelect = strSelect || strColVal
							!
							If SalStrToNumber( strIsCode ) != 0
								If strOrderBy != ''
									Set strOrderBy = strOrderBy || ', '
								Set strOrderBy = strOrderBy || SalNumberToStrX( nNum, 0 )
							!
							! If strInto != ''
								                                      Set strInto = strInto || ', '
							! Set strInto = strInto || ':T00.GR' || SalNumberToStrX(nNum, 0)
						If nNum = 0
							Return FALSE
						! !!!!!
						If sFCode ='01' or sFCode ='02'
							Set nNum = nNum + 1
							! If NOT SalTblCreateColumn( hWndForm, nNum, 5, 200, strColName )
								                                      Return FALSE
							Set strPopulate = 
									"SELECT "||strSelect||", k.TXT INTO 
									 INTO :T00.GR1,:T00.GR2,:T00.GR3,:T00.GR4,:T00.GR5,:T00.GR6,:T00.GR7 
									 FROM anb_xau x, KL_R020 k 
									  WHERE k.PREM='КБ ' AND k.R020=Substr(x.KODP,3,4)
									    and x.file_id="||Str(FILE_ID)||" ORDER BY "||strOrderBy
						Return TRUE
		Window Parameters
			Number: nMode
			Number: FILE_ID
			String: FA
			String: TXT
		Window Variables
			String: sFCode
			Number: nColInKey
			Number: nNum
		Message Actions
			On SAM_Create
				Call SalSetWindowText( hWndForm, "Файл звітності " || FA || ".  " || TXT )
				Set T00.strPrintFileName = FA
				Set T00.nTabInstance = 1
				Set T00.strFilterTblName = 'ANB_XAU'
				Set sFCode = Subs( FA, 2,2)
				If FunMakeColSetNSQLStr(nColInKey,T00.strSqlPopulate )
					Call SalSendClassMessage(SAM_Create, 0, 0)
			! On SAM_FetchRowDone
				                                            If sFCode ='01' or sFCode ='02'
					                                            If SqlPrepareAndExecute( hSqlAux2(),
							  "SELECT txt into :T00#' || SalNumberToStrX(nNum, 0)
							   FROM KL_R020 WHERE PREM='КБ ' and R020=:T00#2" )
							and SqlFetchNext( hSqlAux2(), nFetchRes )
	! cGenericTable: T0
