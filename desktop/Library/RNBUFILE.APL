						Set sSql = 
								"SELECT substr(id,3,2) NPP, name, -v0*1000000
								 INTO :FD.NPP, :FD.NAME,:FD.F00
								 FROM anb1_tmp WHERE id like 'Ф1__' and length(trim(id))=4  
								 Order by 1 "
						Call SalTblPopulate(hWndForm, hSql(), sSql, TBL_FillAll)
