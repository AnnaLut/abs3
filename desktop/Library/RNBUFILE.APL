						and SqlPrepareAndExecute( hSql(),
						 "insert into anb1_tmp(ID,NAME,N0,SID,PR_99,PR_SR,PR_AU)
						  select ID,NAME,N0,SID,PR_99,PR_SR,PR_AU from anb1
						  where id like 'Ф___'" )
				!
				Call SalSendClassMessage(SAM_Create, 0, 0)
			! On UM_Populate
				           Call SalWaitCursor(TRUE)
				           If SqlPLSQLCommand(hSql(), 
						"ANB.U4 ( 1, 0, 'Б', '980', FILE_ID, FILE_ID ,FILE_ID, FILE_ID ) ")
					! Пассивы
					           Set nG =0
					           If SqlPrepareAndExecute( hSql(), 
							  "SELECT substr(id,3,2)+0,name,v0*1000000 INTO :nTmp,:sTmp, :nS
							   FROM anb1_tmp WHERE id like 'Ф2__' and substr(id,4,1)<>' ' ") 
						           While SqlFetchNext( hSql(), nFetchRes )
							           Set nI[nTmp] = nS
							           Set sI[nTmp] = sTmp
							           If nS != NUMBER_Null 
								           Set nG = nG + nS
						!
						           Call SalTblSetColumnTitle( F01, sI[01] )
						           Call SalTblSetColumnTitle( F02, sI[02] )
						           Call SalTblSetColumnTitle( F03, sI[03] )
						           Call SalTblSetColumnTitle( F04, sI[04] )
						           Call SalTblSetColumnTitle( F05, sI[05] )
						           Call SalTblSetColumnTitle( F06, sI[06] )
						           Call SalTblSetColumnTitle( F07, sI[07] )
						! Активы
						! Set sSql = 
								"SELECT substr(id,3,2) NPP, name, v0, 
								        0 f01, 0 f02, 0 f03, 0 f04, 0 f05, 0 f06, 0 f07 
								 INTO :FD.NPP, :FD.NAME,:FD.F00,
