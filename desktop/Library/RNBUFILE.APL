					Else If nLength = 2
						Set nCol =  SalColorFromRGB( 230,255,230 ) ! бледнозеленый
					Else If nLength = 3
						Set nCol= SalColorFromRGB( 235,235,235 ) ! серый
					Else If nLength = 4
						Set nCol= SalColorFromRGB( 250,250,250 ) ! серый
					Call SalColorSet( NAME, COLOR_IndexWindow, nCol )
