		Functions
		Window Parameters
			Number: nRep
			String: sRep
			String: sTIP
		Window Variables
			String: sTitl
			Long String: s_SQL
		Message Actions
			On SAM_Create
				!
				Set sTitl = "Тест формул звіту "|| sRep 
				Call SalSetWindowText( hWndForm, sTitl  )
				If SqlPLSQLCommand(hSql(), "anb.U_FRM ( nRep, sTIP) ")
					Set F0.strSqlPopulate =
							"SELECT f.ID, f.FRM, a.NAME, s.kol, s.F_SQL  
							 INTO :F0.ID, :F0.FRM, :F0.NAME, :F0.KOL, :F0.F_SQL
							 FROM anb1_form f, anb1 a, ANB_SQL s
							 WHERE a.id=f.id AND f.frm is not null 
							   AND a.id in (select id from anb2 where nrep=:F0.nRep) AND a.id=s.id 
							 ORDER BY a.id "
					Call SalSendClassMessage(SAM_Create, 0, 0)
			On SAM_FetchRowDone
				If KOL = 0
					Call XSalTblSetRowBackColor( hWndForm, lParam, SalColorFromRGB( 255,240,255 )  ) 
