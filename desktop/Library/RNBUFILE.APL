					If sTIP = 'У' or sTIP = 'Б'
						Set nV = V0/2 + V1 + V2 + V3+ V4+ V5+ V6+ V7+ V8+ V9+ V10+ V11+ V12/2
						Set VS = nV /T4.kT4
						Set nG = G0/2 + G1 + G2 + G3+ G4+ G5+ G6+ G7+ G8+ G9+ G10+ G11+ G12/2
						Set GS = nG /T4.kT4
						!
						Set PS = Col_SUM1 (13,VS, VS, GS, GS, XS, XS)
						Set P0 = Col_SUM1 (0,V0, V0, G0, G0, X0, X0)
						Set P1 = Col_SUM1 (1,V1, V1, G1, G1, X1, X1)
						Set P2 = Col_SUM1 (2,V2, V2, G2, G2, X2, X2)
						Set P3 = Col_SUM1 (3,V3, V3, G3, G3, X3, X3)
						Set P4 = Col_SUM1 (4,V4, V4, G4, G4, X4, X4)
						Set P5 = Col_SUM1 (5,V5, V5, G5, G5, X5, X5)
						Set P6 = Col_SUM1 (6,V6, V6, G6, G6, X6, X6)
						Set P7 = Col_SUM1 (7,V7, V7, G7, G7, X7, X7)
						Set P8 = Col_SUM1 (8,V8, V8, G8, G8, X8, X8)
						Set P9 = Col_SUM1 (9,V9, V9, G9, G9, X9, X9)
						Set P10= Col_SUM1 (10,V10,V10,G10,G10,X10,X10)
						Set P11= Col_SUM1 (11,V11,V11,G11,G11,X11,X11)
						Set P12= Col_SUM1 (12,V12,V12,G12,G12,X12,X12)
			Function: Col_SUM1
				Description:
				Returns
					Number:
				Parameters
					Number: n
					Window Handle: hV
					Receive Number: nV
					Window Handle: hG
					Receive Number: nG
					Window Handle: hX
					Receive Number: nX
				Static Variables
				Local variables
					Number: nCol
					Number: nP
				Actions
					!
					Set nP = NUMBER_Null
					If nV = 0 or nV = NUMBER_Null
						Set nV = NUMBER_Null
					Else
						If sTIP = 'Б' 
							If nLength = 1
								Set nI1[n+1] = nV
							If nI1[n+1] != NUMBER_Null and nI1[n+1] !=0
								Set nP = nV*100/nI1[n+1]
						Call XSalTblSetCellBackColor(hV, IifN(nV > 0, nColP,nColA ))
