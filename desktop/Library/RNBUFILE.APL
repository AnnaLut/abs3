			On SAM_Create
				Set KK.strPrintFileName = 'KK'
				Set KK.nXS = GetGlobalOptionEx('DMPKCCXS' ) /100
				Set KK.nXL = GetGlobalOptionEx('DMPKCCXL' )
				Set KK.nLL = GetGlobalOptionEx('DMPKCCLL' )
				Set KK.n980= GetBaseVal()
				Set KK.dDat = GetBankDate()
				Set KK.dXL = AddMont( KK.dDat, KK.nXL  )
				Set KK.dLL = AddMont( KK.dDat, KK.nLL +1  )
				Set KK.nTabInstance = 1
				Set KK.strFilterTblName = 'ACCOUNTS'
				Set KK.fFilterAtStart = 1
				If SqlPrepareAndExecute(hSql(),
						"SELECT substr(pul.Get_Mas_Ini_Val('KOD_TOBO'),1 ,12) INTO :sTOBO from dual")
						and SqlFetchNext(hSql(), nFetchRes)
				Set KK.strSqlPopulate =
						"SELECT ACCOUNTS.acc, ACCOUNTS.kv, ACCOUNTS.nls, ACCOUNTS.nms, 
						        ACCOUNTS.ostc/100, ACCOUNTS.lim/100, ACCOUNTS.mdate, n.ND,
						        v.acco,  ACCOUNTS.nlsalt
						 INTO :KK.ACC,:KK.KV,:KK.NLS,:KK.NMS,:KK.OST,:KK.LIM,:KK.MDATE,:KK.ND,
						      :KK.ACCO, :KK.NLSALT 
						 FROM ACCOUNTS, NSMEP_ACC n, acc_over v
						 WHERE ACCOUNTS.dazs is null 
						   and ACCOUNTS.nls like '9%' 
						   and ACCOUNTS.nbs ='8021' 
						   and ACCOUNTS.tip='CRC' 
						   and ACCOUNTS.acc=v.acc (+)
						   and ACCOUNTS.acc=n.acc (+) " ||
						   IifS(KK.sTOBO="","",
						 " and ACCOUNTS.tobo=:KK.sTOBO ") ||"
						 ORDER BY Nvl(n.nd,0) desc, ACCOUNTS.acc "
				Set KK.strSqlUpdate =
						"update ACCOUNTS set lim= :KK.LIM100, mdate=:KK.MDATE  WHERE acc=:KK.ACC and 0=:KK.OTM"
				Call SalTblSetLockedColumns( hWndForm, 10 )
				Call SalHideWindow( KK.ACC )
				Call SalHideWindow( KK.NLSALT )
				!
				Call SalSendClassMessage(SAM_Create, 0, 0)
			! On SAM_DoubleClick
				                                Set ANB_DAT0 = DAT
				                                If FILE_ID > 0 
					                                Call ANB ( 0, FILE_ID, FA, MFO ||' ~ '|| NB || ' за '|| SalFmtFormatDateTime( DAT, "dd/MM/yyyy" ) )
			On SAM_FetchRowDone
				Set OTM = 0
				Set KK.LIM100 = KK.LIM*100
				If ACCO != NUMBER_Null and ACCO > 0
					If SqlPrepareAndExecute( hSqlAux2(),
							 "SELECT nls,ostc/100 into :KK.NLSO,:KK.OSTO
							  from accounts where acc=:KK.ACCO " )
							and SqlFetchNext( hSqlAux2(), nFetchRes )
				If ND != NUMBER_Null and ND > 0
					If SqlPrepareAndExecute( hSqlAux2(),
							 "SELECT CC_ID,SDATE,WDATE into :KK.CC_ID,:KK.DAT1,:KK.DAT4
							  from cc_deal where nd=:KK.ND" )
							and SqlFetchNext( hSqlAux2(), nFetchRes )
						Set OTM = 1
						Set KK.MDATE1 = KK.DAT4
						Call XSalTblSetRowBackColor( hWndForm, lParam,  COLOR_LightGreen ) 
				!
				If SqlPrepareAndExecute( hSqlAux2(),
						 "SELECT count(*) into :KK.DOPK 
						  from nd_acc where acc=:KK.ACC and nd <>Nvl(:KK.ND,-1) " )
						and SqlFetchNext( hSqlAux2(), nFetchRes )
			! On SAM_RowSetContext
				                                Set ANB_DAT0 = DAT
				                                If FAS != STRING_Null
					! Call SalEnableWindow( pbDetailsS )
					                                Call SalEnableWindow( pb_F9 )
					! Call SalDisableWindow( pb_611 )
				                                Else 
					! Call SalDisableWindow( pbDetailsS )
					                                Call SalDisableWindow( pb_F9 )
					! Call SalEnableWindow( pb_611 )
				!
