		Functions
			Function: F_NAM
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					If SqlPrepareAndExecute( hSqlAux(),"SELECT fin,name INTO :nTmp,:sTmp FROM STAN_FIN " )
						While SqlFetchNext(hSqlAux(), nFetchRes)
							Set NAM_FIN[nTmp] = sTmp
		Window Parameters
			Date/Time: pD1
			Date/Time: dTek
		Window Variables
			String: sFiltr
			String: NAM_FIN [*]
			Number: nTmp
			String: sTmp
		Message Actions
			On SAM_Create
				Set L90.strPrintFileName = 'L90'
				Set L90.nTabInstance = 1
				Set L90.strFilterTblName = 'ANB_BANK'
				Call SalTblSetLockedColumns( hWndForm, 9 )
				Set D1 = pD1
				Call SalSendMsg( D1, SAM_Validate, 0, 0 )
				Call F_NAM()
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On UM_Populate
				Call SalWaitCursor(TRUE)
				If D1 != dTek
					Call SalDisableWindow( L90.K3Z_ )
				Else
					Call SalEnableWindow( L90.K3Z_ )
				Call SalSetWindowText( hWndForm,
						'ВСТАНОВЛЕННЯ ЛIМIТIВ на банки по АКТ операціям. Дата розрах.' ||SalFmtFormatDateTime( D1,"dd/MM/yyyy" ) )
				Set sFiltr = ""
				Set sFiltr = cF.GetFilterWhereClause( FALSE )
				If sFiltr !=  ""
					Set sFiltr = " and " || sFiltr
				Set L90.strSqlPopulate =
						"SELECT ANB_BANK.MFO, ANB_BANK.nb, l.H1, l.K2, l.K3R,c.CRISK,c.rnk, -a.V0,
						        l.LD1,l.LD2,l.LD3,l.LD4,l.LD5,l.LD6,
						        l.LZ1,l.LZ2,l.LZ3,l.LZ4,l.LZ5,l.LZ6, 
						        l.LR1,l.LR2,l.LR3,l.LR4,l.LR5,l.LR6 
						   INTO :L90.MFO,:L90.NB,:L90.H1,:L90.K2,:L90.K3R,:L90.K3Z_,:L90.RNK,:L90.AKT,
						    :L90.LD1,:L90.LD2,:L90.LD3,:L90.LD4,:L90.LD5,:L90.LD6,
						    :L90.LZ1,:L90.LZ2,:L90.LZ3,:L90.LZ4,:L90.LZ5,:L90.LZ6, 
						    :L90.LR1,:L90.LR2,:L90.LR3,:L90.LR4,:L90.LR5,:L90.LR6    
						 from  ANB_BANK, 
						  (select k.RNK,u.CRISK,k.MFO from customer u,custbank k 
						   where u.rnk=k.rnk and k.mfo is not null and u.date_off is null ) c,
						  (select mfo,V0 from ANB_BAZ_ARC where id='7' and D0=:D1 ) a, 
						(select mfo, max(H1) H1, max(K2) K2, max(K3) K3R, 
						 sum(decode(id,1,LIM ,0)) LD1, sum(decode(id,2,LIM ,0)) LD2, sum(decode(id,3,LIM ,0)) LD3, 
						 sum(decode(id,4,LIM ,0)) LD4, sum(decode(id,5,LIM ,0)) LD5, sum(decode(id,6,LIM ,0)) LD6, 
						 sum(decode(id,1,VYSN,0)) LZ1, sum(decode(id,2,VYSN,0)) LZ2, sum(decode(id,3,VYSN,0)) LZ3, 
						 sum(decode(id,4,VYSN,0)) LZ4, sum(decode(id,5,VYSN,0)) LZ5, sum(decode(id,6,VYSN,0)) LZ6, 
						 sum(decode(id,1,LROZ,0)) LR1, sum(decode(id,2,LROZ,0)) LR2, sum(decode(id,3,LROZ,0)) LR3, 
						 sum(decode(id,4,LROZ,0)) LR4, sum(decode(id,5,LROZ,0)) LR5, sum(decode(id,6,LROZ,0)) LR6
						 from ANB_MFO_LIM  where fdat=:D1 group by mfo ) l 
						where ANB_BANK.mfo = l.MFO (+) 
						  and ANB_BANK.prp > 0 
						  and ANB_BANK.mfo = a.mfo (+) 
						  and ANB_BANK.mfo = c.mfo (+) " || sFiltr || "
						 order by 1 " 
				Call SalTblDefineSplitWindow(hWndForm, 0, FALSE)
				Call SalTblPopulate(hWndForm, hSql(),  L90.strSqlPopulate, TBL_FillAll)
				Call SalTblSetContext(hWndForm, 0)
				Call SalPostMsg(hWndForm, SAM_TblDoDetails, 0, 0)
				Call SalWaitCursor(FALSE)
			On SAM_FetchRowDone
				Set K3R = NAM_FIN[Val(K3R)]
				Set K3Z = NAM_FIN[K3Z_]
				! 8. Лiмiт дiючий
				Call XSalTblSetCellBackColor( LD1, COLOR_LightGray)
				Call XSalTblSetCellBackColor( LD2, COLOR_LightGray)
				Call XSalTblSetCellBackColor( LD3, COLOR_LightGray)
				Call XSalTblSetCellBackColor( LD4, COLOR_LightGray)
				Call XSalTblSetCellBackColor( LD5, COLOR_LightGray)
				Call XSalTblSetCellBackColor( LD6, COLOR_LightGray)
				! 13. Лiмiт розрахований    - голубой
				Call XSalTblSetCellBackColor( LR1, COLOR_Sky )
				Call XSalTblSetCellBackColor( LR2, COLOR_Sky )
				Call XSalTblSetCellBackColor( LR3, COLOR_Sky )
				Call XSalTblSetCellBackColor( LR4, COLOR_Sky )
				Call XSalTblSetCellBackColor( LR5, COLOR_Sky )
				Call XSalTblSetCellBackColor( LR6, COLOR_Sky )
				! 14. Лiмiт затверджений
				Call XSalTblSetCellBackColor( LZ1, COLOR_LightGreen)
				Call XSalTblSetCellBackColor( LZ2, COLOR_LightGreen)
				Call XSalTblSetCellBackColor( LZ3, COLOR_LightGreen)
				Call XSalTblSetCellBackColor( LZ4, COLOR_LightGreen)
