			Function: All_File
				Description: Авто-прием всех файлов 
				Returns
				Parameters
				Static Variables
				Local variables
					String: sIn 
					String: sOk
					String: sBak
					Number: nFileID
					String: sFA
					String: mFA
					String: cFA
					Date/Time: fDAT
					Number: nI
					Number: nD
					String: S2
					String: sFileM[*]
					Number: nNumFiles
					!
					String: sFAS
					String: sFileS[*]
					Number: nNumFilesS
					Number: nKol_F
					!
					Date/Time: DATS 
					String: DDMMYYYY
					Number: nYear
					Number: nMonth
					Number: nDay
					String: MFOS
					String: SAB3
				Actions
					Call SalWaitCursor(TRUE)
					Call SalUseRegistry(FALSE, '')
					Call SalGetProfileString(INI_ElektroKlients, 'ANB_IN', '', sIn, GetIniFileName() )
					Call SalGetProfileString(INI_ElektroKlients, 'ANB_OK', '', sOk, GetIniFileName())
					Call SalGetProfileString(INI_ElektroKlients, 'ANB_BAK','', sBak, GetIniFileName())
					If SalFileSetCurrentDirectory(sIn)
					Else
						Call MessageAttention('Нет каталога: ' ||   sIn)
						Return -1
					! Выбор файлов #0? разных
					Set sFA = sIn || '\\#0??????.'|| sGxx ||'??'
					Set nNumFiles = VisDosEnumFiles( sFA, FA_Standard, sFileM)
					If nNumFiles > 0
						Set nI = 0
						Set nKol_F = 0
						While nI < nNumFiles 
							Set S2=Subs(sFileM[nI],3,1)
							! только #01, #02
							If S2 ='1' or S2='2'
								Set sFA = SalStrRightX( sFileM[nI], 12 )
								Set nFileID = F_file1( sFA, fDAT )
								Set cFA =  sOk || '\\'||sFA
								If File(cFA)
									Call VisFileDelete(cFA)
								Set nD = VisFileRename(sFA, cFA )
								If nD < 0
									Call SalMessageBox('Не удалось перенести файл ' || cFA, 'Ошибка !', MB_Ok)
								!
								If nFileID >0
									! доприем файла 611
									Set sFAS = STRING_Null
									! предполагаемое имя файла
									Set sFA = 'S'||Subs(sFA,4,3)||SalFmtFormatDateTime( AddMonths(fDAT,-1), 'MMyy' )||'.*'
									Set nNumFilesS = VisDosEnumFiles( sFA, FA_Standard, sFileS)
									If nNumFilesS <= 0
										Set sFA = 'M'||Subs(sFA,2,11)
										Set nNumFilesS = VisDosEnumFiles( sFA, FA_Standard, sFileS)
									If nNumFilesS > 0
										Set sFA = SalStrRightX( sFileS[0], 12 )
										! существующее имя файла
										If SqlPrepareAndExecute( hSql(), "SELECT fa INTO :sFAS FROM anb_611 WHERE FILE_ID=:nFileID ") 
												and not SqlFetchNext( hSql(), nFetchRes )
											Set sFAS = STRING_Null
										Call F_fileS1( sFAS, sFA, nFileID )
										Set cFA =  sOk || '\\'||sFA
										If File(cFA)
											Call VisFileDelete(cFA)
										Set nD = VisFileRename(sFA, cFA )
										If nD < 0
											Call SalMessageBox('Не удалось перенести файл ' || cFA, 'Ошибка !', MB_Ok)
							Set nI = nI + 1
							Set nKol_F = nKol_F + 1
							If nKol_F >= nKol_P
								Call SalWaitCursor ( FALSE )
								If SalMessageBox ( 
										 'Обработано файлов #01/#02  '||Str(nI)||'. Продолжить прием ?',
										 'Внимание !', MB_YesNo | MB_IconQuestion ) = IDNO  
									Break
								Set nKol_F = 0
						Call SalStatusSetText(hWndForm, 'Файлы # (+связанные S) приняты.')
					! Выбор файлов 611 разных (если уже есть #0?)
					Set sFA = sIn || '\\S???????.???'
					Set nNumFiles = VisDosEnumFiles( sFA, FA_Standard, sFileM)
					If nNumFiles > 0
						Set nI = 0
						Set nKol_F = 0
						While nI < nNumFiles 
							Set sFA = SalStrRightX( sFileM[nI], 12 )
							! Найти nFileID - файл #0?, которой он может соответствовать
							! Предполож дата DATS
							Set nYear = 2000+Val(Subs(sFA,7,2))
							Set nMonth=Val(Subs(sFA,5,2))
							Set nDay = 01
							Set DATS = AddMonths( SalDateConstruct (nYear,nMonth,nDay,0,0,0),1)
							Set DDMMYYYY = SalFmtFormatDateTime( DATS, 'ddMMyyyy' )
							! Предполож SAB
							Set SAB3 = SalStrUpperX( Subs(sFA,2,3))
							!
							If SqlPrepareAndExecute( hSqlAux2(),
									"SELECT max(f.FILE_ID) into :nFileID from banks b, ANB_IN_FILES f 
									 where nvl(b.blk,0)<>4 and UPPER(substr(b.sab,2,3))=:SAB3 and b.mfo=f.mfo 
									   and f.NEXT_DATE=:DDMMYYYY " ) 
									and SqlFetchNext( hSqlAux2(), nFetchRes )
								If nFileID >0
									! доприем файла 611
									Set sFAS = STRING_Null
									! Еще раз предполагаемое-оно же факт имя файла
									! Set sFA = 'S'||Subs(sFA,4,3)||SalFmtFormatDateTime( fDAT-1, 'MMyy' )||'.*'
									If SqlPrepareAndExecute( hSql(), "SELECT fa INTO :sFAS FROM anb_611 WHERE FILE_ID=:nFileID ") 
											and not SqlFetchNext( hSql(), nFetchRes )
										Set sFAS = STRING_Null
									Call F_fileS1( sFAS, sFA, nFileID )
									Set cFA =  sOk || '\\'||sFA
									If File(cFA)
										Call VisFileDelete(cFA)
									Set nD = VisFileRename(sFA, cFA )
									If nD < 0
										Call SalMessageBox('Не удалось перенести файл ' || cFA, 'Ошибка !', MB_Ok)
									Set nKol_F = nKol_F + 1
								Set nI = nI + 1
							If nKol_F >= nKol_P
								Call SalWaitCursor ( FALSE )
								If SalMessageBox ( 
										 'Обработано файлов S(611) '||Str(nI)||'. Продолжить прием ?',
										 'Внимание !', MB_YesNo | MB_IconQuestion ) = IDNO  
									Break
								Set nKol_F = 0
						Call SalStatusSetText(hWndForm, 'Файлы приняты.')
						Set L_FA=''
					Call SalWaitCursor(FALSE)
			Function: uf_get_file
				Description: Принять 1 файл отчетности НБУ по выбору
				Returns
					Number:
				Parameters
				Static Variables
				Local variables
					String: strFilters[*]
					Number: nIndex
					String: strFileName
					String: strFilePath
					!
					Number: nFileID
					Date/Time: fDAT
				Actions
					Set strFilters[0] = '#01, #02'
					Set strFilters[1] = '#0*.' || sGxx || '*'
					Set nIndex = 1
					!
					Set nFileID = -1
					If SalDlgOpenFile ( hWndForm, 'Выбор файла отчетности НБУ', strFilters, 2, nIndex, strFileName, strFilePath )
						Set nFileID = F_file1( strFileName,fDAT )
						If nFileID > 0
							Call SalMessageBox ('Файл ' || strFileName || ' принят', 'Сообщение', MB_Ok | MB_IconExclamation)
							Set sFILE=strFileName
							Set nID=nFileID
					!
					Set L_FA = ''
					Return nFileID
			Function: uf_get_fileS
				Description: Принять 1 файл отчетности S по выбору
				Returns
					Number:
				Parameters
				Static Variables
				Local variables
					String: strFilters[*]
					String: strFileName
					String: strFilePath
					Number: nIndex
					! Number: nFILE_ID
					! File Handle: inFile
					! Long String: sTiket
					String: sFAS
				Actions
					Set strFilters[0] = '_.Форма-611, '||MFO||', '|| SalFmtFormatDateTime( DAT, 'dd/MM/yyyy' )
					Set strFilters[1] = '?'||SAB3||SalFmtFormatDateTime( AddMonths(DAT,-1), 'MMyy' )||'.*'
					Set nIndex = 1
					If SalDlgOpenFile ( hWndForm, 'Выбор файла S.Форма-611', strFilters, 2, nIndex, strFileName, strFilePath )
						Set strFileName = SalStrUpperX( strFileName )
						Set sFAS = T1.FAS
						If F_fileS1( sFAS, strFileName, T1.FILE_ID ) > 0
							Set T1.FAS = sFAS
							Set L_FA = ''
							Return T1.FILE_ID
					Return -1
			Function: F_file1
				Description: Принять файл отчетности НБУ.
				Returns
					Number:
				Parameters
					String: sFA
					Receive Date/Time: fDAT
				Static Variables
				Local variables
					String: strFileName
					File Handle: inFile
					String: strBuffer
					Number: i
					Number: nStrLen
					Number: nOffset
					String: strParsed[*]
					Boolean: bHeader
					String: sFA8
					String: sFA10
					Number: nFileID
					Number: nIsResident
					Number: nNBUCode
					!
					String: strFilePath
					Number: nIndex
					Number: nErr
					Number: nExists
					Number: lPoz
				Actions
					Set strFileName = SalStrUpperX( sFA )
					Set L_FA = 'Принимается файл '|| strFileName
					Set bHeader = TRUE
					!
					If not SalFileOpen ( inFile, strFileName, OF_Read )
						Call SalMessageBox ( 'Ошибка открытия файла ' || strFileName  , 'Ошибка', MB_Ok | MB_IconHand )
						Return -1
					Call SalWaitCursor ( TRUE )
					If not SalFileGetStr ( inFile, strBuffer, 256 )
						Call SalMessageBox ( 'Ошибка чтения файла ' || strFileName  , 'Ошибка', MB_Ok | MB_IconHand )
						Return -1
					! Служебная строка
					If SalStrLength ( strBuffer ) != 100
						Call SalWaitCursor( FALSE )
						Call SalFileClose ( inFile )
						Call SalMessageBox ( 'Файл ' || strFileName || ' не соответсвует формату!', 'Ошибка [Служебная]', MB_Ok | MB_IconHand )
						Return -1
					Set lPoz = 101
					! Сухова
					While SalFileGetStr ( inFile, strBuffer, lPoz )
						Set i = 0
						Loop
							Set nStrLen = SalStrLength ( strBuffer )
							Set nOffset = SalStrScan ( strBuffer, '=' )
							If nOffset < 0
								Set strParsed[i] = strBuffer
								If bHeader
									If SalStrLength (strParsed[9])<12 or SalStrLength (strParsed[6])<6 
											   or SalStrLength (strParsed[1])<8 or SalStrLength (strParsed[4])<8
											   or SalStrLength (strParsed[8])<9
										Call SalMessageBox ( 'Ошибка реквизита заглавной строки файла ' || strFileName  , 'Ошибка', MB_Ok | MB_IconHand )
										! Call SalFileClose ( inFile )   ! если закрывать файл, то будет переноситься в ОК
										Call SqlPrepareAndExecute( hSql(), 'ROLLback') 
										Return -1
									! Проверка уникальности файла
									Set sFA8 = Subs(strFileName,1,8)
									Set sFA10 = Subs(strFileName,1,10)
									If SqlPrepareAndExecute (hSql(),
											  "SELECT FILE_ID INTO :nFileID FROM anb_in_files 
											   WHERE UPPER(substr(FILE_NAME,1,10))=:sFA10 and 
											        form_date=:strParsed[4] and mfo=:strParsed[6] " )
											and SqlFetchNext ( hSql (), nErr )
										Call SalWaitCursor ( FALSE )
										If SalMessageBox ( 
												 'Файл '||sFA10||' за дату '||strParsed[4]||' от банка с МФО '||strParsed[6]||' уже принят.'||'Заменить ?',
												 'Внимание !', MB_YesNo | MB_IconQuestion ) = IDNO
											Set sFILE=strFileName
											Call SalFileClose ( inFile )
											Return nFileID  !! Возвращается значение ПК нашего файла :)
										If SqlPrepareAndExecute (hSql(),"delete from anb_IN_INF_RECORDS where file_id=:nFileID" ) 
												and SqlPrepareAndExecute(hSql(),"delete from anb_611t           where file_id=:nFileID" ) 
												and SqlPrepareAndExecute(hSql(),"delete from anb_611            where file_id=:nFileID" ) 
												and SqlPrepareAndExecute(hSql(),"delete from anb_IN_FILES where file_id=:nFileID" ) 
										Call SalWaitCursor ( TRUE )
									! Запись заголовка
									If SqlPrepareAndExecute ( hSql (), 
											  "insert into ANB_IN_FILES 
											       (FILE_ID, FILE_KEY, NEXT_DATE, INIT_DATE, LAST_DATE, FORM_DATE, FORM_TIME, MFO, UNIT, 
											                            INF_ROW_COUNT, FILE_NAME, SPARE1 )
											   values (0, :strParsed[0], :strParsed[1], :strParsed[2], :strParsed[3],
											              :strParsed[4], :strParsed[5], :strParsed[6], :strParsed[7], 
											              :strParsed[8], :strParsed[9], :strParsed[10] ) " ) 
									Else
										Call MessageAttention('Ошибка приема: ' || sFA10)
										! Call SalFileClose ( inFile )
										Call SqlPrepareAndExecute( hSql(), 'ROLLback') 
										Return -1
									If SqlPrepareAndExecute( hSql (), 
											   "select S_RNBU_IN_FILES.currval, to_date( :strParsed[1],'ddmmyyyy')
											    into :nFileID, :fDAT from dual " )
											and SqlFetchNext ( hSql (), nErr )  
									Set bHeader = FALSE
									Set lPoz = 256
									Call SalFileSeek ( inFile, 252, FILE_SeekBegin  )
								Else
									! Запись информационных строк
									If ( SalStrLeftX ( strParsed[0], 1 ) != '#' )
										Set strParsed[0] = SalStrTrimX( strParsed[0] )
