				Set PAS.strFilterTblName = 'ANB_MFO_PAS'
				Set PAS.fFilterAtStart = 1
				Call SalTblSetLockedColumns( hWndForm, 6 )
				If SqlPrepareAndExecute( hSql()," SELECT last_day(gl.BD)+1 into :D1 from dual ")
						and SqlFetchNext( hSql(), nFetchRes )
				Set PAS.strSqlPopulate =
						"SELECT l.MFO,l.nb,l.ID,l.name,l.stat, ANB_MFO_PAS.FDAT,ANB_MFO_PAS.LIMP 
						 INTO :PAS.MFO,:PAS.NB,:PAS.ID,:PAS.NAME,:PAS.STAT,:PAS.FDAT, :PAS.LIMP 
						from ANB_MFO_PAS, anb_lap l 
						where ANB_MFO_PAS.mfo=l.mfo and ANB_MFO_PAS.id=l.id 
						Order By ANB_MFO_PAS.fdat desc, l.stat,l.mfo,l.id " 
				Set PAS.strSqlUpdate =
						"update ANB_MFO_PAS set limp=:PAS.LIMP where MFO=:PAS.MFO and id=:PAS.ID and fdat=:PAS.FDAT" 
				Set PAS.strSqlDelete =
						"delete from ANB_MFO_PAS where MFO=:PAS.MFO and id=:PAS.ID and fdat=:PAS.FDAT" 
