					Number: G17l
					Number: G19b 
					Number: G19l
					Number: G20b 
					Number: G20l
					Number: G21b 
					Number: G21l
					Number: G22b 
					Number: G22l
					Number: nYear
					Number: nMonth
				Actions
					Set i = 4
					Set nMonth = Val (Subs(FA, 5,2) )
					Set nYear = 2000+ Val (Subs(FA, 7,2) )
					Set DAT = SalDateConstruct( nYear, nMonth, 01, 0, 0, 0 )
					If SqlPrepareAndExecute( hSql(),
							"select ID,LNG,  FRS, FRSb, G01b, G01l, G02b, G02l, G03b, G03l, G04b, G04l,
							    G05b, G05l, G06b, G06l, G07b, G07l, G08b, G08l, G11b, G11l, G13b, G13l, G14b,
							    G14l, G16b, G16l, G17b, G17l, G19b, G19l, G20b, G20l, G21b, G21l, G22b, G22l 
							 into :ID,:LNG, :FRS,:FRSb,:G01b,:G01l,:G02b,:G02l,:G03b,:G03l,:G04b,:G04l,
							   :G05b,:G05l,:G06b,:G06l,:G07b,:G07l,:G08b,:G08l,:G11b,:G11l,:G13b,:G13l,:G14b,
							   :G14l,:G16b,:G16l,:G17b,:G17l,:G19b,:G19l,:G20b,:G20l,:G21b,:G21l,:G22b,:G22l  
							 from ANB_F611 where dat = (select max(dat) from ANB_F611 where dat<=:DAT)" )
							and not SqlFetchNext( hSql(), nFetchRes )
						Return FALSE
					Set s7 = '       '
					Set nVRS = ID
					! Открыть файл
					Set sBuf = Spac(LNG)
					If SalFileSeek ( hF, 0, FILE_SeekBegin )
						! Читать строку While SalFileGetStr( hF, sBuf, 250 )
						While SalFileGetStr( hF, sBuf, LNG+1 )
							! Структура 611 пятая
							If Subs(sBuf, G01b+6, 2 )='20' OR 
									   StrDosToWinX( Subs(sBuf,FRSb,12) )=Subs(FRS,1,12)
								Set GS = IifS( Subs(sBuf,G01b+6,2)='20', 'G', 'S' )
								! Call Debug(sBuf) 
								If GS = 'S'
									! Н3
									Set S[4]=Subs(sBuf,G04b, G04l)
									! Н4
									Set S[5]=Subs(sBuf,G05b, G05l)
									! Н11
									Set S[17]=Subs(sBuf, G17b, G17l)
									If S[17] = s7
										Set S[17] ='0'
									! Н12
									Set S[19]=Subs(sBuf,G19b, G19l)
									If S[19] = s7
										Set S[19] ='0'
									! Н13
									Set S[20]=Subs(sBuf,G20b, G20l)
									If S[20] = s7
										Set S[20] ='0'
									! Н13-1
									Set S[21]=Subs(sBuf,G21b, G21l)
									If S[21] = s7
										Set S[21] ='0'
									!
									! Н13-2
									Set S[22] = Subs(sBuf, G22b, G22l)
									If S[22] = s7 
										Set S[22] ='0'
									Return 
											 SqlPrepareAndExecute( hSql(),"INSERT INTO anb_611t
											   (FILE_ID,G01,G04,G05,G17,G19,G20,G21,G22)  values
											   (:nFileID,'Середні',:S[4],:S[5],:S[17],:S[19],:S[20],:S[21],:S[22])" )
								Else
									! Дата
									Set G[1] = Subs(sBuf, G01b, 6 ) || Subs(sBuf, G01b+8, 2 )
									! Сдвиг даты на 1
									Set s1 =  Subs(sBuf, G01b+2 , 1)
									Set strPicture = 'dd'||s1||'MM'||s1||'yy'
									Set dDAT = SalFmtFormatStrDateTime( G[1], strPicture ) - 1
									Set G[1]= SalFmtFormatDateTime( dDAT, strPicture )
									! ! Колонки = пусто считать =0 , кроме H2, H5, H6
									!
									! H1 в коп
									Set G[2]= SalStrLeftX( Str(SalNumberRound( Val(Subs(sBuf,G02b,G02l))/100000 )),7)
									If G[2] = s7
										Set G[2] ='0'
									! Н2
									If Subs(sBuf, G03b, G03l) != s7
										Set G[3]=Subs(sBuf, G03b, G03l)
									! Н3
									Set G[4]=Subs(sBuf,G04b, G04l)
									If G[4] = s7
										Set G[4] ='0'
									! Н4
									Set G[5]=Subs(sBuf,G05b, G05l)
									If G[5] = s7
										Set G[5] ='0'
									! Н5
									If Subs(sBuf, G06b, G06l) != s7
										Set G[6]=Subs(sBuf,G06b ,G06l)
									! Н6
									If Subs(sBuf,G07b, G07l) != s7
										Set G[7]=Subs(sBuf, G07b , G07l)
									! Н7-Ф1
									Set G[8]=Subs(sBuf, G08b, G08l)
									If G[8] = s7
										Set G[8] ='0'
									! Н7-Ф2
									Set G[11]=Subs(sBuf, G11b, G11l)
									If G[11] = s7
										Set G[11] ='0'
									! Н8
									Set G[13]=Subs(sBuf, G13b, G13l)
									If G[13] = s7
										Set G[13] ='0'
									! Н9
									Set G[14]=Subs(sBuf, G14b, G14l)
									If G[14] = s7
										Set G[14] ='0'
									! Н10
									Set G[16]=Subs(sBuf, G16b, G16l)
									If G[16] = s7
										Set G[16] ='0'
									! Н11
									Set G[17]=Subs(sBuf, G17b, G17l)
									If G[17] = s7
										Set G[17] ='0'
									! Н12
									Set G[19]=Subs(sBuf, G19b, G19l)
									If G[19] = s7
										Set G[19] ='0'
									! Н13
									Set G[20]=Subs(sBuf, G20b, G20l)
									If G[20] = s7
										Set G[20] ='0'
									! Н13-1
									Set G[21]=Subs(sBuf, G21b, G21l)
									If G[21] = s7
										Set G[21] ='0'
									! Н13-2
									Set G[22]=Subs(sBuf,G22b, G22l)
									If G[22] = s7
										Set G[22] ='0'
									!
									! Call Debug(G[1])
									! Call Debug(G[2])
									! Call Debug(G[3])
									! Call Debug(G[4])
									! Call Debug(G[5])
									! Call Debug(G[6])
									! Call Debug(G[7])
									! Call Debug(G[11])
									! Call Debug(G[13])
									! Call Debug(G[14])
									! Call Debug(G[16])
									! Call Debug(G[17])
									! Call Debug(G[19])
									! Call Debug(G[20])
									! Call Debug(G[21])
									! Call Debug(G[22])
									If not SqlPrepareAndExecute( hSql(),
											   "INSERT INTO anb_611t
											(FILE_ID,G01,G02,G03,G04,G05,G06,G07,G08,G11,G13,G14,G16,G17,G19,G20,G21,G22)
											 values (:nFileID,:G[1],:G[2],:G[3],:G[4],:G[5],:G[6],:G[7],:G[8],
											:G[11],:G[13],:G[14],:G[16],:G[17],:G[19],:G[20],:G[21],:G[22])" )
										Return FALSE
							Set sBuf = Spac(LNG)
						Return FALSE
		Window Parameters
			Number: nMode
			String: strPar01 !!!!   = 'G' только банки =ЮЛ , !='G' - филиалы
			String: strPar02
		Window Variables
			Number: nCol [13]
			Number: nRep
			Number: nTmp
			Number: nID
			! String: sFCode
			String: sFILE
			String: sTIP
			String: sGxx
			String: s1
