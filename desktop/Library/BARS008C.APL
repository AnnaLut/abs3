		Internal Functions
			Function: Sel008c	! __exported
				Description: Bars008c_Selector
				Returns
				Parameters
					Window Handle: hWndParent
					Number: nMode
					Number: nPar
					String: strPar01
					String: strPar02
				Static Variables
				Local variables
					Number: nM1
					String: sTmp
				Actions
					Set nM1 = SalNumberMod( nMode, 10 )
					Call Bars008c_Init()
					If CP_PAY = STRING_Null
						If SqlPrepareAndExecute( hSql(), "SELECT val INTO :CP_PAY from params WHERE par='CP_PAY' and val='1' " ) 
								and not SqlFetchNext( hSql(), nFetchRes)
							Set CP_PAY = '0'
					If CP_DOPR = STRING_Null
						If SqlPrepareAndExecute( hSql(), "SELECT val INTO :CP_DOPR from params WHERE par='CP_DOPR' and val='1' " ) 
								and not SqlFetchNext( hSql(), nFetchRes)
							Set CP_DOPR = '0'
					!
					If nMode = 49
						! !
						If IsWindow(hWinFK)
							Call SalDestroyWindow(hWinFK )
						Set hWinFK = 
								SalCreateWindow( FCK, hWndMDI, NUMBER_Null, nPar, "Платіжний календар по ЦП ", strPar02 )
					! nMode = x0, x2, x5, x6
					Else If nM1=0 or nM1=2 or nM1=5 or nM1=6 ! Портфель ЦБ
						Set sTmp = 'Sel008%'|| Str(nMode) ||'%'|| strPar02 ||'%' 
						If SqlPrepareAndExecute( hSql(),
								  "SELECT CODEOPER INTO :nCODEOPER from operlist WHERE trim(FUNCNAME) like :sTmp " ) 
								and SqlFetchNext( hSql(), nFetchRes)
							! Call DebugN (nCODEOPER)
						If not IsWindow(hWinP)
							Set hWinP = SalCreateWindow( CPP, hWndMDI, nMode, nPar, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( CPP )
					! nMode = x7
					Else If nM1=7  !!!!!!!!!!!!!!!!!!!!!!!!!!! Рез.фонд для ЦП, що обліковуються по СОБІВАРТОСТІ
						If not IsWindow(hWinRZ)
							Set hWinRZ = SalCreateWindow( CPZ, hWndMDI, nMode, nPar, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( CPZ )
					! nMode = 21, -21, 28, 29 
					Else If nMode=21 or nMode=-21 
						If not IsWindow(hWinA)
							Set hWinA = SalCreateWindow( CP_ARCH, hWndMDI, nMode, nPar, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( CP_ARCH )
					Else If nMode=28 ! ЦП- Проспекти емісії
						If not IsWindow(hWinS)
							! Set hWinS = SalCreateWindow( CP_KOD, hWndMDI, nMode, nPar, strPar01, strPar02 )
							Set hWinS = SalCreateWindow( CP_KODN, hWndMDI, nMode, nPar, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( hWinS )
					Else If nMode=29 ! Дострокове погашення ДС за ініціативою банків
						If not IsWindow(hFA7)
							Set hFA7 = SalCreateWindow( FA7, hWndMDI, nMode, nPar, strPar01, strPar02 )
						Else
							Call SalBringWindowToTop ( hFA7 )
					! nMode = 23, 24, 28
					Else ! Архив, Анализ цены портфеля , Виза на сделку, Расчет средневзвешенного курса
						Select Case nMode
							! Case 21 ! Архив
								                                                                                                                                                                                                                                                                                                             If not IsWindow(hWinA)
									                                                                                                                                                                                                                                                                                                             Set hWinA = SalCreateWindow( CP_ARCH, hWndMDI, nMode, nPar, strPar01, strPar02 )
								                                                                                                                                                                                                                                                                                                             Else 
									                                                                                                                                                                                                                                                                                                             Call SalBringWindowToTop ( CP_ARCH )
								                                                                                                                                                                                                                                                                                                             Break 
							Case 23  ! Анализ цены портфеля 
								If not IsWindow(hWinB)
									Set hWinB = SalCreateWindow( CPB, hWndMDI, nMode, nPar, strPar01, strPar02 )
								Else
									Call SalBringWindowToTop ( CPB )
								Break
							Case 24 ! Первая виза начальника отдела
								If not IsWindow(hWinV)
									Set hWinV = SalCreateWindow( CP_VISA, hWndMDI, nMode, nPar, strPar01, strPar02 )
								Else
									Call SalBringWindowToTop ( CP_VISA )
								Break
							Case 28  ! Расчет средневзвешенного курса
								If not IsWindow(hWinBK)
									Set hWinBK = SalCreateWindow( CBK, hWndMDI, nMode, nPar, strPar01, strPar02 )
								Else
									Call SalBringWindowToTop ( CBK )
								Break
					!
