					Loop
						Set nZ = At(',', sTmp)
						If nZ > 0
							If nTmp = 0
								Set B_4621 = Left( sTmp, nZ-1)
							Else If nTmp = 1
								Set B_1819 =  Left( sTmp, nZ-1)
							Else If nTmp = 2
								Set B_1919 =  Left( sTmp, nZ-1)
							!
							Set nTmp = nTmp + 1
							Set sTmp = SalStrTrimX(Right( sTmp, Len(sTmp) - nZ) )
						Else
							Break
				! Set FA7.nTabInstance1 = 1
				! Set FA7.nTabInstance2 = 1
				Set FA7.strFilterTblName = 'CP_REE2'
				! Set FA7.fFilterAtStart = FALSE
				Call PrepareWindowEx( hWndForm )
