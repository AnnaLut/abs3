			On SAM_Create
				Call SalWaitCursor( TRUE )
				Call SalCenterWindow(hWndForm)
				Call SalSendMsgToChildren( hWndForm, UM_QueryLabelText, 0, 0 )
				Call SalSetWindowText( hWndForm, CurrentLangTable.GetAtomTitle('CBK.cTWndTitle') )
				Set DAT = GetBankDate()
				Set CBK.nTabInstance =1
				If nMode = 47 
					Call SalDisableWindow( pbUpdate )
