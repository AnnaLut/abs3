		Functions
			Function: NASELENIE 
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					Date/Time: vDat
					String: sRet
				Actions
					Call SalWaitCursor(TRUE)
					If SqlPrepareAndExecute( hSql(),"SELECT DAT_ROZ,N/100 INTO :vDat,:nN FROM cp_arch where ref=:REF")
							 and NOT SqlFetchNext( hSql(), nFetchRes) 
						Return FALSE
					! Построены ли потоки
					Call SqlPrepareAndExecute( hSql(),"select 1 from cp_many  where ref=:REF ")
					Set rb_OLD = SqlFetchNext( hSql(), nFetchRes)  
					Set rb_NEW = NOT ( rb_OLD)
					!
					If rb_NEW and nMode1 != 2 !!!!!  Создать денежные потоки из ГПК (nMode1=2 -  только просмотр)
						If NOT SqlPLSQLCommand( hSql(),"CP.RMany(REF,REF,vDat,0,0,sRet)" )
							Call SqlRollback(hSql())
							Return FALSE
						Call SqlCommit(hSql())
					! Вычитать денежные потоки
					Set nn = 1
					Set KolR = 0
					Set KolP = 0
					Set nDP = 0
					Call SalTblPopulate(hWndForm, hSql(),"SELECT FDAT,SS1,SDP,SN2 INTO :FDAT,:SS1,:SDP,:SN2 from CP_many where ref=:REF ORDER BY 1", TBL_FillAll )
					! Параметры сделки
					If SqlPrepareAndExecute( hSql(), "select o.nd,d.erat,d.erate into :NOM_UG,:IRR,:IRE from cp_deal d, oper o where d.ref=o.ref and o.ref=:REF ")
							and not SqlFetchNext( hSql(), nFetchRes)  
						Return FALSE
					Call SalWaitCursor(FALSE)
			Function: F_S
				Description: Поддержка скрытой колонки S "Суммарный поток (в коп)"
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Set S = (SS1 +SDP + SN2 ) * 100
					Set nDP = nDP + SDP
		Window Parameters
			Number: nMode1
			! nMode1 = 2 = только просмотр
			Number: REF
			Number: ACCA
			Number: ID
			String: strPar01
			String: strPar02
			Date/Time: pDAT_UG
		Window Variables
			Number: nIdN
			Number: nIdE
			Number: PERIOD_KUP
			!
			Number: nn
			Date/Time: Dati
			String: sErr
			String: sRef
			Number: SS1_
			Number: nRow
			Date/Time: DAT1_
			Number: n980
			Number: CENA
			!
			Number: nN
			Number: nDP
			Number: nR
			! Date/Time: DAT1
			Number: nInt1
			Number: nKol
			Number: kR2
			!
			Number: nTmpE
			String: sTmpE
			Date/Time: dTmpE
		Message Actions
			On SAM_Create
				Set dREF = REF
				Set dID = ID
				Set IRR0.strPrintFileName = 'СP_IRR'
				Set CP_ID = strPar01
				! Set NMK  = strPar02
				Set n980=GetBaseVal()
				Set DAT_UG = pDAT_UG
				Call SalSetWindowText( hWndForm, "Просмотр" || IifS(nMode1=2,"","/Модиф") || ":План-потоки ден.средств по сделке:"|| strPar01||' '||strPar02)
				Call SalTblSetLockedColumns( hWndForm, 5 )
				If SqlPrepareAndExecute( hSql(),
						 "select tip-1, PERIOD_KUP, dok, dnk, datp, dat_em, KY, cena, IR   , CENA_KUP 
						  into   :nIdN,:PERIOD_KUP,:DOK,:DNK,:DATP,:DAT_EM,:KY,:CENA,:IR_N0,:CENA_KUP   from  cp_kod  where id=:ID ")
						and SqlFetchNext( hSql(), nFetchRes) 
					If nIdN = 0
						Set nIdE = -2
					Else If nIdN = 1
						Set nIdE = -3
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On SAM_CreateComplete
				Call SalDisableWindow( rb_OLD )
				Call SalDisableWindow( rb_NEW )
				If nMode1 = 2  !!! только просмотр
					Call SalDisableWindow( pbDel ) 
					Call SalDisableWindow( pbUpdate ) 
				Call NASELENIE()
			On SAM_FetchRowDone
				! Номинал - серый
				Call XSalTblSetCellBackColor( SS1, COLOR_LightGray )
				! Купон - голубой
				Call XSalTblSetCellBackColor( SN2, SalColorFromRGB( 220,255,255 ) )
				! Дисконт-Премия - зеленый
				Call XSalTblSetCellBackColor( SDP,  SalColorFromRGB( 230,255,230 ) )
				Set KolR = KolR + 1
				If KolP = 0
					Set KolP = 1
				Else
					Set KolP = KolP + ( FDAT - Dati)
					If nInt1 = 0 and SN2 =0 and IR_N0 >0 
						Set kR2 = IR_N0 * ( FDAT - Dati) / 36500 
						Set SN2 = (nN/CENA) * SalNumberRound( CENA * 100 * kR2 ) / 100 
				Call F_S()
				Set Dati = FDAT
			On SAM_ColumnSelectClick
