			! Function: R_KUPON
				                                                                                                                                                          Description: 
				                                                                                                                                                          Returns 
					                                                                                                                                                          Number: nR2
				                                                                                                                                                          Parameters 
					                                                                                                                                                          Date/Time: DAT_ROZ ! Дата розрахунку (валютування) - право власності
					                                                                                                                                                          Number: nID ! ИД ЦП
					                                                                                                                                                          Number: SUMBN ! Номинальная сумма пакета (В целіх , т.е. 1000000,00
				                                                                                                                                                          Static Variables 
				                                                                                                                                                          Local variables 
					                                                                                                                                                          Date/Time: DOK ! Дата останнього  купону
					                                                                                                                                                          Date/Time: DAT_RR ! Дата объявления эмитентом получателя купона
					                                                                                                                                                          Date/Time: DNK ! Дата наступного купону
					                                                                                                                                                          Number: n980
					                                                                                                                                                          Number: CENA ! Номинал 1 шт 
					                                                                                                                                                          Number: CENA_KUP ! Фиксир.купон на Номинал 1 шт 
					                                                                                                                                                          Number: KY ! Кол во купонніх перодов в году
					                                                                                                                                                          Number: IR ! % ставка
					!
					                                                                                                                                                          Number: DEL_KUP1  ! длина ВСЕГО купоннго периода  в днях
					                                                                                                                                                          Number: DEL_KUP2  ! продаваемая длнина купоннго периода  в днях
					                                                                                                                                                          Number: KOL ! Кол ЦБ в пакете
					!
					                                                                                                                                                          Number: kR2 ! Расчетный коефф купона 
					                                                                                                                                                          Number: nR2 ! Расчетная сумма купона 
					                                                                                                                                                          Number: nR_All ! Купон за весь купонній период
					!
					                                                                                                                                                          Number: PR1_KUP 
							! PR1_KUP IS 
							1 = Начислать купон на номинал всего пакета,
							2 = Начислать купон на номинал 1 шт и умножать на кол-во в пакете
							
							 null = для вал 1 = на номинал пакета,
							        для грн 2 = на номинал 1 шт';
				                                                                                                                                                          Actions 
					                                                                                                                                                          Set n980 = GetBaseVal()
					                                                                                                                                                          If SqlPrepareAndExecute( hSqlAux2(), 
							"SELECT (SUMBN/CENA) * cp.kupon1(id,DAT_ROZ,:dat_em,datp) 
							
							 into  :DOK, :DNK,:CENA, :KY, :IR, :DAT_RR, :CENA_KUP, :PR1_KUP
							 from cp_kod 
							 where id=:nID and dox>1 and IR>0 
							   and nvl(DOK,DAT_EM) < :DAT_ROZ and Nvl(CENA,0)>0 " )
							and SqlFetchNext( hSqlAux2(), nFetchRes)
						! Рассчитать накоп купон от год % ставки или от ставки на период
						                                                                                                                                                          Set KOL = SUMBN / CENA 
						! DEL_KUP1 = кол дней в купонном периоде
						                                                                                                                                                          Set DEL_KUP1 = DNK - DOK 
						! DEL_KUP2 = кол дней уже прошло
						                                                                                                                                                          Set DEL_KUP2 = DAT_ROZ - IifD ( DAT_ROZ>DNK, DNK, DOK ) 
						!
						                                                                                                                                                          If CENA_KUP != NUMBER_Null and CENA_KUP > 0
							                                                                                                                                                          If PR1_KUP = 2
								                                                                                                                                                          Set nR2 = KOL * SalNumberRound( CENA_KUP *100*DEL_KUP2/DEL_KUP1 ) /100
							                                                                                                                                                          Else 
								                                                                                                                                                          Set nR2 = SalNumberRound( KOL * CENA_KUP *100*DEL_KUP2/DEL_KUP1 ) /100
							                                                                                                                                                          Return nR2
						                                                                                                                                                          If KY > 1
							                                                                                                                                                          Set kR2 = ( IR /KY ) * DEL_KUP2 / (100 * DEL_KUP1 )
							                                                                                                                                                          If PR1_KUP = 2
								                                                                                                                                                          Set nR2 = KOL * SalNumberRound( CENA * 100 * kR2 ) / 100 
							                                                                                                                                                          Else 
								                                                                                                                                                          Set nR2 = SalNumberRound(  KOL * CENA * 100 * kR2 ) / 100 
							                                                                                                                                                          If DAT_ROZ != DATETIME_Null  and  DOK != DATETIME_Null 
									and DAT_RR != DATETIME_Null  and  DNK != DATETIME_Null 
									and DOK <= DAT_RR and DAT_RR <= DAT_ROZ and DAT_ROZ <= DNK
								                                                                                                                                                          Set DEL_KUP2 = DEL_KUP1 - DEL_KUP2
								                                                                                                                                                          Set kR2 = ( IR /KY ) * DEL_KUP2 / (100 * DEL_KUP1 )
								                                                                                                                                                          If PR1_KUP = 2
									                                                                                                                                                          Set nR2 =  - KOL * SalNumberRound( CENA * 100 * kR2 ) / 100 
								                                                                                                                                                          Else 
									                                                                                                                                                          Set nR2 =  - SalNumberRound( KOL * CENA * 100 * kR2 ) / 100 
						                                                                                                                                                          Else 
							                                                                                                                                                          Set kR2 = IR * DEL_KUP2 / 36500 
							                                                                                                                                                          If PR1_KUP = 2
								                                                                                                                                                          Set nR2 = KOL * SalNumberRound( CENA * 100 * kR2 ) / 100 
							                                                                                                                                                          Else 
								                                                                                                                                                          Set nR2 = SalNumberRound( KOL * CENA * 100 * kR2 ) / 100 
					                                                                                                                                                          Else 
						                                                                                                                                                          Set nR2 = 0
					!
					                                                                                                                                                          Return nR2
			Function: R_KUPON
				Description:
				Returns
					Number: nR2
				Parameters
					Date/Time: DAT_ROZ ! Дата розрахунку (валютування) - право власності
					Number: nID ! ИД ЦП
					Number: SUMBN ! Номинальная сумма пакета (В целіх , т.е. 1000000,00
				Static Variables
				Local variables
					Date/Time: DOK ! Дата останнього  купону
					Date/Time: DAT_RR ! Дата объявления эмитентом получателя купона
					Date/Time: DNK ! Дата наступного купону
					Number: n980
					!
					Number: KUPON1 ! Расчетный купон в коп (от Ларисы, по графику купонных выплат )  
					!
					Number: CENA ! Номинал 1 шт 
					Number: CENA_KUP ! Фиксир.купон на Номинал 1 шт 
					Number: KY ! Кол во купонніх перодов в году
					Number: IR ! % ставка
					!
					Number: DEL_KUP1  ! длина ВСЕГО купоннго периода  в днях
					Number: DEL_KUP2  ! продаваемая длнина купоннго периода  в днях
					Number: KOL ! Кол ЦБ в пакете
					!
					Number: kR2 ! Расчетный коефф купона 
					Number: nR2 ! Расчетная сумма купона 
					Number: nR_All ! Купон за весь купонній период
					!
					Number: PR1_KUP 
							! PR1_KUP IS 
							1 = Начислать купон на номинал всего пакета,
							2 = Начислать купон на номинал 1 шт и умножать на кол-во в пакете
							
							 null = для вал 1 = на номинал пакета,
							        для грн 2 = на номинал 1 шт';
				Actions
					Set n980 = GetBaseVal()
					If SqlPrepareAndExecute( hSqlAux2(), 
							"SELECT nvl(DOK,DAT_EM), 
							        nvl(DNK, nvl(DOK,DAT_EM)+PERIOD_KUP), 
							        CENA, Nvl(KY,1), IR , DAT_RR, CENA_KUP , 
							        nvl( PR1_KUP, decode (kv, :n980, 2, 1) ) ,
							cp.KUPON1 ( id, :DAT_ROZ, dat_em, datp) 
							 into  :DOK, :DNK,:CENA, :KY, :IR, :DAT_RR, :CENA_KUP, :PR1_KUP, :KUPON1
							 from cp_kod 
							 where id=:nID and dox>1 and IR>0 
							   and nvl(DOK,DAT_EM) < :DAT_ROZ and Nvl(CENA,0)>0 
							   and DNK > :DAT_ROZ " )
							and NOT SqlFetchNext( hSqlAux2(), nFetchRes)
						Return 0
					! ------
					Set KOL = SUMBN / CENA 
					! Call DebugN(PR1_KUP)
					! Call DebugN(KOL)
					! Call DebugN(KUPON1)
					If PR1_KUP = 2 
						Set nR2 = KOL * KUPON1 / 100 
						Return nR2
					! ------------------------
					! Рассчитать накоп купон от год % ставки или от ставки на период
					! DEL_KUP1 = кол дней в купонном периоде
					Set DEL_KUP1 = DNK - DOK 
					! DEL_KUP2 = кол дней уже прошло
					! !!
					! 06-12-2010 Д.И. примпр вал ЦБ 1019. Забрать -1
					Set DEL_KUP2 = DAT_ROZ - IifD ( DAT_ROZ >DNK, DNK, DOK ) 
					! Set DEL_KUP2 = DAT_ROZ - IifD ( DAT_ROZ >DNK, DNK, DOK ) + 1
					!
					If CENA_KUP != NUMBER_Null and CENA_KUP > 0
						! If PR1_KUP = 2
							                                                                                                                                        Set nR2 = KOL * SalNumberRound( CENA_KUP *100*DEL_KUP2/DEL_KUP1 ) /100
						! Else
							                                                                                                                                        Set nR2 = SalNumberRound( KOL * CENA_KUP *100*DEL_KUP2/DEL_KUP1 ) /100
						Set nR2 = SalNumberRound( KOL * CENA_KUP *100*DEL_KUP2/DEL_KUP1 ) /100
						Return nR2
					If KY > 1
						Set kR2 = ( IR /KY ) * DEL_KUP2 / (100 * DEL_KUP1 )
						If PR1_KUP = 2
							Set nR2 = KOL * SalNumberRound( CENA * 100 * kR2 ) / 100 
						Else
							Set nR2 = SalNumberRound(  KOL * CENA * 100 * kR2 ) / 100 
						If DAT_ROZ != DATETIME_Null  and  DOK != DATETIME_Null 
								and DAT_RR != DATETIME_Null  and  DNK != DATETIME_Null 
								and DOK <= DAT_RR and DAT_RR <= DAT_ROZ and DAT_ROZ <= DNK
							Set DEL_KUP2 = DEL_KUP1 - DEL_KUP2
							Set kR2 = ( IR /KY ) * DEL_KUP2 / (100 * DEL_KUP1 )
							If PR1_KUP = 2
								Set nR2 =  - KOL * SalNumberRound( CENA * 100 * kR2 ) / 100 
							Else
								Set nR2 =  - SalNumberRound( KOL * CENA * 100 * kR2 ) / 100 
					Else
						Set kR2 = IR * DEL_KUP2 / 36500 
						If PR1_KUP = 2
							Set nR2 = KOL * SalNumberRound( CENA * 100 * kR2 ) / 100 
						Else
							Set nR2 = SalNumberRound( KOL * CENA * 100 * kR2 ) / 100 
					!
					Return nR2
			Function: F_PODPIS
				Description:
				Returns
					Boolean:
				Parameters
					String: sREF
				Static Variables
				Local variables
					Number: nRef
					: cDoc
						Class: cDoc
					Boolean: FL
				Actions
					If GetBaseVal() =981
						Return TRUE
					! подпись
					Set FL = FALSE
					If SqlPrepareAndExecute( hSqlAux(),
							  "SELECT ref INTO :nRef FROM oper WHERE ref in ("|| sREF || ")" )
						While SqlFetchNext( hSqlAux(), nFetchRes  )
							If cDoc.SetDocR(nRef, TRUE ) 
								Set cDoc.m_sOperId = GetIdOper( )
								If cDoc.sDoc() 
									If SqlPrepare(hSqlAux2(), 
											  "UPDATE oper 
											   SET sign =:cDoc.m_lsSign, ref_a=:cDoc.m_sRef_A, id_o =:cDoc.m_sOperId 
											   WHERE ref=:cDoc.m_nRef " )
										If SqlSetLongBindDatatype(1, BLOB_BYTE)
											If SqlExecute( hSqlAux2() )
												Set FL = TRUE
											Else
												Break
										Else
											Break
									Else
										Break
								Else
									Break
							Else
								Break
					!
					If FL 
						Return TRUE
					Else
						Call SqlRollback(hSql())
						Call SalMessageBeep(MB_IconStop)
						Call MessageNoWait("Оперцію НЕ виконано ! Підпис документів",'',2,1)
						Return FALSE
					!
			Function: F_TIKET_CB
				Description:
				Returns
					Boolean:
				Parameters
					Number: nCheckId
					!
					Number: REF
					Number: nRun
				Static Variables
				Local variables
					String: strReport
					File Handle: hFile
					Long String: sTiket
				Actions
					! прочитать из архива этот REF и его показать
					If SqlPrepareAndExecute( hSql(), 
							  "SELECT sTiket INTO :sTiket from CP_ARCH where ref=:REF and sTiket is not null " )
							and SqlFetchNext( hSql(), nFetchRes )
						If nRun = 1 ! - Toлько печать
							Set strReport = GetPrnDir()||'\\CP'||SalFmtFormatDateTime(SalDateCurrent() ,'hhhmmss') ||
									    '.D' || SalStrRightX('0' || Str(SalDateDay(SalDateCurrent())), 2)
							If SalFileOpen( hFile, strReport, 1 ) 
								Call SalFileWrite( hFile, sTiket, SalStrLength(sTiket) )
								Call SalFileClose( hFile )
								Call DosDirectPrint(strReport)
							Else
								Call MessageNoWait("Неможливо відкрити файл для відтворення тікету ! "||strReport ,'',2,1)
								Return FALSE
						Else ! Прсмотр(0), печать(0), визирование (2)
							Call SalModalDialog(dlg_TIK, hWndForm, nCheckId, nRun, REF, sTiket )
					Else
						Call MessageNoWait("Неможливо знайти тікет в архіві ! " ,'',2,1)
						Return FALSE
			Function: DocsSaldoPrintK
				Description: Печать бухгалтерских проводок c консолидир счетами по документу
				Returns
					Boolean:
				Parameters
					Number: nDocRef
					String: strFileName
				Static Variables
				Local variables
					: conn
						Class: cABSConnect
					!
					File Handle: hFile
					!
					String: sTT
					Number: nDk
					String: sNls
					Number: nKv
					String: sNlsK
					Number: nDig
					Date/Time: dtFdat
					Number: nS
					Number: nSQ
					String: sTxt
					!
					String: sLine
				Actions
					Call XConnectGetParams(conn)
					If NOT conn.Connect()
						Return FALSE
					If NOT SalFileOpen(hFile, strFileName, OF_Write | OF_Text | OF_Append)
						Call conn.Disconnect()
						Return FALSE
					!
					Call SalFilePutStr(hFile, '')
					Call SalFilePutStr(hFile, '   Перелік транзакцій для документа # ' ||  Str(nDocRef))
					Call SalFilePutStr(hFile, '')
					Call SalFilePutStr(hFile, 'Тип:  Дата  : Рахунок доч. :Вал: Рахунок конс.:    Дебет      :    Кредит     :   Еквівалент  :')
					Call SalFilePutStr(hFile, 'трн:валютув.:              :доч:              :               :               :               :')
					Call SalFilePutStr(hFile, SalStrRepeatX('-', 95))
					!
					If not SqlPrepareAndExecute(conn.hSql(), '
							   SELECT o.tt,o.dk,a.nls,a.kv,t.dig,o.fdat,o.s,o.sq,o.txt, k.nls
							   INTO :sTT,:nDk,:sNls,:nKv,:nDig,:dtFdat,:nS,:nSQ,:sTxt ,:sNlsK
							   FROM opldok o, accounts a, tabval t, accounts k 
							   WHERE t.kv=a.kv and a.acc=o.acc and o.ref=:nDocRef and a.accc=k.acc (+) 
							   ORDER BY o.fdat,a.kv,o.tt,o.stmt,o.dk,o.s')
						Call conn.Disconnect()
						Return FALSE
					While SqlFetchNext(conn.hSql(), nFetchRes)
						Set sLine = PadL(sTT,3) || ' ' ||
								    SalFmtFormatDateTime(dtFdat, 'dd/MM/yy') || ' ' ||
								    PadR(sNls,14) || PadL(Str(nKv), 4) || ' ' || PadR(sNlsK,14) ||
								    IifS(nDk,
								        Spac(16) || PadL(SalNumberToStrX(nS/SalNumberPower(10,nDig), nDig), 15)  || ' ',  
								        PadL(SalNumberToStrX(nS/SalNumberPower(10,nDig), nDig), 15) || Spac(17)) || 
								    PadL(SalNumberToStrX(nSQ/100, 2), 15) || ' ' !! || sTxt
						Call SalFilePutStr(hFile, sLine)
					!
					! Call SalFilePutStr(hFile, '')
					Call SalFilePutStr(hFile, SalStrRepeatX('- ', 48))
					!
					Call SalFileClose(hFile)
					Call conn.Disconnect()
					Return TRUE
			!
