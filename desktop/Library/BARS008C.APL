			Function: F_ACC
				Description:
				Returns
				Parameters
					Number: nAcc
				Static Variables
				Local variables
					Number: nAccD
					Number: nAccP
					Number: nAccR
					Number: nAccR2
					Number: nAccS
					Number: nAccS5
					Number: nAccS6
					Number: nACCEXPN
					Number: nACCEXPR
					String: sSql_ACC
				Actions
					If GetBankMfo() ='300001'
						Set sSql_ACC = 
								"SELECT nvl(ACCD,-9), nvl(ACCP,-9),nvl(ACCR,-9), nvl(ACCR2,-9), nvl(ACCS,-9), nvl(ACCS5,-9), nvl(ACCS6,-9)
								 INTO :nAccD, :nAccP, :nAccR, :nAccR2, :nAccS, :nAccS5, :nAccS6  FROM cp_deal  WHERE ref=" || Str(CPP.REF) 
					Else
						Set sSql_ACC = 
								"SELECT nvl(ACCD,-9), nvl(ACCP,-9),nvl(ACCR,-9), nvl(ACCR2,-9), nvl(ACCS,-9) , nvl(ACCEXPN,-9), nvl(ACCEXPR,-9)
								 INTO :nAccD, :nAccP, :nAccR, :nAccR2, :nAccS, :nACCEXPN, :nACCEXPR  FROM cp_deal  WHERE ref=" || Str(CPP.REF) 
						Set nAccS5 = -9
						Set nAccS6 = -9
					If SqlPrepareAndExecute( hSqlAux2(), sSql_ACC) and SqlFetchNext ( hSqlAux2(), nFetchRes)
						Set sTmp = 
								" (a.acc in (" ||Str(nAcc) ||","|| Str(nAccD )|| "," || Str(nAccP ) ||","||Str(nAccR)||","|| Str(nAccR2)||","||
								                Str(nAccS )||","|| Str(nAccS5)|| "," || Str(nAccS6) || " ," || Str(nACCEXPN) || "," || Str(nACCEXPR) || ") 
								  OR a.acc in (select acc from cp_ref_acc where ref=" || Str(CPP.REF) ||  " ) 
								  )"
						If nMode1 = 2
							Call ShowAccList ( NUMBER_Null, AVIEW_ALL, AVIEW_ReadOnly | AVIEW_AllOptions, sTmp )
						Else
							Call ShowAccF(hWndMDI, sTmp )
			Function: F_AMORT
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
				Actions
					Call SalWaitCursor(TRUE)
					Set nRow = TBL_MinRow
					!
					While SalTblFindNextRow( hWndForm, nRow, 0, 0 )
						Call SalTblSetContext( hWndForm, nRow )
						If nRow >=0 and 
								SqlPLSQLCommand( hSqlAux(), "CP.CP_AMOR ( CPP.REF, CPP.ID, nGrp, aDAT, sErr )") 
							If sErr != STRING_Null 
								Break
					Call SalWaitCursor(FALSE)
					If sErr = STRING_Null 
						Call SqlCommit( hSqlAux() )
						Call MessageNoWait( CurrentLangTable.GetAtomTitle('CPP.cTMess1'),'',2,0)
						Return TRUE
					Else
						Call SqlRollback( hSqlAux() )
						Call SalMessageBeep(MB_IconStop)
						Call MessageNoWait( CurrentLangTable.GetAtomTitle('CPP.cTMess2')|| sErr ,'',2,1)
						Return FALSE
