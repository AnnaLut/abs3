		Window Parameters
			Number: nMode
			Number: nPar
			String: strPar01
			String: strPar02
		Window Variables
			Number: hBitmap
			String: strRefSemantic		! Семантика справочника
			String: strRefName
			Number: i
			String: sTmp
			Number: nId
		Message Actions
			On SAM_Create
				Set hBitmap = XSalBitmapFromResource( 'bmpEditLine' )
				Set strRefName = 'CP_KOD'
				Call SqlPrepareAndExecute(hSql(), "SELECT semantic INTO :strRefSemantic FROM meta_tables WHERE tabname=:strRefName")
				If not SqlFetchNext(hSql(), nFetchRes)
					Call SalMessageBox('Отсутсвует описание данной таблицы в МетаБазе!', 'Ошибка', MB_IconStop | MB_Ok)
					Call SalDestroyWindow(hWndForm)
				Else
					Call SalCenterWindow(hWndForm)
					! Параметры окна
					Set hWndForm.CP_KODN.strPrintFileName = SalStrLeftX(strRefName, 8) || '.ref'
					! nFlags
					Set hWndForm.CP_KODN.nFlags = GT_NoRet
					! Параметры таблицы
					Set hWndForm.CP_KODN.tblNSI.strNSITableName = strRefName
					Set hWndForm.CP_KODN.tblNSI.strNSIFilterTblName = strRefName
					Set hWndForm.CP_KODN.tblNSI.strNSITblFilter = "tip="||Str(nPar)
					Set hWndForm.CP_KODN.tblNSI.nNSIMode = 0
					Set hWndForm.CP_KODN.tblNSI.nNSIModeEdit = 1
					!
					Call SalSendClassMessage(SAM_Create, 0, 0)
					Call SalSetWindowText(hWndForm, strRefSemantic)
			On SAM_CreateComplete
				If NOT SetQueryFilter(hWndForm.CP_KODN.tblNSI.cF)
					Call SalSendMsg(pbExit, SAM_Click, 0, 0)
				Call SalSendClassMessage(SAM_CreateComplete, wParam, lParam)
			On SAM_Destroy
				Call SalStatusSetText(hWndForm, '')
				Call XSalBitmapClose(hBitmap)
				Call SalSendClassMessage(SAM_Destroy, 0, 0)
				Call SalSetFocus(SalParentWindow(hWndForm))
