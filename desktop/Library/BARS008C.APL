				! Set OTLA
				Set G2 = G3 + G4 
				Set G8 = G7 - G6 
				If Q != NUMBER_Null
					Set G9 = Q - G8
					Set DNEY = FDAT - DATi 
				Set Q = G8
				Set DATi = FDAT
				Set I1 = I1 + G1
				Set I2 = I2 + G2
				Set I3 = I3 + G3
				Set I4 = I4 + G4
				Set I5 = I5 + G5
				Set I9 = I9 + G9
				Set G9a = G9/KOL
				Set K3 = -I3 / CENA 
