		Window Parameters
			String: sREF
		Window Variables
			Number: nCol
		Message Actions
			On SAM_Create
				Call PrepareWindow( hWndForm )
				Call SetWindowFullSize(hWndForm)
				! Присваиваем значения многоязыковым объектам
				Call SalSendMsgToChildren( hWndForm, UM_QueryLabelText, 0, 0 )
				Call SalSetWindowText( hWndForm, CurrentLangTable.GetAtomTitle('BMOD.cTWndTitle')  )
				!
				Call SalTblSetLockedColumns(hWndForm,7)
				Set BMOD.nTabInstance = 1
				Set BMOD.strFilterTblName = 'ACCOUNTS'
				Set BMOD.strPrintFileName = 'BMOD'
				Set BMOD.strSqlPopulate =
						  "SELECT o.ref,o.fdat,o.tt,
						          decode(o.dk,0,o.s,0)/100,decode(o.dk,1,o.s,0)/100,
						          o.sos,ACCOUNTS.nms,ACCOUNTS.nls,ACCOUNTS.kv, o.txt,
						          R.NLS, R.KV, R.NMS
						   INTO :BMOD.REF, :BMOD.FDAT,:BMOD.TT,:BMOD.SD,:BMOD.SK,
						        :BMOD.SOS, :BMOD.TXT, :BMOD.NLS,:BMOD.KV,:BMOD.TXT1,
						        :BMOD.NLSG,:BMOD.KVG, :BMOD.TXTG
						   FROM opldok o, ACCOUNTS, oper p, accounts R
						   WHERE R.ACC(+) =ACCOUNTS.accc AND 
						         (o.ref=p.ref or o.ref=p.refl) and ACCOUNTS.acc=o.acc and
						          p.ref in (" || sREF || " ) 
						   ORDER BY o.fdat,o.stmt, o.dk "
				Call SalSendClassMessage(UM_Create, 0, 0)
			On SAM_FetchRowDone
				If SOS=1
					Set nCol=COLOR_DarkGreen
				Else If SOS=5
					Set nCol=COLOR_Black
				Else If SOS=3
					Set nCol=COLOR_DarkBlue
				Else
					Set nCol=COLOR_DarkGray
				Call SalTblSetCellTextColor(REF,nCol,FALSE)
				Call SalTblSetCellTextColor(FDAT,nCol,FALSE)
				Call SalTblSetCellTextColor(TT,nCol,FALSE)
				Call SalTblSetCellTextColor(SD,nCol,FALSE)
				Call SalTblSetCellTextColor(SK,nCol,FALSE)
				Call SalTblSetCellTextColor(SOS,nCol,FALSE)
				Call SalTblSetCellTextColor(TXT,nCol,FALSE)
				Call SalTblSetCellTextColor(NLS,nCol,FALSE)
				Call SalTblSetCellTextColor(KV,nCol,FALSE)
				If SD=0
					Set SD=NUMBER_Null
				If SK=0
					Set SK=NUMBER_Null
			On UM_Populate
				Call SalWaitCursor(TRUE)
				Call SalTblPopulate(hWndForm, hSql(), BMOD.strSqlPopulate  , TBL_FillAll)
				Call SalWaitCursor(FALSE)
