				Call SalSendClassMessage(SAM_Create, 0, 0)
			On UM_Populate
				Call SalWaitCursor(TRUE)
				! Set CBK.strSqlPopulate = 
						"SELECT CP_INFO.ID, CP_INFO.FDAT, CP_INFO.BYR, CP_INFO.NOMIN, CP_INFO.STOIM,
						        b.name, k.cp_id
						 INTO :CBK.ID, :CBK.FDAT, :CBK.BYR, :CBK.NOMIN, :CBK.STOIM, :CBK.SBYR, :CBK.CP_ID
						 FROM CP_INFO, cp_byr b, cp_kod k
						 WHERE CP_INFO.byr=b.id and CP_INFO.id=k.id and 
						      to_char(CP_INFO.fdat,'yyyymm') = to_char(:DAT,'yyyymm') 
						 ORDER BY  CP_INFO.BYR, CP_INFO.FDAT desc, CP_INFO.id "
				Set CBK.strSqlPopulate = 
						"select  s.id, s.fdat, s.byr, s.nomin, s.stoim,s.name, s.cp_id
						into :CBK.ID, :CBK.FDAT, :CBK.BYR, :CBK.NOMIN, :CBK.STOIM, :CBK.SBYR, :CBK.CP_ID
						from 
						(select 
						row_number() over (partition by cp.id order by cp.fdat desc) rnum,
						CP.ID, 
						CP.FDAT, 
						CP.BYR, 
						CP.NOMIN, 
						CP.STOIM,
						b.name, k.cp_id
						FROM cp_info cp, cp_byr b, cp_kod k
						WHERE CP.byr=b.id 
						and CP.id=k.id
						and cp.fdat in 
						     (select fdat 
						      from fdat 
						      where to_char(fdat,'mmyyyy') = to_char(:DAT,'mmyyyy')
						        and fdat <= :DAT )  
						ORDER BY  CP.id, CP.FDAT 
						) s
						where s.rnum <=5 "
				! Set CBK.strSqlPopulate = 
						"SELECT CP_INFO.ID, CP_INFO.FDAT, CP_INFO.BYR, CP_INFO.NOMIN, CP_INFO.STOIM,
						        b.name, k.cp_id
						 INTO :CBK.ID, :CBK.FDAT, :CBK.BYR, :CBK.NOMIN, :CBK.STOIM, :CBK.SBYR, :CBK.CP_ID
						 FROM CP_INFO, cp_byr b, cp_kod k
						 WHERE CP_INFO.byr=b.id and CP_INFO.id=k.id and k.dox=1 and k.AMORT=0 and
						      to_char(CP_INFO.fdat,'yyyymm') = to_char(:DAT,'yyyymm') 
						 ORDER BY  CP_INFO.BYR, CP_INFO.FDAT desc, CP_INFO.id "
				Call SalTblDefineSplitWindow(hWndForm, 0, FALSE)
				Call SalTblPopulate(hWndForm, hSql(), CBK.strSqlPopulate , TBL_FillAll)
				Set CBK.strSql = 
						"SELECT r.VDATE, r.BSUM, r.RATE_O/r.BSUM, b.name, k.cp_id 
						 INTO :CBK.tbl.FDAT, :CBK.tbl.BSUM, :CBK.tbl.RATE, :CBK.tbl.SBYR, :CBK.tbl.CP_ID
						 FROM cp_rates r, cp_byr b, cp_kod k
						 where r.idb=b.id and r.id=k.id and 
						      r.vdate=:DAT  
						 ORDER BY r.idb, r.VDATE desc, r.id "
				! Set CBK.strSql = 
						"SELECT r.VDATE, r.BSUM, r.RATE_O, b.name, k.cp_id 
						 INTO :CBK.tbl.FDAT, :CBK.tbl.BSUM, :CBK.tbl.RATE, :CBK.tbl.SBYR, :CBK.tbl.CP_ID
						 FROM cp_rates r, cp_byr b, cp_kod k
						 where r.idb=b.id and r.id=k.id and k.dox=1 and k.AMORT=0 and
						      to_char(r.vdate,'yyyymm') = to_char(:DAT,'yyyymm') 
						 ORDER BY r.idb, r.VDATE desc, r.id "
				Call SalTblPopulate(CBK.tbl, hSql(), CBK.strSql, TBL_FillAll)
				Call SalTblSetContext(hWndForm, 0)
				Call SalWaitCursor(FALSE)
