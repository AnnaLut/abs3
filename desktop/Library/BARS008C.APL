					Set CP3.nTmp = SalTblInsertRow(hWndForm, TBL_MinRow)
					Call SalTblSetRowFlags(hWndForm, CP3.nTmp, ROW_New, FALSE)
					Call SalTblSetContext(hWndForm, CP3.nTmp)
					!
					Set CP3.CP_ID = CurrentLangTable.GetAtomTitle('CP3.cTItog')
					Set CP3.VIDD  = CurrentLangTable.GetAtomTitle('CP3.cTEqv')
					Set CP3.N1 = CP3.N11
					Set CP3.N2 = CP3.N21
					Set CP3.OSTN = CP3.OSTN1
					!
					Set CP3.R1 = CP3.R11
					Set CP3.R2 = CP3.R21
					Set CP3.R3 = CP3.R31
					Set CP3.R4 = CP3.R41
					Set CP3.OSTR = CP3.OSTR1
					!
					Set CP3.D1 = CP3.D11
					Set CP3.D2 = CP3.D21
					Set CP3.D3 = CP3.D31
					Set CP3.OSTD = CP3.OSTD1
					!
					Set CP3.P1 = CP3.P11
					Set CP3.P2 = CP3.P21
					Set CP3.P3 = CP3.P31
					Set CP3.OSTP = CP3.OSTP1
					!
					Set CP3.CENA = 1000
					If CP3.N1 !=0
						Set CP3.CENAK = (( CP3.N1 + CP3.R1 + CP3.P1 - CP3.D1) / CP3.N1 ) * CP3.CENA
					If CP3.N2 !=0
						Set CP3.CENAP = (( CP3.N2 + CP3.R2 + CP3.P2 - CP3.D2) / CP3.N2 ) * CP3.CENA
					! Set CP3.CENAP = (( CP3.N2 + CP3.R2 + CP3.P2 - CP3.D2) / CP3.N2 ) * CP3.CENA
					If CP3.OSTN  != 0
						Set CP3.CENAA =  (( CP3.OSTN + CP3.OSTR + CP3.OSTP - CP3.OSTD) / CP3.OSTN ) * CP3.CENA
					Call CP3.COLORA()
