							and SqlFetchNext(hSqlAux2(),  nFetchRes) 
							and SqlPrepareAndExecute(hSqlAux2(), 
							   "SELECT rownum -1, ref, chk  INTO :nInd, :REF_, :CHK_ 
							    FROM oper WHERE  ref in (" || STR_REF || ") 
							    FOR UPDATE OF chk NOWAIT ")
						While SqlFetchNext(hSqlAux2(),  nFetchRes)
							Set RefList[nInd,0] = Str(REF_)
							Set RefList[nInd,1] = CHK_
						!
						Set mInd = 0
						While mInd <= nInd
							Set REF_ = Val(RefList[mInd,0])
							Set CHK_ = RefList[mInd,1] 
							If not f_VISA_REF(nCheckId, nUserId, strKeyId, REF_, nStatus, CHK_)
								Return FALSE
							Set mInd = mInd + 1 
						Return TRUE
					Return FALSE
			Function: f_VISA_REF
				Description:
				Returns
					Boolean:
				Parameters
					Number: nCheckId
					Number: nUserId
					String: strKeyId
					Number: REF_
					Number: nStatus
					String: CHK_
				Static Variables
				Local variables
					String: strHexMark2Ret
					String: sPrich
					Number: n2Pay
					Number: n2Sig
				Actions
					Call SalWaitCursor(TRUE)
					Set strHexMark2Ret = SalStrTrimX(CHK_) || 
							    SalStrRightX('00'   ||  NumberToHexSX(nCheckId), 2) ||
							    SalStrRightX('0000' ||  NumberToHexSX(nUserId), 4) 
					! SERG Проверка подписи n-1 и наложение подписи n
					! If FALSE
					If GetGlobalOptionEx( 'VISASIGN' ) AND GetSignOn()!=0	
						! если не верна, то выдать сообщение 
						If NOT cDoc.SetDocR( REF_, TRUE )
							Call SaveErrorToLog( 'Ошибка установки экземпляра документа. REF='
									||SalNumberToStrX( REF_, 0 ) )
							Return FALSE
						If GetGlobalOptionEx('INTSIGN')=2 		 !-- новая внутрення подпись
							If NOT cDoc.LockCheckSignStore(REF_,nCheckId,nStatus)
								Return FALSE
						Else
							If NOT cDoc.CheckDocSign( 'CUR', 0 )
								Select Case cDoc.m_nSignErrType
									Case 1
										Call SaveErrorToLog( CurrentLangTable.GetAtomTitle('dlg_TIK.cTErrVisa')||' ' || Str(nCheckId) ||': '||CurrentLangTable.GetAtomTitle('dlg_TIK.cTErr1')||' Ref=' || Str(REF_)  )
										Break
									Case 2
										Call SaveErrorToLog( CurrentLangTable.GetAtomTitle('dlg_TIK.cTErrVisa')||' ' || Str(nCheckId) ||': '||CurrentLangTable.GetAtomTitle('dlg_TIK.cTErr2')||' Ref=' || Str(REF_)  )
										Break
									Default
										Call Debug(Str(cDoc.m_nSignErrType) || '-Нерасчетный код возврата!')
										Break
								Return FALSE
							! Подписываем...
							If NOT cDoc.sDocVisa(  )
								Call SaveErrorToLog( CurrentLangTable.GetAtomTitle('dlg_TIK.cTErrVisa')||' ' || Str(nCheckId) || ': '||CurrentLangTable.GetAtomTitle('dlg_TIK.cTErr3') )
								Return FALSE
							! вствка в oper_visa с подписью
							Call SqlSetLongBindDatatype(6, BLOB_BYTE)
							If not SqlPrepareAndExecute(hSqlAux2(),
									"INSERT INTO oper_visa (ref,dat,userid,groupid,status,keyid,sign) VALUES (:REF_,sysdate,:nUserId,:nCheckId,:nStatus,:strKeyId,:cDoc.m_lsSign)")
								Return FALSE
							If not SqlPrepareAndExecute(hSqlAux2(), "UPDATE oper SET chk=:strHexMark2Ret WHERE ref=:REF_")
								Return FALSE
					! без подписи
					Else
						If not SqlPrepareAndExecute(hSqlAux2(),
								"INSERT INTO oper_visa (ref,dat,userid,groupid,status) VALUES (:REF_,sysdate,:nUserId,:nCheckId,:nStatus)")
							Return FALSE
						If not SqlPrepareAndExecute(hSqlAux2(), "UPDATE oper SET chk=:strHexMark2Ret WHERE ref=:REF_")
							Return FALSE
					!
					! If not SqlPrepareAndExecute( hSqlAux2(), "UPDATE oper SET chk=:strHexMark2Ret WHERE ref=:REF_")
						                                                                                                                                                                                                                                                                Return FALSE
					! Хорошая виза  (НЕ ПОСЛЕДНЯЯ ! - без факт оплаты) = уже ВСЕ
					! Плахая виза надо  бекнуть
					Set sPrich = CurrentLangTable.GetAtomTitle('dlg_TIK.cTBACKR')
					If nStatus = 3
						If not Pay_Bck( REF_, 5, '')
								or not SqlPrepareAndExecute( hSqlAux2(), "INSERT INTO operw(ref,tag,value) 
								VALUES(:REF_, 'BACKR', :sPrich ) " )
							Return FALSE
					!
					Return TRUE
