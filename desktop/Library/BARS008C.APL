		Message Actions
			On SAM_Create
				Call PrepareWindow( hWndForm )
				Call SalCenterWindow(hWndForm)
				Set EXPIRATION.nTabInstance = 1
				Call SetWindowFullSize(hWndForm)
				Set EXPIRATION.dDATexp = GetBankDate()
				Set EXPIRATION.strFilterTblName = 'V_CP_EXPCANDIDATES'
				If SqlPrepareAndExecute(hSqlAux2(),
						" begin pul.Set_Mas_Ini('sFdatCPExp', '"
						|| SalFmtFormatDateTime(dDATexp, 'dd/MM/yyyy')
						||"', 'CP expiry'); end; ")
					Call SqlCommitEx( hSqlAux2(), 'sFdatCPExp')
				Set hWndForm.EXPIRATION.strSqlPopulate =
						"SELECT CP_ID, EXPIRY, DOK, DNK, EXP_DATE, OST_N_EXP, OST_R_EXP  
						 INTO :EXPIRATION.CP_ID, :EXPIRATION.EXPIRY, :EXPIRATION.DOK, :EXPIRATION.DNK, :EXPIRATION.EXP_DATE, :EXPIRATION.OST_N_EXP, :EXPIRATION.OST_R_EXP  
						   FROM V_CP_EXPCANDIDATES_NEW"
				Call SalSendClassMessage(SAM_Create, 0, 0)
