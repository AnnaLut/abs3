				Call SalSendMsgToChildren( hWndForm, UM_QueryLabelText, 0, 0 )
				Call SalSetWindowText( hWndForm, CurrentLangTable.GetAtomTitle('CP_VISA.cTWndTitle') )
				!
				Set nUserId = GetUserId()
				If SqlPrepareAndExecute( hSql(),
						  "SELECT otd INTO :CP_VISA.nOtd FROM otd_user 
						   WHERE userid="||Str(CP_VISA.nUserId) ||" 
						   ORDER BY nvl(pr,0) desc" )
						and not SqlFetchNext( hSql(),  nFetchRes)
					Call SalMessageBox( CurrentLangTable.GetAtomTitle('CP_VISA.cTMess1') ,
							  CurrentLangTable.GetAtomTitle('cTAt'), MB_IconStop)
					Call SalDestroyWindow(hWndForm)
				!
				Set CP_VISA.strSqlPopulate =
						"SELECT o.REF, k.CP_ID, c.DAT_UG, c.SUMB/100, o.nazn, C.N/100,  C.STR_REF 
						 INTO :CP_VISA.REF,  :CP_VISA.CP_ID, :CP_VISA.DAT_UG, :CP_VISA.SUMB,
						      :CP_VISA.NAZN, :CP_VISA.N,     :CP_VISA.STR_REF
						 FROM  CP_ARCH c, oper o , cp_kod k
						 where  c.ref=o.ref and k.id=c.ID and
						       o.sos>0 and o.sos<5 and o.chk is null and o.userid in 
						      (select userid from OTD_USER where otd=:CP_VISA.nOtd ) and o.userid<>:CP_VISA.nUserId
						 ORDER BY C.REF "
				! Set CP_VISA.strSqlPopulate =
						"SELECT o.REF, k.CP_ID, c.DAT_UG, c.SUMB/100, o.nazn, C.N/100,  C.STR_REF 
						 INTO :CP_VISA.REF,  :CP_VISA.CP_ID, :CP_VISA.DAT_UG, :CP_VISA.SUMB,
						      :CP_VISA.NAZN, :CP_VISA.N,     :CP_VISA.STR_REF
						 FROM  CP_ARCH c, oper o , cp_kod k, cp_deal d
						 where d.ref=c.ref and c.ref=o.ref and k.id=c.ID and
						       o.sos>0 and o.sos<5 and o.chk is null and o.userid in 
						      (select userid from OTD_USER where otd=:CP_VISA.nOtd ) and o.userid<>:CP_VISA.nUserId
						 ORDER BY C.REF "
