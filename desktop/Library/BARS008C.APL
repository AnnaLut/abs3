			Function: F_SAVE
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
				Actions
					!
					Set FL = FALSE
					If nMode = 1
						If SUMK > SUMI !! (!=)
							Call MessageNoWait(CurrentLangTable.GetAtomTitle('cTMessNO') || 'Суми не співпадають ' ,'',2,1)
							Return FALSE
						Set FL = F_SAVE0()
						Call NASELENIE()
					Else If nMode = 0 
						Call F_SUMI_ALL()
						If SUMK != SUMI_ALL
							Call MessageNoWait(CurrentLangTable.GetAtomTitle('cTMessNO') || 'Суми не співпадають ' ,'',2,1)
							Return FALSE
						If SqlPrepareAndExecute( hSqlAux3(),
								   "SELECT id, DATP, IR, DAT_EM, cena, CP_ID
								    INTO :CPR.nID,:CPR.DATP, :CPR.RATE, :CPR.DAT_EM, :CPR.CENA, :CPR.CP_ID
								    FROM cp_kod  WHERE " || sf || 
								   " and emi =:CPR.nEMI and dox=:CPR.nDOX and kv=:CPR.KV and tip=1 and IR>0 " || 
								sFilter || " ORDER BY DAT_EM ")
							While SqlFetchNext(hSqlAux3(), nFetchRes ) 
								Set CPR.FLAG = TRUE
								Call NASELENIE()
					!
					If FL 
						Call SqlCommit(hSql())
						Call SalEnableWindow( pb_Buch )
						Call MessageNoWait(CurrentLangTable.GetAtomTitle('cTMessOK') || " - REF=" || sREF,'',2,0)
						Call SalSendMsg( pbRefresh, SAM_Click, 0, 0 )
						Return TRUE
					Else
						Set sErr = CurrentLangTable.GetAtomTitle('CPR.cTErrOst')
						Call SqlRollback(hSql())
						Call SalMessageBeep(MB_IconStop)
						Call MessageNoWait(CurrentLangTable.GetAtomTitle('cTMessNO') || sErr ,'',2,1)
						Return FALSE
			Function: F_SAVE0
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					Number: SK_
					Number: nRef
					Number: nVob
				Actions
					Set SK_ = SUMK * 100
					If cb_Zo 
						Set nVob = 96
					Else
						Set nVob = 6
					If SqlPLSQLCommand(hSql(),
							 "CP.CP_KUPON ( nTipD, nVob, nGrp, nID, nRYN, nVidd,SUMK, SK_, B_4621, sREF, sErr, REF_MAIN )") 
							and sErr = STRING_Null
						If not F_PODPIS(sREF)
							Return FALSE
						!
						Set SUMK = SK_ / 100
						Return TRUE
					!
					Return FALSE
			Function: F_SUMI_ALL
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					String: sSqlf
				Actions
					If nMode = 0 
						Set sf = ""
						Set sf = cF.GetFilterWhereClause( FALSE )
						Set CPR.SUMI_ALL = 0
						If sf != ""
							Set sSqlf = 
									"sELECT -nvl( sum( nvl(R.ostb,0) + nvl(R2.ostb,0) + nvl(R3.ostb,0)), 0 ) /100  INTO :CPR.SUMI_ALL " ||  
									 sFROM || sWHERE || " AND D.id in (SELECT id FROM cp_kod WHERE " || sf || 
									" and emi=:CPR.nEMI and dox=:CPR.nDOX and kv=:CPR.KV and tip=1 and IR>0 " || 
									 sFilter || " )"
							If SqlPrepareAndExecute( hSql(), sSqlf ) AND SqlFetchNext( hSql(), nFetchRes ) 
								Call SalTblReset( hWndForm )
			Function: NASELENIE
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					String: sSql_It
				Actions
					Call SalWaitCursor(TRUE)
					! Сумма в заголовке ИТОГО
					Set SUMN = 0
					Set SUMN2 = 0
					! Детальные строки
					! nVidd
					! Call Debug(
							' nVidd = '||Str(nVidd) || 
							' CPR.nRYN ='||Str(CPR.nRYN )|| 
							' CPR.KV='||Str(CPR.KV) ||
							' CPR.nPF=' || Str(CPR.nPF) || 
							' CPR.nEMI=' || Str(CPR.nEMI)
							)
					Set CPR.strSqlPopulate =
							"SELECT A.nls, -A.ostb/100, R.nls, -CP.CP_REAL_INTDATE (d.ACC, (CASE WHEN dnk > bankdate THEN dok ELSE dnk END)+1,1)/100, 
							R2.nls, -R2.ostb/100, a.acc,  
							CASE WHEN dnk > bankdate THEN dok ELSE dnk END, 
							-R.ostb/100,
							R3.nls, nvl(-R3.ostb,0)/100
							 INTO :CPR.NLSA,:CPR.OSTA,:CPR.NLSR,:CPR.OSTR,:CPR.NLSR2,:CPR.OSTR2,:CPR.ACC, :CPR.ACR_DAT_INT, :CPR.OSTR_REAL,:CPR.NLSR3,:CPR.OSTR3 " ||
							 sFROM || sWHERE || " ORDER BY d.ref " 
					! Call Debug(Subs(CPR.strSqlPopulate,  1,200) )
					! Call Debug(Subs(CPR.strSqlPopulate,201,200) )
					! Call Debug(Subs(CPR.strSqlPopulate,401,200) )
					! Call Debug(Subs(CPR.strSqlPopulate,601,200) )
					! Call Debug(Subs(CPR.strSqlPopulate,801,200) )
					Call SalTblPopulate(hWndForm, hSql(), CPR.strSqlPopulate, TBL_FillAll)
					Set SUMI = SUMN + SUMN2  
					Call SalTblSetContext(hWndForm, 0)
					Call SalPostMsg(hWndForm, SAM_TblDoDetails, 0, 0)
					Call SalWaitCursor(FALSE)
					If FLAG and SUMI >0  and 
							   SalMessageBox( " Виконати погашення ? ", CP_ID , MB_IconQuestion | MB_YesNo) = IDYES
						Set FL= F_SAVE0()
					Set CPR.FLAG = FALSE
		Window Parameters
			String: B_4621
			String: strPar01  
			String: strPar02  ! Предустановленный фильтр
			Number: nGrp      ! группа доступа приоткрытии внесист счетов
			Number: nMode     ! =1 по одному ЦП, =0 по групі ЦП
			!
		Window Variables
			String: sFilter
			String: sTmp
			String: sREF
			String: sErr
			String: sFROM
			String: sWHERE
			Number: REF_MAIN
			Number: nTipD
			Number: nRYN
			Number: nPF
			: cDoc
				Class: cDoc
			Boolean: FLAG
			Boolean: FL
			String: sf
		Message Actions
			On SAM_Create
				Call PrepareWindow( hWndForm )
				Call SetWindowFullSize(hWndForm)
				Call SalDisableWindow( pb_Buch )
				Set sFilter = VisStrSubstitute ( SalStrUpperX( strPar02 ), "CP_V.", "CP_KOD." )
				If GetBaseVal() =980 and
						 ( SalDateDay( GetBankDate() ) <10 or SalDateMonth( GetBankDate() ) = 1)
					Call SalShowWindow( cb_Zo )
				Else
					Call SalHideWindow( cb_Zo )
				! Присваиваем значения многоязыковым объектам
				Call SalSendMsgToChildren( hWndForm, UM_QueryLabelText, 0, 0 )
				Call SalSetWindowText( hWndForm, CurrentLangTable.GetAtomTitle('CPR.cTWndTitle')  )
				!
				Call SalTblSetLockedColumns( hWndForm, 7 )
				Set CPR.nTabInstance = 1
				Set CPR.strPrintFileName = 'CPR'
				Set CPR.strFilterTblName = 'CP_KOD'
				Set CPR.nTipD = 1
				Set CPR.FLAG = FALSE
				Set sFROM =
						" FROM cp_deal D, accounts R, accounts R2, accounts R3, accounts A, cp_accc CA, accounts AA, cp_kod ck "
				Set sWHERE = 
						" WHERE D.RYN=:CPR.nRYN 
						    AND D.acc = A.acc   AND D.accR= R.acc(+) AND D.accR2=R2.acc(+)  AND D.accR3=R3.acc(+)
						    AND (R2.ostb <>0 or R.ostb <>0 or R2.ostb <>0 )
						    AND A.accc=AA.acc   AND AA.kv=:CPR.KV AND AA.nls=CA.NLSA
						    AND CA.PF=:CPR.nPF  AND CA.RYN=D.RYN  AND CA.EMI=:CPR.nEMI 
						    and aa.NBS=:nVidd  and d.id = ck.id"
				!
				If nMode = 1 !! по одному ЦП
					Call SalHideWindow( pbFilter )
					Call SalHideWindow( SUMI_ALL )
					Call SalDisableWindow( KV )
					Call SalDisableWindow( nDOX )
					Call SalDisableWindow( nEMI )
					Call FunNSIGetFiltered( "CP_KOD", "CP_ID", " TIP=1 and CP_KOD.ID>0 and IR>0 " || sFilter, CPR.sTmp, CPR.CP_ID )
					Set sWHERE = sWHERE || " AND D.id = :CPR.nID "
					If CPR.sTmp != ''
						Set CPR.nID=Val(sTmp)
						If SqlPrepareAndExecute( hSql(),
								   "SELECT k.DATP, k.IR, k.DAT_EM, k.cena, k.tip, k.kv, k.dox, k.emi
								    INTO :CPR.DATP, :CPR.RATE, :CPR.DAT_EM, :CPR.CENA, :CPR.nTipD, :CPR.KV, :CPR.nDOX,:CPR.nEMI 
								    FROM cp_kod k WHERE k.id=:CPR.nID" )
								and SqlFetchNext( hSql(), nFetchRes ) 
						Call SalSendClassMessage(SAM_Create, 0, 0)
					Else
						Call SalDestroyWindow(hWndForm)
				Else If nMode = 0 !! по групі ЦП
					Set CPR.KV = GetBaseVal()
					Set CPR.nDOX = 2
					Set CPR.nEMI = 1
					Call SalSendClassMessage(SAM_Create, 0, 0)
			On UM_Populate
				If nMode = 0 
					Call F_SUMI_ALL()
				Else
					Call NASELENIE()
			On SAM_CreateComplete
				Call SalWaitCursor(FALSE)
			On SAM_FetchRowDone
				If ACC != NUMBER_Null 
					If SqlPrepareAndExecute( hSqlAux2(), 
							  "select ACR_DAT,APL_DAT into :ACR_DAT,:APL_DAT from int_accn where acc=:ACC and id=0" ) 
							and SqlFetchNext( hSqlAux2(), nFetchRes ) 
					Set SUMN = SUMN + OSTR
					Set SUMN2 = SUMN2 + OSTR2 + OSTR3
