		Functions
			Function: F_VALID
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
				Actions
					Set rDOK = p_DOK
					If SqlPrepareAndExecute( hSql(), 
							"select nvl(sum(nvl(nom,0)),0)  
							 into :NOM from cp_dat  where id = :p_ID and dok<= :p_DOK" ) 
							and SqlFetchNext( hSql(), nFetchRes ) 
						If NOM <= 0 
							Call SalMessageBox('НЕМАЄ зниження ном','Помилка !', MB_IconStop)
							Return FALSE
					!
					If SqlPrepareAndExecute( hSql(), 
							"select k.cp_id, k.cena, k.cena_start, k.KV, substr(nvl(c.nmk,k.name),1,60) 
							     into   :l_cp_id, :CENA_OLD, :l_cena_STR, :l_kv, :l_naz1
							     from cp_kod k , customer c 
							     where k.id  =:p_ID and k.rnk = c.rnk (+)" ) 
							and NOT SqlFetchNext( hSql(), nFetchRes ) 
						Call SalMessageBox('НЕ знайдено в CP_kod','Помилка !', MB_IconStop)
						Return FALSE
					!
					If SqlPrepareAndExecute( hSql(), 
							"select substr(nms,1,38), nls into :S_4621, :B_4621
							 from accounts 
							 where kv = :l_kv and nls = substr(GetGlobalOption('CP_4621'),1,14)  and dazs is null" ) 
							and NOT SqlFetchNext( hSql(), nFetchRes ) 
						Call SalMessageBox('НЕ знайдено тр.рах','Помилка !', MB_IconStop)
						Return FALSE
					!
					Set CENA_NEW = l_cena_STR  - NOM 
					If CENA_NEW >= CENA_OLD
						Call SalMessageBox('Цiну ном уже врегулювано','Помилка !', MB_IconStop)
						Return FALSE
					! текущее снижение 1 штуки
					Set NOM = CENA_OLD - CENA_NEW
					Return TRUE
			Function: F_Opl
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					Number: nRow
					Number: n980
					Number: nS
					String: sTT
					String: sNazn
					Number: nVob
					Number: nRef
					Date/Time: bDat
					Date/Time: vDat
				Actions
					Call SalWaitCursor(TRUE)
					Set n980 = GetBaseVal()
					Set bDat = GetBankDate()
					Set nVob = 6
					Set vDat = bDat
					!
					If SqlPrepareAndExecute( hSql(), 
							"select substr(val,1,3) into :sTT from params where par='FXB-1-'||decode(:l_kv,:n980,'0','1') " ) 
							and NOT SqlFetchNext( hSql(), nFetchRes ) 
						Set sTT = 'FXB'
					If cb_Zo =1 
						Set nVob = 96
						If SqlPrepareAndExecute( hSql(), 
								"select cp.F_VDAT_ZO( add_MONTHS(:bDat, -1 ) ) into :vDat from dual" ) 
								and SqlFetchNext( hSql(), nFetchRes ) 
					! !!!!
					Set nRow = TBL_MinRow
					While SalTblFindNextRow( hWndForm, nRow, 0, 0 )
						Call SalTblSetContext( hWndForm, nRow )
						If NOT SqlPrepareAndExecute( hSql(), 
								"update accounts set blkd=0, blkk=0 where kv=:l_kv and nls=:NLS")
							Return FALSE
						Set nS = NOM * KOL *100
						If nS  > 0
							Set sNazn = Subs( 
									'Часткове погашення емiтентом '  || l_naz1 || ' ' || l_cp_id   || 
									' ном.' || Str(KOL) || ' шт. '   ||
									'Дог.№' || ND       || ' вiд '   || SalFmtFormatDateTime( DATD,'dd.MM.yyyy')
									, 1, 160 )
							! Договiр [№ угоди] вiд [дата угоди]
									ISIN - внешний код ЦБ в cp_kod
							Call cDocG.SetDoc ( 0, sTT, 0, nVob, ND,
									     bDat, bDat, vDat, bDat,
									     NLSG, NMSG,  GetBankMfo(), '',  l_kv,   nS, GetBankOkpoS(), 
									     B_4621, S_4621, GetBankMfo(), '', l_kv, nS, GetBankOkpoS(), 
									     sNazn, '', GetIdOper(), '', NUMBER_Null, 0 )
							Set cDocG.sNlsD = NLS
							If NOT cDocG.oDoc()
								Return FALSE
							Set nRef = cDocG.m_nRef
							! Call Debug( Str(nRef) || ' ' || NLSG)
							Set REF1 =  nRef
							Set STR_REF = Str(REF1)
							If NOT SqlPrepareAndExecute( hSql(),
									"INSERT into cp_arch
									(REF,ID,DAT_UG,DAT_OPL,DAT_ROZ,SUMB, N, STR_REF, OP, d,p,r,s,vd,vp,t,tq)
									values
									(:nRef,:p_ID,:bDat,:bDat,:bDat,:nS,:nS,:STR_REF, 22, 0,0,0,0, 0, 0,0, 0) ")
								Return FALSE
							Call SalWaitCursor(TRUE)
					! !!
					If NOT SqlPrepareAndExecute( hSql(),
							"update cp_kod set cena = :CENA_NEW where id=:p_ID" )
						Return FALSE
					Call SqlCommitEx( hSql(),
							'Часткове погашення НОМIНАЛУ ЦП № ' || 
							Str(p_ID) || ' код ' || l_cp_id || ' на дату ' || SalFmtFormatDateTime( p_DOK, 'dd.MM.yyyy' ))
					! !
					Call SalDisableWindow( pbUpdate )
					Call SalWaitCursor(FALSE)
		Window Parameters
			Number: p_ID
			Date/Time: p_DOK
		Window Variables
			Number: nRow
			Number: l_err 
			String: l_cp_id
			Number: l_cena_STR 
			Number: l_kv
			String: l_naz1 
			String: B_4621
			String: S_4621 
			Date/Time: bDat
			: cDocG
				Class: Tip_cDoc
		Message Actions
			On SAM_Create
				Call PrepareWindow( hWndForm )
				Call SetWindowFullSize(hWndForm)
				Set cb_Zo = 0
				If F_VALID()
					Set FA8.strPrintFileName =  'POG'|| Str(p_ID)
					Set bDat = GetBankDate()
					Call SalSendClassMessage(UM_Create, 0, 0)
			On UM_Populate
				Call SalWaitCursor(TRUE)
				Call SalDisableWindow( pbUpdate )
				Set l_err = 0
				Set FA8.strSqlPopulate =
						"select 
						(select -nvl(sum(ostb),0) from accounts where acc in (d.accR,d.accR2)), 
						(select nls from accounts where acc = a.accc),  
						(select substr(nms,1,38) from accounts where acc = a.accc),
						   d.ref, a.nls,  -a.ostc/100, -a.ostb/100, 
						   o.nd, o.datd, x.str_ref 
						 Into :FA8.OSTR, :FA8.NLSG, :FA8.NMSG,:FA8.REF,  :FA8.NLS, 
						      :FA8.OSTC, :FA8.OSTB, :FA8.ND,  :FA8.DATD, :FA8.STR_REF 
						 from cp_deal d, accounts a, oper o, CP_ARCH x
						 where d.id   =  :p_ID  and d.acc  =  a.acc and o.ref=d.ref
						   and d.DAZS is null  and a.ostc <  0   and d.ref  = x.ref "
				Call SalTblPopulate(hWndForm, hSql(), FA8.strSqlPopulate, TBL_FillAll)
				Call SalTblSetContext(hWndForm, 0)
				Call SalPostMsg(hWndForm, SAM_TblDoDetails, 0, 0)
				If l_err = 0
					Call SalEnableWindow( pbUpdate )
				Call SalWaitCursor(FALSE)
			On SAM_FetchRowDone
				If OSTC != OSTB 
					Set l_err = 1
					Set ERR = 'план.зал НОМ НЕ=факт.зал'
				If OSTR !=0 
					Set l_err = 1
					Set ERR = 'зал купона НЕ=0'
				If NLSG = STRING_Null 
					Set l_err = 1
					Set ERR = 'нет счета ФУ для номинала'
				If l_err = 1
					Call XSalTblSetRowBackColor( hWndForm, lParam, COLOR_Sky  )
				Else
					Set KOL = OSTC / CENA_OLD
