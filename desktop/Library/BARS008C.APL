		Functions
			Function: NAS_2
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Call SalTblReset( tbl2 )
					Set sSql2 =
							"SELECT c.acc, c.sos, c.ref, c.id, c.kv, c.OSTA, c.OSTR, c.ryn, c.mdate, 
							        c.ir, c.OSTAB, c.pfname, c.DATD, c.ND, c.SUMB, i.acr_dat,
							        c.accR 
							 INTO :tbl2.ACC   ,:tbl2.SOS ,:tbl2.REF,:tbl2.ID, :tbl2.KV, :tbl2.OSTA, 
							      :tbl2.OSTR  ,:tbl2.RYN ,:tbl2.DATP,:tbl2.NO_PR,:tbl2.OSTAB,
							      :tbl2.PFNAME,:tbl2.DATD,:tbl2.ND,  :tbl2.SUMB, :tbl2.ACR_DAT,
							      :tbl2.ACCR  
							 FROM  CP_V c, int_accn i
							 WHERE ( i.acc=c.acc and i.id=1 or i.acc=c.accr and i.id=0 )
							   and c.OSTAB>0 and c.mdate>:bDat and c.tip=2 and c.cp_id=:tbl.CP_ID 
							 ORDER BY c.ref desc"
					!
					Call SalTblSetTableFlags ( tbl2, TBL_Flag_SingleSelection, TRUE )
					Set tbl.KOL=0
					Call SalTblPopulate(tbl2,hSql(),sSql2,TBL_FillAll )
					Call SalTblSetLockedColumns( tbl2, 5 )
			Function: F_INT
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					String: sTmpI
				Actions
					If SqlPrepareAndExecute(hSql(),
							"select acr_dat into :tbl2.ACR_DAT from int_accn 
							 where ( acc=:tbl2.ACC and id=1 or acc=:tbl2.ACCR and id=0 ) ") 
							and NOT SqlFetchNext( hSql(), nFetchRes ) 
						! or i.acc=c.accr and i.id=0
						Return FALSE 
					If tbl2.ACR_DAT < bDat -1 OR tbl2.ACR_DAT = DATETIME_Null 
						Set sTmpI = " and s.acc = " || Str(tbl2.ACC)
						Call Sel010(hWndForm,0, 0 , sTmpI, "ЦA"  ) 
						Return FALSE 
					Return TRUE
			Function: F_OPL
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					Number: nVob
					String: sMfo
					String: sNls
					Number: nRef
					String: sNaznN
					Number: kN
					Number: kR
					String: sNaznK
					String: sNazn
					String: sErr
					Number: SK_
					String: sWhere
				Actions
					Call SalWaitCursor(TRUE)
					Call SalDisableWindow( pb_Tiket )
					Call SalDisableWindow( pb_Buch )
					Set nVob = IifN (cb_Zo, 96, 6 )
					If Subs(tbl.ZASTAVA,1,4) = '5831'
						Set sMfo = tbl.MFO 
						Set sNls = tbl.NLS 
					Else
						Set sMfo = GetBankMfo()
						Set sNls = B_1919 
					! сплатити номінал в зовнішній світ
					Set nRef = NUMBER_Null
					Set sNaznN = 
							IifS( tbl2.OSTAB > tbl.NOM ,'Часткове ','')||
							'Погашення номіналу ЦП ' || tbl.CP_ID
					Set kN = tbl.NOM * 100
					Call cDoc.SetDoc( nRef, 'FXA', 1, Vob_FXA, Str(nRef),
							     bDat, bDat, bDat, bDat,
							     B_4621, GetBankName(), aMfo, '', tbl2.KV, kN, GetBankOkpoS(),
							     sNls, tbl.NB, sMfo, '', tbl2.KV,  kN, tbl.OKPO,
							     sNaznN, '',GetIdOper(),'',NUMBER_Null,0)
					If not cDoc.oDoc()
						Return FALSE
					! сплатити купон в зовнішній світ
					Set kR = tbl.KUPON * 100
					If kR > 0 
						Set nRef = NUMBER_Null
						Set sNaznK = 'Погашення '|| Str(nPr_Kup) || '% нарахованого купону ЦП '|| tbl.CP_ID
						Call cDoc.SetDoc( nRef, 'FXA', 1, Vob_FXA, Str(nRef),
								     bDat, bDat, bDat, bDat,
								     B_4621, GetBankName(), aMfo, '', tbl2.KV, kR, GetBankOkpoS(),
								     sNls, tbl.NB, sMfo,'', tbl2.KV, kR, tbl.OKPO,
								     sNaznK, '',GetIdOper(),'',NUMBER_Null,0)
						If not cDoc.oDoc()
							Return FALSE
					! Закрити в портфелі 
					If SqlPLSQLCommand(hSql(),"CP_DPOG( tbl.CP_ID, nVob,tbl2.REF, kN, sNaznN, kR, sNaznK,
							   nPr_Kup, B_4621,  sREF,sErr, REF_MAIN )") 
							and sErr = STRING_Null
						! подпись
						If not F_PODPIS(sREF)
							Return FALSE
					!
					! перенос CP_REE2 в Архив 
					Set sWhere = "from cp_ree2 where CP_ID=:tbl.CP_ID and mfo=:tbl.MFO and ZASTAVA=:tbl.ZASTAVA "
					If NOT SqlPrepareAndExecute( hSql(),
							  "INSERT INTO cp_ree2a(OKPO,MFO,NLS,NOM,CP_ID,KUPON,ZASTAVA,REF) 
							   select OKPO,MFO,NLS,NOM,CP_ID,KUPON,ZASTAVA, :REF_MAIN "|| sWhere ) 
							OR NOT SqlPrepareAndExecute( hSql(), "delete "|| sWhere )
						Return FALSE
					Set bArc = TRUE
					Return TRUE
			Function: F_PRINT_CP
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					String: FIO
					String: sTmp
					String: strReport
					String: strTempl
					Number: nRef_Print
					!
					File Handle: hFile
					Long String: sTiket
					!
					Number: RATE
					Date/Time: DAT_EM
					Date/Time: DATP
					String: TIME
				Actions
					! ---------------     Печатаем тикет    -------------		
					If SqlPrepareAndExecute( hSql(),
							  "SELECT DAT_EM, DATP, nvl(IR,0), to_char(sysdate,'HH24:MM:SS')
							   INTO :DAT_EM, :DATP, :RATE, :TIME
							   FROM cp_kod WHERE cp_id=:sCP_ID " )
							and SqlFetchNext( hSql(), nFetchRes )
					Set FIO = GetUserRealName()
					Set TIME = SalFmtFormatDateTime(bDat, 'dd/MM/yyyy') || ' ' || TIME
					Set strTempl = GetTemplateDir()||'\\CP_'|| 'D.qrd'
					Set strReport = GetPrnDir()||'\\CP'||SalFmtFormatDateTime(SalDateCurrent() ,'hhhmmss') ||
							    '.D' || SalStrRightX('0' || Str(SalDateDay(SalDateCurrent())), 2)
					Set sTmp = 
							Str(REF_MAIN) ||
							'~'|| SalFmtFormatDateTime(bDat, 'dd/MM/yyyy') ||
							'~'|| sZASTAVA || 
							'~'|| sOKPO || 
							'~'|| SalStrUpperX( sNB ) || 
							'~'|| sMFO || 
							'~'|| sNLS || 
							'~'|| SalStrTrimX( SalNumberToStrX(nNOM,2)) || 
							'~'|| SalStrTrimX( SalNumberToStrX(nKUPON,2)) || 
							'~'|| FIO   || 
							'~'|| sCP_ID|| 
							'~'|| SalStrTrimX( SalNumberToStrX(RATE,4)) ||   
							'~'|| SalStrTrimX( SalNumberToStrX(RATE/5,4)) ||
							'~'|| SalFmtFormatDateTime(DAT_EM, 'dd/MM/yyyy') || 
							'~'|| SalFmtFormatDateTime(DATP, 'dd/MM/yyyy') || 
							'~'|| SalFmtFormatDateTime(DATP, 'dd/MM/yyyy') || 
							'~'|| SalFmtFormatDateTime(bDat, 'dd/MM/yyyy') ||
							'~'|| SalStrTrimX( SalNumberToStrX(nNOM+nKUPON,2)) 
					!
					Call DosReport (strTempl,  strReport,
							'REF_MAIN~DAT~ZASTAVA~OKPO~NB~MFO~NLS~NOM~KUPON~'||
							'FIO~CP_ID~RATE~RATE5~DAT_EM~DATP~TIME~DAT_OPL~SUMB',
							 sTmp, 0)
					If SqlPrepareAndExecute( hSqlAux2(),
							  "SELECT ref INTO :nRef_Print FROM oper WHERE ref in ("|| sREF ||")"  )
						While SqlFetchNext(  hSqlAux2(),  nFetchRes)
							Call DocsSaldoPrintK(nRef_Print, strReport)
					Call DosDirectPrint(strReport)
					! вставить в архив это текстовый файл strReport
					If bArc
						If SalFileOpen( hFile, strReport, 0 )
							Call SalFileRead( hFile, sTiket, VisFileGetSize( strReport ) )
							If SqlPrepare( hSql(), "UPDATE cp_arch SET sTiket = :sTiket where ref="||Str(REF_MAIN) )
									and SqlSetLongBindDatatype(1, BLOB_BYTE)
									and SqlExecute(hSql())
								Call SalFileClose( hFile )
								Call SqlCommitEx(hSql(), CurrentLangTable.GetAtomTitle('CP1.cTMess4')||" "||CurrentLangTable.GetAtomTitle('CP1.cTMess5'))
								Set bArc=FALSE
