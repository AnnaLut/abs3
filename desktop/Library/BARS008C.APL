					Set CP_MANY =  CP_MANY || " union all 
							select FDAT,0,0,0 from "|| CP_MANY30 || 
							" where fdat not in (to_date(:DAT_ROZ),to_date(:bDat)) 
							    and fdat not in (select fdat from cp_many where ref=:REF) "
				!
				Set Q = NUMBER_Null
				!
				If rb_SS
					Set sPereoc = " fostzn( nvl(:nAccS,-999), m.FDAT) " 
				Else
					Set sPereoc = " 0 "
				!
				!
				Set MANY.strSqlPopulate =
						"SELECT m.FDAT, m.SS1, m.SN2, m.SDP,        
						       (select SUM( (SS1+SN2) / power( 1 + (:IRR)/100,(FDAT-m.FDAT)/365 ))
						        from cp_many where ref= :REF and fdat >= m.FDAT   ) SR,
						       (select SUM( (SS1+SN2) / power( 1 + (:IRE)/100,(FDAT-m.FDAT)/365 ))
						        from cp_many where ref= :REF and fdat >= m.FDAT   ) SE,
						    -cp.KUPON1 (:ID,  m.FDAT, :DAT_EM, :DATP ) /100, " || sPereoc || " /100 
						 INTO :MANY.FDAT, :MANY.G3, :MANY.G4, :MANY.G5, :MANY.G6, :MANY.G7, :MANY.nInt, :MANY.G11 
						 FROM (" || CP_MANY || ") m  
						union ALL
						 SELECT m.FDAT, m.SS1, m.SN2, m.SDP,        
						       -(m.SS1+m.SN2+m.SDP) SR,
						       -(m.SS1+m.SN2      ) SE,
						-cp.KUPON1 (:ID,  m.FDAT, :DAT_EM, :DATP ) /100, 0
						 FROM cp_many m where ref = :REF and fdat = :DAT1 
						ORDER BY 1"
				If SqlPrepareAndExecute( hSqlAux2(), "delete from tmp_irr" )
					Set FDAT1 = DATETIME_Null
					Call SalTblPopulate(hWndForm, hSql(), MANY.strSqlPopulate, TBL_FillAll )
					Call F_Dox()
				Call SalTblDefineSplitWindow (hWndForm, 3, TRUE)
				Set nTmp = SalTblInsertRow(hWndForm, TBL_MinRow)
				Call SalTblSetRowFlags(hWndForm, nTmp, ROW_New, FALSE)
				Call SalTblSetContext(hWndForm, nTmp)
				Set G1 = I1
				Set G2 = I2
				Set G3 = I3
				Set G4 = I4
				Set G5 = I5
				Set G9 = I9
				Set G10 = I10
				Call VisTblAutoSizeColumn(hWndForm, hWndNULL)
				Call SalTblSetContext( hWndForm, 0 )
				Call SalTblSetRowFlags( hWndForm, 0, ROW_Selected, TRUE )
				Call SalSendMsg( hWndForm, SAM_RowSetContext, 0,0)
				Call SalWaitCursor(FALSE)
			On SAM_FetchRowDone
