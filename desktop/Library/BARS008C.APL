			Number: P11 
			Number: P21 
			Number: P31 
			Number: OSTP1
			Number: n980
			Number: KF
		Message Actions
			On SAM_Create
				Call PrepareWindow( hWndForm )
				Call SetWindowFullSize(hWndForm)
				! Присваиваем значения многоязыковым объектам
				Call SalSendMsgToChildren( hWndForm, UM_QueryLabelText, 0, 0 )
				Call SalSetWindowText( hWndForm, CurrentLangTable.GetAtomTitle('CP3.cTWndTitle')  )
				!
				Call SalTblSetLockedColumns( hWndForm, 5 )
				Set CP3.nTabInstance = 1
				Set CP3.FL_Filtr = TRUE
				Set CP3.n980 = GetBaseVal()
				Call SetWindowFullSize(hWndForm)
				Set CP3.strFilterTblName = 'CP_KOD'
				Set CP3.strPrintFileName = 'CP3'
				Set dFiltr = SalStrTrimX( strPar02 )
				Set CP3.strSqlPopulate =
						"SELECT cp_kod.TIP, substr(a.nls,1,4), CP_KOD.ID, CP_KOD.CP_ID, a.mdate,  CP_KOD.CENA, a.KV, 
						        sum(c.N)/100, sum(c.R)/100, sum(c.D)/100, sum(c.P)/100
						 INTO  :CP3.TIP, :CP3.VIDD, :CP3.ID, :CP3.CP_ID, :CP3.DATP, :CP3.CENA, :CP3.KV, 
						       :CP3.N1, :CP3.R1, :CP3.D1, :CP3.P1
						 FROM  CP_ARCH c, oper o, CP_KOD, accounts a 
						 WHERE c.acc=a.acc AND c.OP=cp_kod.TIP and c.ref=o.ref and o.sos=5 and c.id=CP_KOD.id and
						      CP_KOD.id in (select id from cp_kod where id>0 "|| strPar02 || " )
						 GROUP BY cp_kod.TIP, substr(a.nls,1,4), CP_KOD.ID, CP_KOD.CP_ID, a.mdate, CP_KOD.CENA, a.KV
						 ORDER BY cp_kod.TIP, substr(a.nls,1,4), CP_KOD.ID"
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On UM_FetchRowDone
				! продано
				If SqlPrepareAndExecute( hSqlAux2(), 
						  "SELECT nvl(sum(c.N)/100,0), nvl(sum(c.R)/100,0), nvl(sum(c.D)/100,0), nvl(sum(c.P)/100,0)
						   INTO :CP3.N2,  :CP3.R3, :CP3.D3, :CP3.P3
						   FROM  CP_ARCH c, oper o 
						   WHERE c.OP=2 and c.ref=o.ref and o.sos=5 and id=:CP3.ID") 
						and SqlFetchNext( hSqlAux2(),  nFetchRes )
				! уплачено купона
				If SqlPrepareAndExecute( hSqlAux2(), 
						  "SELECT nvl(sum(c.R)/100,0) INTO :CP3.R4
						   FROM  CP_ARCH c, oper o 
						   WHERE c.OP=3 and c.ref=o.ref and o.sos=5 and id=:CP3.ID") 
						and SqlFetchNext( hSqlAux2(),  nFetchRes )
				! остатки и начислено и амортиз
				If SqlPrepareAndExecute( hSqlAux2(), 
						  "SELECT nvl(-sum(R.ostc)/100,0), nvl(sum(D.ostc)/100,0), 
						          nvl(-sum(P.ostc)/100,0), nvl(-sum(N.ostc)/100,0) 
						   INTO :CP3.OSTR,  :CP3.OSTD, :CP3.OSTP, :CP3.OSTN
						   FROM  accounts R,  accounts D, accounts P, cp_deal e, accounts N
						   WHERE e.id=:CP3.ID and 
						         e.accD = D.acc (+) and 
						         e.accP = P.acc (+) and 
						         e.accR = R.acc (+) and
						         e.acc  = N.acc ") 
						and SqlFetchNext( hSqlAux2(),  nFetchRes )
					Set CP3.R2 = CP3.OSTR 
					! Set CP3.R2 = CP3.OSTR - CP3.R1 + CP3.R3 + CP3.R4
					Set CP3.D2 = CP3.D1 - CP3.D3 - CP3.OSTD 
					Set CP3.P2 = CP3.P1 - CP3.P3 - CP3.OSTP
				Else
					Set CP3.R2=0
					Set CP3.D2=0
					Set CP3.P2=0
				Call CP3.COLORA()
				If CP3.KV  = CP3.n980 
					Set CP3.KF = 1
				Else
					If SqlPrepareAndExecute( hSqlAux2(), 
							  "SELECT RATE_O/BSUM INTO :CP3.KF FROM cur_rates WHERE kv=:CP3.KV and 
							     vdate = (select max(vdate) from cur_rates where kv=:CP3.KV and vdate<=bankdate) ") 
							and SqlFetchNext( hSqlAux2(),  nFetchRes )
				Set CP3.N11 = CP3.N11 + CP3.N1*CP3.KF
				Set CP3.N21 = CP3.N21 + CP3.N2*CP3.KF
				Set CP3.OSTN1 = CP3.OSTN1 + CP3.OSTN*CP3.KF
				!
				Set CP3.R11 = CP3.R11 + CP3.R1*CP3.KF
				Set CP3.R21 = CP3.R21 + CP3.R2*CP3.KF
				Set CP3.R31 = CP3.R31 + CP3.R3*CP3.KF
				Set CP3.R41 = CP3.R41 + CP3.R4*CP3.KF
				Set CP3.OSTR1 = CP3.OSTR1 + CP3.OSTR*CP3.KF
				!
				Set CP3.D11 = CP3.D11 + CP3.D1*CP3.KF
				Set CP3.D21 = CP3.D21 + CP3.D2*CP3.KF
				Set CP3.D31 = CP3.D31 + CP3.D3*CP3.KF
				Set CP3.OSTD1 = CP3.OSTD1 + CP3.OSTD*CP3.KF
				!
				Set CP3.P11 = CP3.P11 + CP3.P1*CP3.KF
				Set CP3.P21 = CP3.P21 + CP3.P2*CP3.KF
				Set CP3.P31 = CP3.P31 + CP3.P3*CP3.KF
				Set CP3.OSTP1 = CP3.OSTP1 + CP3.OSTP*CP3.KF
				!
				If CP3.N1 !=0
					Set CP3.CENAK = (( CP3.N1 + CP3.R1 + CP3.P1 - CP3.D1) / CP3.N1 ) * CP3.CENA
				If CP3.N2 !=0
					Set CP3.CENAP = (( CP3.N2 + CP3.R2 + CP3.P2 - CP3.D2) / CP3.N2 ) * CP3.CENA
				If CP3.OSTN  != 0
					Set CP3.CENAA =  (( CP3.OSTN + CP3.OSTR + CP3.OSTP - CP3.OSTD) / CP3.OSTN ) * CP3.CENA
			On UM_Populate
				If CP3.FL_Filtr ! Только для входа, а далее по требованию
					Set CP3.FL_Filtr = FALSE
					Call SalSendMsg( pbFilter, SAM_Click, 0, 0 )
				!
				! Call Debug('0')
				Call NASELENIE()
