								 INTO :nAccD, :nAccP, :nAccR, :nAccR2, :nAccR3, :nAccS, :nACCEXPN, :nACCEXPR, :nACCUNREC FROM cp_deal  WHERE ref=" || Str(nRef) 
						Set nAccS5 = -9
