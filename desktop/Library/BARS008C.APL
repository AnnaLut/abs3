			Function: F_Stroka
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
				Actions
					Set BSUMG = NLSN + NLSS + NLSR2 + NLSR + NLSD + NLSP
					Set BSUMC = NLSN + NLSS + NLSD + NLSP
					If CENA >0 
						Set KOL = SalNumberAbs( NLSN/CENA )
						If KOL != 0
							Set BSUMG1 = BSUMG/KOL
							Set BSUMC1 = BSUMC/KOL
					Return TRUE
			Function: F_KUP
				Description:
				Returns
					Number:
				Parameters
					Number: nKol
				Static Variables
				Local variables
					Number: DEL_KUP1  ! длина ВСЕГО купоннго периода  в днях
					Number: DEL_KUP2  ! продаваемая длнина купоннго периода  в днях
					Number: kR2 ! Расчетный коефф купона 
					Number: nR2 ! Расчетная сумма купона 
					!
					Number: nKUP1d
					! Number: CENA_KUP 
					Number: nR_All ! Купон за весь купонній период
				Actions
					! Нет смысла считать
					If nDOX = 1 or nKol = 0 or CENA =0 or IR = 0 or DAT_ROZ = DATETIME_Null 
						Return 0
					! Даты
					If SqlPrepareAndExecute( hSql(),
							"select max(to_date(iif_d( dok, :DAT_ROZ, dok,  dok,  null) ) ) ,
							        min(to_date(iif_d( dok, :DAT_ROZ, null, null, dok ) ) ) 
							 into :DOK, :DNK  from cp_dat where id=:nId")
							and SqlFetchNext( hSql(), nFetchRes )
						! Call Debug('1.cp_dat: DOK ='|| SalFmtFormatDateTime( DOK, 'dd/MM/yyyy' )||
								                  ',  DNK ='|| SalFmtFormatDateTime( DNK, 'dd/MM/yyyy' ) )
						If DOK = DATETIME_Null
							If SqlPrepareAndExecute( hSql(),
									"select CENA_KUP, nvl(dok, dat_em), 
									        nvl(DNK, nvl(DOK,DAT_EM)+PERIOD_KUP)
									 into :CENA_KUP, :DOK, :DNK from cp_kod where id=:nId")
									and SqlFetchNext( hSql(), nFetchRes )
							! Call Debug('2.cp_kod: DOK ='|| SalFmtFormatDateTime( DOK, 'dd/MM/yyyy' )||
									                  ',  DNK ='|| SalFmtFormatDateTime( DNK, 'dd/MM/yyyy' ) )
						Else
							If SqlPrepareAndExecute( hSql(),
									"select kup into :CENA_KUP from cp_dat where id=:nId and dok=:DNK ")
									and SqlFetchNext( hSql(), nFetchRes )
					! Нет дат
					If DOK = DATETIME_Null or DNK = DATETIME_Null 
						Return 0
					! Рассчитать nR2 накоп купон от год % ставки или от ставки на период
					Set DEL_KUP1 = DNK - DOK 
					Set DEL_KUP2 = DAT_ROZ - IifD ( DAT_ROZ>=DNK, DNK, DOK )
					! Есть цена купона
					! nKUP1 Купон 1 дня = Цена_купона /(дней_в_периоде)
					If CENA_KUP != NUMBER_Null and CENA_KUP > 0
						! Set nKUP1d = SalNumberRound( CENA_KUP *100*DEL_KUP2/DEL_KUP1 ) /100
						Set nKUP1d =  CENA_KUP *DEL_KUP2/DEL_KUP1 
						Set nR2 = nKol * nKUP1d 
						Set nKUP1 =   nKol * CENA_KUP/DEL_KUP1
						Return nR2
					! Есть годовая ставки и кол периодов
					! nKUP1 К цена_бумаги * (Ставка_периода) /(100% * (дней_в_периоде))
					If KY > 1
						Set kR2 = ( IR /KY ) * DEL_KUP2 / DEL_KUP1 !! -- %
						! Set nKUP1d = SalNumberRound( CENA * kR2 ) / 100 
						Set nKUP1d = ( CENA * kR2 ) / 100 
						Set nR2 = nKol * nKUP1d 
						Set nKUP1 = nKol * CENA *  IR/KY / (DEL_KUP1 *100 )
					Else
						! Есть годовая ставки и на весь год
						! nKUP1 К цена_бумаги * (Ставка_годовая ) /(100% * 365 )
						Set kR2 = IR * DEL_KUP2 / 365 !!=--%
						! Set nKUP1d = SalNumberRound(CENA * kR2 )/100 
						Set nKUP1d = (CENA * kR2 )/100 
						Set nR2 = nKol * nKUP1d
						Set nKUP1  = nKol * CENA * IR / 36500 
					Return nR2
			Function: F_AMR
				Description:
				Returns
				Parameters
					Number: nAcc
				Static Variables
				Local variables
					Number: OstDP
					Number: OstN
					Number: nDel
					Date/Time: Dat
					Number: nDniK
					Number: nDniR
					! Number: nKUP1
				Actions
					If nAcc = NUMBER_Null
						Return NUMBER_Null
					! Data и ост номинала 
					! ост дис/прем
					If SqlPrepareAndExecute( hSql(),
							"select s.FDAT, (s.ostf-s.dos+s.kos) into :Dat, :OstN 
							 from saldoa s  where s.acc=:nACCN and (s.acc,s.fdat)=
							(select acc,max(fdat) from saldoa where (s.ostf-s.dos+s.kos) <>0 
							   and acc=s.acc group by acc)")
							and NOT SqlFetchNext( hSql(), nFetchRes ) 
						Return 0
					!
					If SqlPrepareAndExecute( hSql(),
							"select (ostf-dos+kos) into :OstDP 
							 from saldoa where acc=:nAcc and fdat=:Dat and (ostf-dos+kos)<>0") 
							and NOT SqlFetchNext( hSql(), nFetchRes )
						Return 0
					! Всего планируется дней жизни ЦБ у нас
					Set nDniK = DATP - Dat
					If nDniK = 0 
						Set nDniK = 1
					! дней жизни ЦБ у нас до расчетной даты
					Set nDniR = DAT_ROZ - Dat
					! Линейно
					If PR_AMR = 4
						Return (OstDP  * (1 - nDniR/nDniK ))/100
					! По эф.ставке 
					Set nDel = 0 
					! Call Debug('nKUP1 в цел= '||Str(nKUP1) )
					! Call Debug('ERAT = '||Str(ERAT) )
					While nDniR >0
						! Call Debug('nDniR = '||Str(nDniR) )
						! Call Debug('OstN = '||Str(OstN) )
						! Call Debug('OstDP= '||Str(OstDP) )
						Set nDel = SalNumberAbs( (OstN+OstDP)*ERAT + nKUP1*100 )
						! Call Debug(' nDel = '||Str(nDel) )
						If OstDP > 0 
							Set OstDP = OstDP - nDel
						Else If OstDP <0 
							Set OstDP = OstDP + nDel
						Else
							Return 0
						Set nDniR = nDniR - 1 
					Return OstDP/100 
					!
		Window Parameters
			Number: nId
			Number: nRef
			String: strTitl
			String: strPar02
		Window Variables
			Date/Time: bDat
			Number: nRow
			Number: nKUP1 
			Number: nDOX
			Number: KY
			Number: nTIP 
			Number: PR_AMR
			Number: ERAT
			!
			String: sNLSN
			String: sNLSR2
			String: sNLSR
			String: sNLSD
			String: sNLSP
			String: sNLSS
			!
			Number: nACCN
			Number: nACCR2
			Number: nACCR
			Number: nACCD
			Number: nACCP
			Number: nACCS
			!
			Number: nN
			Number: nR2
			Number: nR
			Number: nD
			Number: nP
			Number: nS
			Number: nK
			Number: nTmpE
			String: sTmpE
			Date/Time: dTmpE
			Date/Time: dDat_Opl
			!
			String: sFDAT
		Message Actions
			On SAM_Create
				Call SalSetWindowText( hWndForm, strTitl )
				Call PrepareWindow( hWndForm )
				Call SetWindowFullSize(hWndForm)
				Call SalTblSetLockedColumns( hWndForm, 3 )
				Set bDat = GetBankDate()
				Set DAT_ROZ = bDat + 5
				Set PROG.strPrintFileName =  'U'|| Str(nRef)
				!
				Set dID = nId 
				Set dREF= nRef 
				If SqlPrepareAndExecute( hSql(),
						"select k.CENA, k.datp, k.dox, nvl(k.ir,0), Nvl(k.KY,1),k.tip, 
						        k.PR_AMR, k.CENA_KUP, k.CP_ID, o.ND , o.vdat 
						 into :CENA, :DATP, :nDOX, :IR, :KY, :nTIP, :PR_AMR, :CENA_KUP, 
						      :CP_ID, :N_UGOD, :DAT_UG 
						 from cp_kod k , oper o 
						 where k.id=:nId and o.ref=:nRef ")
						and SqlFetchNext( hSql(), nFetchRes )
				If SqlPrepareAndExecute( hSql(),
						"select n.nls, r2.nls, r.nls,  d.nls,  p.nls,  s.nls,
						        n.acc, r2.acc, r.acc,  d.acc,  p.acc,  s.acc, e.erat
						 into :sNLSN, :sNLSR2, :sNLSR, :sNLSD, :sNLSP, :sNLSS,
						      :nACCN, :nACCR2, :nACCR, :nACCD, :nACCP, :nACCS, :ERAT 
						 from cp_deal e, accounts n, 
						      accounts r2, accounts r, accounts d, accounts p, accounts s
						 where e.id=:nId and e.ref=:nRef and e.acc=n.acc 
						   and e.accr2=r2.acc(+) and e.accr=r.acc(+) 
						   and e.accd =d.acc(+)  and e.accp=p.acc(+) and e.accs=s.acc(+) " ) 
						and SqlFetchNext( hSql(), nFetchRes )
					Set ER = ERAT*365*100
					! Call Col_T(NLSN, sNLSN )
					! Call Col_T(NLSR2,sNLSR2)
					! Call Col_T(NLSR ,sNLSR )
					! Call Col_T(NLSD ,sNLSD )
					! Call Col_T(NLSP ,sNLSP )
					! Call Col_T(NLSS ,sNLSS )
					Set sFDAT = 
							"select fdat from saldoa where acc="||Str(nACCN) ||
							IifS(nACCR2=NUMBER_Null,"",
							" union select fdat from saldoa where acc="||Str(nACCR2)) ||
							IifS(nACCR=NUMBER_Null,"",
							" union select fdat from saldoa where acc="||Str(nACCR)) ||
							IifS(nACCD=NUMBER_Null,"",
							" union select fdat from saldoa where acc="||Str(nACCD)) ||
							IifS(nACCP=NUMBER_Null,"",
							" union select fdat from saldoa where acc="||Str(nACCP)) ||
							IifS(nACCS=NUMBER_Null,"",
							" union select fdat from saldoa where acc="||Str(nACCS)) 
				Call SalSendClassMessage(UM_Create, 0, 0)
			On UM_Populate
				Call SalWaitCursor(TRUE)
				Set PROG.strSqlPopulate =
						"SELECT fdat, 
						    fost(:nACCN, fdat)/100, fost(:nACCS, fdat)/100,
						    fost(:nACCR2,fdat)/100, fost(:nACCR, fdat)/100,
						    fost(:nACCD, fdat)/100, fost(:nACCP, fdat)/100 
						 into :PROG.FDAT,
						      :PROG.NLSN, :PROG.NLSS, :PROG.NLSR2,:PROG.NLSR, 
						      :PROG.NLSD, :PROG.NLSP
						 FROM ("|| sFDAT || ")
						 ORDER BY 1 desc"
				Call SalTblPopulate(hWndForm, hSql(), PROG.strSqlPopulate  , TBL_FillAll)
				Call SalWaitCursor(FALSE)
			On SAM_FetchRowDone
				Call F_Stroka()
				!
				Set nN = NLSN
				Set nR2= NLSR2
				Set nR = NLSR
				Set nD = NLSD
				Set nP = NLSP
				Set nS = NLSS
				Set nK = KOL
				Set dDat_Opl = FDAT
