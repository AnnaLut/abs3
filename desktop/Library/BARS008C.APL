									and SqlFetchNext( hSql(), nFetchRes )
								! Скріть-пoказать нужное
								Set vKv = nKv
								If not nameCP
									Set nameCP=CurrentLangTable.GetAtomTitle('CP1.cTNaznCP')
								Call F_PB()
								Set tKOL = "Кіл-сть"
								If nDOX=1 ! Акции 
									Call SalHideWindow( DATP )
									Call SalHideWindow( RATE )
									Call SalHideWindow( sBASEY )
									Set RR_ = 0
								If RATE = 0 and nDOX > 1 or DAT_EM=dDat and DAT_EM=DAT_ROZ
									Set RR_ = 0
									Call SalDisableWindow( RR_ )
								! If DATP  =dDat and fl_END
								If nTipd = 1 and rb_P or nTipd = 2 and rb_K
									Call SalHideWindow( tKOL )
									Call SalHideWindow( KOL )
								! Чужие 
								If nTipd = 1
									Set rb_AKT = TRUE
									Call SalDisableWindow( rb_PAS )
									Call SalShowWindow( cmb_PF )
									! Выбор портфеля
									Call cmb_PF.Init(cmb_PF)
									! Set sSqlPF =
											" p.PF=v.PF and v.vidd in (select vidd from cp_accc) " || 
											" and v.TIPD= " || Str(nTipd) ||
											" and v.DOX = " || Str(nDOX ) ||
											" and v.EMI = " || Str(nEMI ) ||
											" and p.PF <> " || IifS(Amort>0,"4","3") || 
											" and p.PF <> " || IifS(nDOX= 1,"3","2")
									! Set sSqlPF =
											" p.PF=v.PF and v.vidd in (select vidd from cp_accc) " || 
											" and v.TIPD= " || Str(nTipd) ||
											" and v.DOX = " || Str(nDOX ) ||
											" and v.EMI = " || Str(nEMI ) ||
											" and p.PF <> " || IifS(nDOX= 1,"3","2")
									Set sSqlPF =
											" p.PF=v.PF and v.vidd in (select vidd from cp_accc) " || 
											" and v.TIPD= " || Str(nTipd) ||
											" and v.DOX = " || Str(nDOX ) ||
											" and v.EMI = " || Str(nEMI ) 
									Call cmb_PF.Populate(hSql(), 
											"p.PF", "v.VIDD || '/' || p.NAME ", 
											"CP_PF p, CP_VIDD v ", " WHERE " || sSqlPF )
									! If rb_K or fl_END
										                                                                                                       Call cmb_PF.Populate(hSql(), 
												"p.PF", "v.VIDD || '/' || p.NAME ", 
												"CP_PF p, CP_VIDD v ", " WHERE " || sSqlPF )
									! Else
										                                                                                                       Call cmb_PF.Populate(hSql(), 
												"p.PF", "v.VIDD || '/' || p.NAME ", "CP_PF p, CP_VIDD v ",
												 " WHERE p.PF<>3 and "|| sSqlPF )
									! Первое возможное значение портфеля
									Call SalListSetSelect( cmb_PF, 0 )
									Call SalSendMsg( cmb_PF, SAM_Click, 0, 0 )
									Call SalEnableWindow( cmb_PF )
									Call SalSetFocus( cmb_PF )
									!
								! Свои
								Else If nTipd = 2
									Set rb_PAS = TRUE
									Call SalDisableWindow( rb_AKT )
									Call SalHideWindow( cmb_PF )
									!
									Call F_NLS_A() 
									Call F_RYN( 2, 0 )
								If nKv = GetBaseVal() and nKv = 980
									Set cb_SWIFT = TRUE
									Call SalDisableWindow( cb_SWIFT )
								Else
									Call SalHideWindow(cb_STP)
									! Внедрен ли SWIFT ?
									Call SalEnableWindow( cb_SWIFT )
									Set cb_SWIFT =
											SqlPrepareAndExecute( hSql(), "SELECT val FROM params WHERE par='SWIFT' and substr(val,1,1)='1' " )
											and SqlFetchNext( hSql(), nFetchRes)
