			On SAM_Create
				Call SalCenterWindow(hWndForm)
				Call SetWindowFont(hWndForm)
				Set RATE = nRATE
				If nRATE > 0 
					Set rb_Y = TRUE
					Call SalDisableWindow( rb_D )
				Else
					Set rb_D = TRUE
				Set dNom = nNom
				Set dDatP = dPog
				Set dDatV = dTek
				Set dBasey = sBASEY
				Set dKoeff = 0
				! Call Debug('0')
			On SAM_CreateComplete
				Call F_Kurs()
			On SAM_Activate
				Call SalSendMsg( SalParentWindow( hWndForm ), UM_MenuSelect, 0, 0 )
