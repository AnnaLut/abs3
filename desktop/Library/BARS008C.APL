				Set CP_MANY = 
						"SELECT FDAT,SS1,SN2,SDP from cp_many where ref=:REF and fdat " ||
						IifS( rb3, " = :DAT2 " ," > :DAT1 " )
				If DAT_ROZ = DATETIME_Null
					Set DAT_ROZ = bDat
				If SqlPrepareAndExecute( hSql(),
						  "select 1 from cp_many where ref=:REF and fdat=:DAT_ROZ " )
						and NOT SqlFetchNext( hSql(), nFetchRes) 
					Set CP_MANY =  CP_MANY || " union all SELECT to_date(:DAT_ROZ), 0,0,0 from dual "
				If DAT_ROZ != bDat 
						and SqlPrepareAndExecute( hSql(),
						  "select 1 from cp_many where ref=:REF and fdat=:bDat " )
						and NOT SqlFetchNext( hSql(), nFetchRes) 
					Set CP_MANY =  CP_MANY || " union all SELECT to_date(:bDat), 0,0,0 from dual "
				If rb2
