		Functions
			Function: F_DOX
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
				Actions
					If TIP = 1
						If DOX = 1 OR DOX = 3 !! Дивиденд !! Дисконт
							Call SalHideWindow( IR )
							Call SalHideWindow( KY )
							Call SalHideWindow( BASEY )
							Call SalHideWindow( sBASEY )
							Call SalHideWindow( DOK )
							Call SalHideWindow( DAT_RR )
							Call SalHideWindow( DNK )
							Call SalHideWindow( PERIOD_KUP )
							Call SalHideWindow( CENA_KUP )
						Else If DOX = 2 !! Купон
							Call SalShowWindow( IR )
							Call SalShowWindow( KY )
							Call SalShowWindow( BASEY )
							Call SalShowWindow( sBASEY )
							Call SalShowWindow( DOK )
							Call SalShowWindow( DAT_RR )
							Call SalShowWindow( DNK )
							Call SalShowWindow( PERIOD_KUP )
							Call SalShowWindow( CENA_KUP )
					Return TRUE
			Function: F_CLOS
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					String: sSql
					! Boolean: b_FL
					String: sErr
				Actions
					Set sErr = ""
					If DOX = 2 and KY > 1
						If DAT_RR !=DATETIME_Null and NOT (DAT_EM<=DOK and DOK<DAT_RR and DAT_RR<=DNK and DNK<=DATP) 
								OR DAT_RR = DATETIME_Null and NOT (DAT_EM<=DOK and DOK< DNK and DNK<=DATP) 
							Set sErr = 'Нарушен Порядок следования ДАТ'
					If sErr != ""
						Call SalMessageBox(sErr,CurrentLangTable.GetAtomTitle('cTAt'), MB_IconStop)
						Return FALSE
					! !!!!
					If nId= NUMBER_Null
						Set sSql =  
								"INSERT into cp_kod
								 (TIP, AMORT, KV, IDT, CP_ID, CENA, DAT_EM, KY, RNK, COUNTRY, DATP, 
								  BASEY, IR, DOK, CENA_KUP, NAME, DAT_RR, DNK, DOX, PERIOD_KUP, DCP, EMI) 
								values 
								(:TIP,:AMORT,:KV,:IDT,:CP_ID,:CENA,:DAT_EM,:KY,:RNK,:COUNTRY,:DATP, 
								 :BASEY,:IR,:DOK,:CENA_KUP,:NAME,:DAT_RR,:DNK,:DOX,:PERIOD_KUP,:DCP,:EMI)"
					Else
						Set sSql =  
								"UPDATE cp_kod SET 
								   TIP =:TIP , AMORT =:AMORT , KV =:KV , IDT=:IDT, CP_ID     =:CP_ID     ,
								   CENA=:CENA, DAT_EM=:DAT_EM, KY =:KY , RNK=:RNK, COUNTRY   =:COUNTRY   ,
								   DATP=:DATP, BASEY =:BASEY , IR =:IR , DOK=:DOK, CENA_KUP  =:CENA_KUP  ,
								   NAME=:NAME, DAT_RR=:DAT_RR, DNK=:DNK, DOX=:DOX, PERIOD_KUP=:PERIOD_KUP,
								   DCP =:DCP , EMI   =:EMI 
								 WHERE id=:nId"
					If NOT SqlPrepareAndExecute( hSql(),sSql) 
						Call SqlRollback( hSql())
						Return FALSE
					!
					Call SqlCommit( hSql())
					Return TRUE
			Function: F_COLOR
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
				Actions
					! Call SalColorSet( TIP, COLOR_IndexWindow, nCol )
					! Call SalColorSet( sTIP, COLOR_IndexWindow, nCol )
					Call SalColorSet( IDT, COLOR_IndexWindow, nCol )
					! Call SalColorSet( sIDT, COLOR_IndexWindow, nCol )
					! Call SalColorSet( ID, COLOR_IndexWindow, nCol )
					Call SalColorSet( CP_ID, COLOR_IndexWindow, nCol )
					Call SalColorSet( DOX, COLOR_IndexWindow, nCol )
					! Call SalColorSet( sDOX, COLOR_IndexWindow, nCol )
					Call SalColorSet( AMORT, COLOR_IndexWindow, nCol )
					! Call SalColorSet( sAMORT, COLOR_IndexWindow, nCol )
					Call SalColorSet( RNK, COLOR_IndexWindow, nCol )
					! Call SalColorSet( NAME, COLOR_IndexWindow, nCol )
					! Call SalColorSet( COUNTRY, COLOR_IndexWindow, nCol )
					! Call SalColorSet( sCOUNTRY, COLOR_IndexWindow, nCol )
					Call SalColorSet( DCP, COLOR_IndexWindow, nCol )
					! Call SalColorSet( sDCP, COLOR_IndexWindow, nCol )
					Call SalColorSet( EMI, COLOR_IndexWindow, nCol )
					! Call SalColorSet( sEMI, COLOR_IndexWindow, nCol )
					Call SalColorSet( KV, COLOR_IndexWindow, nCol )
					! Call SalColorSet( sKV, COLOR_IndexWindow, nCol )
					Call SalColorSet( CENA, COLOR_IndexWindow, nCol )
					Call SalColorSet( DAT_EM, COLOR_IndexWindow, nCol )
					Call SalColorSet( DATP, COLOR_IndexWindow, nCol )
					Call SalColorSet( IR, COLOR_IndexWindow, nCol )
					Call SalColorSet( KY, COLOR_IndexWindow, nCol )
					Call SalColorSet( BASEY, COLOR_IndexWindow, nCol )
					! Call SalColorSet( sBASEY, COLOR_IndexWindow, nCol )
					Call SalColorSet( DOK, COLOR_IndexWindow, nCol )
					Call SalColorSet( DAT_RR, COLOR_IndexWindow, nCol )
					Call SalColorSet( DNK, COLOR_IndexWindow, nCol )
					Call SalColorSet( PERIOD_KUP, COLOR_IndexWindow, nCol )
					Call SalColorSet( CENA_KUP, COLOR_IndexWindow, nCol )
					Return TRUE
		Window Parameters
			Number: nPar
			Number: nCol
			Number: nId
			String: strTitl
			String: strPar02
		Window Variables
			String: sTmp
			String: sTmp1
		Message Actions
			On SAM_Create
				Call SalSetWindowText( FCP, strTitl )
				Call SalSetFocus( pb_Ex )
				Call F_COLOR()
				If nId != NUMBER_Null and SqlPrepareAndExecute( hSql(),
						"SELECT k.TIP,k.CP_ID,k.KV,k.CENA,k.DAT_EM,k.DATP,k.IR,k.KY,k.DOK,k.DAT_RR,k.DNK,
						        k.IDT,k.DOX,k.RNK,k.AMORT,k.CENA_KUP,k.PERIOD_KUP,k.BASEY,k.EMI,k.DCP,
						   k.COUNTRY, t.name,i.name,d.name,a.name, NVL(c.nmk,k.name),b.name,v.name,e.name,g.name 
						  INTO   :TIP, :CP_ID, :KV, :CENA, :DAT_EM, :DATP, :IR, :KY, :DOK, :DAT_RR, :DNK,
						         :IDT, :DOX, :RNK, :AMORT, :CENA_KUP, :PERIOD_KUP, :BASEY, :EMI, :DCP,
						    :COUNTRY, :sTIP, :sIDT, :sDOX, :sAMORT, :NAME, :sBASEY, :sKV, :sEMI, :sCOUNTRY 
						 FROM cp_KOD k, cc_tipd T, cp_type i, cp_dox d, CP_VR a, customer c, BASEY b,
						      tabval v, cp_emi e, COUNTRY g    
						 WHERE k.id=:nId and k.TIP = t.tipd and k.idt =i.idt(+) and k.AMORT=a.vr(+) 
						   and k.RNK=c.rnk(+) and k.BASEY=b.BASEY(+) and k.kv=v.kv and k.emi=e.emi 
						   and k.dox=d.dox and k.COUNTRY=g.COUNTRY (+)" ) 
						and SqlFetchNext( hSql(), nFetchRes )
					Set ID = nId
				Else
					Set TIP = nPar
					If SqlPrepareAndExecute( hSql(),"SELECT name into :sTIP FROM cc_tipd where tipd=:nPar")
							and SqlFetchNext( hSql(), nFetchRes )
				Call F_DOX()
