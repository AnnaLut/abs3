			Function: F_DOPR
				Description:
				Returns
				Parameters
					Number: Ref
				Static Variables
				Local variables
					String: sKURS_FX4
				Actions
					If nKv !=n980
						If BICKB != STRING_Null
							Set FL = SqlPrepareAndExecute(hSql(),
									   "INSERT INTO operw (ref,tag,value) VALUES (:Ref,'54', :BICKB)" )
						If SSB != STRING_Null
							Set FL = SqlPrepareAndExecute(hSql(),
									   "INSERT INTO operw (ref,tag,value) VALUES (:Ref,'58', :SSB)" )
						If BICB !=STRING_Null
							Set FL = SqlPrepareAndExecute(hSql(),
									"UPDATE operw SET value=:BICB WHERE ref=:Ref and tag='57'")
						If KOD_BB !=STRING_Null
							Set FL = SqlPrepareAndExecute(hSql(),
									"INSERT INTO operw (ref,tag,value) VALUES (:Ref,'KOD_B', :KOD_BB)" )
						If KOD_GB !=STRING_Null
							Set FL = SqlPrepareAndExecute(hSql(),
									"UPDATE operw SET value=:KOD_GB WHERE ref=:Ref and tag='KOD_G'")
						If KOD_NB !=STRING_Null
							Set FL = SqlPrepareAndExecute(hSql(), "DELETE FROM operw WHERE ref=:Ref and tag='KOD_N' " )
									    and  SqlPrepareAndExecute(hSql(), "INSERT INTO operw (ref,tag,value) VALUES (:Ref,'KOD_N', :KOD_NB)" )
					Return TRUE
