			Function: F_SAVE
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					String: sREPO
					String: sNam1a
					String: sNam1b
					String: sErr
					String: sPROC
					String: sN7
					!
					String: sNazn
					String: sNaznK
					String: sBankDay
					String: sKb
					String: sNls
					String: sNls_
					Number: SK_
					Number: SKQ_
					Number: SB_
					Number: nVob_
					! Date/Time: DAT_ZO
					Date/Time: VDAT_
					Date/Time: DDAT_
					String: sINI
					String: sMAR
					String: pCP_AI
					String: rNls
				Actions
					Set Nd_   = Left(NTIK,10)
					Set sBankDay= SalFmtFormatDateTime(DAT_OPL,'MM/dd/yyyy')
					! Set sNazn =NBB ||'. '||  NTIK || ' ' || "Угода від " ||SalFmtFormatDateTime(DAT_UG,'dd/MM/yyyy')
					Set sNazn = NBB  ! для своїх ЦП
					If nTipd = 1  ! ЦП інших емітентів
						Set sNazn =nameEMI
					Set sNazn =sNazn  || ". "|| 
							  CurrentLangTable.GetAtomTitle('CP1.cTNazn1') || " " || NTIK || " " || 
							  CurrentLangTable.GetAtomTitle('CP1.cTNazn2') || " " || SalFmtFormatDateTime(DAT_UG,'dd/MM/yyyy')
					Set sNaznK = CurrentLangTable.GetAtomTitle('CP1.cTNaznK')
					Set SK_ = SUMBK * 100 ! сумма комисии
					Set SB_ = SUMB  * 100 ! сумма сделки
					! !=======
					! Date/Time: dDoc  !! Дата док
					! Date/Time: dPos  !! Дата ввода(поступления в банк)
					! Date/Time: dVal1  !! Дата валютирования основной операции
					! Date/Time: dVal2  !! Дата валютирования связаной операции
					! If SqlPLSQLCommand(hSql(), "PUL.Set_Mas_Ini ( 'CP_AI', '', 'А/I' )") 
					Set sREF = STRING_Null
					If rb_K !  П о к у п к а
						Set pCP_AI = IifS(rb_I,'1','0')
						Set sNazn = IifS( fl_END, CurrentLangTable.GetAtomTitle('CP1.cTNazn31') , 
								  CurrentLangTable.GetAtomTitle('CP1.cTNazn32') ) || " " || 
								  nameCP|| " " || sNazn
						If not SalModalDialog(dlg_MSG, hWndForm, sNazn)
							Return FALSE
						If cb_Zo AND DAT_UG = DAT_ROZ 
							Set nVob_ =  96
						Else
							Set nVob_ =  6
						If SqlPLSQLCommand(hSql(), "CP.CP_KUP
								( pCP_AI,
								  nTipd, nVob_,   nGrp,  nID,  nVidd,   nRYN, 
								  DAT_UG, DAT_OPL, DAT_ROZ, DAT_KOM, SUMBN, SUMB, RR_ , SUMBK, sNazn, 
								  NLS9, rNls, B_4621,B_1819,B_1919, sREF, sErr, REF_MAIN )") 
							Set FL = TRUE
							!
							! в DAT_KOM Комиссия (Отправка )
							If SK_ >0
								! Set sNls_ = IifS (DAT_KOM >= DAT_ROZ, B_4621, B_1819 )
								Set nRef0 = NUMBER_Null
								If cb_Zo AND DAT_UG = DAT_KOM 
									Set nVob_ = 96
									Set VDAT_ = dDat31
									Set DDAT_ = GetBankDate()
								Else
									Set nVob_ =  6
									Set VDAT_ = DAT_KOM
									Set DDAT_ = DAT_KOM
								If nKv = n980
									Call cDoc.SetDoc(nRef0, 'FXA', 1, Vob_FXA, Nd_,
											     DDAT_, DAT_UG, DAT_KOM, DAT_KOM,
											     rNls, NMKA, MFOA , '', nKv, SK_, GetBankOkpoS(),
											     NLSB_, NBB_, MFOB_, '', nKv, SK_, OKPOB_,
											     sNaznK, '',     GetIdOper(), '', NUMBER_Null, 
											     cb_STP)
								Else If SalStrTrimX( BICA ) = SalStrTrimX( BICKB_ ) ! прямой КС
									Call cDoc.SetDoc(nRef0, 'FXB', 1, nVob_, Nd_,
											     DDAT_, DAT_UG, VDAT_, VDAT_,
											     rNls,NMKA, MFOA, '', nKv, SK_, GetBankOkpoS(),
											     SSB_, NBB_, MFOA, '', nKv, SK_, OKPOB_,
											     sNaznK, '',     GetIdOper(), '', NUMBER_Null, 0)
								Else
									Call cDoc.SetDoc(nRef0, 'FXC', 1, nVob_, Nd_,
											     DDAT_, DAT_UG, VDAT_, VDAT_,
											     rNls,   NMKA,     MFOA, '', nKv, SK_, GetBankOkpoS(),
											     sNLS_FXC,sNMS_FXC, MFOA, '', nKv, SK_, GetBankOkpoS(),
											     sNaznK, '',     GetIdOper(), '', NUMBER_Null, 0)
								If cDoc.oDoc()
									Set nRef0 = cDoc.m_nRef
									Call F_DOPR_(nRef0)
									Call F_REF( nRef0 )
									Set FL = TRUE
								Else
									Set FL = FALSE
							! Если не ДСП в DAT_OPL сделать отправку в СЭП или SWIFT (FXA, FXC)
									и нет отказа по оплате 
							If nDCP = 0 and cb_NO_PAY = 0
								If FL
									Set nRef2  =NUMBER_Null
									Set sNls = IifS ( DAT_OPL=DAT_ROZ, 
											                  B_4621, 
											                  IifS( DAT_OPL<DAT_ROZ,  B_1819, B_1919 )
											                 )
									!
									If cb_Zo AND  DAT_UG = DAT_OPL 
										Set nVob_ =  96
										Set VDAT_ = dDat31
										Set DDAT_ = GetBankDate()
									Else
										Set nVob_ =  6
										Set VDAT_ = DAT_OPL
										Set DDAT_ = DAT_OPL
									!
									If cb_KS = 1
										Call cDoc.SetDoc(nRef2, 'FXB', 1, nVob_, Nd_,
												     DDAT_, DAT_UG, VDAT_, VDAT_,
												     sNls,NMKA, MFOA, '', nKv, SB_, GetBankOkpoS(),
												     SSB,  NBB, MFOA, '', nKv, SB_, OKPOB,
												     sNazn, '',     GetIdOper(), '', NUMBER_Null, 0)
									Else If nKv=n980
										Call cDoc.SetDoc(nRef2, 'FXA', 1, Vob_FXA, Nd_,
												     DDAT_, DAT_UG, DAT_OPL, DAT_OPL,
												     sNls, NMKA, MFOA, '', nKv, SB_, GetBankOkpoS(),
												     NLSB, NBB,  MFOB, '', nKv, SB_, OKPOB,
												     sNazn, '',     GetIdOper(), '', NUMBER_Null, cb_STP)
									Else If SalStrTrimX( BICA ) = SalStrTrimX( BICKB ) ! прямой КС
										Call cDoc.SetDoc(nRef2, 'FXB', 1, nVob_, Nd_,
												     DDAT_, DAT_UG, VDAT_, VDAT_,
												     sNls,NMKA, MFOA, '', nKv, SB_, GetBankOkpoS(),
												     SSB,  NBB, MFOA, '', nKv, SB_, OKPOB,
												     sNazn, '',     GetIdOper(), '', NUMBER_Null, 0)
									Else
										Set sNam1a = NMKA
										! Set sNam1b = sNMS_FXC
										If SqlPrepareAndExecute( hSql(),
												  "SELECT substr(nms,1,38) INTO :sNam1a FROM accounts WHERE kv=:nKv and nls=:sNls " ) 
												and SqlFetchNext( hSql(), nFetchRes )
										Call cDoc.SetDoc(nRef2, 'FXC', 1, nVob_, Nd_,
												     DDAT_, DAT_UG, VDAT_, VDAT_,
												     sNls,    sNam1a, MFOA, '', nKv, SB_, GetBankOkpoS(),
												     sNLS_FXC,sNMS_FXC, MFOA, '', nKv, SB_, GetBankOkpoS(),
												     sNazn, '',     GetIdOper(), '', NUMBER_Null, 0)
									If cDoc.oDoc()
										Set nRef2 = cDoc.m_nRef
										Call F_DOPR(nRef2)
										Call F_REF( nRef2 )
									Else
										Set FL=FALSE
							!
					Else !     П р о д а ж а
						Set sREPO = IifS ( nOP=-1, Str(CPP.REF), STRING_Null )
						Set sNazn = IifS( fl_END, CurrentLangTable.GetAtomTitle('CP1.cTNazn41') , 
								  CurrentLangTable.GetAtomTitle('CP1.cTNazn42') ) || " " || 
								  nameCP|| " " || sNazn
						If not SalModalDialog(dlg_MSG, hWndForm, sNazn)
							Return FALSE
						Set REF_MAIN = NUMBER_Null
						If cb_Zo AND DAT_ROZ = DAT_UG
							Set nVob_ =  96
						Else
							Set nVob_ =  6
						! Call Debug(sREPO)
						If SqlPLSQLCommand(hSql(), "CP.CP_PROD
								( nTipd, nVob_, 
								  nGrp, nID, nVidd, nRYN, DAT_UG, DAT_OPL, DAT_ROZ, DAT_KOM, SUMBN, SUMB,SUMBK, RR_,
								  sNazn, 
								  NLS9, sREPO, STRING_Null, B_4621, B_1819, B_1919, sREF, sErr, REF_MAIN )") 
							Set FL = TRUE
							If SK_ >0
								If nTipd =1 
									Set sN7 = NLS71
								Else
									If DAT_KOM > DAT_ROZ
										Set sN7 = B_1919 
									Else If DAT_KOM < DAT_ROZ
										Set sN7 =  B_1819 
									Else
										Set sN7 =  B_4621
								Set nRef0 = NUMBER_Null
								If SqlPrepareAndExecute( hSql(),
										  "SELECT gl.p_icurval(:nKv, :SK_, bankdate) INTO :SKQ_ FROM dual " )
										and  SqlFetchNext( hSql(), nFetchRes)
								If cb_Zo AND DAT_UG = DAT_KOM 
									Set nVob_ = 96
									Set VDAT_ = dDat31
									Set DDAT_ = GetBankDate()
								Else
									Set nVob_ =  6
									Set VDAT_ = DAT_KOM
									Set DDAT_ = DAT_KOM
								If nKv = n980
									Call cDoc.SetDoc(nRef0, 'FXA', 1, Vob_FXA, Nd_,
											     DDAT_, DAT_UG, DAT_KOM, DAT_KOM,
											     sN7, NMKA, MFOA , '', nKv, SK_, GetBankOkpoS(),
											     NLSB_, NBB_, MFOB_, '', nKv, SK_, OKPOB_,
											     sNaznK, '',     GetIdOper(), '', NUMBER_Null, 0)
								Else If SalStrTrimX( BICA ) = SalStrTrimX( BICKB_ ) ! прямой КС
									Call cDoc.SetDoc(nRef0, 'FXB', 1, nVob_, Nd_,
											     DDAT_, DAT_UG, VDAT_, VDAT_,
											     sN7,NMKA, MFOA, '', n980, SKQ_, GetBankOkpoS(),
											     SSB_, NBB_, MFOA, '', nKv,  SK_, OKPOB_,
											     sNaznK, '',     GetIdOper(), '', NUMBER_Null, 0)
								Else
									Call cDoc.SetDoc(nRef0, 'FXC', 1, nVob_, Nd_,
											     DDAT_, DAT_UG, VDAT_, VDAT_,
											     sN7,    NMKA, MFOA, '', IifN( nTipd =1 , n980, nKv) , SKQ_, GetBankOkpoS(),
											     sNLS_FXC,NBB_, MFOA, '', nKv, SK_, OKPOB_,
											     sNaznK, '',     GetIdOper(), '', NUMBER_Null, 0)
								If cDoc.oDoc()
									Set nRef0 = cDoc.m_nRef
									Call F_REF( nRef0 )
								Else
									Set FL = FALSE
							! в DAT_OPL ожидать поступления
							If FL and not cb_SWIFT and SSA != NUMBER_Null 
									and  cb_NO_PAY != 1
								Set nRef2 = NUMBER_Null
								! дебиторы-Кредиторы или без них
								If cb_Zo AND DAT_OPL = DAT_UG
									Set nVob_ =  96
									Set VDAT_ = dDat31
									Set DDAT_ = GetBankDate()
								Else
									Set nVob_ =  6
									Set VDAT_ = DAT_OPL
									Set DDAT_ = DAT_OPL
								Set sNls = IifS( DAT_OPL=DAT_ROZ, B_4621, IifS(DAT_OPL<DAT_ROZ, B_1919, B_1819) )
								Call cDoc.SetDoc(nRef2, 'FXD', 1, nVob_, Nd_,
										     DDAT_, DAT_UG, VDAT_, VDAT_,
										     Str(SSA), NBKA, MFOA, '', nKv, SUMB*100, GetBankOkpoS(),
										     sNls,     NMKA, MFOA, '', nKv, SUMB*100, GetBankOkpoS(),
										     sNazn, '',GetIdOper(),'', NUMBER_Null, 0)
								If cDoc.oDoc()
									Set nRef2 = cDoc.m_nRef
									If KOD_BB !=STRING_Null
										Set FL = SqlPrepareAndExecute(hSql(),
												"INSERT INTO operw (ref,tag,value) VALUES (:nRef2,'KOD_B', :KOD_BB)" )
									If FL and KOD_GB !=STRING_Null
										Set FL = SqlPrepareAndExecute(hSql(),
												"INSERT INTO operw (ref,tag,value) VALUES (:nRef2,'KOD_G', :KOD_GB)" )
									If FL and KOD_NB !=STRING_Null
										Set FL = SqlPrepareAndExecute(hSql(), "DELETE FROM operw WHERE ref=:nRef2 and tag='KOD_N' ")
												    and  SqlPrepareAndExecute(hSql(), "INSERT INTO operw (ref,tag,value) VALUES (:nRef2,'KOD_N', :KOD_NB)" )
									Call F_REF( nRef2 )
								Else
									Set FL=FALSE
					!
					If FL and sErr = STRING_Null 
							and SqlPrepareAndExecute( hSql(), "UPDATE cp_ARCH set STR_REF=:sREF where ref= :REF_MAIN " )
							and SqlPrepareAndExecute( hSql(), "UPDATE oper set nd=:Nd_ where ref= :REF_MAIN " )
						! подпись
						If not F_PODPIS(sREF)
							Return FALSE
						!
						Set bArc = TRUE ! надо поместить в архив
						Call SqlCommit(hSql())
						Call MessageNoWait( CurrentLangTable.GetAtomTitle('CP1.cTMess2') || " REF=" || sREF,'',2,0)
						! ЦП інших емітентів,  Покупка, ОБ 
						If nTipd = 1 AND rb_K AND GetBankMfo()='300465'
							Call SalModalDialog(CP_OB, hWndForm, REF_MAIN )
						Call SalSendMsg( pb_Tiket, SAM_Click, 0, 0 )
						Call SalEnableWindow( pb_Tiket )
						Call SalEnableWindow( pb_Buch )
						Call SalEnableWindow( pb_Alien )
						Call SalDisableWindow( pb_OK )
						If CP_DOPR = '1' and rb_K and rb_AKT
							Call SalShowWindow( pb_Dop_Rekv )
						Else
							Call SalHideWindow( pb_Dop_Rekv )
					Else
						Call SqlRollback(hSql())
						Call SalMessageBeep(MB_IconStop)
						Call MessageNoWait( CurrentLangTable.GetAtomTitle('CP1.cTMess3')|| sErr ,'',2,1)
					Return FL
			Function: F_PRINT_CP
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					String: FIO
					String: sTmp
					String: TIME
					String: strReport
					String: strTempl
					String: OPERAC
					Number: nRef_Print
					!
					File Handle: hFile
					Long String: sTiket
					Number: nKol
					Number: nVIDDn
					String: dDAT_UG
				Actions
					! ---------------     Печатаем тикет            -----------------
					Set FIO = GetUserRealName()
					If SqlPrepareAndExecute( hSqlAux2(),
							  "SELECT  to_char(pdat,'DD/MM/yyyy HH24:MM:SS') INTO :TIME FROM oper WHERE ref=:REF_MAIN " )
							and  SqlFetchNext(  hSqlAux2(),  nFetchRes)
					!
					If rb_K
						Set strTempl = GetTemplateDir()||'\\CP_'|| 'K.qrd'
						Call SalGetWindowText( rb_K, OPERAC, 10 )
					Else
						Set strTempl = GetTemplateDir()||'\\CP_'|| 'P.qrd'
						Call SalGetWindowText( rb_P, OPERAC, 10 )
					Set sTmp = Str(REF_MAIN)||'~'||NTIK||'~'|| SalFmtFormatDateTime(DAT_UG, 'dd/MM/yyyy') ||'~'
					Set sTmp = sTmp || TIME || '~'
					Set sTmp = sTmp || SalFmtFormatDateTime( DAT_OPL, 'dd/MM/yyyy' ) || '~'
					Set sTmp = sTmp || SalFmtFormatDateTime( DAT_ROZ, 'dd/MM/yyyy' ) || '~'
					Set sTmp = sTmp || SalFmtFormatDateTime( DAT_KOM,'dd/MM/yyyy' ) || '~'
					Set sTmp = sTmp || SalFmtFormatDateTime( DAT_EM,  'dd/MM/yyyy' ) || '~'
					Set sTmp = sTmp || SalFmtFormatDateTime( DATP,    'dd/MM/yyyy' ) || '~'
					If SqlPrepareAndExecute( hSqlAux2(),
							  "SELECT NAME INTO :TIME FROM tabval WHERE kv=:nKv" )
							and  SqlFetchNext(  hSqlAux2(),  nFetchRes)
					!
					Set sTmp = sTmp || Str(nKv)||'/'||TIME ||'~'|| CP_ID ||'~'|| Str(RATE)||'('|| sBASEY ||')~'
					If SqlPrepareAndExecute( hSqlAux2(),
							  "SELECT NAME INTO :TIME FROM cp_kod WHERE id=:nID" )
							and  SqlFetchNext(  hSqlAux2(),  nFetchRes)
					Set sTmp = sTmp || TIME    || '~' 
							                || cmb_RYN || '~'
							                || cmb_PF  || '~'
					Set sTmp = sTmp || SalNumberToStrX(SUMBN,2) || '~' 
							                || SalNumberToStrX(SUMB,2 ) || '~' 
							                || SalNumberToStrX(SUMBK,2) || '~'
					Set sTmp = sTmp || Subs(NBB, 1,38) ||'~'|| Subs(NBB_, 1,38) ||'~'||
							                   Subs(NBKA,1,32) ||'~'||
							                   Subs(NBKB,1,32) ||'~'||
							                   Subs(NMKA,1,32) ||'~'
					Set sTmp = sTmp || ' ' ||'~'|| IifS(nKv=n980,NLSB,'') ||'~'||IifS(nKv=n980,NLSB_,'')||'~'
					Set sTmp = sTmp || IifS(nKv=n980,MFOA,BICA)||'~'
					Set sTmp = sTmp || IifS(nKv=n980,MFOB,BICB)||'~'||IifS(nKv=n980,MFOB_,BICB_)||'~'
					Set sTmp = sTmp || OKPOA||'~'||OKPOB||'~'||OKPOB_||'~'|| OPERAC||'~'||FIO||'~'||
							           dTip ||'='|| Str(dKoef)
					!
					Set strReport = GetPrnDir()||'\\CP'||SalFmtFormatDateTime(SalDateCurrent() ,'hhhmmss') ||
							    '.D' || SalStrRightX('0' || Str(SalDateDay(SalDateCurrent())), 2)
					!
					Call DosReport (strTempl,  strReport,
							'nRef~NTIK~DAT_UG~TIME~DAT_OPL~DAT_ROZ~DAT_KOM~DAT_EM~DATP~nKv~CP_ID~RATE~NAME_EMIT~'||
							'RYN~cmb_PF~SUMBN~SUMB~SUMBK~NBB~NBB_~NBKA~NBKB~NBA~NLSA~NLSB~NLSB_~MFOA~MFOB~MFOB_~'||
							'OKPA~OKPB~OKPB_~OPERAC~FIO~DTIP',
							 sTmp, 0)
					If SqlPrepareAndExecute( hSqlAux2(),
							  "SELECT ref INTO :nRef_Print FROM oper WHERE ref in ("|| sREF ||")"  )
						While SqlFetchNext(  hSqlAux2(),  nFetchRes)
							Call DocsSaldoPrintK(nRef_Print, strReport)
					Call DosDirectPrint(strReport)
					! вставить в таблицу cp_ticket
					Set nKol = KOL
					Set nVIDDn = nVidd
					! вставить в архив это текстовый файл strReport
					If bArc
						If SalFileOpen( hFile, strReport, 0 )
							Call SalFileRead( hFile, sTiket, VisFileGetSize( strReport ) )
							If SqlPrepare( hSql(), "UPDATE cp_arch SET sTiket = :sTiket where ref="||Str(REF_MAIN) )
									and SqlSetLongBindDatatype(1, BLOB_BYTE)
									and SqlExecute(hSql())
								Call SalFileClose( hFile )
								Call SqlCommitEx(hSql(), CurrentLangTable.GetAtomTitle('CP1.cTMess4')||" " ||CP_ID|| " "||CurrentLangTable.GetAtomTitle('CP1.cTMess5'))
								Set bArc=FALSE
			Function: F_RYN
				Description:
				Returns
				Parameters
					Number: nTipD
					Number: nPf
				Static Variables
				Local variables
				Actions
					! Выбор субпортфеля
					Call cmb_RYN.Init(cmb_RYN)
					If nTipD = 1
						Call cmb_RYN.Populate(hSql(), "RYN","NAME","CP_RYN",
								" WHERE RYN in (select ryn from cp_accc where vidd="||Str(nVidd)||" ) 
								 and (KV is null OR KV =" ||Str(nKv) ||" ) and TIPD=1 
								ORDER BY RYN " )
						! Call cmb_RYN.Populate(hSql(), "RYN","NAME","CP_RYN",
								" WHERE RYN in 
								(select c.ryn from cp_accc c, cp_vidd v 
								 where v.tipd=1 and c.PF=v.PF and c.PF="||Str(nPf)||"
								   and c.vidd="||Str(nVidd)||" ) 
								 and (KV is null OR KV =" ||Str(nKv) ||" ) and TIPD=1 
								ORDER BY RYN " )
					Else If nTipD = 2 
						Call cmb_RYN.Populate(hSql(), "RYN","NAME","CP_RYN",
								" WHERE RYN in 
								(select c.ryn from cp_accc c, cp_vidd v  where c.PF=v.PF and v.tipd=2  ) 
								 and (KV is null OR KV =" ||Str(nKv) ||" ) and TIPD= 2
								ORDER BY RYN " )
					Call SalEnableWindow( cmb_RYN )
			Function: F_SDEL
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					Number: nTema
					Number: nSumB
					String: sTema
					Number: nRef
					String: sRef
					String: sTitl
				Actions
					If nOP = -1
						Call SalGetWindowText( rb_P, sTitl, 10 )
						Set sTitl = sTitl || " R."||Str(CPP.REF)
						Call SalSetWindowText( rb_P, sTitl )
						! Выбрать сделку покупки
						Set nTema = SalNumberAbs( CPP.OSTAF )
						Set sTema = "Ref="||Str(CPP.REF)|| ", Summa Nominal="||Str(nTema)
						If CPP.REF != NUMBER_Null and CPP.OSTAF != 0  
							If SqlPrepareAndExecute(hSql(),
									  "SELECT str_ref into :sRef from CP_ARCH where ref= :CPP.REF ") 
									and SqlFetchNext( hSql(), nFetchRes)
								Set nSumB = CPP.SUMB
								If SalModalDialog(dlg_Repo, hWndForm, sTema, nTema, nSumB, CPP.REF, CPP.ID )= 1
									! ЦБ - выбрана
									Set sTmp = Str(CPP.ID)
									Call SalSendMsg( pbCP, SAM_Click, 0, 0 )
									! Тикет
									! Call SalDisableWindow( NTIK )
									Set NTIK =  CPP.ND 
									! Сумма 
									Call SalDisableWindow( SUMBN )
									Set SUMBN = SalNumberAbs( nTema )
									Call SalSendMsg( SUMBN, SAM_Validate, 0, 0 )
									Set SUMB = SalNumberAbs( nSumB )
									Call SalSendMsg( SUMB, SAM_Validate, 0, 0 )
									! Портфель выбран
									Call cmb_PF.SetSelectById (CPP.PF)
									Set cmb_PF = CPP.VIDD || Subs( cmb_PF,5,30)
									Call SalSendMsg( cmb_PF, SAM_Click, 0, 0 )
									Call SalDisableWindow( cmb_PF )
									! Субпортфель выбран
									Call cmb_RYN.SetSelectById (CPP.RYN)
									Call SalSendMsg( cmb_RYN, SAM_Click, 0, 0 )
									Call SalDisableWindow( cmb_RYN )
									! Партнер выбран 
									If SqlPrepareAndExecute(hSql(),
											  "SELECT mfob, id_b,nlsb, nam_b  INTO :MFOB,:OKPOB,:NLSB,:NBB 
											   FROM oper  WHERE ref=
											(SELECT min(ref) from oper where ref in ("||sRef||") and ref>:CPP.REF ) ")
											and SqlFetchNext( hSql(), nFetchRes)
									Return TRUE
						!
						! Call SalMessageBox( sTema || " !", "Errr REPO -1 ! ", MB_IconStop)
						Call SalDestroyWindow(hWndForm)
						Return FALSE
					Else
						! Выбрать ЦБ
						Call SalSendMsg( pbCP, SAM_Click, 0, 0 )
			Function: F_Rb_AI
				Description:
				Returns
					Boolean: RET
				Parameters
				Static Variables
				Local variables
				Actions
					Set rb_A = TRUE
					Set rb_I = FALSE
					Call SalHideWindow( rb_A )
					Call SalHideWindow( rb_A )
					Call SalHideWindow( rb_I )
					! ! Це питання  давно при нагоді просила врахувати Лариса.Купівля ЦП.
							Закрити радіо-кнопки вибору Акціонери/Регулювання ліквідності
							при роботі з ВАЛЮТНИМИ ЦП НЕрезидентів.
							Іхнє значення аналізується тільки в випадку формування віртуального Д/П.
							Дана ситуація можлива тільки для ЦП України
					If nCOUNTRY = 804 and CPP.CP_AKC = '1' and rb_K and rb_AKT 
						! Для НЕ "торгового" портфеля - откріть
						If nPF != 4
							Call SalShowWindow( rb_A )
							Call SalShowWindow( rb_I )
			Function: CP_VALID
				Description:
				Returns
				Parameters
					Number: nId
				Static Variables
				Local variables
					String: sErr
				Actions
					If SqlPrepareAndExecute( hSql(),"SELECT substr(cp.chek_rekv( :nId ),1,200) into :sErr from dual ")
							 and SqlFetchNext( hSql(), nFetchRes )
						If sErr != STRING_Null
							Call SalMessageBox( sErr, "Робота з ЦП НЕМОЖЛИВА ! ", MB_IconStop)
							Call SalSendMsg( pbCancel1, SAM_Click, 0, 0 )
			Function: F_CP_TICKET
				Description:
				Returns
				Parameters
					Number: nREF
					Number: nKOL
					Number: nVIDDN
					String: sNTIK
					Date/Time: dDAT_UG
					Date/Time: dDAT_OPL
					Date/Time: dDAT_ROZ
					Date/Time: dDAT_KOM
					String: MFOB
					String: OKPOB
					String: NLSB
					String: NBB
					String: MFOB_
					String: OKPOB_
					String: NLSB_
					String: NBB_
				Static Variables
				Local variables
					Number: res
				Actions
					If SqlPrepareAndExecute(hSqlAux2(),
							"BEGIN
							   INSERT INTO CP_TICKET (REF, KOL, VIDDN, NTIK, DAT_UG,DAT_OPL, DAT_ROZ, DAT_KOM, MFOB, OKPOB, NLSB, NBB, MFOB_, OKPOB_,NLSB_,NBB_)
							       VALUES (  " 
								|| Str(nREF) 
								||  ","
								|| Str(nKOL) 
								||  ","
								|| Str(nVIDDN) 
								||  ",'"
								|| sNTIK 
								||  "', to_date('"|| SalFmtFormatDateTime(dDAT_UG, 'dd/MM/yyyy') || "' , 'dd/mm/yyyy')"
								||  ", to_date('"|| SalFmtFormatDateTime(dDAT_OPL, 'dd/MM/yyyy') || "' , 'dd/mm/yyyy')"
								||  ", to_date('"|| SalFmtFormatDateTime(dDAT_ROZ, 'dd/MM/yyyy') || "' , 'dd/mm/yyyy')"
								||  ", to_date('"|| SalFmtFormatDateTime(dDAT_KOM, 'dd/MM/yyyy') || "' , 'dd/mm/yyyy')"
								|| ",'"
								|| MFOB
								|| "','"
								|| OKPOB
								|| "','"
								|| NLSB
								|| "','"
								|| NBB
								|| "','"
								|| MFOB_
								|| "','"
								|| OKPOB_
								|| "','"
								|| NLSB_
								|| "','"
								|| NBB_
							|| "');
							EXCEPTION
							   WHEN DUP_VAL_ON_INDEX
							   THEN NULL;
							END;") 
						Call SqlCommitEx( hSqlAux2(), sNTIK)
		Window Parameters
			Number: nOP  ! = 1 - нова угода купілві інш.емітенту       fl_END = FALSE ,   
					                     Викуп ЦП власної емісії (по одному)   fl_END = TRUE
					             ! = 2 - Нова угода ПРОДАЖУ ЦП                 fl_END = FALSE ,  
					                     Погашення номіналу ЦП інших емітентів fl_END = TRUE
					             ! = 3 - РЕПО , fl_END = FALSE 
			String: B_4621
			String: B_1819
			String: B_1919
			Boolean: fl_END ! FALSE - продать      -купить
					                ! TRUE  - гасит эмитент-гасим мы
			Number: nGrp      ! группа доступа приоткрытии внесист счетов
			String: strPar02  ! Предустановленный фильтр
			String: sTitle   ! Название окна
		Window Variables
			Date/Time: dTmp 
			Number: REF_MAIN 
			Number: Amort
			Number: nTmp
			String: sTmp
			String: aMfo
			String: sSqlPF
			String: sTmp1
			String: sREF
			Number: nDCP
			Number: nTipd
			Number: nBasey
			Boolean: FL
			!
			Number: ACCA
			Number: ACCP
			Number: ACCR
			Number: nRYN
			Number: nPF
			Number: nDOX
			Number: nEMI
			String: nameEMI
			String: nameCP
			String: Nd_
			Number: nId
			Number: n980  
			Number: nCOUNTRY
			Number: mfo_NBU
			Number: nRef
			Number: nRef0
			Number: nRef1
			Number: nRef2
			Number: nRef3
			String: sPoisk
			String: sPoisk_
			String: sPoisk1
			String: NLS9
			String: NLS8
			String: NLS71
			: cDoc
				Class: cDoc
			!
			Number: hBtnYes
			Number: hBtnNo
			Number: hBtns[*]
			!
			Boolean: bArc
			!
			Date/Time: dDat
			Date/Time: dDat31
			String: Day_ZO
		Message Actions
			On SAM_Create
				Call SalCenterWindow(hWndForm)
				Call SetWindowFont(hWndForm)
				Call SalSetWindowText( CP1, sTitle )
				! Присваиваем значения многоязыковым объектам
				Call SalSendMsgToChildren( hWndForm, UM_QueryLabelText, 0, 0 )
				!
				Call SalHideWindow( cb_Zo )
				Set dDat = GetBankDate()
				! Если мы на Украине
				Set SUMBK = 0 
				Set aMfo = GetBankMfo()
				Set cb_NO_PAY = 0
				If GetBaseVal() = 980
					! dd(тек.дня) Меньше или Равно ДНЮ действия ЗО (из PARAMS)  Day_ZO
					If SqlPrepareAndExecute( hSql(),"SELECT trim(val) into :Day_ZO from PARAMS where upper(par)='DAY_ZO'" )
							and SqlFetchNext( hSql(), nFetchRes )
						If Val(Day_ZO) >= SalDateDay( dDat )
							! Достанем последний раб день пред мес
							If SqlPrepareAndExecute( hSql(),"SELECT max(FDAT) into :dDat31 from fdat where to_char(fdat,'yyMM')< to_char(:dDat,'yyMM')")
									 and SqlFetchNext( hSql(), nFetchRes )
								Call SalShowWindow( cb_Zo )
					Set cb_STP = 1
				!
				If fl_END
					Call SalSetWindowText( rb_P, CurrentLangTable.GetAtomTitle('CP1.cTRb') )
					Call SalSetWindowText( rb_K, CurrentLangTable.GetAtomTitle('CP1.cTRb') )
				!
				If nOP = 1
					Set rb_K = TRUE
					Set rb_P = FALSE
					Call SalDisableWindow(rb_P)
					If GetBaseVal() != 980
						Call SalHideWindow(cb_STP)
				Else If nOP = 2 OR nOP = -1
					Set rb_P = TRUE
					Set rb_K = FALSE
					Call SalDisableWindow(rb_K)
					Call SalHideWindow(cb_STP)
				!
				Set n980=GetBaseVal()
				! Поиск партнера с учетом КОДА вал или страны-эмитента
				Set sPoisk =
						"SELECT f.name, f.mfo, f.BIC, f.NLS, f.KOD_G, f.KOD_B, f.OKPO, f.BICK, f.NLSK, s.name,b.COUNTRY
						 INTO   :NBB  , :MFOB, :BICB, :NLSB, :KOD_GB, :KOD_BB, :OKPOB, :BICKB, :SSB,   :NBKB, :NGB
						 FROM CP_alien f , sw_banks s, bopcount b
						 WHERE f.kv= :nKv and UPPER(f.bick)=UPPER(s.bic (+)) and b.KODC (+)=f.kod_G and "
				Set sPoisk_=
						"SELECT f.name, f.mfo, f.BIC, f.NLS, f.KOD_G, f.KOD_B, f.OKPO, f.BICK, f.NLSK, s.name,b.COUNTRY
