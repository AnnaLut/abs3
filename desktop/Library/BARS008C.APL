						 "Потоки денежных средств, Стоимость, Амортизация по сделке № "|| NOM_UG ||', ЦБ '|| CP_ID )
				Set MANY.strPrintFileName = 'СP_MANY'
				Call SalTblSetLockedColumns( hWndForm, 7 )
				Set bDat = GetBankDate()
				If SqlPrepareAndExecute( hSql(),
						  "select D.erat, Nvl(D.erate,D.erat),  A.N/100 , d.acc, -(c.ostb+c.ostf), d.accr2 
						   INTO :IRR, :IRE, :NONIMAL, :nAcc , :nOst , :nAccR2
						   from cp_deal D , CP_ARCH A, accounts c 
						   where c.acc=d.acc and D.ref=:REF and D.erat is not null and a.ref=d.ref " )
						and SqlFetchNext( hSql(), nFetchRes) 
					Call SalTblSetColumnTitle( G6, 
							"Стоимость
							 по реал.ЭС
							"||SalNumberToStrX( IRR,6 )
							 )
					Call SalTblSetColumnTitle( G7, 
							"Стоимость
							 по этал.ЭС
							"||SalNumberToStrX( IRE,6 )
							 )
					If SqlPrepareAndExecute( hSql(),
							  "select min(fdat), max(fdat) into :DAT1, :DAT2  from cp_many where ref=:REF " )
							and SqlFetchNext( hSql(), nFetchRes) 
				Set rb1 = TRUE
				Set rb2 = FALSE
				Set rb3 = FALSE
				! Цена купона : nFixed =1 фиксированная, nFixed =0 - по % ставке по-честному
				If SqlPrepareAndExecute( hSql(),
						"select nvl(cena_kup,0), dat_em, datp, ir , nvl(basey ,0), cena
						 into :nFixed, :DAT_EM , :DATP , :IR , :nBasey, :CENA
						 from cp_kod where id=:ID ") 
						and SqlFetchNext( hSql(), nFetchRes) 
				! !! т.к. в таблице купонніх платежец всегда заполняют сумму купона ! так сказала Юля !!!!!!!!!!!
				Set nFixed = 1
				! !!!!!!!!!!!!!!
				If SqlPrepareAndExecute( hSql(),
						"select name, name_mb 
						 into :sBasey, :sBasey1 from basey where basey=:nBasey  ") 
						and SqlFetchNext( hSql(), nFetchRes) 
				If CENA > 0
					Set KOL = Abs(nOst/ ( CENA * 100))
				! Счет переоц
				If SqlPrepareAndExecute( hSql(), "select accs into :nAccS from cp_deal where ref=:REF " )
						and NOT SqlFetchNext( hSql(), nFetchRes) 
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On UM_Populate
				Call SalWaitCursor(TRUE)
				Set I1 =0
				Set I2 =0
				Set I3 =0
				Set I4 =0
				Set I5 =0
				Set I9 =0
				Set I10 =0
