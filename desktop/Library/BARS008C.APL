		Functions
			Function: F_FORWARD
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					String: sTmp
					String: sErr
					Number: nVidd
					Number: nRYN
					Number: nRR
					Number: nBB
				Actions
					Set nRR = RR_ * 100
					Set nBB = BB_ * 100
					! Call Debug ('CP_ARCH.REF ='||Str(CP_ARCH.REF ))
					Set sErr = 'NOT Nlsa, ref=' || Str(CP_ARCH.REF )
					If SqlPrepareAndExecute( hSql(),
							"SELECT c.VIDD, c.RYN  INTO :nVidd, :nRYN 
							 FROM cp_accc c, oper o, cp_ryn y 
							 WHERE o.nlsa=c.NLSA and o.ref=:CP_ARCH.REF 
							   and o.kv=nvl(y.kv,o.kv) and y.ryn=c.ryn  and c.nlsa=o.nlsa " )
							and SqlFetchNext( hSql(),  nFetchRes ) 
						! Call Debug ('nVidd ='||Str(nVidd ))
						! Call Debug ('nRYN  ='||Str(nRYN  ))
						Set sErr = STRING_Null 
						If SqlPLSQLCommand(hSql(), 
								   "CP.CP_PROD (1,6,0,CP_ARCH.ID, nVidd, nRYN, DAT_ROZ, DAT_R, DAT_R, DAT_R, 
								                CP_ARCH.N, BB_, 0, RR_, '', '', '', STRING_Null,  Str(B_4621),
								                 '', '', sTmp, sErr, CP_ARCH.REF )") 
							! Call Debug(' 3. sErr ='||sErr)
							If sErr = STRING_Null 
								! Call Debug(' 4. DAT_R ='|| SalFmtFormatDateTime( DAT_R, 'dd/MM/yyyy' ) )
								If SqlPrepareAndExecute( hSql(),
										   "UPDATE CP_ARCH set DAT_ROZ=:DAT_R, DAT_OPL=:DAT_R, r=:nRR, SUMB=:nBB 
										    where ref=:CP_ARCH.REF " )
									! Call Debug(' 5. OK ! ' )
									Call SqlCommit(hSql())
									Call MessageNoWait( CurrentLangTable.GetAtomTitle('CP1.cTMess2') || " REF=" || Str(REF),'',2,0)
									Set OP = 2
									Set R  = RR_
									Set SUMB= BB_
									Call SalDisableWindow( pb_PROD )
									Return TRUE
					!
					Call SqlRollback(hSql())
					! Call Debug(' 6. BEK ! ' )
					Call SalMessageBeep(MB_IconStop)
					Call MessageNoWait( CurrentLangTable.GetAtomTitle('CP1.cTMess3')|| sErr ,'',2,1)
					Return FALSE
					! Call Debug(' 7. BEK ! ' )
		Window Parameters
			Number: nMode !!!  =21-весь архив,  =-21 только форвард.продажи с наступ.датой
			Number: B_4621
			String: strPar01  !!   ='0' без активации форвардных сделок
			String: strPar02
		Window Variables
			Number: nCol
			Date/Time: DOK ! Дата ост сплати купону
			Date/Time: DAT_DNK ! Дата наст сплати купону
			!
			String: strReport
			File Handle: hFile
			Long String: sTiket
			Date/Time: DatB
		Message Actions
			On SAM_Create
				Call PrepareWindow( hWndForm )
				Call SetWindowFullSize(hWndForm)
				! Присваиваем значения многоязыковым объектам
				!
				Call SalTblSetLockedColumns( hWndForm, 10)
				Set CP_ARCH.nTabInstance = 1
				Call SetWindowFullSize(hWndForm)
				Set CP_ARCH.strPrintFileName =  'CP_ARCH'
				!
				Set CP_ARCH.strFilterTblName = 'CP_ARCH'
				Set CP_ARCH.fFilterAtStart = 1
				Set CP_ARCH.DatB = GetBankDate()
				!
				Set hWndForm.CP_ARCH.strSqlPopulate =
						"SELECT CP_ARCH.REF_MAIN, CP_ARCH.NOM, CP_ARCH.T/100, CP_ARCH.TQ/100,
						        CP_ARCH.REF_REPO, CP_ARCH.REF, CP_ARCH.ID, CP_ARCH.CP_ID, CP_ARCH.KV, CP_ARCH.DAT_UG, CP_ARCH.DAT_OPL, CP_ARCH.DAT_ROZ, CP_ARCH.SUMB/100, CP_ARCH.IR,
						        CP_ARCH.N/100, CP_ARCH.D/100, CP_ARCH.VD/100, CP_ARCH.P/100, CP_ARCH.VP/100, CP_ARCH.R/100, CP_ARCH.S/100, CP_ARCH.Z/100,
						        CP_ARCH.STR_REF, CP_ARCH.OP, CP_ARCH.SOS, CP_ARCH.SN/100, CP_ARCH.ND, CP_ARCH.NAME, CP_ARCH.NBB, CP_ARCH.NBS
						   INTO :CP_ARCH.REF_MAIN, :CP_ARCH.NOM,  :CP_ARCH.T, :CP_ARCH.TQ,
						        :CP_ARCH.REF_REPO, :CP_ARCH.REF,:CP_ARCH.ID, :CP_ARCH.CP_ID, :CP_ARCH.KV,
						        :CP_ARCH.DAT_UG, :CP_ARCH.DAT_OPL, :CP_ARCH.DAT_ROZ, :CP_ARCH.SUMB, :CP_ARCH.IR,
						        :CP_ARCH.N, :CP_ARCH.D, :CP_ARCH.VD, :CP_ARCH.P,:CP_ARCH.VP, :CP_ARCH.R,
						        :CP_ARCH.S, :CP_ARCH.Z, :CP_ARCH.STR_REF, :CP_ARCH.OP, :CP_ARCH.SOS,:CP_ARCH.SN,:CP_ARCH.ND, :CP_ARCH.NAME_EMI, :CP_ARCH.NBB, :CP_ARCH.NBS
						   FROM v_cp_arch CP_ARCH
						  WHERE CP_ARCH.id > 0"
				If nMode = -21
					Set hWndForm.CP_ARCH.strSqlPopulate = hWndForm.CP_ARCH.strSqlPopulate ||
							    " AND CP_ARCH.OP = -2 and CP_ARCH.DAT_ROZ <= :CP_ARCH.DatB"
				Else
					Call SalSetWindowText( hWndForm, CurrentLangTable.GetAtomTitle('CP_ARCH.cTWndTitle') )
					Call SalSendMsgToChildren( hWndForm, UM_QueryLabelText, 0, 0 )
					Call SalDisableWindow( pb_PROD )
				Set hWndForm.CP_ARCH.strSqlPopulate = hWndForm.CP_ARCH.strSqlPopulate ||
						    " ORDER BY DECODE(CP_ARCH.OP,1,CP_ARCH.REF,least(CP_ARCH.REF,NVL(CP_ARCH.ref_repo,CP_ARCH.REF))) DESC, CP_ARCH.OP DESC"
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On UM_FetchRowDone
				If OP = 1 !! 'Купівля' - голубенькие строки
					Set CP_ARCH.NAME_OP = CurrentLangTable.GetAtomTitle('CP_ARCH.cTName1')
					Call XSalTblSetCellBackColor( CP_ARCH.NAME_OP, COLOR_Sky) 
				Else If OP = 2  !! 'Продаж' - ЖЕЛТЕНЬКИЕ СТРОКИ
					Set CP_ARCH.NAME_OP = CurrentLangTable.GetAtomTitle('CP_ARCH.cTName2')
					Call XSalTblSetCellBackColor( CP_ARCH.NAME_OP, SalColorFromRGB( 255,255,180)) 
				Else If OP = 20  !! 'Погашение' - бледно ЖЕЛТЕНЬКИЕ СТРОКИ
					Set CP_ARCH.NAME_OP = 'Погашення'  !! бледно ЖЕЛТЕНЬКИЕ СТРОКИ
					Call XSalTblSetCellBackColor( CP_ARCH.NAME_OP, SalColorFromRGB( 255,255,220) ) 
				Else If OP = 22  !! 'Часткове Погашення' - бледно ЖЕЛТЕНЬКИЕ СТРОКИ
					Set CP_ARCH.NAME_OP = 'Погашення (частк)'  !! бледно ЖЕЛТЕНЬКИЕ СТРОКИ
					Call XSalTblSetCellBackColor( CP_ARCH.NAME_OP, SalColorFromRGB( 255,255,220) ) 
				Else If OP = -2  !! 'Продаж' в будущем - сильно ЖЕЛТЕНЬКИЕ СТРОКИ
					Set CP_ARCH.NAME_OP = 'FORWARD - '||CurrentLangTable.GetAtomTitle('CP_ARCH.cTName2')
					Call XSalTblSetRowBackColor( hWndForm, lParam, SalColorFromRGB( 255,255,120)  )
				Else If OP = 3  !! 'Переведення' - белые  строки
					Set CP_ARCH.NAME_OP = CurrentLangTable.GetAtomTitle('CP_ARCH.cTName3')
				Else If OP = 4 !!  'Сплата %' - зелененькие строки
					Set CP_ARCH.NAME_OP = CurrentLangTable.GetAtomTitle('CP_ARCH.cTName4')
					Call XSalTblSetCellBackColor( CP_ARCH.NAME_OP, SalColorFromRGB( 230,255,230 )) 
				Else If OP = 5 !!  'Приход гарантии на 1 пакет ЦБ' - фиолетовые строки
					Set CP_ARCH.NAME_OP = CurrentLangTable.GetAtomTitle('CP_ARCH.cTName5')
					Call XSalTblSetCellBackColor( CP_ARCH.NAME_OP, SalColorFromRGB(189,187,215)) 
				Else If OP = 6 !!  'Изменение гарантии на 1 пакет ЦБ' - фиолетовые строки
					Set CP_ARCH.NAME_OP = CurrentLangTable.GetAtomTitle('CP_ARCH.cTName6')
					Call XSalTblSetCellBackColor( CP_ARCH.NAME_OP, SalColorFromRGB(189,187,215))
				Else If OP = 7 !!  'Полное удаление гарантии на 1 пакет ЦБ' - фиолетовые строки
					Set CP_ARCH.NAME_OP = CurrentLangTable.GetAtomTitle('CP_ARCH.cTName7')
					Call XSalTblSetCellBackColor( CP_ARCH.NAME_OP, SalColorFromRGB(189,187,215))
				If CP_ARCH.SOS = 1
					Set nCol=COLOR_DarkGreen
				Else If CP_ARCH.SOS = 3
					Set nCol=COLOR_DarkBlue
				Else If CP_ARCH.SOS = 5
					Set nCol=COLOR_Black
				Else
				Call SalTblSetCellTextColor(CP_ARCH.REF, CP_ARCH.nCol, FALSE)
			On SAM_TblDoDetails
				Call SalHideWindow( DAT_R )
				Call SalHideWindow( RR_ )
				Call SalHideWindow( BB_ )
				Call SalDisableWindow( pb_PROD )
				Call SalHideWindow( pb_HELP )
				If REF > 0 and OP = -2 
					Set DAT_R = CP_ARCH.DAT_ROZ
					Set RR_ = R
					Set BB_ = SUMB
					Call SalShowWindow( DAT_R )
					Call SalShowWindow( RR_ )
					Call SalShowWindow( BB_ )
					! Call SalShowWindow( pb_HELP )
					If DAT_R <= GetBankDate()
						Call SalEnableWindow( pb_PROD )
			On SAM_DoubleClick
				If NBUBANK = 0
					Call FunNSIEditFFiltered("NBU23_REZ",1,"(ND = "|| Str(REF) || " /*and fdat = sysdate - 180*/)")
							
