		Functions
			Function: Denis
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					String: sFSql
					Number: mKy
					Number: mBasey
					Number: nSum_I
					Number: nSum_C
					Number: nSum_D
					! Date/Time: bDat
				Actions
					Set sTip = IifS( rb_D, 'Discont', 'Yield' )
					Set nKoef = dKoeff
					If nRATE =0 
						Set dSum_I = 0
						If SqlPrepareAndExecute( hSql(), 
								"SELECT cp.UFORMULA01(:dNom,:sTip,:dDatP,:dDatV,:nBasey,:dKoeff) 
								 INTO :dSum from dual" )
								and SqlFetchNext( hSql(), nFetchRes)
							Set dSum_C = dSum - dSum_I
					Else If nRATE >0 and rb_Y
						! Код количества выплат в году 
						! mKy = 1, KY=1 раз в году
						! mKy = 2, KY=2 раза в году
						! mKy = 3, KY=4 раза в году
						!
						! Код базового года по международной кодировке 
						! mBasey = 1, nBasey = 0  -- ActualActual
						! mBasey = 3, nBasey = 1  -- Actual365
						! mBasey = 4, nBasey = 2  -- Europ30_360
						! mBasey = 2, nBasey = 3  -- Actual360
						! mBasey = 0, nBasey = 12 -- UsPsa30_360
						! mBasey = 1, -- пока не знаем , считаем факт/факт
						If SqlPrepareAndExecute( hSql(),
								"SELECT Decode(KY   ,1,1, 2,2, 4,3, 1),
								        Decode(BASEY,0,1, 1,3, 2,4, 3,2, 12,0,  1) ,
								        cp.GEO_RATE_Y(:dNom, ID,:dKoeff,:dDatV,0)
								 INTO :mKy, :mBasey, :nSum_I 
								 FROM cp_kod where id = :nID " )
								and SqlFetchNext( hSql(), nFetchRes)
						! Set sSettlement = SalFmtFormatDateTime( dDatV, 'dd.MM.yyyy' ) = -- дата сделки
						! Set sMaturity   = SalFmtFormatDateTime( dDatP, 'dd.MM.yyyy' ) -- дата пог.ЦП
						! Set nCoupon= nRATE/100 -- % ставка /100
						! Set nYield =dKoeff 0.00476528 -- коефф.доходности Yield
						! Set nRedemption = dNom -- номинал
						! Пример
						! Set nPrice=Price('27.05.2010','24.11.2010',nCoupon,nYield,nRedemption,nFrequency_Annual,nDayCountBasis_ActualActual)
						!
						! чистая цена 
						Set nSum_C = 
								Price( 
								SalFmtFormatDateTime( dDatV, 'dd.MM.yyyy' ),
								SalFmtFormatDateTime( dDatP, 'dd.MM.yyyy' ),
								nRATE/100,
								dKoeff,
								dNom,
								mKy,
								mBasey )
						Set nSum_D = nSum_C + nSum_I 
						!
						Set dSum   = nSum_D
						Set dSum_C = nSum_C 
						Set dSum_I = nSum_I 
					!
					Set KURS_RCL = (dSum_C / dNom) * NOM_KURS 
					Set KURS_RGR = (dSum / dNom )  * NOM_KURS 
					Return TRUE
					!
					! If SqlPrepareAndExecute( hSql(), "SELECT " || sFSql  )
							and SqlFetchNext( hSql(), nFetchRes)
						                                                                                                                                                      Set dSum_C = dSum - dSum_I
						!
						                                                                                                                                                      Set KURS_RCL = (dSum_C / dNom) * NOM_KURS 
						                                                                                                                                                      Set KURS_RGR = (dSum / dNom )  * NOM_KURS 
						                                                                                                                                                      Return TRUE
					! Return FALSE
					!
			Function: F_Kurs
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					If SqlPrepareAndExecute( hSql(),
							"Select VDATE, BSUM, RATE_O, DY,KOEFF, idb, rate_b, rate_s
							 into :DAT_KURS, :NOM_KURS, :CEN_KURS,:DY_KURS, :KOE_KURS ,:IDB ,
							      :KURS_RCL, :KURS_RGR 
							 from CP_RATES where id=:nID and VDATE =
							 (select max(VDATE) from CP_RATES where id=:nID and VDATE<=:dTek)" )
							and SqlFetchNext( hSql(), nFetchRes)   
					If NOM_KURS = NUMBER_Null
						If SqlPrepareAndExecute( hSql(),
								"Select CENA into :NOM_KURS  from CP_KOD  where id=:nID " )
								and SqlFetchNext( hSql(), nFetchRes)   
							Set CEN_KURS = NOM_KURS
					If IDB = NUMBER_Null
						If SqlPrepareAndExecute( hSql(),
								"Select Nvl(TO_NUMBER(GetGlobalOption('CP_BYR')),3)
								 into :IDB from dual" )
								and SqlFetchNext( hSql(), nFetchRes)   
					If KURS_RCL != NUMBER_Null
						Set rb_CL = TRUE
						Set rb_GR = FALSE
					Else
						Set rb_GR = TRUE
						Set rb_CL = FALSE
			Function: F_KursR
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					String: sSql
					String: sVopr
				Actions
					If SqlPrepareAndExecute( hSql(),"select 1 from CP_RATES where id=:nID and vdate = :dTek ")
							and  SqlFetchNext( hSql(), nFetchRes)   
						Set sSql = "Update CP_RATES set " || 
								IifS(rb_GR, " RATE_s = :KURS_RGR, rate_b=null " , 
								            " RATE_b = :KURS_RCL, rate_s=null ") ||
								" where id=:nID and vdate = :dTek "
					Else
						Set sSql = "insert into
								CP_RATES( id, bsum,  vdate, rate_o, " ||
								        IifS (rb_GR, "RATE_s" , "RATE_b") || " ,idb ) 
								values ( :nID, :NOM_KURS, :dTek, :CEN_KURS, " ||
								        IifS (rb_GR, ":KURS_RGR" , ":KURS_RCL") || " ,:IDB ) "
					!
					Set sVopr = 'Установити 
							' ||  IifS (rb_GR, '"Грязний"' , '"Чистий"') ||
							' ринковий курс = 
							' ||  IifS (rb_GR, Str(KURS_RGR) , Str(KURS_RCL) ) || ' ? '
					If SalMessageBox( sVopr,'Увага ! ', MB_IconQuestion | MB_YesNo) = IDYES
						If SqlPrepareAndExecute( hSql(), sSql )   
								and  SqlCommitEx(  hSql(),"Зміна РИНКОВОГО курсу від угоди № ЦП="||Str(nID) )
							If rb_GR
								Set KURS_RCL = NUMBER_Null
							Else
								Set KURS_RGR = NUMBER_Null
							Return TRUE
						Call SqlRollback( hSql() )
					Return FALSE
		Window Parameters
			Number: nID
