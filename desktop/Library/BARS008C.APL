		Functions
			Function: F_OPL
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					Number: kR
					Number: nVob
					Number: SK_
					Number: nKup
					Number: kN
					Number: nRow 
					Number: nRef
					! String: sNazn
					String: sMfo
					String: sNls
					String: sNaznN
					String: sNaznK
					Number: REF_MAIN
					Date/Time: DAT_
					String: rNls
				Actions
					Call SalWaitCursor(TRUE)
					Set nRow = TBL_MinRow
					Set kR = 0
					Set kN = 0
					Set nVob = IifN (cb_Zo, 96, 6 )
					Set nKup = CPV.SUMRK 
					While SalTblFindNextRow( hWndForm, nRow, 0, 0 )
						Call SalTblSetContext( hWndForm, nRow )
						If Subs(CPV.SERR,1,4) = '5831'
							Set sMfo = CPV.MFO 
							Set sNls = CPV.NLS 
						Else
							Set sMfo = GetBankMfo()
							Set sNls = CPV.B_1919 
						! сплатити номінал
						If CPV.SUMNK >= CPV.NOM 
							Set nRef = NUMBER_Null
							Set sNaznN = 'Погашення номіналу ЦП '||CPV.CP_ID
							Call cDoc.SetDoc( nRef, 'FXA', 1, Vob_FXA, Str(nRef),
									     GetBankDate(), GetBankDate(), GetBankDate(), GetBankDate(),
									     CPV.B_4621, GetBankName(), GetBankMfo(), '', CPV.KV, 
									     CPV.NOM * 100, GetBankOkpoS(),
									     sNls, CPV.NB, sMfo,'', CPV.KV, CPV.NOM * 100, CPV.OKPO,
									     sNaznN, '',GetIdOper(),'',NUMBER_Null,cb_STP )
							If not cDoc.oDoc()
								Return FALSE
							!
							Set CPV.OSTA = CPV.NOM
							Set CPV.SUMNK = CPV.SUMNK - CPV.NOM
							Set kN = kN + CPV.NOM
						! сплатити купон
						If CPV.SUMRK > 0 
							Set CPV.OSTR = SalNumberMin( CPV.SUMRK, nKup * (CPV.NOM / CPV.SUMNI) )
							Set nRef = NUMBER_Null
							Set sNaznK = 'Погашення нарахованого купону ЦП '||CPV.CP_ID
							Call cDoc.SetDoc( nRef, 'FXA', 1, Vob_FXA, Str(nRef),
									     GetBankDate(), GetBankDate(), GetBankDate(), GetBankDate(),
									     CPV.B_4621, GetBankName(), GetBankMfo(), '', CPV.KV, 
									     CPV.OSTR * 100, GetBankOkpoS(),
									     sNls, CPV.NB, sMfo,'', CPV.KV, CPV.OSTR * 100, CPV.OKPO,
									     sNaznK, '',GetIdOper(),'',NUMBER_Null,cb_STP)
							If not cDoc.oDoc()
								Return FALSE
							Set kR = kR + CPV.OSTR
							Set CPV.SUMRK = CPV.SUMRK - CPV.OSTR
					Set DAT_ = GetBankDate() 
					If kR > 0
						! Set sNazn = CurrentLangTable.GetAtomTitle('CPV.cTNazn2')||' '||CPV.CP_ID
						If SqlPLSQLCommand(hSql(),
								"CP.CP_KUPON(nTipd,nVob,nGrp,nID,nRYN,nVidd,kR,SK_,B_4621,sREF,sErr,REF_MAIN)") 
								and sErr = STRING_Null
							! подпись
							If not F_PODPIS(sREF)
								Return FALSE
						Else
							Call SqlRollback(hSql())
							Call SalMessageBeep(MB_IconStop)
							Call MessageNoWait(CurrentLangTable.GetAtomTitle('CPV.cTMess1')||" "||sNaznK||" "|| CurrentLangTable.GetAtomTitle('CPV.cTMess2') || sErr ,'',2,1)
							Return FALSE
					If kN > 0
						! Set sNazn = CurrentLangTable.GetAtomTitle('CPV.cTNazn1')||' '||CPV.CP_ID
						If SqlPLSQLCommand(hSql(), "CP.CP_KUP
								(STRING_Null,nTipd,6,nGrp,nID,nVidd,nRYN,DAT_,DAT_,DAT_,DAT_,kN,kN,0,0,sNaznN,STRING_Null, 
								  rNls, B_4621,  STRING_Null,   STRING_Null,   sREF,   sErr,   REF_MAIN )") 
								and sErr = STRING_Null
							! подпись
							If not F_PODPIS(sREF)
								Return FALSE
						Else
							Call SqlRollback(hSql())
							Call SalMessageBeep(MB_IconStop)
							Call MessageNoWait(CurrentLangTable.GetAtomTitle('CPV.cTMess1')||" "||sNaznN||" "||CurrentLangTable.GetAtomTitle('CPV.cTMess2') || sErr ,'',2,1)
							Return FALSE
					! перенос CP_REE2 в Архив 
					If kR > 0 or kN > 0
						If not SqlPrepareAndExecute( hSql(),
								  "INSERT INTO cp_ree2a(OKPO,MFO,NLS,NOM,CP_ID,KUPON,ZASTAVA,REF) 
								   select OKPO,MFO,NLS,NOM,CP_ID,KUPON,ZASTAVA, :REF_MAIN 
								   from cp_ree2 where CP_ID=:CPV.CP_ID" ) 
								or not SqlPrepareAndExecute( hSql(), "delete from cp_ree2 where CP_ID=:CPV.CP_ID " )
							Call SqlRollback(hSql())
							Return FALSE
					Call SqlCommit(hSql())
					Call MessageNoWait(CurrentLangTable.GetAtomTitle('cTMessOK')||" - REF=" || sREF,'',2,0)
					Call SalWaitCursor(FALSE)
					Return TRUE
			Function: F_KONTR
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					Boolean: FL
				Actions
					Call SalWaitCursor(TRUE)
					Set FL = TRUE
					If SqlPrepareAndExecute( hSql(),
							   "SELECT nvl(sum(r.ostb),0)/100, nvl(sum(a.ostb),0)/100 
							    INTO :CPV.SUMRI, :CPV.SUMNI
							    FROM cp_deal d, accounts r, accounts a
							    WHERE d.id=:CPV.nID and d.accr=r.acc(+) and d.acc=a.acc " )
							and SqlFetchNext( hSql(), nFetchRes ) 
							and SqlPrepareAndExecute( hSql(),
							   "SELECT nvl(sum(r.nom ),0)/100, nvl(sum(r.kupon),0)/100 
							    INTO :CPV.SUMNZ, :CPV.SUMRZ
							    FROM CP_REE2 r 
							    WHERE r.CP_ID=:CPV.CP_ID " )
							and SqlFetchNext( hSql(), nFetchRes ) 
						!
						If CPV.SUMNI != CPV.SUMNZ
							Call SalWaitCursor(FALSE)
							Call SalMessageBox( CurrentLangTable.GetAtomTitle('CPV.cTMessNoNominal') ,
									  CurrentLangTable.GetAtomTitle('cTAt')||' '||
									  CurrentLangTable.GetAtomTitle('cTErr'), MB_IconStop)
							Call SalDisableWindow( pbUpdate)
							Set FL = FALSE
						!
						If CPV.SUMRI != CPV.SUMRZ
							Call SalWaitCursor(FALSE)
							Call SalMessageBox( CurrentLangTable.GetAtomTitle('CPV.cTMessNoKupon') ,
									  CurrentLangTable.GetAtomTitle('cTAt')||' '||
									  CurrentLangTable.GetAtomTitle('cTErr'), MB_IconStop)
							Set FL = FALSE
						!
					If SqlPLSQLCommand(hSql(), "CP.CP_REE2( CPV.CP_ID, sErr )") and sErr = STRING_Null
						Call SqlCommit(hSql())
					Else
						Call SqlRollback(hSql())
						Call SalMessageBeep(MB_IconStop)
						Call MessageNoWait( CurrentLangTable.GetAtomTitle('CPV.cTMessProc')||" cp.CP_REE2 !" || sErr ,'',2,1)
						Set FL = FALSE
					Return FL
