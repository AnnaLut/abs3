			! Function: F_Dox
				                                                                                                      Description: 
				                                                                                                      Returns 
				                                                                                                      Parameters 
				                                                                                                      Static Variables 
				                                                                                                      Local variables 
					                                                                                                      Number: nIrr0
					                                                                                                      Number: nIrr
					                                                                                                      Number: nNew
					                                                                                                      Number: nOld
				                                                                                                      Actions 
					                                                                                                      Set nIrr0 = IR /36500
					                                                                                                      Set nRow = TBL_MinRow
					                                                                                                      While SalTblFindNextRow(hWndForm, nRow, 0, 0)
						                                                                                                      Call SalTblFetchRow(hWndForm, nRow)
						                                                                                                      If NDD != NUMBER_Null and NDD > 0 and FDAT < DATP 
							!
							                                                                                                      Set nSum = G13*100
							! Call Debug(Str(NDD) || ' * ' || Str(nSum) )
							                                                                                                      If SqlPrepareAndExecute ( hSqlAux2(), "delete from tmp_irr where n<= :NDD" ) 
									and SqlPrepareAndExecute( hSqlAux2(), "insert into tmp_irr (n,s) values (:NDD,:nSum )" ) 
									and SqlPrepareAndExecute( hSqlAux2(), "select IRR ( :nIrr0 ) into :nIrr from dual " )
									and SqlFetchNext( hSqlAux2(), nFetchRes) 
								                                                                                                      Set DOX =  ( SalNumberPower( 1+nIrr, 365 ) -1 ) * 100
			Function: F_Dox
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					Number: nIrr0
					Number: nIrr
					Number: nNew
					Number: nOld
				Actions
					Set nIrr0 = IR /100
					Set nRow = TBL_MinRow
					While SalTblFindNextRow(hWndForm, nRow, 0, 0)
						Call SalTblFetchRow(hWndForm, nRow)
						If NDD != NUMBER_Null and NDD > 0 and FDAT < DATP 
							!
							Set nSum = G13*100
							! Call Debug(Str(NDD) || ' * ' || Str(nSum) )
							If SqlPrepareAndExecute ( hSqlAux2(), "delete from tmp_irr where n<= :NDD" ) 
									and SqlPrepareAndExecute( hSqlAux2(), "insert into tmp_irr (n,s) values (:NDD,:nSum )" ) 
									and SqlPrepareAndExecute( hSqlAux2(), "select round(xIRR ( :nIrr0 ),4) into :nIrr from dual " )
									and SqlFetchNext( hSqlAux2(), nFetchRes) 
								Set DOX = nIrr * 100
