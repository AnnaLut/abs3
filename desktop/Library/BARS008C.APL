		Functions
			Function: NASELENIE
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					If CPB.fFilterAtStart
						Set CPB.fFilterAtStart = NOT CPB.fFilterAtStart
						If NOT SetQueryFilter( CPB.cF )
							Call SalDestroyWindow( hWndForm )
					Call SalWaitCursor(TRUE)
					!
					Call SalTblDefineSplitWindow(hWndForm, 0, FALSE)
					Set CPB.strDSql = CPB.cQ.GetFullSQLString(CPB.cF)
					Call SalTblPopulate(hWndForm, hSql(), CPB.strDSql, TBL_FillAll)
					Call SalTblSetContext(hWndForm, 0)
					Call SalPostMsg(hWndForm, SAM_TblDoDetails, 0, 0)
					Call SalWaitCursor(FALSE)
		Window Parameters
			Number: nMode
			Number: nPar
			String: strPar01
			String: strPar02
		Window Variables
		Message Actions
			On SAM_Create
				Call SetWindowFullSize(hWndForm)
				! Присваиваем значения многоязыковым объектам
				Call SalSendMsgToChildren( hWndForm, UM_QueryLabelText, 0, 0 )
				Call SalSetWindowText( hWndForm, CurrentLangTable.GetAtomTitle('CPB.cTWndTitle')  )
				!
				Set DAT_ = GetBankDate()
				Set CPB.nTabInstance = 1
				Set CPB.strFilterTblName = 'CP_KOD'
				Set CPB.strPrintFileName = 'CPP'
				Set CPB.strSqlPopulate =
						"SELECT CP_KOD.ID, CP_KOD.cp_id, CP_KOD.kv, CP_KOD.datp, CP_KOD.IR, substr(a.nls,1,4), CP_KOD.CENA,
						   Sum(h.N)/100, Sum( h.sumb)/100
						 INTO :CPB.ID, :CPB.CP_ID, :CPB.KV, :CPB.DATP, :CPB.RATE, :CPB.VIDD, :CPB.CENA,
						      :CPB.N1, :CPB.B1
						 FROM cp_deal D, CP_KOD, cp_arch H, accounts a 
						 WHERE CP_KOD.id=d.id AND CP_KOD.tip=1 AND d.acc=a.acc AND d.ref=h.ref        
						 GROUP BY CP_KOD.ID, CP_KOD.cp_id, CP_KOD.kv, CP_KOD.datp, CP_KOD.IR, substr(a.nls,1,4), CP_KOD.CENA
						 ORDER BY CP_KOD.DATP "
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On SAM_FetchRowDone
				! цена приобретения
				If N1 != NUMBER_Null and N1 != 0
					Set CENP = B1*CENA / N1 
				! текущий номинал
				If SqlPrepareAndExecute( hSqlAux2(),
						  "SELECT nvl(sum(s.ostf-s.dos+s.kos),0)/100 INTO :N FROM saldoa s, cp_deal d, accounts a
						   WHERE d.id=:CPB.ID  and d.acc=s.acc and d.acc is not null and 
						         a.acc=d.acc and substr(a.nls,1,4)=:VIDD and 
						        (s.acc,s.fdat)=
						        (select acc,max(fdat) from saldoa where acc=d.acc and fdat<=:DAT_ group by acc) " )
						and SqlFetchNext ( hSqlAux2(), nFetchRes)
					! текущее количество
					If CENA != NUMBER_Null and CENA > 0
						Set KOL = -N / CENA
					! аморт.цена
					If N !=0 
						If SqlPrepareAndExecute( hSqlAux2(),
								  "SELECT nvl(sum(s.ostf-s.dos+s.kos),0)/100 INTO :D FROM saldoa s, cp_deal d, accounts a
								   WHERE d.id=:CPB.ID and d.accD=s.acc and d.accD is not null and 
								         a.acc=d.acc and substr(a.nls,1,4)=:VIDD and 
								        (s.acc,s.fdat)=
								     (select acc,max(fdat) from saldoa where acc=d.accD and fdat<=:DAT_ group by acc) " )
								and SqlFetchNext ( hSqlAux2(), nFetchRes) 
								
								and SqlPrepareAndExecute( hSqlAux2(),
								  "SELECT nvl(sum(s.ostf-s.dos+s.kos),0)/100 INTO :P FROM saldoa s, cp_deal d, accounts a 
								   WHERE d.id=:CPB.ID and d.accP=s.acc and d.accP is not null and 
								         a.acc=d.acc and substr(a.nls,1,4)=:VIDD and 
								        (s.acc,s.fdat)=
								     (select acc,max(fdat) from saldoa where acc=d.accP and fdat<=:DAT_ group by acc) " )
								and SqlFetchNext ( hSqlAux2(), nFetchRes) 
								
								and SqlPrepareAndExecute( hSqlAux2(),
								  "SELECT nvl(sum(s.ostf-s.dos+s.kos),0)/100 INTO :R FROM saldoa s, cp_deal d, accounts a
								   WHERE d.id=:CPB.ID and d.accR=s.acc and d.accR is not null and 
								         a.acc=d.acc and substr(a.nls,1,4)=:VIDD and 
								        (s.acc,s.fdat)=
								     (select acc,max(fdat) from saldoa where acc=d.accR and fdat<=:DAT_ group by acc) " )
								and SqlFetchNext ( hSqlAux2(), nFetchRes) 
								
								and SqlPrepareAndExecute( hSqlAux2(),
								  "SELECT nvl(sum(s.ostf-s.dos+s.kos),0)/100 INTO :R2 FROM saldoa s, cp_deal d, accounts a
								   WHERE d.id=:CPB.ID and d.accR2=s.acc and d.accR2 is not null and 
								         a.acc=d.acc and substr(a.nls,1,4)=:VIDD and 
								        (s.acc,s.fdat)=
								     (select acc,max(fdat) from saldoa where acc=d.accR2 and fdat<=:DAT_ group by acc) " )
								and SqlFetchNext ( hSqlAux2(), nFetchRes) 
							Set CENS = (N - D + P + R + R2)*CENA / N 
				!
			On UM_Populate
				Call NASELENIE()
