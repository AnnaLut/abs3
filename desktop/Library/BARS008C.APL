		Window Parameters
			Number: nPar
			! Number: nCol
			Number: nId
			String: strTitl
			String: strPar02
		Window Variables
			Date/Time: Dat1
			Date/Time: bDat
			Date/Time: pbDat  
			Date/Time: rDOK  
			Date/Time: dDOK
			Date/Time: dDNK
			Number: nI
			Number: kNum
			Number: DOX
			Number: nCol
			String: CP_ID
			!
			Number: nRazbit
			Number: end_NOM
			Number: nTmp
		Message Actions
			On SAM_Create
				Call PrepareWindow( hWndForm )
				Call SetWindowFullSize(hWndForm)
				Call SalTblSetLockedColumns( hWndForm, 2 )
				Set nCol = COLOR_LightGreen 
				If NBUBANK = 1 !Для НБУ задизеблить кнопки залогов
					Call SalDisableWindow(pbZalog) 
					Call SalDisableWindow(pbZalog_All)
				! Тек банк-дата
				Set bDat = GetBankDate()
				! Прошл банк-дата
				If SqlPrepareAndExecute( hSqlAux2(), "select max(fdat) into :pbDat from fdat where fdat < :bDat " )
						and SqlFetchNext( hSqlAux2(), nFetchRes ) 
				!
				Set FCK.strSqlPopulate =
						  "  SELECT cd.NPP,
						         cd.DOK,
						         cd.KUP,
						         NVL (cd.NOM, 0),
						         cd.ROWID, cd.ir,
						         case when cd.dok >= gl.bd then cd.dok + NVL (ck.expiry, 0) else null end as exp_dat
						    INTO :FCK.NPP,
						         :FCK.DOK,
						         :FCK.KUP,
						         :FCK.NOM,
						         :FCK.RI, :FCK.IR,
						         :EXP_DAT
						    FROM cp_Dat cd, cp_kod ck
						   WHERE cd.id = ck.id AND cd.id = :FCK.nId
						   ORDER BY cd.NPP"
				Set FCK.strSqlUpdate = 
						"UPDATE cp_dat SET NPP=:FCK.NPP, DOK =:FCK.DOK, KUP =:FCK.KUP, NOM =:FCK.NOM, IR = :FCK.IR 
						 where id=:FCK.nId and rowid=:FCK.RI"
				Set FCK.strSqlDelete = "delete from CP_Dat Where id=:FCK.nId and rowid=:FCK.RI "
				Set FCK.strSqlInsert = 
						"insert into CP_Dat(id,NPP,DOK,KUP, NOM,EXPIRY_DATE,IR ) values (:FCK.nId,:FCK.NPP,:FCK.DOK,:FCK.KUP,:FCK.NOM, :EXP_DAT,:FCK.IR )"
				Set FCK.strPrintFileName =  'DOK'|| Str(nId)
				!
				Call SalDisableWindow( pb_Razbit )
				Set nRazbit = 0
				If SqlPrepareAndExecute( hSql(),
						"select KY,DAT_EM,CENA_KUP,CENA,DATP,dox,dok,dnk,CP_ID,CENA_START, 
						  rnk, (select nmk from customer where rnk = cp_kod.rnk),ir
						 into :KY,:DAT_EM,:CENA_KUP,:CENA,:DATP,:DOX,:dDOK,:dDNK,:CP_ID,:CENA_START,:dRnk,:dNmk, :CPKOD_IR
						 from cp_kod 
						 where id=:FCK.nId " ) 
						and SqlFetchNext( hSql(), nFetchRes )
					Call SalSetWindowText( FCK, strTitl||' '||CP_ID )
					If KY = NUMBER_Null and dDOK != DATETIME_Null and dDNK != DATETIME_Null
						Set MES = SalNumberRound( (dDNK-dDOK)/30 )
						Set KY  = 12 / MES
					If (KY > 0 and KY <= 12 ) 
							and DAT_EM != DATETIME_Null 
							and DATP != DATETIME_Null 
						Set MES = SalNumberRound( 12 / KY )
						Set kNum = SalNumberRound ((DATP - DAT_EM) / (30*MES) )
						If MES >= 1
							! Надо график 
							Set nRazbit = 1
				Set nI = 1
				Call SalDisableWindow( pb_Nom )
				Call SalSendClassMessage(UM_Create, 0, 0)
			On SAM_FetchRowDone
				If FCK.DOK <= pbDat
					Call XSalTblSetRowBackColor( hWndForm, lParam, COLOR_LightGray ) ! серый
				Else
					If nI = 1 
						Call XSalTblSetRowBackColor( hWndForm, lParam, nCol )
						Call F_DISA()
						Set nI = 2
				If DATP = FCK.DOK
					If SqlPrepareAndExecute( hSqlAux2(),
							"select k.cena - sum(nvl(d.nom,0) )  into :end_NOM
							 from cp_dat d, cp_kod k 
							 where k.id = :FCK.nId and d.id= k.id and d.dok >= :bDat and d.dok < k.datp 
							 group by k.cena " )
							and SqlFetchNext( hSqlAux2(), nFetchRes )
						Set FCK.NOM = end_NOM
				!
				If SqlPrepareAndExecute( hSqlAux2(),
						"select max(dok) into :DOK1 from cp_dat where id=:FCK.nId and dok<:FCK.DOK " )
						and SqlFetchNext( hSqlAux2(), nFetchRes )
				If DOK1 = DATETIME_Null
					Set DOK1 = DAT_EM
				Set DNEY = DOK - DOK1
				Set Kol = Kol + 1
				Set KOLD = KOLD + DNEY 
			On UM_Populate
				!
				Call SalDisableWindow( pb_Nom )
				Set Kol = 0 
				Set nI = 1
				Set KOLD = 0
				!
				Call SalSendClassMessage(UM_Populate, 0, 0)
				!
				If nRazbit = 1
					If Kol = 0 
						Call SalEnableWindow( pb_Razbit )
					Else
						Call SalDisableWindow( pb_Razbit )
			! On SAM_TblDoDetails
				                                                                  Call F_DISA()
