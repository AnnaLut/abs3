							    WHERE k.id=:CPV.nID and v.ryn in (select ryn from cp_deal where id=:CPV.nID) " )
							and SqlFetchNext( hSql(), nFetchRes ) 
						Set CPV.strSqlPopulate =
								"SELECT r.OKPO, b.nb, r.mfo, r.nls, r.nom/100, r.kupon/100, to_char(NVL(r.ZASTAVA,58310)) 
