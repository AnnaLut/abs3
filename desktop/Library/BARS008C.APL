									and SqlPrepareAndExecute( hSql(),
									" insert into cp_rates(idb,id,vdate,bsum,rate_o)
									   with s 
									   as
									  (
									  select 
									  i.byr, 
									  i.id, 
									  i.fdat,  
									  k.cena, 
									  i.stoim,
									  sum(i.stoim ) over (partition by i.id order by i.fdat rows 4 PRECEDING) stoim5, 
									  i.nomin,
									  sum(i.nomin)  over (partition by i.id order by i.fdat rows 4 PRECEDING) nomin5
									  from CP_INFO i, cp_kod k 
									  where  i.nomin>0 and i.id=k.id
									    and i.fdat in 
									     (select fdat 
									      from fdat 
									      where to_char(fdat,'mmyyyy') = to_char(:DAT,'mmyyyy')
									        and fdat <= :DAT ) 
									  order by 2),
									 d 
									 as
									 (
									 select id,max(fdat) fdat
									 from cp_info
									 where fdat in 
									     (select fdat 
									      from fdat 
									      where to_char(fdat,'mmyyyy') = to_char(:DAT,'mmyyyy')
									        and fdat <= :DAT )
									   and nomin >0
									 group by id
									 ) 
									 select s.byr, s.id, :DAT, s.cena,round(s.stoim5/s.nomin5,2) 
									 from s, d
									 where s.fdat = d.fdat
									     and s.id = d.id  " )
