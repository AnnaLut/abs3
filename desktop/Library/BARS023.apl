								Set sTmp=SalStrReplaceX(sTmp,SalStrScan(sTmp,','),1,'.')
							Set nSum   = SalStrToNumber(SalStrTrimX(sTmp))   !--*100000
							Set strComm   = SalStrTrimX(SalStrMidX( strBuffer, 144, 512 ))
						Else
							Set strKkd  = ''
						If strKkd and nIterator>5 and (strFond='1' or strFond='2')
