									        INTO :colKkd,:colGr,:colS,:colS01,:colS02,:colS03,:colS04,:colS05,:colS06,:colS07,:colS08,:colS09,:colS10,:colS11,:colS12,:colKkdN,:colRowId
									        FROM plan_MB p,budj b 
