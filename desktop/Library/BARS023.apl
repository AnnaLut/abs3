									                  WHEN '7' THEN    SIGN(INSTR(','||NVL(b.OP_TER,'00')||',','"||','||sObl||','||"'||p.id_key||','))
									                  ELSE 0 END) > 0 "||
									            IifS( cb1 & cb2,' and p.gr IN (1,2) ',IifS(cb1,' and p.gr=1','')||
									            IifS( cb2,' and p.gr=2',''))||   
