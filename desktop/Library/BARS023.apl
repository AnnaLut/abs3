						Set nRow=SalTblInsertRow (  hWndForm, TBL_MinRow )
						Call SalTblSetContext ( hWndForm, nRow)
						Call SalTblSetRowFlags( hWndForm, nRow, ROW_New, FALSE )
						Set tbl2.colS=nS
