.head 6 -  Set nInd=COLOR_DarkBlue
.head 5 +  Else If Lim_No < 0
.head 6 -  Set nInd=COLOR_Red
.head 5 +  Else
.head 6 -  Set nInd=COLOR_Black
.head 5 -  Call SalTblSetCellTextColor( Lim_No, nInd, FALSE )
