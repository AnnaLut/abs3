						  from cc_deal where prod is not null and sos>=10")
						and NOT SqlFetchNext(hSql(),nFetchRes)
					Set hWndForm.F1.DAT = GetBankDate()
				! Set hWndForm.F1.strFilterTblName = 'TMP_REP'
				If SqlPrepareAndExecute( hSql(),
