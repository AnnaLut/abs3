							 FROM TMP_VAR t, ACCOUNTS, tabval v, " || VP_LIM || " l 
							 WHERE ACCOUNTS.nls=t.nls and ACCOUNTS.kv=t.kv and t.kv=v.kv 
							   and ACCOUNTS.acc=l.acc(+) 
