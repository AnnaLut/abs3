					String: strPar01
					String: strPar02
				Static Variables
				Local variables
				Actions
					! надо вставить инициализацию apd
					Select Case nMode
						Case 0
							If not IsWindow(hWin0)
								Set hWin0 = SalCreateWindow(DOP, hWndMDI, nMode, nPar, strPar01, SalStrTrimX(strPar02))
							Else
								Call SalBringWindowToTop(hWin0)
							Break
						Case 1
							If not IsWindow(hWin1)
								Set hWin0 = SalCreateWindow(tblSetLimit, hWndMDI, nPar, strPar01, strPar02)
							Else
								Call SalBringWindowToTop(hWin1)
							Break
						Default
							Break
