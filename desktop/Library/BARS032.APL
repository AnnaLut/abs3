		Message Actions
			On SAM_Create
				Call SetWindowFullSize(hWndForm)
				Call PrepareWindowEx(hWndForm)
				Call SalTblSetTableFlags(hWndForm, TBL_Flag_SingleSelection, TRUE)
				Call SalDisableWindow(pbUpdate)
				Call SalDisableWindow(pbDetails)
				Set hWndForm.tblSetLimit.nTabInstance = 1
				Set hWndForm.tblSetLimit.strPrintFileName = 'dpk.txt'
				Set hWndForm.tblSetLimit.strSqlPopulate = 
						"SELECT d.LIMIT_ID, d.OPERDATE, d.DEPOSIT_ID, d.ACCOUNT, d.CURRENCY, d.NMK, 
						        d.OSTC/power(10,t.dig), d.CURRENT_LIM/power(10,t.dig), d.SUMA/power(10,t.dig), d.BLOCK, 
						        d.VIDD, d.TYPE_NAME, d.DAT_BEGIN, d.DAT_END, d.rnk, d.acc, t.dig
						 INTO :hWndForm.tblSetLimit.colLimitId, :hWndForm.tblSetLimit.colOperDate,
						      :hWndForm.tblSetLimit.colDepositId, :hWndForm.tblSetLimit.colNls,
						      :hWndForm.tblSetLimit.colKv, :hWndForm.tblSetLimit.colNmk,
						      :hWndForm.tblSetLimit.colOstC, :hWndForm.tblSetLimit.colCurrentLim,
						      :hWndForm.tblSetLimit.colSumLim, :hWndForm.tblSetLimit.colBlock,
						      :hWndForm.tblSetLimit.colVidd, :hWndForm.tblSetLimit.colViddName, 
						      :hWndForm.tblSetLimit.colDatBegin, :hWndForm.tblSetLimit.colDatEnd, 
						      :hWndForm.tblSetLimit.colRnk, :hWndForm.tblSetLimit.colAcc, :hWndForm.tblSetLimit.colDig
						 FROM V_DPT_LIMIT_QUERY d, tabval t
						 WHERE d.currency=t.kv
						 ORDER BY d.LIMIT_ID"
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On UM_Populate
				Call SalSendClassMessage(UM_Populate, 0, 0)
				Call VisTblAutoSizeColumn(hWndForm, hWndNULL)
			On UM_Insert
				If colLimitId
					If NOT SqlPLSQLCommand(hSql(), "priocom_abs.proc_limit_on_deposit(colLimitId)")
						Call SqlRollbackEx(hSql(), "VKLAD. Ошибка выполнения sql-процедуры priocom_abs.proc_limit_on_deposit для запроса " || Str(colLimitId))
					Else
						If SqlCommitEx(hSql(), "VKLAD. Выполнена процедура Установки/Снятия лимита для запроса " || Str(colLimitId))
							Call SalMessageBox("Успешно установили/сняли лимит", "Информация", MB_Ok | MB_IconAsterisk)
							Call OperWithAccountEx(0, colAcc, colRnk, ACCESS_READONLY, hWndForm, '01')
					Call SalPostMsg(hWndForm, UM_Populate, 0, 0)
			On UM_Delete
				If colLimitId
					If NOT SqlPLSQLCommand(hSql(), "priocom_abs.remove_dpt_limit_query(colLimitId)")
						Call SqlRollbackEx(hSql(), "VKLAD. Ошибка выполнения sql-процедуры priocom_abs.remove_dpt_limit_query для запроса " || Str(colLimitId))
					Else
						If SqlCommitEx(hSql(), "VKLAD. Выполнена процедура Изъятия из картотеки запроса " || Str(colLimitId))
							Call SalMessageBox("Успешно изъяли запрос из картотеки", "Информация", MB_Ok | MB_IconAsterisk)
					Call SalPostMsg(hWndForm, UM_Populate, 0, 0)
