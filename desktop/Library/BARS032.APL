		Functions
			Function: F_BROW 
				Description:
				Returns
					String:
				Parameters
					String: BROWSER
				Static Variables
				Local variables
					String: sTmp
					!
					Number: nType
					Number: nError
					Number: nPos
					Number: nTmp
					Date/Time: dTmp
					Window Handle: hNextDF
				Actions
					Set nType = SalCompileAndEvaluate( BROWSER,
							    nError, nPos, nTmp, sTmp, dTmp, hNextDF, TRUE, SalContextCurrent())
					If nError = 0
						If nType = EVAL_Number
							Set sTmp = SalNumberToStrX(nTmp, 0)
						Else If nType = EVAL_String
						Else If nType = EVAL_Date
							Call SalDateToStr ( dTmp, sTmp )
					Else
						Call SalMessageBox( 'Невозможно вызвать '||BROWSER, 'Информация', MB_IconInformation )
					Return sTmp
		Window Parameters
			Number: nMode
			Number: nPar
			String: sFiltr
			String: TAG
		Window Variables
			String: name_TAG
			String: sFiltr1
			String: BROWSER
		Message Actions
			On SAM_Create
				Call SalWaitCursor( TRUE )
				Call PrepareWindow( hWndForm )
				Call SetWindowFullSize(hWndForm)
				Set DAT1 = GetBankDate()
				Set DAT2 = GetBankDate()
				If SqlPrepareAndExecute( hSql(),
						"SELECT name,BROWSER into :name_TAG, :BROWSER from OP_FIELD where tag=:TAG" )
						and not SqlFetchNext(hSql(), nFetchRes)
					Call SalMessageBox("Не описан доп.реквизит "|| TAG ,
							"Ошибка в ком.строке візова єтой функции ! ", MB_IconStop)
					Call SalDestroyWindow(hWndForm)
				If BROWSER = STRING_Null
					Call SalDisableWindow( pb_F12 )
				Call SalMapEnterToTab( TRUE )
				Call SalSetWindowText( hWndForm, 
						'Редактирование доп реквизита "'||TAG ||'" = " '||  name_TAG )
				Call SalTblSetLockedColumns( hWndForm, 1 )
				!
				Set hWndForm.DOP.fFilterAtStart = 1
				Set hWndForm.DOP.strPrintFileName = TAG
				Set hWndForm.DOP.strFilterTblName = 'OPER'
				Set hWndForm.DOP.nTabInstance = 1
				!
				! Set hWndForm.DOP.strSqlDelete =
						   "DELETE from OPERW where ref=:hWndForm.DOP.REF and tag=:hWndForm.DOP.TAG "
				Set hWndForm.DOP.strSqlUpdate =
						   "UPDATE OPERW set VALUE=:hWndForm.DOP.VALUE
						    where ref=:hWndForm.DOP.REF and tag=:hWndForm.DOP.TAG "
				Set hWndForm.DOP.strSqlInsert =
						   "INSERT into OPERW (ref,tag,value) values 
						(:hWndForm.DOP.REF,:hWndForm.DOP.TAG,:hWndForm.DOP.VALUE) "
				Call SalSendClassMessage(SAM_Create, 0, 0)
			On UM_Populate
				Call SalWaitCursor(TRUE)
				If hWndForm.DOP.fFilterAtStart
					Set hWndForm.DOP.fFilterAtStart = NOT hWndForm.DOP.fFilterAtStart
					If NOT SetQueryFilter( cF )
						Call SalDestroyWindow( hWndForm )
				!
				Set sFiltr1 = ""
				Set sFiltr1 = cF.GetFilterWhereClause( FALSE )
				!
				If pb_Summ
					Call SalHideWindow( REF )
					Call SalHideWindow( TT )
					Call SalHideWindow( VDAT )
					Call SalHideWindow( DK )
					Call SalHideWindow( MFOA )
					Call SalHideWindow( MFOB )
					Call SalHideWindow( NLSA )
					Call SalHideWindow( NLSB )
					Call SalHideWindow( NAZN )
					!
					Call SalDisableWindow( pbDetails )
					Call SalDisableWindow( pbUpdate )
					Call SalDisableWindow( pb_F12 )
					!
					Call SalDisableWindow( VALUE )
					!
					Set hWndForm.DOP.strSqlPopulate =
							"SELECT w.VALUE, oper.KV, Sum(oper.S) /100
							 INTO :hWndForm.DOP.VALUE,  :hWndForm.DOP.KV, :hWndForm.DOP.S  FROM oper, operw w
							 WHERE oper.sos=5 and oper.ref=w.ref and w.tag =:hWndForm.DOP.TAG 
							   and EXISTS (SELECT 1 FROM OPLDOK 
							               WHERE REF=oper.REF AND FDAT BETWEEN :hWndForm.DOP.DAT1 AND :hWndForm.DOP.DAT2 )
							   and " || sFiltr || 
							 IifS (sFiltr1 = "", "", " AND "||sFiltr1 ) || " 
							 GROUP BY w.VALUE, oper.KV Order by w.VALUE, oper.KV "
					!
				Else
					Call SalShowWindow( REF )
					Call SalShowWindow( TT )
					Call SalShowWindow( VDAT )
					Call SalShowWindow( DK )
					Call SalShowWindow( MFOA )
					Call SalShowWindow( MFOB )
					Call SalShowWindow( NLSA )
					Call SalShowWindow( NLSB )
					Call SalShowWindow( NAZN )
					!
					Call SalEnableWindow( pbDetails )
					Call SalEnableWindow( pbUpdate )
					Call SalEnableWindow( pb_F12 )
					!
					Call SalEnableWindow( VALUE )
					!
					Set hWndForm.DOP.strSqlPopulate =
							"SELECT w.TAG, w.VALUE, oper.REF, oper.TT, oper.VDAT, 
							        oper.KV, oper.DK, oper.S/100, oper.NLSA, 
							       oper.MFOA, oper.NLSB, oper.MFOB,oper.NAZN 
							 INTO :hWndForm.DOP.YN,
							      :hWndForm.DOP.VALUE, :hWndForm.DOP.REF, :hWndForm.DOP.TT, :hWndForm.DOP.VDAT,  
							      :hWndForm.DOP.KV, :hWndForm.DOP.DK, :hWndForm.DOP.S, :hWndForm.DOP.NLSA,
							      :hWndForm.DOP.MFOA, :hWndForm.DOP.NLSB, :hWndForm.DOP.MFOB, :hWndForm.DOP.NAZN 
							 FROM oper, operw w
							 WHERE oper.sos=5 and oper.ref=w.ref (+) and w.tag (+) =:hWndForm.DOP.TAG 
							   and EXISTS (SELECT 1 FROM OPLDOK 
							               WHERE REF=oper.REF AND FDAT BETWEEN :hWndForm.DOP.DAT1 AND :hWndForm.DOP.DAT2 )
							   and " || sFiltr || IifS (sFiltr1 = "", "", " AND "||sFiltr1 ) || " Order by oper.REF"
				! Call Debug(hWndForm.DOP.strSqlPopulate) 
				Call SalTblPopulate(hWndForm, hSql(), hWndForm.DOP.strSqlPopulate, TBL_FillAll)
				Call SalTblSetContext(hWndForm, 0)
				Call SalPostMsg(hWndForm, SAM_TblDoDetails, 0, 0)
				Call SalWaitCursor(FALSE)
			On SAM_DoubleClick
				Call DocViewContentsEx(hWndMDI, REF)
