.head 4 +  Actions
.head 5 -  ! -- 2-х ур
.head 5 +  If nParam = 1 ! Построение налогового учета КБ 
.head 6 -  ! Call FunNSIEdit("NAL_DEC8")
.head 6 -  ! Call SalCreateWindow(TB_NAL_DEC, hWndMDI)
.head 6 -  Call SalCreateWindow(TB0, hWndMDI)
.head 5 +  Else If nParam = 2 ! Состояние нал.декл ЦЕНТРА 
.head 6 -  Call SalCreateWindow(TB_N, hWndMDI, nParam)
.head 5 +  Else If nParam = 12 ! Состояние нал.декл по исп (только для ОБ) за любую дату
.head 6 -  Call SalCreateWindow(TB_I, hWndMDI, nParam)
.head 5 +  Else If nParam = 4 ! Открыть сводные счета НУ в гл НБУ 
.head 6 -  Call SalCreateWindow(Frm_4, hWndMDI)
.head 5 +  Else If nParam = 6 ! Просмотр в гл НБУ связных таблиц 
.head 6 -  Call SalCreateWindow(TB6, hWndMDI)
.head 5 -  ! -- 3-х ур
.head 5 +  Else If nParam = 31 !! Открытие счетов подразделения 
.head 6 -  Call SalCreateWindow(TB31, hWndMDI, nParam)
.head 5 +  Else If nParam = 3 or nParam=33 ! Построение налогового учета обл НБУ SBU.dbf 
.head 6 -  Call SalCreateWindow(TB3, hWndMDI, nParam)
.head 5 +  Else If nParam = 32 !! Сверка с балансом ФУ (сальдо документов за интервал по датам валютирования)
.head 6 -  Set DAT2p = GetBankDate()
.head 6 -  Set DAT1p = SalDateQuarterBegin(DAT2p)
.head 6 -  Call SalCreateWindow(FU, hWndMDI, nParam,'',DAT1p,DAT2p)
.head 5 +  Else If nParam = 33 !! Сверка с балансом ФУ (сальдо документов за интервал по датам документа)
.head 6 -  Set DAT2p = GetBankDate()
.head 6 -  Set DAT1p = SalDateQuarterBegin(DAT2p)
.head 6 -  Call SalCreateWindow(FU, hWndMDI, nParam,'',DAT1p,DAT2p)
.head 5 +  Else If nParam = 5 ! Состояние нал.декл ОБЛАСТИ НБУ 
.head 6 -  Call SalCreateWindow(TB_N, hWndMDI, nParam)
.head 5 -  ! другое не налоговое
.head 5 +  ! Else If nParam = 44 ! REZAK=TAIL
.head 6 -  Return SalModalDialog(REZAK, hWndMDI)
