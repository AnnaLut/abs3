.head 3 +  On SAM_Create
.head 4 -  Set TB_I.strPrintFileName = 'NAL_DECI'
.head 4 -  Set DAZ=GetBankDate()
.head 4 -  Set DAT2p = GetBankDate()
.head 4 -  Set DAT1p = SalDateQuarterBegin(DAT2p)
.head 4 -  Set TB_I.strFilterTblName = 'NAL_DEC'
.head 4 -  Call SalTblSetLockedColumns(hWndForm, 5)
.head 4 -  Call PrepareWindow(hWndForm)
.head 4 -  Call SetWindowFullSize(hWndForm)
.head 4 -  Set ss1 = "
    SELECT nal_dec.dd, nal_dec.pp, nal_dec.rr, nal_dec.nms,
           b.nls, b.acc, b.ost/100, b.isp
    INTO :TB_I.N1,:TB_I.N2,:TB_I.N3,:TB_I.NAM,:TB_I.NLS,
         :TB_I.ACC,:TB_I.S, :B_I.ISP
    FROM nal_dec, accounts a, sal b
    WHERE nal_dec.nls = a.nls and a.acc=b.accc and b.fdat = :TB_I.DAZ  and
          substr(b.nls,1,1)='8' 
    ORDER BY b.isp, dd, ORD"
