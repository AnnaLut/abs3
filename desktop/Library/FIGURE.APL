				Functions
				Window Variables
					Number: nI
				Message Actions
					On SAM_Create
						! Call SalSetWindowText( hWndForm, 'МОДЕЛИРОВАНИЕ ОПЕРАЦИЙ')
						! Call SalDisableWindow( pbExit)
						Set strSqlPopulate = "
								  SELECT kvd, kvk, deb, krd, s, s1, rowid
								  INTO :tbl_IGRA.KVD,:tbl_IGRA.KVK,:tbl_IGRA.DEB,
								       :tbl_IGRA.KRD,:tbl_IGRA.S,:tbl_IGRA.S1,:tbl_IGRA.RI
								  FROM igra "
						Set strSqlInsert = "
								  INSERT INTO igra (deb, krd, s, s1, kvd, kvk)
								  VALUES (:tbl_IGRA.DEB,:tbl_IGRA.KRD,:tbl_IGRA.S,
								          :tbl_IGRA.S1,:tbl_IGRA.KVD,:tbl_IGRA.KVK)"
						Set strSqlDelete = "
								  DELETE FROM igra WHERE rowid=:tbl_IGRA.RI"
						Set strSqlUpdate = "
								  UPDATE igra
								  SET kvd=:tbl_IGRA.KVD, kvk=:tbl_IGRA.KVK, deb=:tbl_IGRA.DEB,
								      krd=:tbl_IGRA.KRD, s=:tbl_IGRA.S, s1=:tbl_IGRA.S1
								  WHERE rowid=:tbl_IGRA.RI"
						!
						Call SalWaitCursor(TRUE)
						Call XConnectGetParams(cMain)
						If NOT cMain.Connect()
							Call SalWaitCursor(FALSE)
							Call SalEndDialog(hWndForm, 0)
						Call XSalTooltipSetUserDefault()
						! If strTitleAux != ''
							     Call SalGetWindowText(hWndForm, strOldTitle, 256)
							     Set strOldTitle = strOldTitle ||  ': [' || strTitleAux || ']'
							     Call SalSetWindowText( hWndForm, strOldTitle )
							     Call SaveInfoToLog(MSG_Invoke() || strOldTitle)
						If strFilterTblName != ''
							Call cF.Init(strFilterTblName, strFilterTblName)
						Else
							Call SalDisableWindow(pbFilter)
						If strSqlPopulate != ''
							Call cQ.Init(strSqlPopulate)
						If nFlags & GT_NoIns
							Call SalDisableWindow(pbIns)
						If nFlags & GT_NoDel
							Call SalDisableWindow(pbDel)
						If nFlags & GT_NoUpd
							Call SalDisableWindow(pbUpdate)
						Call SalTblSetTableFlags( tbl_IGRA, TBL_Flag_SelectableCols, TRUE )
					! On SAM_Close
						           Call SalSendMsg( pbExit, SAM_Click, 0,0 )
					On SAM_CreateComplete
						Call SalSendMsg(tbl_IGRA, UM_Populate, 0, 0)
						Call SalWaitCursor(FALSE)
					On UM_Insert
						If not SalTblAnyRows(tbl_IGRA, ROW_New, 0)
							Call SalTblDefineSplitWindow(tbl_IGRA, 3,
									     TBL_Split_Adjustable)
						Set nRow = SalTblInsertRow(tbl_IGRA, TBL_MinRow)
						Call SalTblSetContext(tbl_IGRA, nRow)
						Call SalTblSetFocusCell(tbl_IGRA, nRow,
								     GetFirstEditableColumn(tbl_IGRA), 0, 1)
					On UM_Delete
						Set nRow = TBL_MinRow
						While SalTblFindNextRow(tbl_IGRA, nRow, ROW_Selected, 0)
							If NOT SalTblQueryRowFlags(tbl_IGRA, nRow, ROW_New)
								Call SalTblSetRowFlags(tbl_IGRA, nRow, ROW_MarkDeleted,
										     not SalTblQueryRowFlags(tbl_IGRA, nRow,
										     ROW_MarkDeleted))
							Else
								Call SalTblDeleteRow(tbl_IGRA, nRow, TBL_NoAdjust)
					On UM_Populate
						If strSqlPopulate != ''
							! If fFilterAtStart
								     Set fFilterAtStart = NOT fFilterAtStart
								     If NOT SetQueryFilter( cF )
									     Call SalDestroyWindow( hWndForm )
							Call SalWaitCursor(TRUE)
							Call SalTblDefineSplitWindow(tbl_IGRA, 0, FALSE)
							Set strDSql = cQ.GetFullSQLString(cF)
							Call SalTblPopulate(tbl_IGRA, cMain.hSql(), T(strDSql), TBL_FillAll)
							Call SalTblSetContext(tbl_IGRA, 0)
							Call SalPostMsg(tbl_IGRA, SAM_TblDoDetails, 0, 0)
							Call SalWaitCursor(FALSE)
					On UM_Update
						Call SalWaitCursor(TRUE)
						Call SaveInfoToLog(MSG_SaveDataRef() || ' ' || strTitleAux)
						If strSqlDelete != ''
							Call SqlPrepare(cMain.hSql(), T(strSqlDelete))
							Call SalTblDoDeletes(tbl_IGRA, cMain.hSql(),
									     ROW_MarkDeleted)
						If strSqlInsert != ''
							Call SqlPrepare(cMain.hSql(), T(strSqlInsert))
							Call SalTblDoInserts(tbl_IGRA, cMain.hSql(), TRUE)
						If strSqlUpdate != ''
							Call SqlPrepare(cMain.hSql(), T(strSqlUpdate))
							Call SalTblDoUpdates(tbl_IGRA, cMain.hSql(), TRUE)
						Call SqlCommit(cMain.hSql())
						Call SalPostMsg(tbl_IGRA, UM_Populate, 0, 0)
						Call SalWaitCursor(FALSE)
					On UM_Close
						If SalTblAnyRows(tbl_IGRA,  ROW_Edited | ROW_MarkDeleted | ROW_New, 0)
							Call SalMessageBeep(MB_IconQuestion)
							If SalMessageBox('В таблице присутсвуют несохраненные ' ||
									   'изменения. Игнорировать их?', 'Внимание!',
									   MB_IconQuestion | MB_YesNo) != IDYES
								Return FALSE
						! Call SalDestroyWindow(hWndForm)
					On UM_Print
						If strPrintFileName
							Call SalGetWindowText(hWndForm, strOldTitle, 99)
							Call TablePrint(hWndForm, strOldTitle, GetPrnDir() || '\\'
									     || strPrintFileName, '')
					On UM_Search
						Call TblFindString(tbl_IGRA,  DFIND_First)
					On SAM_Destroy
						If cMain.IsConnected()
							Call cMain.Disconnect()
						Call SaveInfoToLog('Выход из диалога: ' || strOldTitle)
					On SAM_ContextMenu
						Call SalTblQueryFocus( tbl_IGRA, nRow, hWndCol )
						If SalTblQueryRowFlags ( tbl_IGRA, nRow, ROW_Selected )
							Call Fun_SumCol()
						Else
							Call SalTrackPopupMenu(hWndForm, 'popupEditTbl', TPM_CursorX | TPM_CursorY | TPM_CenterAlign, 0, 0)
					On SAM_EndCellTab
						If not (nFlags & GT_NoIns)
							Call SalSendMsg(tbl_IGRA, UM_Insert, 0, 0)
						Return TRUE
					On SAM_ColumnSelectClick
						Call SalTblSetColumnFlags( SalNumberToWindowHandle(wParam), COL_Selected, FALSE )
						Set bSortOrd = not bSortOrd
						Call SalTblSortRows(tbl_IGRA, SalTblQueryColumnID(SalNumberToWindowHandle(wParam)), bSortOrd)
		Functions
			Function: GetFirstEditableColumn
				Description: Получить первую редактируюмую колонку
				Returns
					Window Handle:
				Parameters
					Window Handle: hWndTbl
				Static Variables
				Local variables
					Number: nCol
					Window Handle: hWndCol
				Actions
					Set nCol=1
					Loop
						Set hWndCol = SalTblGetColumnWindow(hWndTbl, nCol,
								    COL_GetPos)
						If hWndCol = hWndNULL
							Break
						If SalTblQueryColumnFlags(hWndCol, COL_Editable) and
								   SalIsWindowVisible(hWndCol)
							Break
						Set nCol = nCol + 1
					Return hWndCol
			Function: Fun_SumCol
				Description: Сумирует SELECTED колонку
				Returns
				Parameters
				Static Variables
				Local variables
					Window Handle: hWndCol
					String: strTitle
					String: strSuma
					Number: nX
					Number: nY
					Number: nW
					Number: nH
				Actions
					Call GetWindowRect( hWndForm, nX, nY, nW, nH )
					Call SalTblObjectsFromPoint( hWndForm, wParam-nX, lParam-nY, nH, hWndCol, nH )
					Set strTitle=PadR(' ',100  )
					Call SalTblGetColumnTitle ( hWndCol, strTitle, 100 )
					If SalIsValidNumber( hWndCol )
						Set nH=SalTblColumnSum( hWndForm, SalTblQueryColumnID( hWndCol ), ROW_Selected, 0 )
						If nH
							Set strTitle=SalStrTrimX(strTitle)
							Set strSuma=SalStrTrimX(SalFmtFormatNumber ( nH, 'Default' ))
							Set nH = SalNumberMax( SalStrLength( strTitle ), SalStrLength( strSuma ) )
							Call XSalTooltipSetColors( COLOR_Black, COLOR_Yellow )
							Call XSalTooltipShow( hWndForm, strTitle ||PutLf()||PadL(strSuma,nH))
							Call XSalTooltipSetColors( COLOR_Black, COLOR_White )
