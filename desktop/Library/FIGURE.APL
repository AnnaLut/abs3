				Window Variables
					Number: i
					Number: nCol
					String: sd
				Message Actions
					On SAM_Create
						! Call cCstTab.Init( hWndForm, nTabInstance, GetUserLoginName(), NOT fNoReposition)
						Call SalWaitCursor(TRUE)
						Call SalTblSetLockedColumns(hWndForm, 4)
						Set nCol = COLOR_LightGreen
						If SqlPrepareAndExecute(hSqlAux(),
								"SELECT max(fdat),ek.ek_NULL INTO :MDAT,:i FROM ek_pok_day where fdat<:bDat ")
							Set sd = IifS( SqlFetchNext(hSqlAux(), nFetchRes),
									  SalFmtFormatDateTime( MDAT, 'dd/MM/yyyy' ), STRING_Null )
							Call SalTblSetColumnTitle( F2, "Факт за
									"|| sd||"
									2(Ф)")
							Set ss = "
									 SELECT nvl(s.SORT_ORDER,e.SORT_ORDER), e.PR_MOD, 
									        e.pok, e.mpok, e.name, e.fpok, e.gran, d.s/100 
									 INTO :SORT_ORDER,:PR_MOD, :POK, :MPOK, :NAME, :FPOK, :G1, :F2
									 FROM ek_pok e, staff_ek s, ek_pok_day d 
									 WHERE e.pok=s.pok AND s.id=" ||Str(nID)||" AND e.pok=d.pok (+) AND d.fdat (+)=:MDAT
									 ORDER BY 1 "
							Call SalTblPopulate(tbl1, hSql(), ss, TBL_FillAll)
							Call SalEnableWindow(WORD)
						Call SalWaitCursor(FALSE)
					On SAM_User
						Call SalWaitCursor(TRUE)
						If SqlPrepareAndExecute(hSqlAux(),"SELECT ek.ek_NULL INTO :i FROM dual")
								and SqlFetchNext(hSqlAux(), nFetchRes)
						Set i = TBL_MinRow
						While SalTblFindNextRow(hWndForm, i,  0, 0)
							Call SalTblSetContext(hWndForm, i)
							If SqlPrepareAndExecute(hSql(),"SELECT "||FPOK||"(NULL,0)/100 INTO :T3 FROM dual")
									and SqlFetchNext(hSql(), nFetchRes) 
								If PR_MOD = 1
									If SqlPrepareAndExecute(hSql(),
											"SELECT to_number(pul.Get_Mas_Ini_Val('EK"||Str(POK)||"') )/100 INTO :M4 FROM dual")
											and SqlFetchNext(hSql(), nFetchRes) 
									Call F_Col()
						Call SalWaitCursor(FALSE)
						!
					On SAM_FetchRowDone
						If G1 != NUMBER_Null and F2 != NUMBER_Null
							Set DG_F = G1 - F2
