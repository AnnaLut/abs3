				Window Variables
				Message Actions
					On SAM_Create
						Set strSql = "
								SELECT e.pok, e.mpok, e.name, e.fpok, e.gran 
								INTO  :colPok, :colMPok, :colName, :colFormula, :colGran
								FROM ek_pok e,staff_ek s 
								WHERE e.pok=s.pok and s.id=:nID 
								ORDER BY nvl(s.SORT_ORDER,e.SORT_ORDER)"
						Call SalTblPopulate ( hWndItem, hSql (), strSql, TBL_FillAll )
						Call SalTblSetRowFlags ( hWndItem, 0, ROW_Selected, TRUE )
