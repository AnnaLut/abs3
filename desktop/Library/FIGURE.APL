					On SAM_Create
						Call SalSetWindowText( hWndForm, 'МОДЕЛИРОВАНИЕ КУРСОВ')
						Set strSqlPopulate = "
								  SELECT k.kv, c.rate_o, k.RATE_o, k.vdate, k.bsum
								  INTO :tbl_IGRAK.KV,:tbl_IGRAK.RATE_OF,:tbl_IGRAK.RATE_O,
								       :tbl_IGRAK.VDATE,:tbl_IGRAK.BSUM
								  FROM IGRAK k, cur_rates c
								  WHERE k.kv=c.kv and c.vdate=(SELECT MAX(vdate) FROM cur_rates WHERE kv=k.kv) "
						Set strSqlInsert = "
								  INSERT INTO igraK (kv, RATE_o,vdate,bsum) VALUES (
								  :tbl_IGRAK.KV,:tbl_IGRAK.RATE_O,:tbl_IGRAK.VDATE,:tbl_IGRAK.BSUM) "
						Set strSqlDelete = "
								  DELETE FROM igraK
								  WHERE kv=:tbl_IGRAK.KV and vdate=:tbl_IGRAK.VDATE"
						Set strSqlUpdate = "
								  UPDATE igrak SET
								  RATE_o=:tbl_IGRAK.RATE_O,
								  bsum=:tbl_IGRAK.BSUM,
								  vdate=:tbl_IGRAK.VDATE
								  WHERE kv=:tbl_IGRAK.KV"
						!
						Call SalWaitCursor(TRUE)
						Call XConnectGetParams(cMain)
						If NOT cMain.Connect()
							Call SalWaitCursor(FALSE)
							Call SalEndDialog(hWndForm, 0)
						Call XSalTooltipSetUserDefault()
						! If strTitleAux != ''
							     Call SalGetWindowText(hWndForm, strOldTitle, 256)
							     Set strOldTitle = strOldTitle ||  ': [' || strTitleAux || ']'
							     Call SalSetWindowText( hWndForm, strOldTitle )
							     Call SaveInfoToLog(MSG_Invoke() || strOldTitle)
						If strFilterTblName != ''
							Call cF.Init(strFilterTblName, strFilterTblName)
						Else
							Call SalDisableWindow(pbFilter)
						If strSqlPopulate != ''
							Call cQ.Init(strSqlPopulate)
						If nFlags & GT_NoIns
							Call SalDisableWindow(pbIns)
						If nFlags & GT_NoDel
							Call SalDisableWindow(pbDel)
						If nFlags & GT_NoUpd
							Call SalDisableWindow(pbUpdate)
						Call SalTblSetTableFlags( tbl_IGRAK, TBL_Flag_SelectableCols, TRUE )
					On SAM_CreateComplete
						Call SalSendMsg(tbl_IGRAK, UM_Populate, 0, 0)
						Call SalWaitCursor(FALSE)
					On UM_Insert
						If not SalTblAnyRows(tbl_IGRAK, ROW_New, 0)
							Call SalTblDefineSplitWindow(tbl_IGRAK, 3,
									     TBL_Split_Adjustable)
						Set nRow = SalTblInsertRow(tbl_IGRAK, TBL_MinRow)
						Call SalTblSetContext(tbl_IGRAK, nRow)
						Call SalTblSetFocusCell(tbl_IGRAK, nRow,
								     GetFirstEditableColumn(tbl_IGRAK), 0, 1)
					On UM_Delete
						Set nRow = TBL_MinRow
						While SalTblFindNextRow(tbl_IGRAK, nRow, ROW_Selected, 0)
							If NOT SalTblQueryRowFlags(tbl_IGRAK, nRow, ROW_New)
								Call SalTblSetRowFlags(tbl_IGRAK, nRow, ROW_MarkDeleted,
										     not SalTblQueryRowFlags(tbl_IGRAK, nRow,
										     ROW_MarkDeleted))
							Else
								Call SalTblDeleteRow(tbl_IGRAK, nRow, TBL_NoAdjust)
					On UM_Populate
						If strSqlPopulate != ''
							! If fFilterAtStart
								     Set fFilterAtStart = NOT fFilterAtStart
								     If NOT SetQueryFilter( cF )
									     Call SalDestroyWindow( hWndForm )
							Call SalWaitCursor(TRUE)
							Call SalTblDefineSplitWindow(tbl_IGRAK, 0, FALSE)
							Set strDSql = cQ.GetFullSQLString(cF)
							Call SalTblPopulate(tbl_IGRAK, cMain.hSql(), T(strDSql), TBL_FillAll)
							Call SalTblSetContext(tbl_IGRAK, 0)
							Call SalPostMsg(tbl_IGRAK, SAM_TblDoDetails, 0, 0)
							Call SalWaitCursor(FALSE)
					On UM_Update
						Call SalWaitCursor(TRUE)
						Call SaveInfoToLog(MSG_SaveDataRef() || ' ' || strTitleAux)
						If strSqlDelete != ''
							Call SqlPrepare(cMain.hSql(), T(strSqlDelete))
							Call SalTblDoDeletes(tbl_IGRAK, cMain.hSql(),
									     ROW_MarkDeleted)
						If strSqlInsert != ''
							Call SqlPrepare(cMain.hSql(), T(strSqlInsert))
							Call SalTblDoInserts(tbl_IGRAK, cMain.hSql(), TRUE)
						If strSqlUpdate != ''
							Call SqlPrepare(cMain.hSql(), T(strSqlUpdate))
							Call SalTblDoUpdates(tbl_IGRAK, cMain.hSql(), TRUE)
						Call SqlCommit(cMain.hSql())
						Call SalPostMsg(tbl_IGRAK, UM_Populate, 0, 0)
						Call SalWaitCursor(FALSE)
					On UM_Close
						If SalTblAnyRows(tbl_IGRAK,  ROW_Edited | ROW_MarkDeleted | ROW_New, 0)
							Call SalMessageBeep(MB_IconQuestion)
							If SalMessageBox('В таблице присутсвуют несохраненные ' ||
									   'изменения. Игнорировать их?', 'Внимание!',
									   MB_IconQuestion | MB_YesNo) != IDYES
								Return FALSE
						! Call SalDestroyWindow(hWndForm)
					On UM_Print
						If strPrintFileName
							Call SalGetWindowText(hWndForm, strOldTitle, 99)
							Call TablePrint(hWndForm, strOldTitle, GetPrnDir() || '\\'
									     || strPrintFileName, '')
					On UM_Search
						Call TblFindString(tbl_IGRAK,  DFIND_First)
					On SAM_Destroy
						If cMain.IsConnected()
							Call cMain.Disconnect()
						Call SaveInfoToLog('Выход из диалога: ' || strOldTitle)
					On SAM_ContextMenu
						Call SalTblQueryFocus( tbl_IGRAK, nRow, hWndCol )
						If SalTblQueryRowFlags ( tbl_IGRAK, nRow, ROW_Selected )
							Call Fun_SumCol()
						Else
							Call SalTrackPopupMenu(hWndForm, 'popupEditTbl', TPM_CursorX | TPM_CursorY | TPM_CenterAlign, 0, 0)
					On SAM_EndCellTab
						If not (nFlags & GT_NoIns)
							Call SalSendMsg(tbl_IGRAK, UM_Insert, 0, 0)
						Return TRUE
					On SAM_ColumnSelectClick
						Call SalTblSetColumnFlags( SalNumberToWindowHandle(wParam), COL_Selected, FALSE )
						Set bSortOrd = not bSortOrd
						Call SalTblSortRows(tbl_IGRAK, SalTblQueryColumnID(SalNumberToWindowHandle(wParam)), bSortOrd)
		Functions
			Function: GetFirstEditableColumn
				Description: Получить первую редактируюмую колонку
				Returns
					Window Handle:
				Parameters
					Window Handle: hWndTbl
				Static Variables
				Local variables
					Number: nCol
					Window Handle: hWndCol
				Actions
					Set nCol=1
					Loop
						Set hWndCol = SalTblGetColumnWindow(hWndTbl, nCol,
								    COL_GetPos)
						If hWndCol = hWndNULL
							Break
						If SalTblQueryColumnFlags(hWndCol, COL_Editable) and
								   SalIsWindowVisible(hWndCol)
							Break
						Set nCol = nCol + 1
					Return hWndCol
			Function: Fun_SumCol
				Description: Сумирует SELECTED колонку
				Returns
				Parameters
				Static Variables
				Local variables
					Window Handle: hWndCol
					String: strTitle
					String: strSuma
					Number: nX
					Number: nY
					Number: nW
					Number: nH
				Actions
					Call GetWindowRect( hWndForm, nX, nY, nW, nH )
					Call SalTblObjectsFromPoint( hWndForm, wParam-nX, lParam-nY, nH, hWndCol, nH )
					Set strTitle=PadR(' ',100  )
					Call SalTblGetColumnTitle ( hWndCol, strTitle, 100 )
					If SalIsValidNumber( hWndCol )
						Set nH=SalTblColumnSum( hWndForm, SalTblQueryColumnID( hWndCol ), ROW_Selected, 0 )
						If nH
							Set strTitle=SalStrTrimX(strTitle)
							Set strSuma=SalStrTrimX(SalFmtFormatNumber ( nH, 'Default' ))
							Set nH = SalNumberMax( SalStrLength( strTitle ), SalStrLength( strSuma ) )
							Call XSalTooltipSetColors( COLOR_Black, COLOR_Yellow )
							Call XSalTooltipShow( hWndForm, strTitle ||PutLf()||PadL(strSuma,nH))
							Call XSalTooltipSetColors( COLOR_Black, COLOR_White )
