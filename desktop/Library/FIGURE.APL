								"SELECT c.rate_o,c.bsum
								INTO :IGRAK.RATE_OF,:IGRAK.BSUM
								FROM cur_rates c
								WHERE c.kv=:KV and c.vdate=(SELECT MAX(vdate) FROM cur_rates WHERE kv=:KV)" )
						     Call SqlFetchNext( hSqlAux2(), nI)
						     Set VDATE=GetBankDate()
						     Set RATE_O=RATE_OF
