			     On SAM_Create
				     Call SalSetWindowText( hWndForm, 'МОДЕЛИРОВАНИЕ КУРСОВ')
				     Set IGRAK.strSqlPopulate = "
						SELECT k.kv, c.rate_o, k.RATE_o, k.vdate, k.bsum 
						INTO :IGRAK.KV,:IGRAK.RATE_OF,:IGRAK.RATE_O,
						     :IGRAK.VDATE,:IGRAK.BSUM 
						FROM IGRAK k, cur_rates c 
						WHERE k.kv=c.kv and c.vdate=(SELECT MAX(vdate) FROM cur_rates WHERE kv=k.kv) "
				     Set IGRAK.strSqlInsert = "
						INSERT INTO igraK (kv, RATE_o,vdate,bsum) VALUES (
						:IGRAK.KV,:IGRAK.RATE_O,:IGRAK.VDATE,:IGRAK.BSUM) "
				     Set IGRAK.strSqlDelete = "
						DELETE FROM igraK
						WHERE kv=:IGRAK.KV and vdate=:IGRAK.VDATE"
				     Set IGRAK.strSqlUpdate = "
						UPDATE igrak SET
						  RATE_o=:IGRAK.RATE_O,
						  bsum=:IGRAK.BSUM,
						  vdate=:IGRAK.VDATE
						WHERE kv=:IGRAK.KV"
				     Call SalSendClassMessage(SAM_Create,0,0)
			! On SAM_Close
				           Call SalSendMsg( pbExit, SAM_Click, 0,0 )
