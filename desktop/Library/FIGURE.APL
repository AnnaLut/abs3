		Functions
		Window Parameters
			String: pok
			String: tt
			Number: nn
			Window Handle: hT
		Window Variables
			String: a0
			String: a1
			String: a2
			String: a3
			String: a4
			String: a5
			String: DD
			String: ss
			String: ii
			String: sj
			String: mfo[5]
			Number: i
			Number: j
			Number: nWidth
			Number: nHeight
			Window Handle: hC
		Message Actions
			On SAM_Create
				Call SalWaitCursor ( TRUE )
				Call SalSetWindowText( hWndForm, 'Сравнение показателя разных банков' )
				!
				Set ss=' '
				Set ii=' '
				Set i = TBL_MinRow
				Set j=0
				While SalTblFindNextRow( hT, i, 0, 0 )
					Set j=j+1
					Set sj=SalNumberToStrX(j,0) 
					Call SalTblFetchRow( hT, i )
					!
					Set hC= SalTblGetColumnWindow( tblTable, j+1, COL_GetID )
					Call SalTblSetColumnTitle(  hC, 'Банк '||sj||'
							'||hT.tbl2.MFO )
					!
					Set ss=ss||',
							 pok_mfo(fdat ,'||pok||','||hT.tbl2.MFO||')'
					Set ii=ii||' ,
							 :tblTable.colValues'||sj 
					Set mfo[j-1] = hT.tbl2.MFO
				!
				Set DD=" 
						SELECT distinct  fdat, to_char(fdat,'dd-mm-yyyy') "||ss||"
						INTO :tblTable.FDat, :tblTable.colFDat "||ii||" 
						FROM ek_pok_MFO
						ORDER BY fdat "
				Call SalTblPopulate ( tblTable, hSql (), DD, TBL_FillAll )
				! Call SalTblPopulate ( tblTable, hSql (), DD, TBL_FillNormal )
				! график
				Set ccGraf.Graph.DataSource = "tblTable"
				Set ccGraf.Graph.LabelField = "colFDat"
				Set ccGraf.Graph.GraphTitle = tt
				!
				Call SalTblGetColumnText( tblTable, 2, a1 )
				Call SalTblGetColumnText( tblTable, 3, a2 )
				Call SalTblGetColumnText( tblTable, 4, a3 )
				Call SalTblGetColumnText( tblTable, 5, a4 )
				Call SalTblGetColumnText( tblTable, 6, a5 )
				Call SalTblGetColumnText( tblTable, 7, a0 )
				Set ccGraf.Graph.LegendText[0] = "База"
				Set ccGraf.Graph.LegendText[1] = "Банк1 " || mfo[0]
				Set ccGraf.Graph.LegendText[2] = "Банк2 " || mfo[1]
				Set ccGraf.Graph.LegendText[3] = "Банк3 " || mfo[2]
				Set ccGraf.Graph.LegendText[4] = "Банк4 " || mfo[3]
				Set ccGraf.Graph.LegendText[5] = "Банк5 " || mfo[4]
				!
				Set ccGraf.Graph.DataField[0] = "colEtalon"
				Set ccGraf.Graph.DataField[1] = "colValues1"
				Set ccGraf.Graph.DataField[2] = "colValues2"
				Set ccGraf.Graph.DataField[3] = "colValues3"
				Set ccGraf.Graph.DataField[4] = "colValues4"
				Set ccGraf.Graph.DataField[5] = "colValues5"
				Set ccGraf.Graph.NumDataFields = j+1
				Set ccGraf.Graph.Background[1] = 7
				Set ccGraf.Graph.Background[6] = 7
				Call ccGraf.Draw()
				Call SalWaitCursor ( FALSE )
			! On WM_Size
				           Call SalGetWindowSize ( hWndForm, nWidth, nHeight )
				!
				           Call SalSetWindowSize ( tblTable, nWidth, nHeight / 3 )
				           Call SalSetWindowLoc ( tblTable, 0, 2 * nHeight / 3 )
				           Call SalSetWindowSize ( ccGraf, nWidth, 2 * nHeight / 3 )
				           Call SalSetWindowLoc ( ccGraf, 0, 0 )
	!
