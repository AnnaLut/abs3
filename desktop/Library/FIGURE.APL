					Function: uf_MinMax_
						Description:
						Returns
						Parameters
						Static Variables
						Local variables
						Actions
							If not isDone
								Set nMin = tblDay.colVal
								Set nMax = tblDay.colVal
								Set isDone = TRUE
								If not SalIsNull ( dlgDay.tblDay.colEtalon )
									If dlgDay.tblDay.colEtalon < nMin
										Set nMin = dlgDay.tblDay.colEtalon
									If dlgDay.tblDay.colEtalon > nMax
										Set nMax = dlgDay.tblDay.colEtalon
							Else
								If tblDay.colVal < nMin
									Set nMin = tblDay.colVal
								If tblDay.colVal > nMax
									Set nMax = tblDay.colVal
								! If not SalIsNull ( dlgDay.tblDay.colEtalon )
									           If dlgDay.tblDay.colEtalon < nMin
										           Set nMin = dlgDay.tblDay.colEtalon
									           If dlgDay.tblDay.colEtalon > nMax
										           Set nMax = dlgDay.tblDay.colEtalon
					Function: uf_GetValues_
						Description:
						Returns
						Parameters
						Static Variables
						Local variables
							String: strSql
							Date/Time: dFrom
							Date/Time: dTo
						Actions
							Set strSql =
									'SELECT to_char (d.fdat, \'dd/mm/yyyy\'),d.s/100,e.gran 
									 INTO :colFdat, :colVal, :colEtalon
									 FROM ek_pok_day d, ek_pok e '
							Set strSql =strSql||'
									WHERE d.pok = :dlg_Ind.tblInd.colPok and 
									      e.pok = :dlg_Ind.tblInd.colPok AND 
									      d.fdat>= :dFrom AND 
									      d.fdat<= :dTo
									ORDER BY d.fdat '
							If dlgDay.dfFrom AND dlgDay.dfTo
								Set dFrom=SalFmtFormatStrDateTime( dlgDay.dfFrom, 'dd/MM/yyyy' )
								Set dTo=SalFmtFormatStrDateTime( dlgDay.dfTo, 'dd/MM/yyyy' )
								Call SalTblPopulate ( hWndItem, hSql (), strSql, TBL_FillAll )
