		Functions
			Function: uf_GetValues
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					String: strEtalon
					String: strHostVar
				Actions
					If SalIsNull ( dlg_Ind.tblInd.colGran )
						Set strEtalon = ' '
						Set strHostVar = ' '
					Else
						Set strEtalon = ' , '||SalNumberToStrX ( dlg_Ind.tblInd.colGran, 0 )
						Set strHostVar = ' , :hWndForm.dlg_Table.tblTable.colEtalon '
					Set strSql = 'select to_char (fdat, \'dd/mm/yyyy\'), '||
							                dlg_Ind.tblInd.colFormula || '( fdat ) / 100 ' || strEtalon ||
							                ' into :hWndForm.dlg_Table.tblTable.colFDat, :hWndForm.dlg_Table.tblTable.colValues ' || strHostVar || '  from fdat'
					Call SalTblPopulate ( tblTable, hSql (), strSql, TBL_FillAll )
			Function: uf_Redraw
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					! Set ccGraf.Graph.DataSource = "tblTable"
					! Set ccGraf.Graph.DataField[0] = "colValues"
					! Set ccGraf.Graph.DataField[1] = "colEtalon"
					Set ccGraf.Graph.GraphType = 6
					Set ccGraf.Graph.YAxisStyle[0] = 2
					Set ccGraf.Graph.YAxisTicks[0] = 4
					Set ccGraf.Graph.YAxisMin[0] = nMin
					Set ccGraf.Graph.YAxisMax[0] = nMax
					! Set ccGraf.Graph.LegendField = "NN"
					! Set ccGraf.Graph.NumDataFields = 2
					! Set ccGraf.Graph.LegendText[0] = dlg_Ind.tblInd.colMPok
					! Set ccGraf.Graph.LegendText[1] = "Эталон"
					Set ccGraf.Graph.GraphTitle = dlg_Ind.tblInd.colMPok || '   ' || dlg_Ind.tblInd.colName
					Set ccGraf.Graph.LegendPos = 2
					Set ccGraf.Graph.Background[0] = 7
					Set ccGraf.Graph.Background[1] = 7
					Set ccGraf.Graph.Background[6] = 7
					Set ccGraf.Graph.DrawMode = 2
					Call ccGraf.Draw()
			Function: uf_MinMax
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					If not isDone
						Set nMin = tblTable.colValues
						Set nMax = tblTable.colValues
						Set isDone = TRUE
						If not SalIsNull ( dlg_Table.tblTable.colEtalon )
							If dlg_Table.tblTable.colEtalon < nMin
								Set nMin = dlg_Table.tblTable.colEtalon
							If dlg_Table.tblTable.colEtalon > nMax
								Set nMax = dlg_Table.tblTable.colEtalon
					Else
						If tblTable.colValues < nMin
							Set nMin = tblTable.colValues
						If tblTable.colValues > nMax
							Set nMax = tblTable.colValues
						If not SalIsNull ( dlg_Table.tblTable.colEtalon )
							If dlg_Table.tblTable.colEtalon < nMin
								Set nMin = dlg_Table.tblTable.colEtalon
							If dlg_Table.tblTable.colEtalon > nMax
								Set nMax = dlg_Table.tblTable.colEtalon
		Window Parameters
		Window Variables
			String: strSql
			Number: nWidth
			Number: nHeight
			Number: nMax
			Number: nMin
			Boolean: isDone
		Message Actions
			On WM_Size
				Call SalGetWindowSize ( hWndForm, nWidth, nHeight )
				Call SalSetWindowSize ( tblTable, nWidth / 3, nHeight )
				Call SalSetWindowLoc ( ccGraf, nWidth/3, 0 )
				Call SalSetWindowSize ( ccGraf, nWidth/3 * 2, nHeight )
			On PAGEM_Activate
				If not isRetrieve
					Set isDone = FALSE
					! Call SalTblSetColumnTitle ( hWndForm.dlg_Table.tblTable.colValues, dlg_Ind.tblInd.colMPok )
					Call SalWaitCursor ( TRUE )
					Call uf_GetValues ()
					Call SalWaitCursor ( FALSE )
					Set isRetrieve = TRUE
					Call uf_Redraw ()
