							UNION ALL
							SELECT  /*+ INDEX(o XIE_FDAT_ACC_SOS_OPLDOK) INDEX(o2 XAK_REF_OPLDOK) INDEX(x) INDEX(a) INDEX(a2) INDEX(s) INDEX(k1) INDEX(k2) */
							        k2.id_key,
							        DECODE(x.tt,o.tt,x.mfoa,"||GetBankMfo()||"),
