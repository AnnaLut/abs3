						     COLOR_Gray, TRUE)
				Call cc1.SpecialDate(SPECIAL_Weekly, DATETIME_Null, DAY_Saturday, SEQ_Unset, COLOR_Black,
						     COLOR_Gray, TRUE)
				Call SqlPrepareAndExecute(hSql(), T("
						     SELECT fdat
						     INTO :dSelect
						     FROM fdat"))
				Set nI = 0
