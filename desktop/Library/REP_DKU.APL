						Set sBigPayersMask = GetGlobalOption('KTKMASK')
								                   
						Call SalTblPopulate(tbl1, hSql(), "
								        SELECT substr(ktk, -6, 2), d.id, d.name, 
								               id_key||' - '||k.name, ktk, id_key, k.sab
								        INTO  :Kod1,  :colDPI,   :Name1, :ktk_name, :colKtk, :colid_key, :colSab
