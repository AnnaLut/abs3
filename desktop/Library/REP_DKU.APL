						If not SalIsWindowEnabled(pbPrint)
							Call SalEnableWindow(pbPrint)
					On SAM_SetFocus
						Call SalSendMsg(tblReps, SAM_TblDoDetails , 0, 0)
					On SAM_DoubleClick
						Call Make_Rep()
