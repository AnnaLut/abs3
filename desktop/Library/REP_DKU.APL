								        WHERE t.kod_dpi=d.id AND k.idt = t.ktk
								                          AND substr(ktk, -6,6)= substr(k.idt, -6,2)||'0000'         
								        ORDER BY substr(ktk, length(ktk)-5,2), d.id", TBL_FillAll)
