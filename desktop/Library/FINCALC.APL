.head 5 +  On SAM_Click
.head 6 -  ! -------------- Очистка полей результатов
.head 6 -  Set dfPV2 = NUMBER_Null
.head 6 -  Set dfFV2 = NUMBER_Null
.head 6 -  Set dfCF2 = NUMBER_Null
.head 6 -  Set dfR2 = NUMBER_Null
.head 6 -  Set dfN2 = NUMBER_Null
.head 6 -  ! --------------------- Проверка задания входных значений
.head 6 +  If dfM1 = 0 OR SalIsNull(dfM1)
.head 7 -  Set dfM1 = 1
.head 6 -  ! ------------------------------------ Вычисляем F V по PV, CF, r, n
.head 6 +  If 
            NOT SalIsNull (dfCF1)
AND  NOT SalIsNull (dfPV1)
AND  NOT SalIsNull (dfR1)
AND  NOT SalIsNull (dfN1)
AND             SalIsNull (dfFV1)
.head 7 -  Set dfFV2 =  FV ( dfR1/dfM1, dfN1*dfM1, dfCF1, dfPV1)
.head 6 -  ! ------------------------------------ Вычисляем PV по FV, CF, r, n
.head 6 +  Else If 
                       SalIsNull (dfPV1)
AND  NOT SalIsNull (dfFV1)
AND   NOT SalIsNull (dfR1)
AND   NOT SalIsNull (dfN1)
AND   NOT SalIsNull (dfCF1)
.head 7 -  Set dfPV2 =  PV ( dfR1/dfM1, dfN1*dfM1, dfCF1, dfFV1)
.head 6 -  ! ------------------------------------ Вычисляем n по  FV, PV, CF, r
.head 6 +  Else If 
             NOT SalIsNull (dfFV1)
AND   NOT SalIsNull (dfR1)
AND	  SalIsNull (dfN1)
AND   NOT SalIsNull (dfCF1)
.head 7 -  Set dfN2 = NPER ( dfR1/dfM1, dfCF1, dfPV1, dfFV1)/dfM1
.head 6 -  ! ------------------------------------ Вычисляем r по  FV, PV, n
.head 6 +  Else If 
             NOT SalIsNull (dfPV1)
AND              SalIsNull (dfR1)
AND   NOT  SalIsNull (dfN1)
AND   NOT SalIsNull (dfFV1)
.head 7 -  Set dfR2 = RATE ( dfN1*dfM1, dfCF1, dfPV1, dfFV1)*dfM1
.head 6 -  ! ------------------------------------ Вычисляем CF по  FV, PV, n,r
.head 6 +  Else If 
             NOT SalIsNull (dfPV1)
AND   NOT SalIsNull (dfR1)
AND   NOT  SalIsNull (dfN1)
AND   NOT SalIsNull (dfFV1)
AND              SalIsNull (dfCF1)
.head 7 -  ! Set dfCF2 = PMT ( dfR1, dfN1*dfM1, dfPV1, dfFV1)/dfM1
.head 7 -  Set dfCF2 = PMT ( dfR1/dfM1, dfN1*dfM1, dfPV1, dfFV1)
.head 6 +  Else
.head 7 -  Call SalMessageBox( 'Некорректные исходные данные!', 'Ошибка',
       MB_Ok | MB_IconExclamation)
