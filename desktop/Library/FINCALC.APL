.head 4 +  Message Actions
.head 5 +  On SAM_Click
.head 6 -  ! --------------------- Проверка задания входных значений
.head 6 +  If dfMc = 0 OR SalIsNull(dfMc)
.head 7 -  Set dfMc = 1
.head 6 +  If NOT SalIsNull (dfPpl)
AND NOT SalIsNull (dfRc)
AND NOT SalIsNull (dfNc)
AND NOT SalIsNull (dfMc)
.head 7 -  ! ------------------------------------ Очистка таблицы
.head 7 -  Call SalTblReset( tblPlan )
.head 7 -  Set dfMN = dfNc*dfMc
.head 7 -  ! ------------------------------------ Вычисляем CF по  FV, PV, n,r
.head 7 -  Set dfCFc = SalNumberAbs( PMT ( dfRc/dfMc, dfMN, dfPpl, 0))
.head 7 -  Set i=1
.head 7 -  Set nInt=0
.head 7 -  Set nBal = dfPpl
.head 7 +  While (i <= dfMN -1)
.head 8 -  Call SalTblInsertRow (tblPlan, i)
.head 8 -  Call SalTblSetContext( tblPlan, i )
.head 8 -  Set tblPlan.colNper = i
.head 8 -  Set tblPlan.colPpl = IPMT ( dfRc/dfMc, i-1, dfMN, dfCFc )
.head 8 -  Set nBal = nBal - tblPlan.colPpl
.head 8 -  Set tblPlan.colBal = nBal
.head 8 -  Set tblPlan.colPint = dfCFc - tblPlan.colPpl
.head 8 -  Set tblPlan.colDebt = dfPpl - tblPlan.colBal
.head 8 -  Set nInt = nInt + tblPlan.colPint
.head 8 -  Set tblPlan.colInt = nInt
.head 8 -  Set tblPlan.colCF = tblPlan.colPint + tblPlan.colPpl
.head 8 -  Set i = i+1
.head 7 -  Call SalTblSetContext( tblPlan, i)
.head 7 -  Call SalTblInsertRow (tblPlan, i)
.head 7 -  Set tblPlan.colNper = i
.head 7 -  Set tblPlan.colBal = 0
.head 7 -  Set tblPlan.colPpl = nBal
.head 7 -  Set tblPlan.colPint = dfCFc - tblPlan.colPpl
.head 7 -  Set tblPlan.colDebt = dfPpl
.head 7 -  Set nInt = nInt + tblPlan.colPint
.head 7 -  Set tblPlan.colInt = nInt
.head 7 -  Set tblPlan.colCF = tblPlan.colPint + tblPlan.colPpl
.head 7 -  Call SalTblSetFlagsAnyRows( tblPlan, ROW_New, FALSE, ROW_New, 0 )
.head 7 -  ! Call SalTblSetRowFlags( tblPlan, i, ROW_New, FALSE )
.head 6 +  Else
.head 7 -  Call SalMessageBox( 'Заполните все исходные данные!', 'Ошибка',
       MB_Ok | MB_IconExclamation)
