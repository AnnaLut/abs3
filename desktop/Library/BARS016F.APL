							If fKV
								Set HAV=
										"    nvl(sum(fdos(d.acc,:DAT1,:DAT2)),0)<>0 OR
										     nvl(sum(fdos(decode(i.acc,i.acra, 0,i.acra),:DAT1,:DAT2)),0)<>0 OR
										     nvl(sum(fkos(d.acc,:DAT1,:DAT2)),0)<>0 OR
