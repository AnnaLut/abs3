												     sum(iif_d(a.daos,:DAT1,iif_d(a.dazs,:DAT1,0,1,1),0,0))<>0 OR
												     sum(iif_d(a.daos,:DAT1,0,1,iif_d(a.daos,:DAT2,1,1,0)))<>0 OR
												     sum(iif_d(a.dazs,:DAT1,0,1,iif_d(a.dazs,:DAT2,1,1,0)))<>0 "
									If tbl1.sSELE = 'd.branch, b.name, '
