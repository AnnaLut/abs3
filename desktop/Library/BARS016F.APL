			Function: setViddBonus
				Description:
				Returns
					Boolean:
				Parameters
					Number: p_bonusid
					Number: p_typeid
					Number: p_rang
					Number: p_activity
					Long String: p_condition
					Number: p_finally
				Static Variables
				Local variables
					Boolean: bReturn
					String: l_activity
					String: l_finally
				Actions
					Set bReturn = TRUE
					Set l_activity = IifS(p_activity=1, 'Y', 'N')
					Set l_finally  = IifS(p_finally=1, 'Y', 'N')
					If not SqlPLSQLCommand(hSql(), "dpt_bonus.manage_dptviddbonus(p_bonusid, p_typeid, p_rang, l_activity, p_condition, l_finally)")
						Set bReturn = FALSE
